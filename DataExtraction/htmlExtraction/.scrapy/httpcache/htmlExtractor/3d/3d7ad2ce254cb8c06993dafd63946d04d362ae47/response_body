			jQuery(document).ready(function()
			{
				jQuery('select[name=limit]').change(function()
				{
					var limit = jQuery(this).children('option:selected').val();
				
					jQuery('select[name^=filter]').each(function()
					{
						var optionVal = jQuery(this).children('option:selected').val();
						if(optionVal != '')
						{
							limit += '&filter[]=' + optionVal;
						}
					});

					if(jQuery('input[name=searchInput]').val() != '')
					{
						limit += '&search=' + jQuery('input[name=searchInput]').val();
					}

					var address = constants.siteAddress + '/^limit=' + limit;
					window.location = address;
				});
			});
		</script>
