                <script type="text/javascript">
                    $('#site-login-opener').click(function() {
                        if($(this).hasClass('expanded'))
                        {
                            $(this).addClass('collapsed').removeClass('expanded');
                            $(this).css('backgroundImage','url(/themes/vidrimolde/css/imagens/botao_areareservada0.png)');
                        }
                        else
                        {
                            $(this).addClass('expanded').removeClass('collapsed');
                            $(this).css('backgroundImage','url(/themes/vidrimolde/css/imagens/botao_areareservada1.png)');
                        }
                        $('#site-login-form').slideToggle('slow');
                        $('#site-login-wrapper').css('background', 'none');
                    });
                </script>

