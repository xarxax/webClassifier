<script type="text/javascript">
    if (null != window.optimizely) window.optimizely.push(["activate"]);

    function clearIframeSrc() {
        if (document.getElementById('iframecres')) {
            if(!!document.getElementById('iframecres').contentWindow)
                document.getElementById('iframecres').contentWindow.location.src = '';
        }
    }
    function clearSignIn() {
        if(!!document.getElementById("reorderSignedOut")){document.getElementById("reorderSignedOut").style.display = "none";}
        if(!!document.getElementById("reorderSignedIn")){document.getElementById("reorderSignedIn").style.display = "block";}
    }
    function CloseModalPopUp() {
        $('.modal_wrapper').removeClass('active');
        $('#idtheftbasiccustomizecontainer').css({ 'display': 'none' });
        $('#idtheftconfirmationcontainer').css({ 'display': 'none' });
        $('#idtheftdowngradecontainer').css({ 'display': 'none' });
    }
    function getCookie(c_name) {
        var i, x, y, ARRcookies = document.cookie.split(";");
        for (i = 0; i < ARRcookies.length; i++) {
            x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
            y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
            x = x.replace(/^\s+|\s+$/g, "");
            if (x == c_name) {
                return unescape(y);
            }
        }
    }

    function footerSearchFormSubmit(event) {
        event & event.preventDefault();
        var savedSearchQuery = sessionStorage.getItem("IMSearchParams");
        clearIframeSrc();
        if(window.location.pathname == "/search") {
            return;
        }
        window.footerSearchQuery = {
            searchText : $("#txtSearchFt").val().trim(),
            currentUrl : window.location.href
        };
        window.trackInfo = {
            currentUrl : window.location.href,
            currentPageTitle : document.title,
            userAgt : navigator.userAgent.toLowerCase(),
            language : navigator.language,
            screenWidth : screen.width,
            screenHeight : screen.height
        };
        if(!!savedSearchQuery) {
            sessionStorage.removeItem("IMSearchParams");
        }
        Backbone.history.navigate("/search", true);
        return;
    }

    document.getElementById("IMFooterSearchForm").addEventListener("submit", footerSearchFormSubmit, false);
    document.getElementById("IMFooterSearchFormSubmit").addEventListener("click", footerSearchFormSubmit, false);


</script>
