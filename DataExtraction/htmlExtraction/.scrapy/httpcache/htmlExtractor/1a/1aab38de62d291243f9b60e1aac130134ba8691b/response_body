    jQuery().ready(function ($) {

        function HoverOverd2dcef1640() {

            $(this).addClass('rmhover').find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutd2dcef1640() {
            $(this).removeClass('rmhover').find(".subMenu:first").animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        function HoverOversubd2dcef1640() {
            $(this).find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutsubd2dcef1640() {
            $(this).find(".subMenu:first").stop().animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        var configd2dcef1640 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOverd2dcef1640,
            timeout: 1000,
            out: HoverOutd2dcef1640
        };
        var configsubd2dcef1640 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOversubd2dcef1640,
            timeout: 1000,
            out: HoverOutsubd2dcef1640
        };
        $("#gomenud2dcef1640 li.dir").hoverIntent(configd2dcef1640);
        $("#gomenud2dcef1640 .subMenu li.dir").hoverIntent(configsubd2dcef1640);
