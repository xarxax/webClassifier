<!-- JW PLAYER SETUP -->

<script type="text/javascript">
              function runVideo(vurl) {
				 // mygallery.navigate(mygallery.setting.imgindex);
				 //alert(vurl);

				var containerHeight = $('html').height();
                      $("#video_overlay").css({ 'display': '' });
                      $("#video_overlay").css({ 'height': containerHeight + 'px' });
                      $("#video_container").css({ 'display': '' });
                      
                      $("#video_overlay").show();
                      $("#video_container").show();
                      $("#close").css({ 'display': '' });
                      $("#close").show();
					  
					  var bQuarterPlayback, bHalfPlayback, b3QuarterPlayback;
					  //Player setup
                      jwplayer("media_player").setup({
						skin: "bekle",
						playlist: 'http://content.jwplatform.com/feed/kwr9G3G5.rss',
						//file:"https://videos-jp.jwpsrv.com/zxweQGqL/videos/8AqprInw-6341239.mp4?77c801d752d5207784c49e7ed80fb953798fae0fcca03ecf79558796aa72b609e2d045414f10b766e7162e9ab5cbcb69d65d8b39fec285bc00e81e4021859534231e1624e4032ba51d71ee00721a3f0b26a1067155",
						width: '100%',
						height: '100%',
						fallback: 'false',
						autostart: 'true',
						primary: 'flash',
						events: {
                        	onPlay: function (event) {
                            	dcsMultiTrack('WT.dl', '110', 'WT.clip_t', 'Video', 'WT.clip_n', vurl, 'WT.clip_ev', 'p');
							  	//AMO Updatde:Fix for INC700013661231 Dated:9th Aug 2011
								SetTimeOt();
								},
								onPause: function (event) {
								},
							  onComplete: function (event) {
								  dcsMultiTrack('WT.dl', '110', 'WT.clip_t', 'Video', 'WT.clip_n', vurl, 'WT.clip_ev', 'f');
							  },
							  onTime: function (event) {
								  if ((Math.floor(event.position) / Math.floor(event.duration)) >= 0.25) {
									  if (!bQuarterPlayback) {
										  dcsMultiTrack('WT.dl', '110', 'WT.clip_t', 'Video', 'WT.clip_n', vurl, 'WT.clip_ev', '25');
										  bQuarterPlayback = true;
									  }
								  }
								  if ((Math.floor(event.position) / Math.floor(event.duration)) >= 0.5) {
									  if (!bHalfPlayback) {
										  dcsMultiTrack('WT.dl', '110', 'WT.clip_t', 'Video', 'WT.clip_n', vurl, 'WT.clip_ev', '50');
										  bHalfPlayback = true;
									  }
								  }
								  if ((Math.floor(event.position) / Math.floor(event.duration)) >= 0.75) {
									  if (!b3QuarterPlayback) {
										  dcsMultiTrack('WT.dl', '110', 'WT.clip_t', 'Video', 'WT.clip_n', vurl, 'WT.clip_ev', '75');
										  b3QuarterPlayback = true;
									  }
								  }
							  },
				  onPlaylistItem: function (event) {
				  }
				  
			  }
		  });
					  
		      //AMO Updatde:Fix for INC700013661231 Dated:9th Aug 2011
              function SetTimeOt(obj) 
			  {
			 	player = document.getElementsByName(obj.id)[0];
                setTimeout("player.sendEvent('PLAY', 'true')", 100);
              } 
					  
			  }
			  //document.getElementById("media_player").error.code
      </script>

<!-- END OF JWPLAYER SETUP --> 


