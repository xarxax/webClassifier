	document.addEventListener( 'wpcf7mailsent', function( event ) {
		console.log('contact form submitted');
	    ga( 'send', 'event', 'Contact Form', 'submit' );
	}, false );
	</script>
			<script type="text/javascript">

		// Contact Us form
		function initialiseContactFormTrigger(){
		    jQuery(document).ready(function(){
		        jQuery(".wpcf7-submit").each(function(){
		            jQuery(this).on('click',function(){
		                ga('send', 'event', 'ContactUsPageForm', 'submit', 'Contact Us form submitted');
		            });
		        });
		    });
		}

		// Expertise pages upper form
		function initialiseExpertiseUpperFormTrigger(){
		    jQuery(document).ready(function(){
		        jQuery(".wpcf7-submit:first").each(function(){
		            jQuery(this).on('click',function(){
		                ga('send', 'event', 'ContactUsMainForm', 'submit', 'Expertise upper form submitted');
		            });
		        });
		    });
		}

		// Expertise pages upper form
		function initialiseExpertiseLowerFormTrigger(){
		    jQuery(document).ready(function(){
		        jQuery(".wpcf7-submit:last").each(function(){
		            jQuery(this).on('click',function(){
		                ga('send', 'event', 'ContactUsLowerForm', 'submit', 'Expertise lower form submitted');
		            });
		        });
		    });
		}

		// Secure File Upload form
		function initialiseUploadFormTrigger(){
		    jQuery(document).ready(function(){
		        jQuery('.jotform-form input[type="submit"]').each(function(){
		            jQuery(this).on('click',function(){
		                ga('send', 'event', 'Send Files', 'submit', 'Secure File Upload form submitted');
		            });
		        });
		    });
		}

		if(window.location.href.indexOf("expertise") > -1) {
			setTimeout(initialiseExpertiseUpperFormTrigger(),2000);
			setTimeout(initialiseExpertiseLowerFormTrigger(),2000);
		}
		else if(window.location.href.indexOf("secure-file-upload") > -1) {
			setTimeout(initialiseUploadFormTrigger(),2000);
		}
		else if(window.location.href.indexOf("contact") > -1) {
			setTimeout(initialiseContactFormTrigger(),2000);
		}

		</script>
