//<!--

function resetFrmVal(){	
	$('#vchBusiness').val('');
	$('#vchProduceType').val('');
	$('#vchWebsite').val('');
	$('#vchContactName').val('');
	$('#vchEmail').val('');
	$('#vchTelephone').val('');
	$('#vchProcessName').val('');
	$('#vchMonthlyVolume').val('');
	$('#txtComment').val('');
}

function checkform(theForm)
{
	 
	 var processVal = $('#enumCurrentProcess:checked').val();
	 var ProcessName = $('#vchProcessName').val();
	 //alert(val);
	arr = new Array();
	arr["theControl"]="vchBusiness,vchProduceType,vchWebsite,vchMonthlyVolume,vchContactName,vchEmail,vchTelephone,vchVarificationCode";
	arr["theMessage"]="business,type of product,website,monthly volume,contact name,email address,telephone,security code";
	arr["theempty"]="Y,Y,Y,Y,Y,Y,Y,Y";
	arr["theConfirmPassword"]=",,,,,,,";
	arr["theEmail"]=",,,,,Y,,";
	arr["theURL"]=",,Y,,,,,";
	arr["theNumeric"]=",,,,,,Y,";
	if(validateForm(theForm,arr,2)){
		if(processVal == "Y" && ProcessName == ""){
			alert("Please enter processor.");
			$('#vchProcessName').focus();			
			return false;
		}
		return true;
	}
	
	return false;
}

//-->
