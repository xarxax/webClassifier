<script> 

$(document).ready(main);

function isChrome() {
    return navigator.userAgent.indexOf("Chrome") >- 1;   
}
function main() {
    if(isChrome()) {
        $("a,h3,h4,h5,h6").addClass("for-chrome");
    }
}

$(document).ready(function() {

//TweenLite.to(".A", 0.5, {scale:0, alpha:0, ease:Back.easeIn});

    var tl = new TimelineMax({repeat:-1});

    tl.from(".A", 0.7, {scale:0, alpha:0, ease:Expo.easeOut});
    tl.from(".B", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".C", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".textBar1", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".D", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");

    tl.to(".slideshow1", 0.3, {scale:0.8, autoAlpha:0, ease:Expo.easeIn}, "+=6");

    tl.from(".E", 0.7, {scale:0, alpha:0, ease:Expo.easeOut});
    tl.from(".textBar2", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".H", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".F", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".G", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");

    tl.to(".slideshow2", 0.3, {scale:0.8, autoAlpha:0, ease:Expo.easeIn}, "+=6");

    tl.from(".I", 0.7, {scale:0, alpha:0, ease:Expo.easeOut});
    tl.from(".J", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".textBar3", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".K", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    tl.from(".L", 0.7, {scale:0, alpha:0, ease:Expo.easeOut}, "-=0.65");
    
    tl.to(".slideshow3", 0.3, {scale:0.8, autoAlpha:0, ease:Expo.easeIn}, "+=6");


	//tl.add(TweenLite.from(".A", 0.5, {scale:0, alpha:0, ease:Expo.easeOut}));
});

</script>

