<script>
jQuery.noConflict();
  (function($) {
    $.fn.parallax = function(options) {
    var windowHeight = $(window).height();
    // Establish default settings
    var settings = $.extend({
    speed: 0.15
  }, options);
  // Iterate over each object in collection
  return this.each(function() {
  // Save a reference to the element
  var $this = $(this);
  // Set up Scroll Handler
  $(document).scroll(function() {
    var scrollTop = $(window).scrollTop();
    var offset = $this.offset().top;
    var height = $this.outerHeight();
    // Check if above or below viewport
    if (offset + height <= scrollTop || offset >= scrollTop + windowHeight) {
    return;
  }
  var yBgPosition = Math.round((offset - scrollTop) * settings.speed);
  // Apply the Y Background Position to Set the Parallax Effect
  $this.css('background-position', 'center ' + yBgPosition + 'px');
  });
  });
  }

}(jQuery)); 
jQuery(document).ready(function($){
  $('#featured-video-play-btn').click(function(event) {
    /* Act on the event */
    $(this).addClass('hide');
    $('#featured-video-thumb').addClass('hide');
    $('.embed-container').removeClass('hide');
  });

    var stat_mod_time = $("#stat-mod-time").attr("data-mod"); 
  
    var dValue = $("#stat-mod-time").attr("data-mod");

    var d = new Date();
    var n = d.getTimezoneOffset()/60;


    var dateClientSide = new Date(dValue);
    var dateClientSideZone = dateClientSide.getTimezoneOffset()/60;
    var hours = dateClientSide.getHours();
    var minutes = dateClientSide.getMinutes();
    var offset = dateClientSide.setHours(dateClientSideZone + n);
    
    var newTime = new Date(offset);
    var currentTime = new Date().toLocaleTimeString();
    var c_hours = newTime.getHours(); 
    var c_minutes = newTime.getMinutes();
    c_minutes = minutes > 9 ? minutes : '0' + minutes;

    if(c_hours>12){
      var c_time = (newTime.getHours()-12) + ":" + c_minutes;
    } else {
      var c_time = newTime.getHours() + ":" + c_minutes;
    }
    
    if((c_hours >= 12)){
      c_time = c_time+' '+"PM";
    } else {
      c_time = c_time+' '+"AM";
    }

