		<!-- tracking -->		
		<script type="text/javascript">
			var navPos = $('#nav').offset().top;
			
			$(window).scroll(function(){
			
				var fixIT = $(this).scrollTop() >= navPos;
				var setPos = fixIT ? 'fixed' : 'relative' ;
				var logoSH = fixIT ? 'show' : 'hide' ;
				var logoDISP = fixIT ? 'addClass' : 'removeClass' ;
				var padit	 =  fixIT ? '30px' : '0' ;
				var mrgn	 =  fixIT ? '0px' : '' ;
				var navBG = fixIT ? 'show' : 'hide' ;
				
				$('#nav').css({position: setPos});
				//$('#mini-logo')[logoSH](50);
				$('#mini-logo')[logoDISP]('fixedNav');
				$('#nav').css({paddingTop: padit});
				$('a.btn_team').css({marginRight: mrgn});
				$('#bg')[navBG](50);
				console.log(fixIT);
			});
		</script>
