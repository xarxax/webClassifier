<script>
	var sourceSwap = function()
	{
		var $this = $(this);
		var newSource = $this.data('alt-src')
		$this.data('alt-src', $this.attr('src'));
        return newSource;
	}
	
	$(function() {
		$('li.topmenu2grey').hover(function(){
			$(this).find('a > img').attr('src', sourceSwap);
		}, function() {
			$(this).find('a > img').attr('src', sourceSwap);
		})
	});
</script>
