function setCookie(name, value) {
	var now = new Date();
	var date = new Date(now.getTime() + 30*24*60*60*1000);
	var cookieDate = date.toGMTString();
	var orealCookie = name + "=" + escape(value) + ";expires=" + cookieDate;
	document.cookie = orealCookie;
	//alert(orealCookie);
}

function readCookie(name) {
	var cookies = document.cookie;
	var start = cookies.indexOf(name + "=");
	if(start == -1) {
		//alert("cookie not found");
	}
	start = cookies.indexOf("=", start) + 1;
	var end = cookies.indexOf(";", start);
	if(end == -1) {
		end = cookies.length;
	}

	var value = unescape(cookies.substring(start,end));
	return value;
}

function jumptoPage() {
	if(document.form1.choice[0].checked == false && document.form1.choice[1].checked == false) {
		alert("Select either Medical or Dental to enter the site.");
	}
	else {
	if(document.form1.choice[0].checked) {
		if(document.form1.goaway.checked)
			setCookie("remember", "medical");
		else
			document.cookie = "";
		document.location.href="index_med.htm";
	}
	else if(document.form1.choice[1].checked) {
		if(document.form1.goaway.checked)
			setCookie("remember", "dental");
		else
			document.cookie = "";
		document.location.href="index_den.htm";
	}
}
}


var whereto = readCookie("remember");

if(whereto == "medical") {
	setCookie("remember", "medical");
	//alert("Going medical!");
	document.location.href="index_med.htm";
}
else if(whereto == "dental") {
	setCookie("remember", "dental");
	//alert("Going dental!");
	document.location.href="index_den.htm";
}

