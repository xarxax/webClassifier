	$("#aodoodle").css("opacity", 1.0);
	$("#menu_wrapper").css("left", realleft+ doodlewidth +"px");
	$("#menu_wrapper").css("width",middlewidth-doodlewidth + "px");
	$("#menu_wrapper").css("visibility", "visible");
