<script>
    $(function () {
        function launch() {
            $('.main').lightbox_me({ centered: true, onLoad: function () { $('.main').find('input:first').focus() } });
        }

        $('.userLogin').click(function (e) {
            $(".main").lightbox_me({ centered: true, onLoad: function () {
                $(".main").find("input:first").focus();
            }
            });

            e.preventDefault();
        });

        $('table tr:nth-child(even)').addClass('stripe');
    });
    $(function () {
        function launch() {
            $('.register').lightbox_me({ centered: true, onLoad: function () { $('.register').find('input:first').focus() } });
        }

        $('.userRegister').click(function (e) {
            $(".register").lightbox_me({ centered: true, onLoad: function () {
                $(".register").find("input:first").focus();
            }
            });

            e.preventDefault();
        });

        $('table tr:nth-child(even)').addClass('stripe');
    });
