    <script type="text/javascript">
       
        var sessionTimeout = 20;
        var sessionExpiresAt = "1/28/2019 5:27:22 PM";
        var sessionCartItemCount =  0;
       // alert(sessionTimeout);
        var approximateStart = new Date();
        var timeoutWarning =  1;
    var notifyAfter = new Date(approximateStart.getTime() + ((sessionTimeout - timeoutWarning)*60000)); //shows timeout warning at 1 minutes before timeout
        //TO DO: timeout is not repeating?????

        function countDown() {
            //alert("in countdown");
            setTimeout(function () {
                var now = moment();
               // alert(now + ":" + moment(sessionExpiresAt) + ":" + moment(now).isAfter(moment(sessionExpiresAt)));
               // document.getElementById('countDown').style.display = 'inline';
                document.getElementById('countDown').innerHTML = "Countdown: " + now;

                if (moment(now).isAfter(moment(sessionExpiresAt))) {
                   // alert("test1")
                    window.location("/Home/SessionTimeout");
                }
               // else if (new Date(now) >= new Date(notifyAfter)) {
                    //alert("test");
                    //jQuery(function () {
                    //    $('#myModalWarning').modal('show');
                    //}); (jQuery);
               // }

                countDown();
            }, 5000); //repeat every 5 seconds
        }
        if (sessionTimeout != null && sessionCartItemCount > 0) {

            countDown();

        }


    </script>
