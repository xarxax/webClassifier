                        <script language="javascript" type="text/javascript">
                            $("#ctl00_UcSide1_btnCHTSubmit").click(function () {
                                if ( $(".ddlCHTCountry").val() == "USA" || $(".ddlCHTCountry").val() == "CAN") {
                                    if ($(".ddlCHTCountry").val() == "USA") {
                                        var regex = new RegExp("^\\d{5}$");
                                        var qs = regex.exec($("#ctl00_UcSide1_txtCHTZipPostal").val());
                                        
                                        if (qs == null) {
                                            $(".dist-search-error").html("Invalid Zip Code");
                                            return false
                                        }
                                        else {
                                            if (qs.length == 0 ) {
                                                $(".dist-search-error").html("Invalid Zip Code");
                                                return false
                                            }
                                            else {
                                                return true
                                            }
                                        }
                                    } 
                                    else {
                                        var regex = new RegExp("^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]\\d[A-Za-z] \\d[A-Za-z]\\d$");
                                        var qs = regex.exec($("#ctl00_UcSide1_txtCHTZipPostal").val());
                                        
                                        var regex2 = new RegExp("^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]\\d[A-Za-z]\\d[A-Za-z]\\d$");
                                        var qs2 = regex2.exec($("#ctl00_UcSide1_txtCHTZipPostal").val());
                                                                            
                                        if (qs == null && qs2 == null) {
                                            $(".dist-search-error").html("Invalid Postal Code");
                                            return false
                                        }
                                        else {
                                            if (qs.length == 0 ) {
                                                $(".dist-search-error").html("Invalid Postal Code");
                                                return false
                                            }
                                            else {
                                                return true
                                            }
                                        }
                                    
                                    }
                                    return false;
                                }    
                            });
                        </script>

