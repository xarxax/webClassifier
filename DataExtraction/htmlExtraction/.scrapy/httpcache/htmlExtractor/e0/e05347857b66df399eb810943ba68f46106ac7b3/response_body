<script type="text/javascript">
    function navigateBrand1()
    {
        var selectedValue = "";
        var selectedValue1 = "";
        var search1 = "";
        var search2 = "";
        var radioButtons = document.getElementsByName('drop_1');
        var radioButtons1 = document.getElementsByName('drop_2');
        for (var x = 0; x < radioButtons.length; x++)
        {
            if (radioButtons[x].checked)
            {
                selectedValue += radioButtons[x].value + "-";
            }
        }
        for (var x1 = 0; x1 < radioButtons1.length; x1++)
        {
            if (radioButtons1[x1].checked)
            {
                selectedValue1 += radioButtons1[x1].value + "-";
            }
        }

        selectedValue = selectedValue.slice(0, -1);
        selectedValue1 = selectedValue1.slice(0, -1);
        //alert(selectedValue);
        //alert(selectedValue1);

        //if(selectedValue=="")
        //{
        //return false;
        //}
        if (selectedValue != "")
        {
            var selectedValueArr = selectedValue.split("-");
            var searchvar = "refine_search1[]=";


            for (var y = 0; y < selectedValueArr.length; y++)
            {
                search1 += searchvar + selectedValueArr[y] + "&";
            }
            search1 = search1.slice(0, -1);
            //alert(search1);
        }

        //if(selectedValue1=="")
        //{
        //return false;
        //}
        if (selectedValue1 != "")
        {
            var selectedValueArr1 = selectedValue1.split("-");
            var searchvar1 = "refine_search2[]=";
            //var search2 = "";

            for (var y1 = 0; y1 < selectedValueArr1.length; y1++)
            {
                search2 += searchvar1 + selectedValueArr1[y1] + "&";
            }
            search2 = search2.slice(0, -1);
            //alert(search2);
        }
        if ((search1 != "") && (search2 != ""))
        {
            var url = search1 + "&" + search2;
        }
        if ((search1 != "") && (search2 == ""))
        {
            var url = search1;
        }
        if ((search1 == "") && (search2 != ""))
        {
            var url = search2;
        }
        if ((search1 == "") && (search2 == ""))
        {
            return false;
        }
        else
        {
            window.location = "https://www.quantumrlv.com.au/catalogsearch/advanced/result/?" + url;
        }
    }

    function navigateBrand2()
    {
        var selectBox = document.getElementById("drop_2");
        var selectedValue = selectBox.options[selectBox.selectedIndex].value;
        if (selectedValue == "")
        {
            return false;
        }
        else
        {
            window.location = "https://www.quantumrlv.com.au/catalogsearch/advanced/result/?" + "refine_search2[]=" + selectedValue;
        }
    }
</script>
<!--Footer End-->

