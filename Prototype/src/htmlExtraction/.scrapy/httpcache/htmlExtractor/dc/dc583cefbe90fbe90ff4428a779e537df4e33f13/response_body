        <script type="text/javascript" language="javascript">

            function getParameterByName(name, url) {
                if (!url) {
                    url = window.location.href;
                }
                name = name.replace(/[\[\]]/g, "\\$&");
                var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
        results = regex.exec(url);
                if (!results) return null;
                if (!results[2]) return '';
                return decodeURIComponent(results[2].replace(/\+/g, " "));
            }


            function freetrialpopbasedonblog() {
                           
                var url = window.location.href;
                if (url.indexOf("_ref") != -1) {
                    var findblog = getParameterByName("_ref", url);

                    if (findblog == "ftblog") {
                        decreasePopUpHide();
                        $('#ifmFreetrial').attr("src", "free-trial-popup.aspx");
                        $("#divFreetrialIframe, ._ftbackdrop").css("display", "block");
                        setTimeout(function () { $("#loadimg").hide(); }, 2000);
                    }
                }
            }


            function testit() {

                $(document).ready(function () {

                    freetrialpopbasedonblog();

                    $('.main-container').click(function (e) {
                        setTimeout(function () { manageHotSpot(); }, 500);
                    });

                    $('#loginBtn').on('click', function () {
                        setTimeout(function () { manageHotSpot(); }, 500);
                    });


                    $('#forgot_btn').on('click', function () {

                        if ($(this).hasClass("toggle")) {
                            $('.forgotP').show();
                            $("#ctl00_Login1_txtforgotPassword").val("");
                            $("#ctl00_Login1_txtforgotPassword").attr("autocomplete", "off");
                            $(this).removeClass("toggle");
                        }
                        else {
                            $(this).addClass("toggle");
                            $('.forgotP').hide();
                        }

                    });
                    $('#canforgotBtnp').on('click', function () {
                        $('#forgot_btn').addClass("toggle");
                        $('.forgotP').hide()
                    });
                    $('.signInner').delegate('#cansignBtnp', 'click', function () {
                        // alert('out');
                        $('.sign-in').hide()

                        manageHotSpot();
                    });

                    var geturl = $(location).attr("pathname").replace("/", "");
                    //   alert(geturl);

                    if (geturl == "freetrial.aspx") {

                        $("#onclickevent").attr("onclick", "");
                        $("#loginBtn").unbind("click");
                        $("#loginBtn").attr("href", "login.aspx")

                    }

                });


                $("#ctl00_Login1_ibntSubmit").click(function () {
                    $("#ctl00_Login1_errmsgouter").hide(500);
                    var username = $("#ctl00_Login1_txtforgotPassword").val().trim().toLowerCase();
                    if (username.length == 0 || username == "") {
                        CommonOnPage2('Please enter your email address');
                        return false;
                    }
                    else {

                        $("#ctl00_Login1_errmsgouter").hide(500);
                        $("#ctl00_Login1_ibntSubmit").css("pointer-events", "none");
                        $("#ctl00_Login1_lblProgMsg").css("display", "block");
                        $("#ctl00_Login1_lblProgMsg").text('Please wait, we are processing your request..');         
                      //  $("#ctl00_Login1_ibntSubmit").attr("disabled", "disabled");
                    }
                });

                $("#divfreetrial a, .freetrial a, #digitalLibFreeTrial").click(function () {
                    $(".newyearPOp").hide();
                    $("#loadimg").show();
                    setTimeout(function () { $("#loadimg").hide(); }, 2000);

                    decreasePopUpHide();

                    $("#divFreetrialIframe, ._ftbackdrop").css("display", "block");
                    $('#ifmFreetrial').attr("src", "free-trial-popup.aspx");

                });



                function CommonOnPage2(msg) {
                    $("#ctl00_Login1_errmsgouter").show();
                    $("#ctl00_Login1_errmsg").text(msg);
                    $("#ctl00_Login1_errmsg").hide().slideDown('medium');
                }

            }

            function parentFreeTrialClosePopUp() {
                $("#divFreetrialIframe, ._ftbackdrop").css("display", "none");
            }

            function increasePopUpHide() {
                $("._ftp").css("height", "470px");
            }

            function decreasePopUpHide() {
                $("._ftp").css("height", "383px");
            }

            function decreasePopUpAfterSubmission() {
                $("._ftp").css("height", "250px");
            }

            function manageIncreasePopUpScroll() {
                $("._ftp").css("height", "600px");
                $("._ftp").addClass("_ftn");
            }

            function redirectthanks() {
                $('#ifmFreetrial').attr("src", "");
                $('#ifmFreetrial').attr("src", "free-trial-thanks.aspx");
            }

            function manageDecreasePopUpScroll() {
                $("._ftp").removeClass("_ftn");
                $("._ftp").css("height", "470px");
            }

            function fillUserMasterDetails(u, p, isChecked) {
                $("#ctl00_Login1_UserName").val(u);
                $("#ctl00_Login1_Password").val(p);

                if (isChecked == "true") {
                    $("#ctl00_Login1_chkRememberMe").prop("checked", true);
                }
                else {
                    $("#ctl00_Login1_chkRememberMe").prop("checked", false);
                }
            }
            function manageHotSpot() {

                var display = $("#divLoginPanel").css("display")
                if (display == "block") {
                    if ($("#hs-analytics").attr("src") == '//js.hs-analytics.net/analytics/1496844300000/2733749.js') {
                        $("#hs-analytics").attr("src", '');
                    }
                    if ($("#messages-2733749").attr("src") == 'https://api.usemessages.com/messages/v2/embed/2733749.js') {
                        $("#messages-2733749").attr("src", '');
                    }

                    if ($("#CollectedForms-2733749").attr("src") == 'https://js.hscollectedforms.net/collectedforms.js') {
                        $("#CollectedForms-2733749").attr("src", '');
                    }

                    if ($("#hs-script-loader").attr("src") == '//js.hs-scripts.com/2733749.js') {
                        $("#hs-script-loader").attr("src", '');
                    }

                }
                else {
                    $("#hs-analytics").attr("src", '//js.hs-analytics.net/analytics/1496843100000/2733749.js');
                    $("#messages-2733749").attr("src", 'https://api.usemessages.com/messages/v2/embed/2733749.js');
                    $("#CollectedForms-2733749").attr("src", 'https://js.hscollectedforms.net/collectedforms.js');
                    $("#hs-script-loader").attr("src", '//js.hs-scripts.com/2733749.js');
                }
            }
        </script>
