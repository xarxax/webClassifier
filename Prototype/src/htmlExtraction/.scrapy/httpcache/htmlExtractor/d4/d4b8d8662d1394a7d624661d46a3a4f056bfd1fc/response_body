<script language='javascript'>
	var loc1
	var locabstract
	loc1=document.location.href
	loc1=loc1.split("/")
	locabstract=loc1[loc1.length-2]
	loc1=loc1[loc1.length-1]

	//alert(locabstract)

	function movetofrenchpage(){
		var locf
		if (loc1=="index.asp" || loc1==""){
			locf='https://www.visserproduce.com/index_es.asp';
		}

		else {
			locf='https://www.visserproduce.com/espanol/'+loc1
		}

		//alert(locf);		

		document.location.href=locf

	}
	
</script>

  
