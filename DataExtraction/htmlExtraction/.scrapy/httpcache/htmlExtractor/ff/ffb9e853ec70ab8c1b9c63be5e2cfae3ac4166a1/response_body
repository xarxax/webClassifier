function validate() {
	
	var minWords	= 3;
	
	if($("#search").val()=="") {
		alert("โปรดกรอกคำค้นที่นี่");		
		$("#search").focus();
		return false;
		}	
		
	if($("#search").val().length<minWords) {
		alert("กรุณากรอกคำค้นมากกว่า "+minWords+" ตัวอักษร");
		$("#search").focus();
		return false;
		}
	}
</script>
