<script type="text/javascript">
    $(document).ready(function () {
        if ($('#dvVideoView_JQYTV').closest("form")[0]) { /* iOS devices fix  */
            $('#dvVideoView_JQYTV').insertAfter($('#dvVideoView_JQYTV').closest("form").children().last());
        }

        $('.dvVideoView_JQYTV').css('opacity', '0.9');
        $('.dvVideoView_JQYTV').on('click', function () {
            $('#dvVideoView_JQYTV').css('display', 'none');
            $('#frmYTvideo_JQYTV').remove();
        });

        $('.imgYTvideos_JQYTV').each(function () {
            if ($(this).attr('data-vidid')) {
                if ($(this).is('img')) {
                    $(this).css({
                        'cursor': 'pointer',
                        'border': '3px solid #4E89E3'
                    });

                    $(this).hover(function () {
                        $(this).css('border', '3px solid red');
                    }, function () {
                        $(this).css('border', '3px solid #4E89E3');
                    });
                }

                var vidAutoPlay = ($(this).attr('data-vidautoplay') ? ($(this).attr('data-vidautoplay') == '1' ? 1 : 0) : 0);
                var vidID = 'http://www.youtube.com/embed/' + $(this).attr('data-vidid') + '?rel=0' + String.fromCharCode(38) + 'fs=1' +String.fromCharCode(38) + 'wmode=transparent' + String.fromCharCode(38) + 'autoplay=' + vidAutoPlay;
                $(this).on('click', function (e) {
                    e.preventDefault ? e.preventDefault() : e.returnValue = false;
                    e.stopPropagation ? e.stopPropagation() : e.cancelBubble = true;

                    $('<iframe />', {
                        id: 'frmYTvideo_JQYTV',
                        name: 'frmYTvideo_JQYTV',
                        style: 'border: 5px solid #b07d00; background-color: black;',
                        src: vidID
                    }).appendTo('#dvVideoView_frameHolder_JQYTV');

                    $('#frmYTvideo_JQYTV').attr({
                        width: '420',
                        height: '315',
                        frameborder: '0',
                        wmode: 'Opaque'
                    });
                    $('#dvVideoView_JQYTV').css('display', '');
                });
            }
        });
    });
</script>
