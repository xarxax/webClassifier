<script>

$("#job_id").on('input', function() {
   	var helloCont = $("#job_id").val();
	if (helloCont == '') {
    	$('#job_list_id').hide();
	}
});


$("#country_id").on('input', function() {
   	var helloCount = $("#country_id").val();
	if (helloCount == '') {
    	$('#country_list_id').hide();
	}
});

$('body').click( function() {
	$('#job_list_id').hide();
	$('#country_list_id').hide();

});

