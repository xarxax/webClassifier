<script>
  $(function(){
	$('.nav-sub-toggle').on('click', function(){
	  if ($(this).siblings('.uk-nav-sub').hasClass('closed')) {
		$(this).siblings('i').removeClass('uk-icon-chevron-right').addClass('uk-icon-chevron-down');
		$(this).siblings('.uk-nav-sub').removeClass('closed');
	  } else {
		$(this).siblings('i').removeClass('uk-icon-chevron-down').addClass('uk-icon-chevron-right');
		$(this).siblings('.uk-offcanvas-bar .uk-parent .uk-nav-sub').addClass('closed');
	  }
	});
  });
</script>

