         $('#gomenucef6cc0e07').mobile_menu({
             slidingSubmenus: true,
             counters: true,
             navbartitle: "Menu",
             headerbox: ".menu_header",
             footerbox: ".menu_footer"			 
         });
		 
		  $('#gomenucef6cc0e07').find("a[href='javascript:;']").on("click",function () {
			  $('#gomenucef6cc0e07').find(".mm-close.mm-btn").click();
		  })
     });
</script>


 
