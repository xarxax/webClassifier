    $(document).ready(function(){                    
        $(window).scroll(function(){                          
            if ($(this).scrollTop() > 210) {
                $('#menu').fadeIn(100);
            } else {
                $('#menu').fadeOut(100);
            }
        });
    });
})(jQuery);
	
</script>
