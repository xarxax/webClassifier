	<script>
		// set banner image to appropriate size
		function sizeHomeBanner(){
			// video fill width requires a min-width of 100%; 
			var $divs = document.querySelectorAll(".home-block"),
				$viewX = Math.max(document.documentElement.clientWidth, window.innerWidth || 0),
				$viewY = Math.max(document.documentElement.clientHeight, window.innerHeight || 0),
				$headerHeight = document.getElementById('header-nav').clientHeight;
				if( $viewX < 500 ){
					$viewY = '700px';
					$minstyle = "min-height:100%";					
				}
				else{
					$viewY = ( $viewY - $headerHeight ) + 'px';
					$minstyle = "min-width:100%";
				}
			document.getElementById('banner_video').setAttribute("style", $minstyle );
			if( $divs.length > 0 ){
				for (var i = 0, len = $divs.length; i < len; i++) {
	  				var $div = $divs[i];
					$div.setAttribute("style", "height:" + $viewY + "; width:" + $viewX + "px");
				}
	
			}
		}
		sizeHomeBanner();
		if( window.addEventListener ){
			window.addEventListener("resize", function(){
				headerOffset();
				sizeHomeBanner;
			});
		}
		else{
			window.attachEvent("resize", function(){
				headerOffset();
				sizeHomeBanner;
			});
		}
	</script>
