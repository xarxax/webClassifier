<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery(".wysija-submit").click(function() {
      
        var name = jQuery('input[name="wysija[user][firstname]"]').val();
        var email = jQuery( 'input[name="wysija[user][email]"]').val();
        var adminURL = jQuery('.shortex_adminurl').val();
                
        var datapot = "action=shortex_savesubscriber&name=" + name + "&email=" + email;
        jQuery.ajax({
                        type: "POST",
                        url: adminURL,
                        data: datapot,
                        success: function (data) {
					jQuery('.shortcode_wysija').submit();
                        }
                    });
        return false;
    });
    
});
</script>
