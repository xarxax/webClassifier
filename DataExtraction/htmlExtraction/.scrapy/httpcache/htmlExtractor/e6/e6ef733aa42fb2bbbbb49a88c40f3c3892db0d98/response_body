<script>
(function (jQuery, document, window) {
  jQuery('nav ul li ul li', this).css('display', 'none');
  jQuery('nav ul li').hover(  
    function () {  
      //show its submenu  
      jQuery('ul:first', this).css('display', 'block');  
    },   
    function () {  
      //hide its submenu  
      jQuery('ul:first', this).css('display', 'none');   
    }  
  );  
  
}(jQuery, document, window));
</script>

</body>
</html>