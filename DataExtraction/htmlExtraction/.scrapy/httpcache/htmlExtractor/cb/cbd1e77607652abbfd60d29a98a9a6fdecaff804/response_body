    function clean_options( id ) {
      while( jQuery(id+ " option").length > 1 ) {
            jQuery(id + " option:last").remove();
      }
    }
 
    function select_options( id, tab ) {
      clean_options( id ) ;
      jQuery.each(tab, function() {
        jQuery(id).append("<option value='"+jQuery(this).attr('id')+"'>"+jQuery(this).attr('name')+"</option>");
      });
    }

