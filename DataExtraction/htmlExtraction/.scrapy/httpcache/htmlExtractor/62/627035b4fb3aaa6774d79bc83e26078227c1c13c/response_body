<script>
	$(window).load(function(){
		event_each_a_height= 0
		$('.event_each a').each(function(){
			event_each_a_height = Math.max(event_each_a_height,$(this).height())
		}).height(event_each_a_height)
	})
	$(function(){
		$('#banner_box li').click(function(e){
			e.preventDefault()
			 href = $(this).find('a').attr('href')
			 if(href != undefined) window.location.href = href;

		})

		$('.banner_video_btn').click(function(e){
			e.preventDefault()
			// window.location.href="https://www.stc.group/en/about#video"
			window.location.href=$(this).data('url')
		})
		$('.inner_child').click(function(){
			window.location.href = $(this).data('link');
		});

		$('.index_tabs1 span:first-child').addClass('active');


		news_events = {
			init: function (){
					var news_events = this
					$('.index_tabs1 span').click(function(){
						$('.index_tabs1 span').removeClass('active')
						that = $(this)
						that.addClass('active')
						news_events.update(that.data('index'))

					})

					news_events.update(155) // ALL
			},

			update: function(index){
					 $.ajax({
			                url: 'https://www.stc.group/en/index/get_news_event/1'+'/'+index,
			                type:"GET",
			                dataType:'text',
			                beforeSend: function(text){
								$('.event_content').html('loading...')
							},

			                success: function(text){
			                	if(text!='') $('.event_content').html(text)
			                	else  $('.event_content').html('No Records')

			                },
			            	complete: function (){
			            		$("#desktop_search").css("min-height",$(document).height());
			            		// $('#mobile_menu, #desktop_search').height($(document).height());
			            		// $("head").append("<style>.search_result .container{min-height:"+$(document).height()+"px}</style>")

			            	}

			          })
			}



		}

		news_events.init();


		promotions = {

			init: function(){
					var promotions = this
					$('.index_tabs2 span').click(function(){
						$('.index_tabs2 span').removeClass('active')
						that = $(this)
						that.addClass('active')
						promotions.update(that.data('index'))
					})
					$('.index_tabs2 span:nth-child(1)').addClass('active')
					promotions.update(155)  // ALL
			},
			update : function (index){
					$.ajax({
			                url: 'https://www.stc.group/en/index/get_promotion'+'/'+index,
			                type:"GET",
			                dataType:'text',
			                beforeSend: function(text){
								$('.promotion_content').html('loading...')
							},

			                success: function(text){
			                	if(text!='') $('.promotion_content').html(text)
			                	else  $('.promotion_content').html('No Records')
			                	news_listingHeight = 0;
			                	$('.news_listing').each(function(){
			                		_this = $(this)
			                		news_listingHeight = Math.max(news_listingHeight,_this.height());

			                	}).height(news_listingHeight)
			                },
			            	complete: function (){
			            		$("#desktop_search").css("min-height",$(document).height());
			            		// $('#mobile_menu, #desktop_search').height($(document).height());
			            	}

			          })
			}

		}

		promotions.init()

		// $('.news_listing').each(function(){
		// 		if($(this).data('webpage_id')==$('.index_tabs2 span:first-child').data('index'))$(this).show()
		// 		else $(this).hide()
		// })





	})
</script>
            </div>
            
