    function writeDateJS() {
        var date = new Date();
        document.write(date.getMonth() + 1); //Months go from 0 to 11
        document.write("/");
        document.write(date.getDate());
        document.write("/");
        document.write(date.getUTCFullYear());
    }
</script>

