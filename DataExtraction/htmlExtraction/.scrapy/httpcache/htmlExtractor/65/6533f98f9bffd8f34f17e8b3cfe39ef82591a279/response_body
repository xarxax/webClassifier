    <script>
        $(document).ready(function () {

            //var touchsupport = ('ontouchstart' in window) ||
            //    (navigator.maxTouchPoints > 0) ||
            //    (navigator.msMaxTouchPoints > 0);

            var initialBarHeight = $(".medical").height();

            if (!Modernizr.touchevents) {
                document.documentElement.className += " non-touch";
            } else {

                $("#medical-wrapper .medical-label").on('click', function () {
                    
                    if ($("#test").hasClass("show-medical-content")) {
                        $("#test").removeClass("show-medical-content");
                        $(".medical").removeClass("onclick-top-parent");

                        $(".medical").css("height", initialBarHeight + "px");
                    } else {
                        var newHeight = window.innerHeight - initialBarHeight;  
                        
                        $("#test").addClass("show-medical-content");
                        $(".medical").addClass("onclick-top-parent");
                        window.scroll({
                            top: 0,
                            left: 0,
                            behavior: "smooth"
                        });

                        $(".medical").css("height", newHeight + "px");

                    }
                });


                $("#fertility-wrapper .fertility-label").on('click', function () {
                    if ($("#test1").hasClass("show-fertility-content")) {
                        $("#test1").removeClass("show-fertility-content");
                        $(".fertility").removeClass("onclick-top-parent");

                        $(".fertility").css("height", initialBarHeight + "px");
                    } else {
                        var newHeight = window.innerHeight;

                        $("#test1").addClass("show-fertility-content");
                        $(".fertility").addClass("onclick-top-parent");

                        window.scroll({
                            top: 0,
                            left: 0,
                            behavior: "smooth"
                        });
                        
                        $(".fertility").css("height", newHeight + "px");
                    }
                });
            }

            $('.responsive-menu .nav-button').addClass('icon-menu');
            $('.responsive-menu .nav-button').on('click', function () {
                $(this).toggleClass('icon-close');
                $(this).toggleClass('icon-menu');
            });

            $(".fertiliy-menu li").on("mousedown", function (evt) {
                if (evt.which == 1 || evt.which == 2) {
                    var loc = $(this).find('a:first').attr("href");
                    window.open(loc);
                }
            });

            $(".see-products").on("click",
                function (evt) {
                    evt.stopPropagation();
                });
        });
    </script>
