            <script language="javascript" type="text/javascript">
                $(document).ready(function() {

                    $(".cancel-AaeForm").click(function() {
                        $(".modalBackCover").fadeOut(20).addClass("hide");
                        $(".AaeForm").fadeOut(20).addClass("hide");

                        return false;
                    });

                    $(".btnAaeForm").click(function() {

                        var title = $(this).parent().parent().parent().find(".hypTitle").html();

                        $(".modalBackCover").css("width", window.screen.width);
                        $(".modalBackCover").css("height", window.screen.height);

                        var w = $(".AaeForm").width();
                        var sw = $(window).width();
                        var h = $(".AaeForm").height();
                        var sh = $(window).height();

                        $(".AaeForm").css("left", (sw / 2) - (w / 2));
                        $(".AaeForm").css("top", 25);

                        $(".modalBackCover").fadeIn(20).removeClass("hide");
                        $(".AaeForm").fadeIn(20).removeClass("hide");

                        return false;

                    });

                    $(".cancel-modal-form").click(function() {
                        $(".modalBackCover").fadeOut(20).addClass("hide");
                        $(".modalForm").fadeOut(20).addClass("hide");

                        return false;
                    });

                    $(".cancel-RadForm").click(function () {
                        $(".modalBackCover").fadeOut(20).addClass("hide");
                        $(".RadForm").fadeOut(20).addClass("hide");

                        return false;
                    });

                    $(".btnRadForm").click(function () {

                        var title = $(this).parent().parent().parent().find(".hypTitle").html();

                        $(".modalBackCover").css("width", window.screen.width);
                        $(".modalBackCover").css("height", window.screen.height);

                        var w = $(".RadForm").width();
                        var sw = $(window).width();
                        var h = $(".RadForm").height();
                        var sh = $(window).height();

                        $(".RadForm").css("left", (sw / 2) - (w / 2));
                        $(".RadForm").css("top", 25);

                        $(".modalBackCover").fadeIn(20).removeClass("hide");
                        $(".RadForm").fadeIn(20).removeClass("hide");

                        return false;

                    });

                    $(".cancel-modal-form").click(function () {
                        $(".modalBackCover").fadeOut(20).addClass("hide");
                        $(".modalForm").fadeOut(20).addClass("hide");

                        return false;
                    });

                    $(".btnRequestInfo").click(function() {

                        var title = $(this).parent().parent().parent().find(".litTitle").html();
                        title = title.replace("<BR>", " ");
                        title = title.replace("<br>", " ");
                        title = title.replace(/(^[\s]+|[\s]+$)/g, '');

                        $(".labelProductName").val(title);

                        $(".modalBackCover").css("width", window.screen.width);
                        $(".modalBackCover").css("height", window.screen.height);

                        var w = $(".modalForm").width();
                        var sw = $(window).width();
                        var h = $(".modalForm").height();
                        var sh = $(window).height();

                        $(".modalForm").css("left", (sw / 2) - (w / 2));
                        $(".modalForm").css("top", (sh / 2) - (h / 2));

                        $(".modalBackCover").fadeIn(20).removeClass("hide");
                        $(".modalForm").fadeIn(20).removeClass("hide");

                        return false;

                    });

                    $(window).scroll(function() {
                        if (!$(".modalBackCover").hasClass("hide")) {
                            $(".modalBackCover").css("height", window.screen.availHeight)
                        }
                    });

                    $(".btnAddToRfq").hover(
                        function() {
                            $(this).siblings(".AddToRfqPop").removeClass("hide");
                        },
                        function() {
                            $(this).siblings(".AddToRfqPop").addClass("hide");
                        }
                    );

                    $(".btnRequestInfo").hover(
                        function() {
                            $(this).siblings(".RequestInfoPop").removeClass("hide");
                        },
                        function() {
                            $(this).siblings(".RequestInfoPop").addClass("hide");
                        }
                    );

                });
            </script>       
           
