<script>

$.fn.LLSpotlight = function(){
  return this.each(function(){
    var elem = this;
    var $elem = $(this);
    var navItems = $elem.find('ul.nav').children('li');
    var detailDiv = $elem.find('.details').find('.content');
    var initClass = navItems.first().attr('class').replace('active','').replace(/\s/g, '') + '1';
    $('.top-bg').addClass(initClass);
    //
    var ChangeSlide = function(){
      var itemContent = $(this).clone();
      if ( $(this).hasClass('active') ){
        return false;
      }      
      var clss = $(this).attr('class');
      clss = clss.replace('active','').replace(/\s/g, '');
      itemContent.find('.hidden').removeClass('hidden');
      $(this).addClass('active').siblings().removeClass('active');
      detailDiv.hide();
      $('.details').removeClass('data info dist results control').addClass(clss);          
      $('.top-bg').removeClass('data data1 info info1 dist dist1 results results1 control control1').addClass(clss+'1');          
      detailDiv.html(itemContent.html());
      detailDiv.hide().fadeIn('slow');
    };
    navItems.bind('mouseenter', ChangeSlide);  
  });
};

// dom ready ------------------------
$(document).ready(function(){
  $('.spotlight').LLSpotlight({});  
});

</script>
