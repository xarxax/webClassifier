    <script type="text/javascript">
        $(document).ready(function () {

            var currentCookie = getCookie("OfferPopupShown");

            if ((currentCookie != null || currentCookie !== undefined)) {
                if (currentCookie == "true") {
                    $("#popup, #backdropBg").fadeOut();
                }
            }
            else {

                var url = window.location.href;
                if (url.indexOf("_ref") != -1) {
                    var findblog = getParameterByName("_ref", url);

                    if (findblog == "ftblog") {
                        $("#popup.newyearPOp").hide();
                    }
                    else {
                        $("#popup, #backdropBg").delay(500).fadeIn();
                    }
                }
                else {
                    $("#popup, #backdropBg").delay(500).fadeIn();
                }


                var d = new Date();
                d.setTime(d.getTime() + (1 * 60 * 60 * 1000));
                var expires = "expires=" + d.toUTCString();
                document.cookie = "OfferPopupShown=true;" + expires + ";path=/";
            }

            $('#popup-close').click(function (e) {

                $('#popup, #backdropBg').fadeOut(); // Now the pop up is hiden.

            });
        });

        function getCookie(name) {

            var value = "; " + document.cookie;
            var parts = value.split("; " + name + "=");
            if (parts.length == 2) return parts.pop().split(";").shift();
        }
    </script>

