          font_size = parseFloat(jQuery(this).attr("wdps_fsize")) * ratio;
          font_size_new = font_size;
          ratio_new = ratio;
          if (jQuery(this).attr('data-type') == 'wdps_text_parent') {
            min_font_size = jQuery(this).attr("wdps_fmin_size");
