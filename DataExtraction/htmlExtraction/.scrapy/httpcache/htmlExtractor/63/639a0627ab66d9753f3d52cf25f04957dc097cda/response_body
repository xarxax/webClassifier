<script language="javascript" type="text/javascript"> 
function addEvent(eventName , funcName ){ 
	    if(window.addEventListener){
			if(eventName == 'onload'){
			   eventName = 'load';
			}else if(eventName == 'onerror'){
			   eventName = 'error';
			}
			window.addEventListener(eventName, funcName, false);
		}else if(window.attachEvent){
			window.attachEvent(eventName, funcName);
		}else{ 
	        if(eventName == 'onload'){
			   var oldEvent = window.onload ;
			   window.onload = function(){
				   oldEvent();
				   funcName();
			   }
		    }else if(eventName == 'onerror'){
			   var oldEvent = window.onerror;
			   window.onerror = function(){
				  oldEvent();
				  funcName();
			  }
		    }
	   }
}

function scroll_news(){
  var layer = document.getElementById('sohophp_news');
  var speed = 100;
  var lineHeight = 1;
  var h = layer.scrollHeight - layer.clientHeight;
  function marquee(){
      if(layer.scrollTop >= h ){
	     layer.scrollTop = 0;
	  }
	  layer.scrollTop +=lineHeight;
   }
   var MyMar=setInterval(marquee,speed); 
   layer.onmouseover=function() {clearInterval(MyMar)} 
   layer.onmouseout=function() {MyMar=setInterval(marquee,speed)} 
}
addEvent('onload', scroll_news );

</script>
