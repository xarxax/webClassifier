	 function checkWaterfront(){
  		//alert(localStorage.getItem("MyTumbleweed"));
  		if (localStorage.getItem("MyTumbleweed") === null) { 
  			return false;
  		} else if (localStorage.getItem("MyTumbleweed") == 'Waterfront') {
  			window.localStorage.clear();
  		}
  		return true;
		}
		checkWaterfront();
	 </script>
