	$(document).ready(function() {
	  var toggle = function(direction, display) {
	    return function() {
	      var self = this;
	      var ul = $("ul:first", this);
	      if( ul.css("display") == display &amp;&amp; !self["block" + direction] ) {
	        self["block" + direction] = true;
	        ul["slide" + direction]("slow", function() {
	          self["block" + direction] = false;
	        });
	      }
	    };
	  }
	  $("li.navPrimary").hover(toggle("Down", "none"), toggle("Up", "block"));
	  $("li.navPrimary ul:first").hide();
	});
	</script>

