<script language="JavaScript" type="text/javascript"><!--
function doNavigate(oButton)
{
	var oForm	= document.GoForm;
	window.location.href	= oForm.action;
}
function setAction(oElement)
{
	var oForm	= document.GoForm;
	var sPage	= getSelectValue(oElement, oForm.action);
	oForm.action= (sPage === "client") ? "https://client.property2000.com/" : sPage;
}
// --></script>
