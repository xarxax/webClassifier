                var player;
                var first_time_playing= 0;
                function onYouTubeIframeAPIReady() {
                  player = new YT.Player('ytplayer', {
                    height: '337',
                    width: '600',
                    videoId: 'y34koJyBZJM',
                    events: {
                      'onStateChange': function(event) {
                        if (event.data == YT.PlayerState.PLAYING && first_time_playing == 0) {
                          goog_report_conversion();
                          first_time_playing = 1;
                        }
                      }
                    }
