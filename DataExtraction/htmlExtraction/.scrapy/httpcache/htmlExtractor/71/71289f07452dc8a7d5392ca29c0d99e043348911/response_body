                myFunction();
                $(window).resize(myFunction);
            };

            function myFunction() {
                var intHeight = jQuery(window).height();
                var intWidth = jQuery(window).width();
                $('#ctl00_pnlMastheadContainer').height(intHeight - 24);
                $('#background').height(intWidth * 0.6);
            }
            //end masthead supersized


            //back to top button
            $(document).ready(function () {
                $("#ctl00_pnlBackToTopContainer").hide();

                $(window).scroll(function () {
                    if ($(this).scrollTop() > 35) {
                        $("#ctl00_pnlBackToTopContainer").fadeIn();
                    } else {
                        $("#ctl00_pnlBackToTopContainer").fadeOut();
                    }
                });
            });
            function goTop() {
                $('html,body').animate({ scrollTop: 0 }, 1700, 'easeInOutExpo');
                return false;
            }
            //end back to top button
