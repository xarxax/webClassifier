<script type="text/template" id="searchresult_shop">
    {{ for (var i = 0; i < items.length; i++) { }}
    <article class="clearfix">
        <div class="image">
            <a onclick="return clickAjax(this)" href="{{=items[i].url}}" data-name="{{=items[i].title}}" data-id="{{=items[i].sku}}" data-price="{{=items[i].unitPrice}}"
               data-category="{{=items[i].category}}" data-list="search results"
               data-position="{{=i+1}}" data-currency="{{=summary.currency}}">
                <img src="{{=items[i].image}}" />
            </a>
        </div>

        <div class="main">
            <a onclick="return clickAjax(this)" href="{{=items[i].url}}" class="b-secondary product-title" data-name="{{=items[i].title}}" data-id="{{=items[i].sku}}" data-price="{{=items[i].unitPrice}}"
               data-category="{{=items[i].category}}" data-list="search results"
               data-position="{{=i+1}}" data-currency="{{=summary.currency}}">
                <strong>{{=items[i].title}}</strong>
            </a>
            {{ if( items[i].old_sku ) { }}
            <span class="product-id product-id--blind">{{=items[i].old_sku}}</span>
            {{ } }}
            <span class="product-id">{{=items[i].sku}}</span>
            <p><strong>{{=items[i].unitPrice}}</strong></p>
        </div>

        {{ if (typeof items[i].favoriteUrl === "string" && items[i].favoriteUrl !== "") { }}
        <div class="fav js-fav-container{{ if (items[i].favorited) { }} state-fav-active{{ } }}">
            <div class="js-add-fav e-link-fav-add">
                <a href="{{=items[i].favoriteUrl}}" data-item-number="{{=i}}" class="b">Add to my catalog</a>
            </div>
            <div class="js-remove-fav e-link-fav-remove">
                <a href="{{=items[i].favoriteUrl}}" data-item-number="{{=i}}" class="b">Remove from my catalog</a>
            </div>
        </div>
        {{ } }}

        {{ if (typeof items[i].addUrl === "string" && items[i].addUrl !== "") { }}
        <div class="cart">
            <form class="b js-add-to-cart" action="{{=items[i].addUrl}}">
                <input type="text" name="qty" value="1" id="search-result-qty-{{=i+1}}" />
                <button onclick="return AddToCartAjax(this)" type="submit" class="b colored ga-button-primaryAction "
                        data-name="{{=items[i].title}}" data-id="{{=items[i].sku}}" data-price="{{=items[i].unitPrice}}"
                        data-category="{{=items[i].category}}"
                        data-position="{{=i+1}}"
                        data-currency="{{=summary.currency}}"
                        data-list="search results"
                        id="button_primaryAction_{{=i+1}}_25f57b0847bf6e32daacfa1435094b81"
                        title="Add to cart">Add to cart</button>
                <p class="hint js-add-to-cart-hint">Added to cart</p>
            </form>
        </div>
        {{ } }}
    </article>
    {{ } }}

    {{ if (summary.totalResult === 0) { }}
        <p class="no-results"><strong>We found 0 matches for that search.<br /><br />Search tips<br />1. Double-check your search words. Check for spelling, spacing, etc.<br />2. Broaden your search. Use a product type, brand name, or Nobel Biocare item number.</strong></p>
    {{ } }}
</script>
<script type="text/template" id="searchresult_course">
    {{ for (var i = 0; i < items.length; i++) { }}
    <article class="clearfix">

        <p class="rich">{{=items[i].date}}</p>
        <h3>
            <a href="{{=items[i].url}}">
                {{=items[i].title}}
            </a>
        </h3>
        <p class="rich">Location: {{=items[i].location}}</p>

    </article>
    {{ } }}

    {{ if (summary.totalResult === 0) { }}
        <p class="no-results"><strong>We found 0 matches for that search.<br /><br />Search tips<br />1. Double-check your search words. Check for spelling, spacing, etc.<br />2. Broaden your search. Use a product type, brand name, or Nobel Biocare item number.</strong></p>
    {{ } }}
</script>
