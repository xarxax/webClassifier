      if (document.tongue.myselection.selectedIndex==1) {
         open("espanol/index.htm","_top"); }
      if (document.tongue.myselection.selectedIndex==2) {
         open("swedish/index.htm","_top"); }
    }
    function sageInfo(htmlName) {
      newWin=window.open(htmlName, "sageInfoWin", "menubar=no,resizable=no,toolbar=no,scrollbars=yes,height=410,width=400")
	  newWin.focus();
    }
