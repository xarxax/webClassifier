    <p>Login</p>
    <b>E-Mail Address:</b>
    <br />
    <input style="width:95%;" placeholder="Enter Your E-Mail Address" type="text" name="email" value="">
    <br /><br />
    <b>Password:</b>
    <br />
    <input style="width:95%;" placeholder="Enter Your Password" type="password" name="password" value="">
    <br /><br />
    <div style="text-align: center;"><input id="price_login" type="submit" value="Submit" class="button"></div>
    <br />
    <a class="forgotten">Forgotten Password</a> <br /><br /> <a href="https://soleildirect.com/estore/index.php?route=account/register">Register Account</a>
</div>
</div>			
<script type="text/javascript">
$(document).ready(function() {
$("#closer").on("click", function() {
 	$("#logmein").bPopup().close(); 
});
$(".login_price").live("click", function() { 
$("#forgotten").bPopup().close(); 
$('#logmein').bPopup({
	speed: 600,
	easing: 'easeInOutBack',
	transition: 'fadeIn',
	transitionClose: 'slideUp',
	opacity: 0.6
});
});
$('#price_login').on('click', function() {
$('.warning').hide();
$('.warning').html('');
	$.ajax({
		url: 'index.php?route=account/login/send_login',
		type: 'POST',		
		data: $('#login-form :input'),
		dataType: 'json',
		beforeSend: function() {
			$('#price_login').attr('disabled', true);
			$('#price_login').after('<span class="wait">&nbsp;<img src="catalog/view/theme/default/image/loading.gif" alt="" /></span>');
		},	
		complete: function() {
			$('#price_login').attr('disabled', false);
			$('.wait').remove();
		},				
		success: function(json) {
			if (json['error']) {
				$('#login-form').after('<div class="warning">' + json['error'] + '</div>');
				setTimeout(function(){$('.warning').fadeOut();}, 5000);
			} 
			if (json['success']) {
				$('#login-form input[name=\'email\']').val("");
				$('#login-form input[name=\'password\']').val("");	
        $("#logmein").bPopup().close();

        window.location.reload();
// window.location.href = 'http://soleildirect.com/estore/index.php?route=account/account';
// window.location.href = 'http://soleildirect.com/estore/';

			}
		  }
	  });	
  });
});
</script>
