<script>
  $("a#foreget").click(function(){
    $("#forgotpassmain").slideToggle();
  }); 
  $('.menuc #loginopen').on('click', function (event) {
    $(this).parent().toggleClass('open');
});
$('body').on('click', function (e) {
    if (!$('.menuc').is(e.target) 
        && $('.menuc').has(e.target).length === 0 
        && $('.open').has(e.target).length === 0
    ) {
        $('.menuc').removeClass('open');
    }
});

</script>
