<script type="text/javascript">
    function checkCallCaptcha() {
        var exists = document.getElementById("callgrc-response");
            if(exists == null){
                    
            }      
           else{
            var check = document.getElementById("callgrc-response").value;          

                if(check=='' || check == null){
                    document.getElementById("recaptchacall").className += " recaptchaerror";
                }
                else{
                    document.getElementById("recaptchacall").classList.remove("recaptchaerror");
                }
            }
            var chks = document.getElementsByName('interested[]');
			var checkCount = 0;
			for (var i = 0; i < chks.length; i++) {
				if (chks[i].checked) {
					checkCount++;
				}
			}
			if (checkCount < 1) {
				document.getElementById("option_interested_val").style.visibility = 'visible';
				document.getElementById("option_interested_val").style.color = '#e02b27';
			}else{
                 document.getElementById("option_interested_val").style.visibility = 'hidden';
            }

             if(check=='' || check == null || checkCount < 1){
				 return false;
			}else{
				return true;
			}

   
}

function saveCallrequest() {
        require(["jquery"],function($) {
            
            var customurl = "https://www.bushrefrigeration.com/quickrfq/index/postCall/";
             $.ajax({
                  showLoader: true,
                  url: customurl,
                  type: 'POST',
                   dataType: 'json',
                   data: $("#quickcall_form").serialize() ,
                  complete: function(response) {  
                        document.getElementById('response_msg').innerHTML = response.responseJSON.message;
                        document.getElementById('response_msg').style.display = 'block';
                        if(response.responseJSON.status == 'success'){
                             document.getElementById("response_msg").className = "message-success success message";
                        }else{
                            
                             document.getElementById("response_msg").className = " message-error error message";
                        }
                        document.getElementById("quickcall_form").reset();
                        grecaptcha.reset(gcaptchacall);
                        $('#response_msg').focus();
                        setTimeout(function() {$('#popup-mpdal').modal('closeModal');}, 3000);
                  },
                  error: function (xhr, status, errorThrown) {
                     document.getElementById('response_msg').innerHTML = 'Error happens. Try again.';
                     document.getElementById("response_msg").className = " message-error error message";
                     document.getElementById('response_msg').style.display = 'block';
                     document.getElementById("quickcall_form").reset();
                     $('#response_msg').focus();
                  }
                 
             });
                 
        });
            

}
 
</script>

       
    </div>
  </div>  
</div>



<script>
    require(
        [
            'jquery',
            'Magento_Ui/js/modal/modal',
            'mage/validation'
        ],
        function(
            $,
            modal
        ) {
            var calloptions = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: 'Request A Call',
                buttons: [{
                    text: $.mage.__('Close'),
                    class: '',
                    click: function () {
                        this.closeModal();
                    }
                },
                {
                text: 'Call Me',
                class: '',
                click: function() {
                    
                    
                   var dataForm = $('#quickcall_form');
                    var ignore = null;
                
                    dataForm.mage('validation', {
                        ignore: ignore ? ':hidden:not(' + ignore + ')' : ':hidden'
                    }).find('input:text').attr('autocomplete', 'off');
                
                    
                      var success = dataForm.validation('isValid'); 
                      var successcaptcha =  checkCallCaptcha() ;
                      if((success) && (successcaptcha) ) {
                          saveCallrequest();
                      }
                  
                                   
                }
            }
]
            };

            var popup = modal(calloptions, $('#popup-mpdal'));
            $("#request_call_btn").on('click',function(){ 
                $("#popup-mpdal").modal("openModal");
            });

             var options = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: 'Request A Quote',
                buttons: [{
                    text: $.mage.__('Close'),
                    class: '',
                    click: function () {
                        this.closeModal();
                    }
                },
                {
                text: 'Request A Quote',
                class: '',
                click: function() {                    
                    requestQuotePopup();              
                  
                                   
                }
            }
                
                
                ]
            };
            var popup = modal(options, $('#popup-quote'));
            $("#request_quote_btn").on('click',function(){ 
                $("#popup-quote").modal("openModal");
            });



        }
    );

 
</script>
