<script type="text/javascript">
    function navigateBrand1()
    {
        var selectedValue = "";
        var selectedValue1 = "";
        var search1 = "";
        var search2 = "";
        var radioButtons = document.getElementsByName('drop_1');
        var radioButtons1 = document.getElementsByName('drop_2');
        for (var x = 0; x < radioButtons.length; x++)
        {
            if (radioButtons[x].checked)
            {
                selectedValue += radioButtons[x].value + "-";
            }
        }
        for (var x1 = 0; x1 < radioButtons1.length; x1++)
        {
            if (radioButtons1[x1].checked)
            {
                selectedValue1 += radioButtons1[x1].value + "-";
            }
        }

        selectedValue = selectedValue.slice(0, -1);
        selectedValue1 = selectedValue1.slice(0, -1);
        //alert(selectedValue);
        //alert(selectedValue1);

        //if(selectedValue=="")
        //{
        //return false;
        //}
        if (selectedValue != "")
        {
            var selectedValueArr = selectedValue.split("-");
            var searchvar = "refine_search1[]=";


            for (var y = 0; y < selectedValueArr.length; y++)
            {
                search1 += searchvar + selectedValueArr[y] + "&";
            }
            search1 = search1.slice(0, -1);
            //alert(search1);
        }

        //if(selectedValue1=="")
        //{
        //return false;
        //}
        if (selectedValue1 != "")
        {
            var selectedValueArr1 = selectedValue1.split("-");
            var searchvar1 = "refine_search2[]=";
            //var search2 = "";

            for (var y1 = 0; y1 < selectedValueArr1.length; y1++)
            {
                search2 += searchvar1 + selectedValueArr1[y1] + "&";
            }
            search2 = search2.slice(0, -1);
            //alert(search2);
        }
        if ((search1 != "") && (search2 != ""))
        {
            var url = search1 + "&" + search2;
        }
        if ((search1 != "") && (search2 == ""))
        {
            var url = search1;
        }
        if ((search1 == "") && (search2 != ""))
        {
            var url = search2;
        }
        if ((search1 == "") && (search2 == ""))
        {
            return false;
        }
        else
        {
            window.location = "https://www.quantumrlv.com.au/catalogsearch/advanced/result/?" + url;
        }
    }

    function navigateBrand2()
    {
        var selectBox = document.getElementById("drop_2");
        var selectedValue = selectBox.options[selectBox.selectedIndex].value;
        if (selectedValue == "")
        {
            return false;
        }
        else
        {
            window.location = "https://www.quantumrlv.com.au/catalogsearch/advanced/result/?" + "refine_search2[]=" + selectedValue;
        }
    }
</script>
<!--Footer End-->

<!-- Home page from adding jQuery -->
<script language="javascript">
//<![CDATA[
    var $jk = jQuery.noConflict();
//]]>
</script>
<script type="text/javascript">

    function submitFree() {
        $jk('.btnSubmit').click(function() {


            // Validation of the fields

        	var nameregx = /^[^\s][A-Za-z ]{3,}$/;
        	var phoneregx = /^[0-9 ]{10,}$/;
        	var postcoderegx = /^\d{4}$/;
        	var suburbregx = /^[^\s][A-Za-z ]{3,}$/;

            var fullName = $jk("#fullName").val();
            //fullName = fullName.replace("&", "$#");

            var emailId = $jk("#emailId").val();
            //emailId = emailId.replace("&", "$#");

            var contactNo = $jk("#contactNo").val();
            //contactNo = contactNo.replace("&", "$#")

            var suburb = $jk("#suburb").val();

            var postCode = $jk("#postCode").val();
            postCode = postCode.replace("&", "$#");

            //comment code

            var comentValue = $jk("#cmntid").val();


            //var cutomerMsg	= $jk("#cutomerMsg").val();
            var selectedValue = "";
            //count = 0;
            var radioButtons = document.getElementsByName('cutomerMsg');
            for (var x = 0; x < radioButtons.length; x++)
            {
                if (radioButtons[x].checked)
                {
                    selectedValue += radioButtons[x].value + ", ";
                   // count = 1;
                }
            }

            var cutomerMsg = selectedValue;
            cutomerMsg = cutomerMsg.slice(0, -2);

            //How we can help options Code
            var homeselectedValue = "";
            //chCount=0;
            var radioButtons = document.getElementsByName('howwecanhelp');
            for (var x = 0; x < radioButtons.length; x++)
            {
                if (radioButtons[x].checked)
                {
                    homeselectedValue += radioButtons[x].value + ", ";
                    //chCount=1;
                }
            }


            var homecutomerMsg = homeselectedValue;
            homecutomerMsg = homecutomerMsg.slice(0, -2);
            //Ends here

            if (fullName == "" || !(nameregx.test(fullName)))
            {
                $jk("#nameError").show();

                $jk('#fullName').addClass('validation-failed');
                return false;
            }
            else
            {
				$jk("#nameError").hide();
                $jk('#fullName').removeClass('validation-failed');

			}

            if (emailId == "")
            {
                $jk("#emailError").show();

                $jk('#emailId').addClass('validation-failed');
                return false;
            }else{

				$jk("#emailError").hide();
                $jk('#emailId').removeClass('validation-failed');
			}

            if (contactNo == "" || !(phoneregx.test(contactNo)))
            {
                $jk("#phoneError").show();

                $jk('#contactNo').addClass('validation-failed');
                return false;
            }else{

				 $jk("#phoneError").hide();
                 $jk('#contactNo').removeClass('validation-failed');

				}

            if (suburb == "" || !(suburbregx.test(suburb)))
            {

                $jk("#suburbError").show();

                $jk('#suburb').addClass('validation-failed');
                return false;
            }else{

				$jk("#suburbError").hide();
                $jk('#suburb').removeClass('validation-failed');

				}

            if (postCode == "" || !(postcoderegx.test(postCode)))
            {
                $jk("#postError").show();

                $jk('#postCode').addClass('validation-failed');
                return false;
            }else{
                $jk("#postError").hide();
                $jk('#postCode').removeClass('validation-failed');

				}
            if(selectedValue == "")
            {
				$jk("#IntrestError").show();
				return false;

			}else{
				$jk("#IntrestError").hide();
			}


            if (validateEmail(emailId)) {
                $jk.ajax({
                    type: "POST",
                    url: "https://www.quantumrlv.com.au/ajax.php",
                    data: "action=suggest&fullName=" + fullName + "&emailId=" + emailId + "&contactNo=" + contactNo + "&suburb=" + suburb + "&postCode=" + postCode + "&cutomerMsg=" + cutomerMsg + "&commentCode=" + comentValue + "&howwecanhelp=" + homecutomerMsg,
                    success: function(result) {


                        //alert("your query has been submitted.");
                        $jk("#emailError").hide();
                        $jk('#emailId').removeClass('validation-failed');
                        $jk("#emailSuccess").show();
                        $jk("input#fullName").val('');
                        $jk("input#emailId").val('');
                        $jk("input#contactNo").val('');
                        $jk("input#suburb").val('');
                        $jk("input#postCode").val('');
                        $jk("textarea#cmntid").val('');
                        $jk("select#cutomerMsg").val('');
                        $jk('#cutomerMsg1').attr('checked', false);
                        $jk('#cutomerMsg2').attr('checked', false);
                        $jk('#cutomerMsg3').attr('checked', false);
                        $jk('#cutomerMsg4').attr('checked', false);


                        $jk('.btnSubmit').unbind("click");
                        submitFree();
                    }
                });

            }
            else {
                $jk("#emailError").show();
                return false;
            }


        });
    }
    $jk(document).ready(function() {
        submitFree();
    });
</script>
<script  type="text/javascript">
    function validateEmail(emailId) {
        var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        if (filter.test(emailId)) {
            return true;
        }
        else {
            return false;
        }
    }
</script>
 
