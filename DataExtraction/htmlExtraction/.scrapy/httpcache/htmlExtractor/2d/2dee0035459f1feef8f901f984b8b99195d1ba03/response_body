    <script>
try{
        // feel free to adjust the poll interval and count per your preference\
        var POLL_INTERVAL = 25;
        var MAX_POLL_COUNT = 40;
        var pollCount = 0;
        function poll() 
        {
            pollCount++;

            if ((window.Demandbase && window.Demandbase.companyProfile) || pollCount === MAX_POLL_COUNT) 
            {
                clearTimeout(poll);

                // featured resource component
                if( jQuery('.featureResource3Column').length ) {
                    var attr = jQuery('.featureResource3Column').attr('style');
	            if (typeof attr == typeof undefined || attr == false) {
                    jQuery('.featureResource3Column').each(function(){
                        this.style.setProperty('visibility', 'visible', 'important');
                    });
                    }
                }
         
                // home page slider
                if( jQuery('.module_gallery').length ) {
                    var attr = jQuery('.module_gallery').attr('style');
	            if (typeof attr == typeof undefined || attr == false) {
                    jQuery('.module_gallery').each(function(){
                        this.style.setProperty('visibility', 'visible', 'important');
                    });
                    }
                }

                // chosen resources
                if( jQuery('.column_two').length ) {
                    var attr = jQuery('.column_two').attr('style');
	        
                    jQuery('.column_two').each(function(){
                        this.style.setProperty('visibility', 'visible', 'important');
                    });
                    
                }

            } else if (pollCount < MAX_POLL_COUNT) {
                setTimeout(poll, POLL_INTERVAL);
            }

        } poll();
    }catch(e){
		if(window.console)
            {console.log("DB__ "+e.message);}
}
</script>

    
    

