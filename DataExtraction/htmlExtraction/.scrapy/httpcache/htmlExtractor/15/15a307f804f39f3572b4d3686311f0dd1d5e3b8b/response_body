	<script type="text/javascript"> 
		jQuery.noConflict();
		var star=0; var sw=false;
		var slider_links_index = 0;	
		function moveslider(num){		
			var slider_links = jQuery('.stripTransmitter li a');		
			slider_links_index+=num;
			if(slider_links_index<0)slider_links_index=(slider_links.size()-1);
			if(slider_links_index>(slider_links.size()-1))slider_links_index=0;
			//alert(slider_links_index);
			slider_links.eq(slider_links_index).trigger('click');
			sw=true;
		}
		
		
		function headerpresent() { 
			jQuery("div#mygalone").slideView();
			jQuery('.stripTransmitter li:first').addClass('first');
			jQuery('.stripTransmitter li:last').addClass('last');	
			
			//=======intro=====		
			var slider_links = jQuery('.stripTransmitter li a');	
				
			function slider_intro(){	
				if(!sw){
					slider_links.eq(slider_links_index).trigger('click');
					//alert(slider_links.eq(slider_links_index).trigger('click'));			
					slider_links_index++;	
					if(slider_links_index <= slider_links.size()){
						setTimeout(function(){slider_intro()}, 3000); //select change time
					}
					else if(slider_links_index > slider_links.size()){				
						slider_links.eq(0).trigger('click');
						slider_links_index=0;
						setTimeout(function(){slider_intro()}, 3000);
					}
					//select change time
				}else{
					sw=false;
					setTimeout(function(){slider_intro()}, 3000);
				}
			}		 
			//slider_intro();
			//===============	
		}
		jQuery(window).bind("load",headerpresent);
	</script>
