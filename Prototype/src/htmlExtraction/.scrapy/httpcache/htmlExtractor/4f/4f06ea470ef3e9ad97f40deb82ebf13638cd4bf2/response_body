<!-- jquery for just menu page (book now button and menu dropdown on mobile -->
    <script>
    
//remove booking button on some pages (mobile and desktop)
var str = window.location.pathname;
if ((str.indexOf("booking") >= 0)||(str.indexOf("locations") >= 0)||(str.indexOf("store") >= 0)){
jQuery('#sticky-book, .book-tab').css('display','none');
};

    //mobile menu nav
    jQuery('#menu-navigation-trigger').click(function(){
        if(jQuery('#menu-navigation-dropdown').hasClass('drop-menu')){
            jQuery('#menu-navigation-dropdown').removeClass('drop-menu');
            var headerheight = jQuery('header').height();
            var dropmenuheight = jQuery('#menu-navigation-dropdown').height();
            var marginTop = headerheight - dropmenuheight;
            jQuery('#content').css('margin-top', marginTop);
            jQuery('.dropdown-arrow img').removeClass('turn-arrow');
        }else{
            jQuery('#menu-navigation-dropdown').addClass('drop-menu');
            jQuery('.dropdown-arrow img').addClass('turn-arrow');
            jQuery('#content').css({
                'transition':'all 1s',
                'margin-top': '100vh'});
        };
    });
    </script>
    <!-- END jquery for just menu page (book now button and menu dropdown on mobile -->    
<script type="text/javascript"> 



//submit locator form when selection made
jQuery('#wpsl-search-input').attr('placeholder','search by suburb or post code');
jQuery('#wpsl-search-input').change(function(){
    setTimeout(function(){
        jQuery('#wpsl-search-btn').trigger("click");
    }, 500);

});

//do the location results formatting 
jQuery(document).ajaxStop(function(){
    //format buttons etc
    jQuery('#wpsl-stores li').each(function(){
	var storeUrl = jQuery('.wpsl-store-location a', this).attr('href');
        jQuery('.wpsl-directions', this).replaceWith("<a href='"+storeUrl+"' class='wpsl-book-online'>View Restaurant Page</a>");
      });
      jQuery('.wpsl-contact-details span').each(function(){
         var trimmed = jQuery(this).text().slice(2);
         jQuery(this).text(trimmed);
      });
      jQuery('#wpsl-result-list').addClass('reveal-list');
});

//only on desktop make map the same height
jQuery(window).ready(function(){
   var descriptionheight = jQuery('.next-to-map').height();
    jQuery('#wpsl-base-gmap_0').height(descriptionheight); 
    console.log(descriptionheight);
}) ;

//read more expand and contract
jQuery('.foldout-grip').click(function(){
if(jQuery('.mobile-foldout').hasClass('mobile-foldout-closed')){
jQuery('.mobile-foldout').removeClass('mobile-foldout-closed');
jQuery('.open-grip').css('display','block');
jQuery('.close-grip').css('display','none');
}else{
jQuery('.mobile-foldout').addClass('mobile-foldout-closed');
jQuery('.close-grip').css('display','block');
jQuery('.open-grip').css('display','none');
};
});

//mobile nav open and close
jQuery('.hamburger-icon').click(function(){
var slidemenu = jQuery('#mobile-slidemenu');
if(jQuery(slidemenu).hasClass('slidenav')){
jQuery(slidemenu).removeClass('slidenav');
jQuery('.hamburger-icon img').attr('src','https://www.montezumas.com.au/wp-content/uploads/2017/09/hamburger.png');
jQuery('#mobile-menu-navigation').css('display','block');
}else{
jQuery(slidemenu).addClass('slidenav');
jQuery('.hamburger-icon img').attr('src','https://www.montezumas.com.au/wp-content/uploads/2017/09/close.png');
jQuery('#mobile-menu-navigation').css('display','none');
};
});

//play and pause video button
var video = document.getElementById('home-video');
jQuery('.play-video-button').click(function(){
if (video.paused == true) {
    video.play();
    jQuery('img', this).attr('src','https://www.montezumas.com.au/wp-content/uploads/2017/10/pause.png');
  } else {
    video.pause();
    jQuery('img', this).attr('src','https://www.montezumas.com.au/wp-content/uploads/2017/09/play.png');
  }
});

//replace slider nav
jQuery('.nextend-arrow-previous img').attr("src",'https://www.montezumas.com.au/wp-content/uploads/2017/09/previous.png').parent().addClass('new-prev');
jQuery('.nextend-arrow-next img').attr("src",'https://www.montezumas.com.au/wp-content/uploads/2017/09/next.png').parent().addClass('new-next');


jQuery(document).ready(function(){
  


    //offset the content by the navigation height (set in function above)
    var headerheight = jQuery('header').height();
    jQuery('#content').css('margin-top',headerheight);

//replace back to top icon
jQuery('#back-to-top a').html("<img src='https://www.montezumas.com.au/wp-content/uploads/2017/10/back_to_top_button_blue.png'>");

    //stick menu nav when it reaches the top
    if(jQuery('#menu-navigation').length){ //only do this if you have the nav for it - otherwise it freaks out the home page
        var menunavheight = jQuery('#menu-navigation').height() / 2;
        var menunavpos = jQuery('#menu-navigation').offset().top + menunavheight;
    jQuery(window).scroll(function(){
        var topnavpos = jQuery('#menu-new-sub-navigation').offset().top;
        var topnavheight = jQuery('#menu-new-sub-navigation').height();
        var targetpos = topnavpos + topnavheight;
        var currentpos = jQuery(window).scrollTop();
        var headerheight = jQuery('header').height();
        var toppos = headerheight + menunavheight;
        if( (currentpos + headerheight) >= menunavpos){
          jQuery('#menu-navigation').css({
              'position':'fixed',
              'top': toppos
          });
          //jQuery('.header-sub-menu').css('background','rgba(0,0,0,0)'); //when the menu-nav goes under the header sub-nav, it screws the transparency, but this will fix it
      }else{
          jQuery('#menu-navigation').css({
              'position':'absolute',
              'top':'auto'
          });
          jQuery('.header-sub-menu').css('background','rgba(0,0,0,.7)'); //when the menu-nav goes under the header sub-nav, it screws the transparency, but this will fix it
      };
    });
    };
        
    
});
</script>
