<script>
(function()
{

	// scroll
	$(".pg-anchor a").pagescroll();
	
	
	
	// SP用 マイストアモーダルを開く
	$("#carousel-col1-top").each(function()
	{
		if( $("body").hasClass("s"))
		{
			
		}else {
			
			
			
		}
	
	
	});



	$(".js-modal-mystore-open").click(function()
	{
		if( $("body").hasClass("s"))
		{
			window.mystore();
		}
	});
	


	$("#carousel-col1-top").each(function()
	{
		var _tar = $(this);
		_tar.find(".-list").carousel({
			column   :1,
			speed    :500,
			interval :5000,
			nav      :_tar.find(".-nav > span"),
			next     :_tar.find(".-next"),
			prev     :_tar.find(".-prev")
		});
	});
	
	//ピックアップ
	$("#carousel-col4-sp").append( $("#carousel-col4").children().clone() );

	//ピックアップ カルーセル：pc4カラム
	$("#carousel-col4").each(function()
	{
		var _tar = $(this);
		_tar.find(".-list").carousel({
			column   :[1,4,4],
			speed    :400,
			interval :4000,
			next     :_tar.find(".-next"),
			prev     :_tar.find(".-prev")
		});
	});

	// ピックアップ もっと見るの制御
	// @original -- 元データ
	// @button   -- moreボタン
	// @numFirst -- 最初に表示する件数
	// @numMore  -- ボタンクリック時に追加する件数 0指定で残りすべて
	$("#carousel-col4-sp .js-target").more({
		original  :$("#carousel-col4-sp .js-original").remove(),
		button    :$("#js-more-btn"),
		numFirst  :4, 
		numMore   :4
	});


	if( !$("body").hasClass("s") )
	{
		
		$("#carousel-col4-sp .js-target").more({
			original  :$("#carousel-col4-sp .js-original").remove(),
			button    :$("#js-more-btn"),
			numFirst  :4, 
			numMore   :4
		});
		
		// accordion
		$(".js-accordion").each(function()
		{
			var _toggleBlockCheck = $(this);
			if(_toggleBlockCheck.hasClass("-open"))
			{
				$(this).find('.-list').css('display', 'block');
			}
		});
		

		
		$(".js-accordion-open").on("click", function()
		{
			var _toggleBlock = $(this).parent(".js-accordion");
				_toggleBlock.toggleClass("-open");
				if(_toggleBlock.hasClass("-open"))
				{
					$(this).next().slideDown();
				} else {
					$(this).next().slideUp();
				}
				return false;
		});


	} else {



		//チラシモーダル
		$(".js-modal-flyer-open").click(function()
		{
			var flyerId   = $(this).attr("data-flyer");
			var storeName = $(this).attr("data-store");
			var blogURL   = $(this).attr("data-blog");
			
			
			var content = $("#js-modal-flyer").clone();
			content.find("iframe").attr("src","/flyer/my_" + flyerId + ".html");
			content.find(".store-name").text(storeName).attr("href",blogURL);
			content.find("#btn-close").click(function()
			{
				window.modal.close();
				return false;
			});
			window.modal.open({
				content:content.show()
			});
		
			
			return false;
		});

		// accordion
		$(".js-accordion").each(function()
		{
			var _toggleBlockCheck = $(this);
			
				if( !$("body").hasClass("s") )
				{
					if(_toggleBlockCheck.hasClass("-open"))
					{
						$(this).find('.-list').css('display', 'block');
					}
				} else{
					$(this).removeClass("-open");
				}

		});
		

		
		
		$(".js-accordion-open").on("click", function()
		{
			var _toggleBlock = $(this).parent(".js-accordion");
				_toggleBlock.toggleClass("-open");
				if(_toggleBlock.hasClass("-open"))
				{
					$(this).next().slideDown();
				} else {
					$(this).next().slideUp();
				}
				return false;
		});

	}
	
	$(function() {
		//ウィンドウのリサイズ再処理
		var resizecheck = function(){

			if(!$('body').hasClass('s')){
				$("#js-news").appendTo("#js-main-column");
			} else {
				$("#js-news").insertAfter("#js-side-column");
			}

		}
		resizecheck();

		var timer = false;
		$(window).on('resize', function(){
			if (timer !== false) {
				clearTimeout(timer);
			}
			timer = setTimeout(function() {
				resizecheck();
			}, 100);
		});
	});
	
})();


</script>
