<script>
$(document).ready(function() {
jQuery('#basic-modal-content-popup').hide();
});

    var mainId;

    function aa(id,pid){

        mainId = id;

        var checkVal = jQuery('#'+mainId).attr('checked');

        if(checkVal==true){

            jQuery('#titleDiv').html(jQuery('.'+mainId).html());

            jQuery('#basic-modal-content').modal();
            jQuery('#'+pid).attr('checked', true);
        }
    }

    function bb(){
        jQuery('#'+mainId).attr('checked', false);

        mainId=null;

        }

    function cc(){

            jQuery('#basic-modal-content-popup').modal();
    }

        function enquiry(){
            jQuery('#basic-modal-content').modal();
        }

    function read_more()
    {
        jQuery("#more_testimonial_id").show();
        jQuery("#read_more").hide();
        jQuery("#quotes_close").hide();
        jQuery("#hide_text").show();
    }
    function hide_readmore()
    {
        jQuery("#more_testimonial_id").hide();
        jQuery("#read_more").show();
        jQuery("#hide_text").hide();
        jQuery("#quotes_close").show();
    }

</script>
