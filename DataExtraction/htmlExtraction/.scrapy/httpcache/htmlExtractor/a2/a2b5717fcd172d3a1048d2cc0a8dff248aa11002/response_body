<script type="text/javascript">
function loginec() {
	if(document.l.username.value=='') { alert('Please input an email address.'); document.l.username.focus(); return false; }
	else if(document.l.pass.value=='') { alert('Please input a password.'); document.l.pass.focus(); return false; }
	else { return true; }
}
function showHide(id) {
	var div = document.getElementById(id);
	if(div.style.display=='block')
	{
		div.style.display='none';
		if(id=="logindiv") { document.getElementById('loginimg').src='images/login.jpg'; }
	}
	else
	{
		div.style.display='block';
		if(id=="logindiv") { document.getElementById('loginimg').src='images/close.jpg'; document.l.username.focus(); }
	}
}
</script>
