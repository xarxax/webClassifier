function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		accueil_over = newImage("images/menu/accueil-over.jpg");
		philo_over = newImage("images/menu/philo-over.jpg");
		heureux_over = newImage("images/menu/heureux-over.jpg");
		proprietes_over = newImage("images/menu/proprietes-over.jpg");
