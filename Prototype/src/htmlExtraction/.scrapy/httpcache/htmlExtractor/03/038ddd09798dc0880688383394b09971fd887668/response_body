	<!-- mbYTPlayer Home -->
	<script type="text/javascript">
	jQuery(function(){
	    var homevideo = "<div id=\"bgndVideo_home\" data-property=\"{videoURL:'https://www.youtube.com/watch?v=Fyu_SbrneXs', opacity:1, autoPlay:true, containment:'body', startAt:1, stopAt:13, mute:true, optimizeDisplay:true, showControls:false, printUrl:false, loop:true, addRaster:false, quality:'small', ratio:'auto', realfullscreen:'false', stopMovieOnClick:'true', gaTrack:'true'}\"></div>";
	    jQuery("body").prepend(homevideo);
	    jQuery("#bgndVideo_home").mb_YTPlayer();
    });

	</script>
