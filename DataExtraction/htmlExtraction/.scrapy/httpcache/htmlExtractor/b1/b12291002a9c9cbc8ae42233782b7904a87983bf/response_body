                    <div class="col-xs-12">
            <script>
                $(document).ready(function(){
                    var blvd_maxHeight = 0;
                    $(".blvd_max_height").each(function(){
                        if ($(this).height() > blvd_maxHeight) { blvd_maxHeight = $(this).height(); }
                    });
                    $(".blvd_max_height").height(blvd_maxHeight);

                    var total_reviews = 4;
                    var page = 0;
                    var desired_rows_paging = 1;
                    var columns = 3;
                    var total_pages = Math.ceil((total_reviews / columns));
                    var box_height = $(".blvd_row").height() * desired_rows_paging;
                    var cpage = 1;
                    var writer = '';
                    while(cpage <= total_pages) {
                        if(cpage == 1) { isactive = 'active'; } else { isactive = ''; }
                        writer += '<a href="javascript:void(0)" class="blvd_page background blvd_page_'+(cpage-1)+' '+isactive+'" data-page="'+(cpage-1)+'">'+cpage+'</a>';
                        cpage = cpage + 1;
                    }
                                        $('.blvd_prev').after(writer);

                                        if(total_pages < 2) {
                        $('.blvd_next').addClass('disabled');
                    }



                    $('.blvd_box').css('height', box_height+'px');

                    function check_next_prev() {
                        if(page < (total_pages-1)) {
                            $('.blvd_next').removeClass('disabled').addClass('background');
                        } else {
                            $('.blvd_next').removeClass('background').addClass('disabled');
                        }
                        if(page > 0) {
                            $('.blvd_prev').removeClass('disabled').addClass('background');
                        } else {
                            $('.blvd_prev').removeClass('background').addClass('disabled');
                        }
                    }
                    $('.blvd_next').click(function() {
                        if(page != (total_pages-1)) {
                            page = page + 1;
                            var pos = (page * box_height);
                            $(".blvd_container").stop().animate({top: '-'+pos+'px'}, 300);
                            $('.blvd_page').removeClass('active');
                            console.log(page);
                            $('.blvd_page_'+page).addClass('active');
                            check_next_prev();
                        }
                    });
                    $('.blvd_prev').click(function() {
                        if(page > 0) {
                            page = page - 1;
                            var pos = (page * box_height);
                            $(".blvd_container").stop().animate({top: '-'+pos+'px'}, 300);
                            $('.blvd_page').removeClass('active');
                            console.log(page);
                            $('.blvd_page_'+page).addClass('active');
                            check_next_prev();
                        }
                    });
                    $('.blvd_page').click(function() {
                        page = Number($(this).attr('data-page'));
                        var pos = (page * box_height);
                        $(".blvd_container").stop().animate({top: '-'+pos+'px'}, 300);
                        $('.blvd_page').removeClass('active');
                        console.log(page);
                        $('.blvd_page_'+page).addClass('active');
                        check_next_prev();
                    });

                    $('.avg_boxes').mouseenter(function() {
                        $(this).find('.right').stop().show(200);
                    }).mouseleave(function() {
                        $(this).find('.right').stop().hide(200);
                    });


                });
            </script>
