}
var opacity = 100;
var j = 0;
var k = 1;
///////////////////

function fadeOut() {
document.getElementById('SlideShow').src = Pic[j];
document.getElementById('alink_id').href = href[j];
document.getElementById('SlideShow2').style.background = 'url('+Pic[k]+') no-repeat top left';

	if (document.getElementById) {
		if (opacity >= 0) {

		if(typeof document.getElementById('SlideShow').style.opacity == "string") {
		document.getElementById('SlideShow').style.MozOpacity = opacity/100;
		} else {
		document.getElementById('SlideShow').style.filter="alpha(opacity="+opacity+")";
		}

		opacity -= 10;
		window.setTimeout("fadeOut()", 100);
		} else {

		opacity = 100;
j = j + 1;
k = k + 1;
if (j > (p - 1)) j = 0;
if (k > (p - 1)) k = 0;
		window.setTimeout("fadeOut()", 4000);
		}

	}
}

document.write('<div style="width:'+myWidth+'px:height:'+myHeight+'px;background: url('+Pic[j]+') no-repeat top left;" id="SlideShow2"><a id="alink_id" href="'+href[j]+'"><img src="'+Pic[j]+'" width="'+myWidth+'" height="'+myHeight+'" border="0" id="SlideShow"></a></div>');

