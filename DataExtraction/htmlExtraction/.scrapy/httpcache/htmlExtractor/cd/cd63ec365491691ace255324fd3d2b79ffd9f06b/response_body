<script Language="JavaScript">
		 //Scrolling text string.
		 //var strText="Welcome to NetMotion, Inc.";
		 var strText="NetMotion, Inc: Quality Products and great pricing! Please contact us at toll free: 1-800-790-7837!"; 
		 //Length of text
		 var intText=strText.length;
		 //Speed of the scroll. Higher the number, slower the scroll.
		 var intSpeed=100;
		 //Width of the scrolling area.
		 var intWidth=100;
		 var intPos=1-intWidth;
		 function scroll()
		 {
		 	//Initialize the string to be printed.
		 	intPos++;
		 	var strScroll="";
		 	//Move to the right in the string.
		 	if (intPos==intText)
		 	{
		 		//Start over if the string is done.
		 		intPos=1-intWidth;
		 	}
		 	//Scrolling
		 	if (intPos<0)
		 	{
		 	//Add space to beginning if necessary.
		 		for (var i=1; i<=Math.abs(intPos); i++)
		 		{
		 		 strScroll=strScroll+" ";
		 		}
		 		strScroll=strScroll+strText.substring(0, intWidth-i+1);
		 	}
		 	else
		 	{
		 	strScroll=strScroll+strText.substring(intPos, intWidth+intPos);
		 	}
		 	window.status = strScroll;
		 	setTimeout("scroll()", intSpeed);
		 }
</script>
		 	
