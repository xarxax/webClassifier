	<!--//--><![CDATA[//><!--
	
	function expandMenu(id) {
		menu = document.getElementById(id);
		menu.className = "on";
	}
	
	function collapseMenus(id) {		// collapse them all
	if (document.getElementById) {
		navRoot = document.getElementById(id);
		for (i=0; i<navRoot.childNodes.length; i++) {
			node = navRoot.childNodes[i];
			if (node.nodeName=="LI") node.className = "off";
			}
	}
	}
	
	startList = function() {
	
	if (document.getElementById) {
		navRoot = document.getElementById("nav");
		for (i=0; i<navRoot.childNodes.length; i++) {
			node = navRoot.childNodes[i];
			if (node.nodeName=="LI") {
			node.onclick=function() {

				if (this.className == "on") {
					this.className = "off";
					}
				else {
					collapseMenus("nav");		// collapse them all
					this.className = "on";	// but turn this one on
					}
				
				}
				}
			}

		navRoot = document.getElementById("nav2");
		for (i=0; i<navRoot.childNodes.length; i++) {
			node = navRoot.childNodes[i];
			if (node.nodeName=="LI") {
			node.onclick=function() {
			
				if (this.className == "on") {
					this.className = "off";
					}
				else {
					collapseMenus("nav2");	// collapse them all
					this.className = "on";	// but turn this one on
					}
	
				}
				}
			}
//			expandMenu("leftMenu01");
	 }
	}
	window.onload=startList;
	//--><!]]>
	</script>

