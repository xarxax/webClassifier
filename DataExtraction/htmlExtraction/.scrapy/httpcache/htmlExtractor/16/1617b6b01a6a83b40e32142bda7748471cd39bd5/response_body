function loadspecials() {
  if (MDSSpecialsTemplate !== '') {
    var dummy = widgetload(MDSRnUrl + '?orderno=' + OID + '&CUSTOMERID=' + CID + '&LOGINPATH=HOTV&pageid=31&CUSTOMLIST=WEB*' + MDSSpecialsTemplate, '#RSdivcontainer');
  } else {
            $("#RSdivcontainer").html("Missing Template Id in Parameter Web Order Options") ;
  }
  var lp = "BRX" ; // web types 
  var dummy = widgetload(MDSRnUrl + '?orderno=' + OID + '&CUSTOMERID=' + CID + '&LOGINPATH=' + lp + '&PAGEID=31', '#typeholder');
}
  function bxsliderload() {
   var pageurl = MDSRnUrl + '?orderno=' + OID + '&CUSTOMERID=' + CID + '&LOGINPATH=DL&pageid=31&SUBMITVAR=WEBPROD.HTM' ;
   $( "#infoBoxContentsLeft" ).load( pageurl , function() { 
      $('#mycarousel').bxSlider({    minSlides: 1,  maxSlides: 6,  slideWidth: 150, auto: true , slideMargin: 10 }); 
   });
}
function flexsliderload() {
  if ( jQuery().flexslider ) {
        // Flex Slider for Home page
        $('.home-slider .flexslider').flexslider({
            animation: "fade",
            controlNav: false,
            directionNav: true,
            pauseOnHover: true,
            pauseOnAction: false,
            smoothHeight: false,
            start: function (slider) {
                slider.removeClass('loading');
            }
        });
        }
} ;
        
// $(document).keypress(function(e) { if(e.which == 13) { document.login.submit();}}) ;
$(document).ready(function() {
 loadspecials()  ;
 // bxsliderload() ;
 flexsliderload() ;
$('.tab_content').hide();
$(".RTab").addClass("active").show();
$('#Tab1').fadeIn();
// Add interactive accordion logic
 $('div.FaccordionButton').click(function() { $(this).next().toggle( "slow" ); })
 	//When page loads...
	$(".midtab_content").hide(); //Hide all content
  var linkpass = $(location).attr('hash');
  if (linkpass !== '') {
      	$(linkpass).addClass("active"); //Add "active" class to selected tab
        $(linkpass).fadeIn();
   } else {     
	$("ul.midtabs li:first").addClass("active").show(); //Activate first tab
	$(".midtab_content:first").show(); //Show first tab content
	}
	//On Click Event
	$("ul.midtabs li").click(function() {
		$("ul.midtabs li").removeClass("active"); //Remove any "active" class
		$(this).addClass("active"); //Add "active" class to selected tab
		$(".midtab_content").hide(); //Hide all tab content
		var activeTab = $(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
		$(activeTab).fadeIn();
		return false;
	});
});      
 </script> 
