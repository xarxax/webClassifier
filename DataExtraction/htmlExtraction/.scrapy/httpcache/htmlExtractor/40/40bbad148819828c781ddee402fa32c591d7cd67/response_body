<script type="text/javascript">
    $('.SS li:gt(0)').hide();
    $('.SS li:last').addClass('last');
    $('.SS li:first').addClass('first');

    var cur = $('.SS li:first');
    var interval;

    $('#fwd').click(function () {
        goFwd();
    });

    $('#back').click(function () {
        goBack();
    });

    function goFwd() {
        stop();
        forward();
        start();
    }

    function goBack() {
        stop();
        back();
        start();
    }

    function back() {
        cur.fadeOut(1000);
        if (cur.attr('class') == 'first')
            cur = $('.SS li:last');
        else
            cur = cur.prev();
            cur.fadeIn(1000);
    }

    function forward() {
        cur.fadeOut(1000);
        if (cur.attr('class') == 'last')
            cur = $('.SS li:first');
        else
            cur = cur.next();
            cur.fadeIn(1000);
    }

    function start() {
        interval = setInterval("forward()", 10000);
    }

    function stop() {
        clearInterval(interval);
    }

    $(function () {
        start();
    });

    function goSolid(x) {
        x.style.opacity = "1";

    }

    function goTransparent(x) {
        x.style.opacity = "0.6";
    }

</script>

