$(document).ready(function(){
$('.slidecon').hide();
if($.trim($(".slidecon").text()) == "") {
  $(".parent").remove();
}
$('.add a').click(function(){
	

	$('.slidecon').toggle('slow');
	$(".slidecon p").css("float" , "left") .css(("margin-top"),10);
	if($(".add a").hasClass('active')){
	   $(".add a").removeClass('active');
	   $(".add").removeClass('active_content');
  }else{
	   $(".add a").addClass('active');
	   $(".add").addClass('active_content');
  }
	});
	
		if(CCM_EDIT_MODE){
		$('.slidecon').show();
				if($.trim($(".slidecon").text()) == "") {
				$('.add').show();
		}		
		}
});


</script>






