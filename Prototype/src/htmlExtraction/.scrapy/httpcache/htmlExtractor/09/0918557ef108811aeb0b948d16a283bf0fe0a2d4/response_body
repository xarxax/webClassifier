			<script>

      var u341443_delay = 0;
      u341443_delay = u341443_delay * 1000;

      $(document).ready(function(){
        u341443_animationClass();
        u341443_viewPort();
        if( 'false' == 'true'){
          onHover();
        }
      });

      function onHover(){
        if($('.' + 'animate').hasClass != 'fadeInDown'){
          $('.' + 'animate').hover(function(){
            console.log('test');
            $('.' + 'animate').addClass('fadeInDown');
          }, function(){
            setTimeout(function(){
              console.log('leaving');
              $('.' + 'animate').removeClass('fadeInDown');
            }, (1 * 1000) );

          });
        }
      }

      function u341443_animationClass(){
        if( $('.' + 'animate').hasClass != 'animated' ){
          $('.' + 'animate').addClass('animated');
          $('.animated').css({
            'opacity': 0
          })
          if('loopRepeat' == 'loopInfinite' ){
            $('.' + 'animate').addClass("infinite");
          }
        }
        else{
          return;
        }
      };

      function u341443_viewPort(){
        const $animation_elements = $('.' + 'animate');
        const $window = $(window);

        $window.on('scroll resize', check_view);
        $window.trigger('scroll');

        function check_view(){
          var windowHeight = $window.height();
          var windowTopPosition = $window.scrollTop();
          var windowBottomPosition = (windowHeight + windowTopPosition);

          //Configure variables to get elements height + height from top of page.
          $.each($animation_elements, function(){
            var $element = $(this);
            var elementHeight = $element.outerHeight();
            var elementTopPosition = $element.offset().top;
            var elementBottomPosition = (elementTopPosition + elementHeight);

          //Check to see if current container is within viewport.
          if ((elementTopPosition <= windowBottomPosition) &&
          (elementBottomPosition >= windowTopPosition)){
            setTimeout(function(){
              $element.addClass('fadeInDown');
              $element.css({'opacity': 1});
              if( 'false' == 'true'){
                $('.' + 'animate').addClass('animation-direction');
              }
            }, u341443_delay);
          }
          else if( 'loopRepeat' == 'loopRepeat' ){
             $element.removeClass('fadeInDown');
             $element.css({'opacity': 0});
           };
        });
        };
      };


      </script>
		
