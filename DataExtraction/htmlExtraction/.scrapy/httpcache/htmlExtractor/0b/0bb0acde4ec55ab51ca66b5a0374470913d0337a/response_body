    <script language="JavaScript">

function checkform(){

	var fn = document.Buyfrm.fname.value;
	var fnl= document.Buyfrm.lname.value;
	var comp = document.Buyfrm.company.value;
	var email = document.Buyfrm.email.value;
	var user = document.Buyfrm.username.value;
	var pass= document.Buyfrm.pass.value;
	var pass1 = document.Buyfrm.confirmpass.value;

	if(fn==0){
		alert("Please Enter Your First Name");
		document.Buyfrm.fname.focus();
		return false;
	}
	if(fnl==0){
		alert("Please Enter Your Last Name");
		document.Buyfrm.lname.focus();
		return false;
	}
	if(comp==0){
		alert("Please Enter Your Company Name");
		document.Buyfrm.company.focus();
		return false;
	}
	if(email==0){
		alert("Please Enter Your Email Id");
		document.Buyfrm.email.focus();
		return false;
	}
	if(user==0){
		alert("Please Enter a Username");
		document.Buyfrm.username.focus();
		return false;
	}
	if(pass==0){
		alert("Please Enter a Password");
		document.Buyfrm.pass.focus();
		return false;
	}
	if(pass1==0){
		alert("Please Enter Confirm Password");
		document.Buyfrm.confirmpass.focus();
		return false;
	}
	
	if(document.Buyfrm.confirmpass.value!=document.Buyfrm.pass.value){
		alert("Password and Confirm Password Mismatch");
		document.Buyfrm.confirmpass.focus();
		return false;
	}	
	
	else{
		re=/\w{1,}/;
		emailExp= /^\w+(\-\w+)*(\.\w+(\-\w+)*)*@\w+(\-\w+)*(\.\w+(\-\w+)*)+$/ ; 

		if(!(emailExp.test(email)))
		{
			alert("Please Enter A Valid Email Id");
			document.Buyfrm.email.select();
			return false;
		}
	}
}
    </script>
