<script type="text/javascript">

//variable that will increment through the images
var step = 0
var whichimage = 0

function slideit(){
 //if browser does not support the image object, exit.
 if (!document.images)
  return
 document.getElementById('slide').src = slideimages[step].src
 whichimage = step
 if (step<5)		//< #of pics less 1
  step++
 else
  step=0
 //call function "slideit()" every 3.5 seconds
 setTimeout("slideit()",3500)
}

function slidelink(){
 if (whichimage == 0)
  window.location = "idx_value_line.html"

else if (whichimage == 1)
  window.location = "st/about_suburban_tool.html"
else if (whichimage == 2)
  window.location = "st/qoc-sineplate.html"
else if (whichimage == 3)
  window.location = "vl/vl-015_collet_sets.html"
else if (whichimage == 4)
  window.location = "vl/vl-016_tool_post_sets.html"

}

slideit()

</script>	
