<script LANGUAGE="javascript">

<!-------HIDING

var q_array=new Array();
var no_questions=0;
var s = 0
leftPos=0
leftPos1=0 


   leftPos=screen.width-400
   leftPos1=screen.width-6000


function not_cheating(q_no)
{
        var bool=true;

        for(i=0;i<q_array.length;i++)
        {
                if (q_no==q_array[i])
                {
                        //question has already been answered
                        bool=false;
                }
        }
        if (bool==true)
        //New question
        {
                        no_questions+=1;
                        q_array[no_questions]=q_no;
        }
        return bool;
}

function quiz(pt,q_no) {
 q_no=(q_no*3)-1;
if(not_cheating(q_no))
        {
         s = s + pt             
        }
        document.total.score.value = s

}


//--------------DONE HIDING------>
</script>

