        //Reload these javascript functions after each page postback
        function pageLoad(sender, args) {
            $(document).ready(function () {
                //expanding panels originally in yeah.js
                $('.toggle').click(function () {
                    var $tr = $(this).closest('tr');
                    $tr.add($tr.next('.edit')).toggle();
                });
                $('.toggle').click(function () {
                    var $tr = $(this).closest('tr');
                    $tr.add($tr.prev('.view')).toggle();
                });

                //this stops the youtube video when the modal video window closes
                $("#featurevid1").on('hidden.bs.modal', function (e) {
                    $("#featurevid1 iframe").attr("src", $("#featurevid1 iframe").attr("src"));
                });

                $("#featurevid2").on('hidden.bs.modal', function (e) {
                    $("#featurevid2 iframe").attr("src", $("#featurevid2 iframe").attr("src"));
                });
            });
        }

        function openFeatureVid1Modal() {
            $('#featurevid1').modal('show');
        }

        function openFeatureVid2Modal() {
            $('#featurevid2').modal('show');
        }
    </script>
