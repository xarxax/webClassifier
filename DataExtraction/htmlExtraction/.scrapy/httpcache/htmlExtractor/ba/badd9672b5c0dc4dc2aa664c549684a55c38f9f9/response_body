function SetupNewValidatorUpdateDisplay() {
  try {
    var functionBody, newFunction;
    functionBody = ValidatorUpdateDisplay.tstringWriter();
    functionBody = functionBody.substring(functionBody.indexOf('{') + 1, functionBody.lastIndexOf('}'));
    newFunction = new Function('val', 'ChangeColor(val);' + functionBody);
    ValidatorUpdateDisplay = newFunction;
  } catch (e) {}
 }
function ChangeColor(val){
  var validationControls = new Array();
  var lup;
  for (lup = 0; lup < Page_Validators.length; lup++) {
    if (Page_Validators[lup].controltovalidate == val.controltovalidate) validationControls.push(Page_Validators[lup]);
  }
  var isValid = true;
  for (lup = 0; lup < validationControls.length; lup++) isValid = (isValid && validationControls[lup].isvalid);
  if (isValid) {
    $(val.controltovalidate).style.backgroundColor = '';
  } else {
    $(val.controltovalidate).style.backgroundColor = '#cc3333';
  }
}
SetupNewValidatorUpdateDisplay();

</script>
