function getSmRlogs(){
    if(gSPI.Login){
        return  {"prsnlId" : gSPI.prsnlId,
                 "member"  : { birth           : gSPI.birth,
                               entry           : gSPI.entry,
                               cityCode        : gSPI.cityCode,
