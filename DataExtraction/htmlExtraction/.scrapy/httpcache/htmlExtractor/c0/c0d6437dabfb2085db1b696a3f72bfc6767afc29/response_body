<!-- A11y Move focus to close button -->
<script>
	var $focusModal = $(".modal"),
    $focusModalClose = $(".close");

    $focusModal.on("shown.bs.modal", function() {
        document.activeElement.blur();
        $focusModalClose.focus();
    });
</script>
