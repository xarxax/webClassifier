                                $(function () {

                                    // Setup Owl Carousel 2
                                    var owl = $("#carousel1050").owlCarousel({
                                        items: 1,
                                        //loop: true,
                                        navRewind: false,
                                        smartSpeed: 1000,
                                        fluidSpeed: 1000,
                                        navSpeed: true,
                                        autoplay: false, //true
                                        autoplayHoverPause: true,
                                        lazyLoad: true
                                    });


                                    //set the initial margin for slide content
                                    var marginSize = (($(window).width() - (960)) / 2);
                                    //$("#carousel1050 .slideContent").css("margin-left", marginSize);

                                    //on window resize adjust slideContent accordingly to make responsive.
                                    $(window).on('resize', function () {
                                        if ($(window).width() >= 960) {
                                            marginSize = (($(window).width() - (960)) / 2);
                                            //$("#carousel1050 .slideContent").css("margin-left", marginSize);
                                        }
                                    });

                                    // Custom Navigation Events
                                    $("#btnNext1050").click(function () {
                                        owl.trigger('next.owl.carousel', [300]);
                                        //owl.trigger('owl.next');
                                    });
                                    $("#btnPrev1050").click(function () {
                                        owl.trigger('prev.owl.carousel', [300]);
                                        //owl.trigger('owl.prev');
                                    });

                                });
