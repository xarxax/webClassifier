 <script>
$(document).ready(function(){
	        $(".ross").bind('click', function(){                              
                 $(".hidden_ross").empty(); 
                $(".closeProfile").css('display', 'block');
                var id = $(this).attr("data-attr");
                var rowsid = $(this).attr("rows-attr");               
                $( ".hiddenLeader-"+id).clone(true).appendTo(".rows-"+rowsid);
                //$(".hidden_ross").css('display','block');
                $(".hidden_ross .hiddenLeader-"+id).css('display','block');
                $(".hidden_ross").slideDown("slow","swing");
				
				var self = $(".first_block");
				 var top = $(".second_block")
				 
				 if (self.hasClass("slide_margin_bottom")) {
                     self.removeClass("slide_margin_bottom");
                 } else {
                     self.removeClass("slide_margin_bottom").addClass("slide_margin_bottom");
                 }
				 
				 if (top.hasClass("slide_margin_top")) {
                     top.removeClass("slide_margin_top");
                 } else {
                     top.removeClass("slide_margin_top").addClass("slide_margin_top");
                 }
               });   
               $(".closeProfile").bind('click', function(){                                         
                    $(".hidden_ross").slideUp("slow","swing");				
                    var self = $(".first_block");
                      var top = $(".second_block")
                          if (self.hasClass("slide_margin_bottom")) {
                        self.removeClass("slide_margin_bottom");
                             } else {
                              self.removeClass("slide_margin_bottom").addClass("slide_margin_bottom");
                                 }
                                  if (top.hasClass("slide_margin_top")) {
                            top.removeClass("slide_margin_top");
                                } else {
                           top.removeClass("slide_margin_top").addClass("slide_margin_top");
                               }
                  });
               
});            

		</script>
