var preloadImages=new Array() // preloads images
for(var i=0;i<images.length;i++) {
preloadImages[i]=new Array()
for(var j=0;j<images[i].length;j++){
preloadImages[i][j]=new Image()
preloadImages[i][j].src=images[i]
}
}

lastN = ""
lastPic=""

function mOver(obj){
num=obj.name.replace(/[^0-9]/g,"")
if(lastN != obj.name){
document.images[obj.name].src = (images[num].length==3?images[num][2]:images[num][1])
}
}

function mOut(obj){
num=obj.name.replace(/[^0-9]/g,"")
if(lastN != obj.name){
document.images[obj.name].src = images[num][0]
}
}

function mActive(obj){
if(typeof obj!="string"){obj=obj.name}
num=obj.replace(/[^0-9]/g,"")

document.images[obj].src = images[num][1]
if(lastN != ""&&lastN != obj){
document.images[lastN].src = lastPic
}
lastN = obj
lastPic=images[num][0]
}
