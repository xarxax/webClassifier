							  	function validate()
								{
									var Email = document.ccoptin.ea.value;;
									if(Email.indexOf("@")==-1 || Email.indexOf(".")==-1 || Email.length<=5)
										{
											alert("Please enter a valid email address.");
											document.ccoptin.ea.focus();
											return false;
										}
										return true;
									}
							  </script>
							  
