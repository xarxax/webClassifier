	<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript">
    <!--

		var gStandardStatus		= 'Karl Schnell'; window.defaultStatus = gStandardStatus

		var gKS = new Array()
			gKS.ANI = new Array()
			gKS.ANI.StartWartenMS = 500  // Ms-Warten nach Laden aller Elemente, bevor Animation startet.
			gKS.ANI.SchritteMax   = 50   // Anzahl Maximaler Animationsschritte
			gKS.ANI.SchrittMS	  = 40   // Schritt-Intervall in Ms ( 25 Schritte pro Sekunde = 40 )
			gKS.ANI.F_VerzStart   = 0.2  // Start-Verzoegerungs-Faktor ( 0 = linear )
			gKS.ANI.F_VerzZiel    = 2    // Ziel-Verzoegerungs-Faktor ( 0 = linear )

		function KS_START() {
			// Deckungsstufen-Array
			gKS.DECKUNGSSTUFEN = new Array()
			var xSchritte    = gKS.ANI.SchritteMax - 1
			var xFaktorStart = gKS.ANI.F_VerzStart + 1
			var xFaktorZiel  = gKS.ANI.F_VerzZiel  + 1
			for (var xAktSchritt=0;xAktSchritt<=xSchritte;xAktSchritt++) {
				var xFaktor = xAktSchritt / xSchritte
				gKS.DECKUNGSSTUFEN[xAktSchritt] = Math.round(  100*((Math.pow(xFaktor,xFaktorStart)*(1-xFaktor))+((1-Math.pow(1-xFaktor,xFaktorZiel))*xFaktor))  )
			}
			while(gKS.DECKUNGSSTUFEN[0]==gKS.DECKUNGSSTUFEN[1]) {
				gKS.DECKUNGSSTUFEN.shift()
			}
			while(gKS.DECKUNGSSTUFEN[gKS.DECKUNGSSTUFEN.length-1]==gKS.DECKUNGSSTUFEN[gKS.DECKUNGSSTUFEN.length-2]) {
				gKS.DECKUNGSSTUFEN.pop()
			}
			// ENDE Deckungsstufen-Array
			var xZEIT = Math.floor( (new Date().getTime()) / (1000*60) )
			var xA_HREFS = document.getElementsByName('SP_LINK')
			for ( var X=0 ; X<xA_HREFS.length ; X++ ) {
				var xString = xA_HREFS[X].href.split( '?x=' )
				xString[1] = xString[1].substring( 1 , xString[1].length )
				xA_HREFS[X].href = xString[0] + '?x=' + xZEIT + xString[1]
			}
			var xA_BILDER = document.getElementsByName('SP_BILD')
			gKS.SPRACHEN_NOCH_LADEN = xA_BILDER.length
			gKS.SPRACHBILDER = new Array()
			gKS.SPRACHBILDER[0] = '#'
			for ( var X=0 ; X<xA_BILDER.length ; X++ ) {
				var xNr = X+1
				xA_BILDER[X].NUMMER = xNr
				gKS.SPRACHBILDER[xNr] = new Array()
				gKS.SPRACHBILDER[xNr].PFAD_0 = new Image()
				gKS.SPRACHBILDER[xNr].PFAD_0.src = xA_BILDER[X].src
				gKS.SPRACHBILDER[xNr].PFAD_1 = new Image()
				gKS.SPRACHBILDER[xNr].PFAD_1.onload = function() { KS_SPRACHE_GELADEN() }
				gKS.SPRACHBILDER[xNr].PFAD_1.src = ( (''+xA_BILDER[X].src).split('_0.') ).join('_1.')
			}
		}

		function KS_PICOVER ( nOBJ , nOuOv ) {
			if (nOBJ.NUMMER) {
				nOBJ.src = ( nOuOv>0 ) ? gKS.SPRACHBILDER[nOBJ.NUMMER].PFAD_1.src : gKS.SPRACHBILDER[nOBJ.NUMMER].PFAD_0.src
			}
		}

		function KS_SPRACHE_GELADEN() {
			gKS.SPRACHEN_NOCH_LADEN --
			if (gKS.SPRACHEN_NOCH_LADEN<=0) {
				gKS.ANI_AKT_POS = 0
				gKS.ANIMATION = window.setTimeout( 'KS_STARTE_ANIMATION()' , gKS.ANI.StartWartenMS )
			}
		}

		function KS_STARTE_ANIMATION() {
			gKS.ANIMATION = window.setInterval( 'KS_ANIMATION()' , gKS.ANI.SchrittMS )
		}

		function KS_ANIMATION() {
			gKS.ANI_AKT_POS ++
			if (gKS.ANI_AKT_POS<gKS.DECKUNGSSTUFEN.length) {
				var xDECK = gKS.DECKUNGSSTUFEN[gKS.ANI_AKT_POS]/100
				with (BILD_ANIMATION.style) {
					opacity=xDECK; MozOpacity=xDECK; KhtmlOpacity=xDECK; filter='Alpha(opacity='+gKS.DECKUNGSSTUFEN[gKS.ANI_AKT_POS]+')'
				}
			} else {
				window.clearInterval( gKS.ANIMATION )
			}
		}

    //-->
    </SCRIPT>
