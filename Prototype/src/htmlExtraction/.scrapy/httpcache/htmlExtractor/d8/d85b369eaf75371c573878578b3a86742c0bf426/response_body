	<script type="text/javascript">
		function clickclear(thisfield, defaulttext) {
			if (thisfield.value == defaulttext) {
				thisfield.value = "";
			}
		}

		function clickrecall(thisfield, defaulttext) {
			if (thisfield.value == "") {
				thisfield.value = defaulttext;
			}
		}

	</script>
	<script type="text/javascript">
		$(function() {
				$('.suggested_product').attr("id", function (arr) { return "sp" + arr;});
				$('.quicklook').attr("id", function (brr) { return "p" + brr;});
		});

		function showlook(id){
			id = id.replace(/s/g, "");
			$('#' + id).css({display: 'block'});
		}
		function hidelook(id){
			id = id.replace(/s/g, "");
			$('#' + id).css({display: 'none'});
		}
	</script>
