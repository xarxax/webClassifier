  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-72477602-1', 'auto');
  ga('send', 'pageview');

</script>



	<script>


		$(function() {
						$('.contact-tab').click(function(e) {
				$('.contact-box').animate({
				  marginLeft: parseInt($('.contact-box').css('marginLeft'),10) == 0 ?
					$('.contact-box').outerWidth() :
					0
				});

			});

			var objNavSystem = $(".nav");

			$('a[href="#"]').click(function(e) {
				e.preventDefault();
			});


			objNavSystem.find("li").each(function() {
				if($(this).find("ul").length > 0) {

					$(this).click(function() {
						$(this).find("ul").stop(true, true).slideToggle('fast');
					});					


					//show subnav on hover
					$(this).mouseenter(function() {

					  if (window.matchMedia) 
					  {
						//Make sure we are in a desktop mode menu
						var bolDesktopMode = window.matchMedia( "(min-width: 980px)" );

						if(bolDesktopMode.matches)
						{					
							$(this).find("ul").stop(true, true).slideDown('fast');
						}
					  }
					  else
					  {
						  $(this).find("ul").stop(true, true).slideDown('fast');
					  }

					});

					//hide submenus on exit
					$(this).mouseleave(function() {

					  if (window.matchMedia) 
					  {

						//Make sure we are in a desktop mode menu
						var bolDesktopMode = window.matchMedia( "(min-width: 980px)" );

						if(bolDesktopMode.matches)
						{					
							$(this).find("ul").stop(true, true).slideUp('fast');
						}
					  }
					  else
					  {
						$(this).find("ul").stop(true, true).slideUp('fast');
					  }						  
					});


				}
			});

						var strCurrentTitle = "Steel Plate Distributors & Steel Plate Service Centers";

			$('#HeroCarousel').carousel({
  				interval: 6000,
				cycle: true
			});
			
			$('.bluebar-inner').html($('#HeroCarousel div.active h2').html());

			$('#HeroCarousel').on('slid', function(e){

				//First we need to determine if headline available for slide
				if($('#HeroCarousel div.active h2').length > 0)
				{
					var strSlideHeadline = $('#HeroCarousel div.active h2').html();
					if(strCurrentTitle != strSlideHeadline)
					{
						if(strSlideHeadline.trim() == '')
						{
							strSlideHeadline = 'Steel Plate Distributors & Steel Plate Service Centers';
						}

						$('.bluebar-inner').html(strSlideHeadline);

						strCurrentTitle = strSlideHeadline;
					}

				}
				else
				{
					if(strCurrentTitle != "Steel Plate Distributors & Steel Plate Service Centers")
					{
						$('.bluebar-inner').html('Steel Plate Distributors &amp; Steel Plate Service Centers');
						strCurrentTitle = "Steel Plate Distributors & Steel Plate Service Centers";
					}
				}

			});

		});

	</script>
