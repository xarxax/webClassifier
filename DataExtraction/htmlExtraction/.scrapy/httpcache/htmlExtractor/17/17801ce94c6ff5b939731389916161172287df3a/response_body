function transBox(){

	jQuery('#transInput').val('');

	jQuery('#transBox').submit();

}



jQuery('#makedir').click(function(){

	var b = jQuery('#drname').val();

	jQuery('#makedir').attr('href','/createDir.php?mkdr=' + b);

	jQuery('#makedir').click();

});



function classDropdown(){

	var a = jQuery('#classSelect').val();

	if (a == 12){

		window.location='/calendar/ultrasound';

	} else if (a == 13) {

		window.location='/calendar/high-frequency-leakage-in-esu-s';

	} else if (a == 14) {

		window.location='/calendar/patent-prosecution';

	} else if (a == 15) {

		window.location='/calendar/defibrillator-pm';

	} else if (a == 16) {

		window.location='/calendar/reliable-wireless-networks';

	} else if (a == 17) {

		window.location='/calendar/cost-of-service-ratio-cosr';

	} else if (a == 18) {

		window.location='/calendar/ventilator-maintenance';

	} else if (a == 19) {

		window.location='/calendar/test-equipment-calibration';

	} else if (a == 21) {

		window.location='/calendar/dunlee';

	} else if (a == 22) {

		window.location='/calendar/medical-wireless-network-future';

	}

}



function dinnerType(){

	if (jQuery('#dinner-type').val() == 'Guest'){

		jQuery('#attendee-box').hide();

		jQuery('#guest-box').show();

	} else if (jQuery('#dinner-type').val() == 'Attendee'){

		jQuery('#attendee-box').show();

		jQuery('#guest-box').hide();

	}

}



function hoverdiv(e,divid){

	var left = e.clientX + "px";

	var top = (e.clientY - 200) + "px";

	

	jQuery("#"+divid).css('left',left);

	jQuery("#"+divid).css('top',top);

	jQuery("#"+divid).css('position','fixed');

	

	jQuery("#"+divid).toggle(); return false;

}



function hoverdiv1(e,divid){

	var left = (e.clientX - 500) + "px";

	var top = (e.clientY - 450) + "px";

	

	jQuery("#"+divid).css('left',left);

	jQuery("#"+divid).css('top',top);

	jQuery("#"+divid).css('position','fixed');

	

	jQuery("#"+divid).toggle(); return false;

}



function deleteRecords(){

	if (jQuery("#deleteRecordsForm input:checkbox:checked").length > 0)

	{

		jQuery("#deleteRecordsForm").submit();

	} else {

		alert("Please select at least one record.");	

	}

}



