$(document).ready(function(){
	
	$(".cs-slider-bucket .sliding-block4 a").hover(function(){
		
		$(this).find(".features-slideup-content").css({"top":"0"});
		
	},function(){
		
		$(this).find(".features-slideup-content").css({"top":"213px"});
		
	});
	
	$.get("/customers?ArticleTags=Featured",function(data){
		
		var pagesLinksVar = $(data).find(".ip-articleindex-results .ipf-ai-articletitle");
		
		$.each(pagesLinksVar, function(index, value){
			var linkContents = $('<div></div>').append(pagesLinksVar[index]);
			getExternalImages(linkContents.find("a").attr("href"), ".ipf-article-photo img", index+1);
		});
		$.each(pagesLinksVar, function(index, value){
			var linkContents = $('<div></div>').append(pagesLinksVar[index]);
			getExternalTitle(linkContents.find("a").attr("href"), "#bg-two h1", index+1);
		});
		$.each(pagesLinksVar, function(index, value){
			var linkContents = $('<div></div>').append(pagesLinksVar[index]);
			getExternalText(linkContents.find("a").attr("href"), ".syndicate-statistic", index+1);
		});
		
	});
	
});

function getExternalImages(url, element, parentIndex) {
	$.get(url,function(data){
		var pageContentVar = $(data).find(element);
		$.each(pageContentVar, function(index, value){
			var elementContents = $('<div></div>').append(pageContentVar[index]);
			//console.log("getExternalImages( " + elementContents.find("img").attr("src") + " )");
			$(".cs-slider-bucket #features-image-" + parentIndex).css({"background-image":"url("+elementContents.find("img").attr("src")+")"});
		});
	});
}
function getExternalTitle(url, element, parentIndex) {
	$.get(url,function(data){
		var pageContentVar = $(data).find(element);
		$.each(pageContentVar, function(index, value){
			var elementContents = $('<div></div>').append(pageContentVar[index]);
			if(index == 0) {
				$(".cs-slider-bucket #features-image-" + parentIndex).parent().parent().find("a").attr("href",url);
				$(".cs-slider-bucket #features-image-" + parentIndex + " .cs-slider-title").text(elementContents.text());
			}
		});
	});
}

function getExternalText(url, element, parentIndex) {
	$.get(url,function(data){
		var pageContentVar = $(data).find(element);
		$.each(pageContentVar, function(index, value){
			var elementContents = $('<div></div>').append(pageContentVar[index]);
			$(".cs-slider-bucket #features-image-" + parentIndex + " .cs-slider-desc .syndicate-statistic").html(elementContents.find(".syndicate-statistic").html());
		});
	});
}

</script>

