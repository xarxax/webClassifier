                                    $(document).ready(function () {
                                        var enableCountry = function(obj) {
                                                if ( $(obj).val() == "USA" || $(obj).val() == "CAN") {
                                                    $(".ZipPostal").removeAttr("disabled");
                                                    $(".ZipPostal").css({backgroundColor: '#FFFFFF'});
                                                    $(".ZipPostal").css({ color: '#000000' });
                                                    $(".State").removeAttr("disabled");
                                                    $(".State").css({ backgroundColor: '#FFFFFF' });
                                                    $(".State").css({ color: '#000000' });
                                                    //if ($(obj).val() == "USA") {
                                                    //    //$(".DistributorTypeUSA").removeClass("hide");
                                                    //    //$(".DistributorBrandUSA").removeClass("hide");
                                                        
                                                    //    //$(".DistributorTypeCAN").addClass("hide");
                                                    //    //$(".DistributorBrandCAN").addClass("hide");
                                                        
                                                    //}else
                                                    //{
                                                    //    //$(".DistributorTypeCAN").removeClass("hide");
                                                    //    //$(".DistributorBrandCAN").removeClass("hide");
                                                        
                                                    //    //$(".DistributorTypeUSA").addClass("hide");
                                                    //    //$(".DistributorBrandUSA").addClass("hide");
         
                                                    //}

                                                    //$(".ZipPostal").attr("disabled", "disabled");
                                                    //$(".State").attr("disabled", "disabled");
                                                }
                                                else {
                                                    $(".ZipPostal").attr("disabled", "disabled");
                                                    $(".ZipPostal").css({backgroundColor: '#E0E0E0'});
                                                    $(".ZipPostal").css({color: '#E0E0E0'});
                                                    $(".ZipPostal").val("");
                                                    $(".State").attr("disabled", "disabled");
                                                    $(".State").css({ backgroundColor: '#E0E0E0' });
                                                    $(".State").css({ color: '#E0E0E0' });
                                                    $(".State").val("");
                                                    
                                                    //$(".DistributorTypeUSA").addClass("hide");
                                                    //$(".DistributorBrandUSA").addClass("hide");
                                                    //$(".DistributorTypeCAN").addClass("hide");
                                                    //$(".DistributorBrandCAN").addClass("hide");
                                                }
                                                
                                            }

                                            enableCountry($(".ddlCountry"));
                                            $(".ddlCountry").change( function() {
                                                enableCountry($(this));
                                            });
                                            
                                            $("#btn-modal").click(function () {

                                                var page = "https://ourhistory.cmworks.com";

                                                var $dialog = $('<div></div>')
                                                               .html('<iframe style="border: 0px; z-index:50;" src="' + page + '" width="980" height="630"></iframe>')
