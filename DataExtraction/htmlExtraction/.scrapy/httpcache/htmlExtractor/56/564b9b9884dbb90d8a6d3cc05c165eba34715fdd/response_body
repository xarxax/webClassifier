    <script src="js/jquery.simplemodal-1.1.1.min.js" type="text/javascript" charset="utf-8"></script>
    <script type="text/javascript">
        (function () {
            var $o = $(".operator").click(function (e) {
                // show overlay
                $("#operator").modal({
                    onOpen: function (dialog) {
                        dialog.overlay.fadeIn(200, function () {
                            dialog.container.fadeIn(200);
                            dialog.data.fadeIn(200);
                        }).click(function () {
                            $(".modalClose").trigger("click");
                        });
                        Kellogg.track("/Overlay/operator/Show");
                    },
                    onClose: function (dialog) {
                        dialog.data.fadeOut(200);
                        dialog.container.fadeOut(200, function () {
                            dialog.overlay.fadeOut(200, function () {
                                $.modal.close();
                            });
                        });
                    },
                    overlay: 80
                });

                e.preventDefault();
            });

            if (location.hash.match(/operator/i)) {
                $o.triggerHandler("click");
            }
        })();
    </script>
