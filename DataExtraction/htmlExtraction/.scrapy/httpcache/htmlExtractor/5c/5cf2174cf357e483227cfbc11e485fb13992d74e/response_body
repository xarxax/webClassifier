        $('animation1').update(aniBgImage[0]);
  $('animation2').update(aniHtml[0]);
  $('spaceButton').update(aniButton[0]);     
      
  if(aniBgImage.length>1){
    var aniStartAnimation=new PeriodicalExecuter(homeAnimation, 6);
  }
    
function homeAnimation(){
    
  if(aniRound++ == 0){
    aniStartAnimation.stop();
    new PeriodicalExecuter(homeAnimation, 12);
  }
  
  if(aniHomeCounter++ + 1 == aniBgImage.length){
    aniHomeCounter=0;
  }
  
  if($browser.indexOf("IE")==-1){
      new Effect.Parallel([
        new Effect.Opacity($('animation2'), { sync: true, from: 1, to: 0}),
        new Effect.Opacity($('animation1'), { sync: true, from: 1, to: 0}),
        new Effect.Opacity($('spaceButton'), { sync: true, from: 1, to: 0})
      ],{
        duration: 2,
        afterFinish:function(){
          $('animation1').innerHTML=aniBgImage[aniHomeCounter];
          $('animation2').innerHTML=aniHtml[aniHomeCounter];
          $('spaceButton').update(aniButton[aniHomeCounter]);

          new Effect.Parallel([
            new Effect.Opacity($('animation2'), { sync: true, from: 0, to: 1}),
            new Effect.Opacity($('animation1'), { sync: true, from: 0, to: 1}),
            new Effect.Opacity($('spaceButton'), { sync: true, from: 0, to: 1})
          ], {
            duration: 2,
          });
          linkButtonAnimation();
        }
      });
  }else{
     $('spaceButton').update("");
      new Effect.Opacity(
        $("spaceButton"),{ from: 1.0, to: 0, duration: 0, afterFinish:function(){
            new Effect.Parallel([
            new Effect.Opacity($('animation1'), { sync: true, from: 1, to: 0}),
            new Effect.Opacity($('animation2'), { sync: true, from: 1, to: 0})
            ],{
                duration: 2,
                afterFinish:function(){
                  $('animation1').innerHTML=aniBgImage[aniHomeCounter];
                  $('animation2').innerHTML=aniHtml[aniHomeCounter];

                  new Effect.Parallel([
                    new Effect.Opacity($('animation2'), { sync: true, from: 0, to: 1}),
                    new Effect.Opacity($('animation1'), { sync: true, from: 0, to: 1})
                  ], {
                    duration: 2,afterFinish:function(){
                      $('spaceButton').update(aniButton[aniHomeCounter]);
                      new Effect.Opacity($("spaceButton"), { from: 0, to: 1.0, duration: 0.0 });
                      linkButtonAnimation();
                    }
                  });
                  
                }
              });
         }})
  }
  
}
</script>
