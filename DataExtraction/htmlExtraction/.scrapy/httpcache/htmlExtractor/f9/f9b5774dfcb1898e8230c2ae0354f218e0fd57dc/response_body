        function cntFocus(){
            jQuery('.newsArea .topics > .inner a').keydown(function(e){
              if(e.shiftKey && e.keyCode == 9) {
                e.preventDefault();
                jQuery('.box-news .tab > li.active a').focus();
              }
            });
            jQuery('.newsArea .link-list > li:last-child a').keydown(function(e){
              if(e.shiftKey && e.keyCode == 9) {
                e.preventDefault();
                jQuery('.box-news .tab > li.active a').focus();
              } else if(e.keyCode == 9) {
                e.preventDefault();
                if($('.box-news .tab > li#n_m0').hasClass("active")){
                  jQuery('.box-news .tab > li#n_m1 a').focus();
                } else if($('.box-news .tab > li#n_m1').hasClass("active")) {
                  jQuery('.box-news .tab > li#n_m2 a').focus();
                } else if($('.box-news .tab > li#n_m2').hasClass("active")) {
                  jQuery('.box-news .tab > li#n_m3 a').focus();
                } else if($('.box-news .tab > li#n_m3').hasClass("active")) {
                  jQuery('.box-news .tab > li#n_m4 a').focus();
                } else if($('.box-news .tab > li#n_m4').hasClass("active")) {
                  jQuery('.box-news .tab > li#n_m5 a').focus();
                } else if($('.box-news .tab > li#n_m5').hasClass("active")) {
                  jQuery('.box-news .tab > li#n_m6 a').focus();
                } else if($('.box-news .tab > li#n_m6').hasClass("active")) {
                  jQuery('#ad1 a:nth-of-type(1)').focus();
                }
              }
            });
          }
      });
