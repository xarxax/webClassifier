<script type="text/javascript" language="javascript">

    $(".designs-container").html("<div style='text-align:center'><img src='App_Themes/RenLor/images/ajax-loader.gif' /></div>");
    var webService = NopSolutions.NopCommerce.Web.WebServices;

    function applySlider(html) {

        $(".designs-container").html($(html));

        $("a.zoom").fancybox({
            'opacity': true,
            'overlayShow': false,
            'titlePosition': 'inside'
        });

        $(".designs-container ul.design-group")
		.anythingSlider({
		    width: null,
		    height: null,
		    startStopped: false,
		    autoPlay: false,
		    animationTime: 500,
		    navigationFormatter: function (index, panel) {
		        return index; // This would have each tab with the text 'X Panel' where X = index
		    }
		});
    }


    function handleDesignMouseOver(design) {
        $(design).find("div#menu" + design.id).show();
    }

    function handleDesignMouseOut(design) {
        $(design).find("div#menu" + design.id).hide();
    }

    $("#listdesigns").change(function () {

        var list = document.getElementById("listdesigns");
        var selectedCategory = list.options[list.selectedIndex].value;
        ShowRelatedDesigns(selectedCategory);
    });

    function ShowRelatedDesigns(categoryId) {

        $(".designs-container").html("<div style='text-align:center'><img src='App_Themes/RenLor/images/ajax-loader.gif' /></div>");
        webService.GetFrontPageDesigns(categoryId, applySlider);
    }

    function OnClick_BuyButton(object) {
        var designProductId = object.id.replace("buy", "");
        webService.FrontPageBuyDesign(designProductId, function (pageNumber) { window.location = "StConfigurator.aspx?PageMethod=3#panel1-" + pageNumber; });
    }
</script>

    
