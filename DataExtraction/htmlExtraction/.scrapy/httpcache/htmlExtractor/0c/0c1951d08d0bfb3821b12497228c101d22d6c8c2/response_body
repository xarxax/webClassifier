function WM_preloadImages() {
	if (document.images) {
		if (typeof(document.WM) == 'undefined') {
			document.WM = new Object();
		}
		document.WM.loadedImages = new Array();
		var argLength = WM_preloadImages.arguments.length;
		for(arg=0;arg<argLength;arg++) {
			document.WM.loadedImages[arg] = new Image();
			document.WM.loadedImages[arg].src = WM_preloadImages.arguments[arg];
		}
	}
}

function changeImagesBack() {
	//wait(100);
	document.welcome.src='/es/images/welcomeMessageNew.gif';
	document.home_0.src='/es/hpimages/dust_1.jpg';
	document.home_1.src='/es/hpimages/compressor_3.jpg';
	document.home_2.src='/es/hpimages/engine_1.jpg';
}

function wait(millis)
{
date = new Date();
var curDate = null;

do { var curDate = new Date(); }
while(curDate-date < millis); 
} 

