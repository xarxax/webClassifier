<script>
jQuery(document).ready(function($){
	$("#herniamesh-request-info .bt-blue").on("click", function(e){
		e.preventDefault();
		var base = $(".herniamesh-columns-request-info").offset();
		var my = $(this).offset();
		var what = $(this).attr('data-what');
		$("#herniamesh-contact-us #what").attr('name', "w"+what);
		
		$("#herniamesh-contact-us").attr("autocomplete", "off");
		$("#herniamesh-contact-us input, #herniamesh-contact-us textarea").val("");
		$('#herniamesh-contact-us button').removeAttr('disabled', 'disabled');
		$("#herniamesh-contact-us").fadeIn(200);
		$("#herniamesh-contact-us > div").css("top", (my.top - base.top)-20 + "px");
		$("#herniamesh-contact-us").css("left", (my.left - base.left)+95 + "px");
	});
	$("#herniamesh-contact-us").append('<a href="#" class="fancybox-item fancybox-close" title="Chiudi"></a>');
	$("#herniamesh-contact-us .fancybox-close").on("click", function(e){
		e.preventDefault();
		$("#herniamesh-contact-us").fadeOut(200);
	});
	
	
	$('.vjs-tech').on('ended', function(){
		$('.vjs-tech').attr('src', $('.vjs-tech').attr('src'));
	});
	
		
});
$('.cms a.fancybox').fancybox();
$('.cms a.fancybox-iframe').fancybox({
		'width'				: '50%',
		'height'			: '50%',
        'autoScale'     	: false,
        'transitionIn'		: 'none',
		'transitionOut'		: 'none',
		'type'				: 'iframe'
	});

if(('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {

}else{


$(document.body).on('click', '#leo-top-menu [data-toggle="dropdown"]' ,function(e){
	e.preventDefault();
	location.href = "http://www.herniamesh.com/#prod";
});


}

$('body :not(script)').contents().filter(function() {
    return this.nodeType === 3;
}).replaceWith(function() {
    return this.nodeValue.replace(/[™®©]/g, '<sup>$&</sup>');
});

$('#herniamesh-contact-us').on('submit', function(){
	$('#herniamesh-contact-us button').html('<img src="http://www.herniamesh.com/themes/starshop/img/icon/form-ok.png" />').attr('disabled', 'disabled');
	window.setTimeout(function(){
		$('#herniamesh-contact-us').hide();
	}, 1000);
});

</script>
