	<script>
	$(function() {
		var siteNavPadding = 7;
		var siteNavigationTop = $('#site-navigation').offset().top - $('#top-nav').innerHeight() - siteNavPadding;
		var brandFixed = false;
		

		var brandAdjust = function() {
			var scrTop = $(window).scrollTop();
			if (window.innerWidth < 992)
				return;
			if ( scrTop >= siteNavigationTop && !brandFixed) {
				$('#site-navigation').addClass('fixed');
				$('#home-carousel').addClass('brandFixed');
				$('#brand-ribbon').addClass('hidden');
				$('#moving-brand').addClass('brandFixed');
				brandFixed = true;
			} else if ( scrTop < siteNavigationTop && brandFixed ) {
				$('#site-navigation').removeClass('fixed');
				$('#home-carousel').removeClass('brandFixed');
				$('#brand-ribbon').removeClass('hidden');
				$('#moving-brand').removeClass('brandFixed');
				brandFixed = false;
			} else {
				console.log(window.scrollY);
			}
			
		}

		$(window).resize(function() {brandAdjust();});
		$(window).scroll(function() {brandAdjust();});
		brandAdjust();

		var homeCarousel = function(direction) {
			direction = typeof direction !== 'undefined' ? direction : 1;
			var ind = $('#home-carousel li:not(.off-screen)').index();
			var toGet = 0;
			if (ind != -1) {
				toGet = (ind + direction).mod($('#home-carousel li').length);
			}

			$('#home-carousel li:not(.off-screen)').addClass('off-screen');
			$($('#home-carousel ul li').get(toGet)).removeClass('off-screen');
		}
		$('#home-carousel li').not(':first-child').addClass('off-screen');

		$('#home-carousel li').click(function() {$(this).toggleClass('off-screen')});


		homeCarousel();

		var carousel = setInterval(homeCarousel, 5000);
		

		$('#home-carousel').hover(function() {
			clearInterval(carousel);
		}, function() {
			carousel = setInterval(homeCarousel, 5000);
		});

		$('#home-carousel .arrow.right').click(function() {
			clearInterval(carousel);
			homeCarousel();
		});

		$('#home-carousel .arrow.left').click(function() {
			clearInterval(carousel);
			homeCarousel(-1);
		});

	})
	</script>
		<script>
		Number.prototype.mod = function(n) {
			return ((this%n)+n)%n;
		}

		$(function() {
			$('#mobile-menu-toggle').click(function(e) { e.preventDefault(); $('#mobile-nav').slideToggle( 300, function() { $(this).css('overflow', 'scroll'); }); });
			$('#contact_us_today a').click(function(e) { e.preventDefault(); $('#contact_us_container .form_container').slideToggle(); });
			//$('.-c-quote-form-mobile').hide();
			$('#free-quote-expand').click(function(e) { e.preventDefault(); $('.free-quote-form-mobile').slideToggle();	});
		});
	</script>
