<script type="text/javascript">

    function findPos(obj) {
        var curleft = curtop = 0;
            if (obj.offsetParent) {
                do {
                    curleft += obj.offsetLeft;
                    curtop += obj.offsetTop;
                } while (obj = obj.offsetParent);
            }

        return [curleft, curtop];
    }

    //this is for level3 menu
    var menu2PosTop, menu2PosLeft;
    function FindControlDisplayHorizonal(ctlobj, targetDivID) {

        try {            
            var myObj = document.getElementById(ctlobj);       
            var myTargetObj = document.getElementById(targetDivID);
            var pos = findPos(myObj);
            var browser = navigator.appName;
            
            //alert(browser);
            if (browser == 'Netscape') {
                    var h_style = document.defaultView.getComputedStyle(myObj, '').getPropertyValue('height');
                    var w_style = document.defaultView.getComputedStyle(myObj, '').getPropertyValue('width');
                    //this is for hidden object record image order
                    var myObjImageCount = document.getElementById(ctlobj+'hidden');
                    var imgCount = parseInt(myObjImageCount.value);
                    var menu3Top = parseInt(menu2PosTop) + (parseInt(h_style) * (imgCount - 1));
                    var menu3Left = parseInt(menu2PosLeft) + parseInt(w_style);
                    //alert(menu2PosTop + ',' + menu3Left);
                    myTargetObj.style.position = 'absolute';                
                    myTargetObj.style.left = menu3Left + 'px';
                    myTargetObj.style.top = menu3Top + 'px';
            }
            else if(browser == 'Opera' ){
                    var h_style = myObj.style.height;
                    var w_style = myObj.style.width;
                var myObjImageCount = document.getElementById(ctlobj+'hidden');
                var imgCount = parseInt(myObjImageCount.value);
                var menu3Top = parseInt(menu2PosTop) + (parseInt(h_style) * (imgCount - 1));
                var menu3Left = parseInt(menu2PosLeft) + parseInt(w_style);
                //alert(menu2PosTop + ',' + menu3Left);
                myTargetObj.style.position = 'absolute';                
                myTargetObj.style.left = menu3Left  ;
                myTargetObj.style.top = menu3Top ;
            }
            else  {
                //alert(myObj.clientWidth);
                myTargetObj.style.position = 'absolute';
                myTargetObj.style.posLeft = pos[0] + myObj.clientWidth;
                myTargetObj.style.posTop = pos[1];
            }

        }
        catch (e)
        {
            //alert('FindControlDisplayHorizonal:' + e);
        }
        //alert('x:' + pos[0] + 'Y:' + pos[1] + 'image width:' + myObj.clientWidth);
    }

//this is for level2 menu
    function FindControlDisplayVertical(ctlobj, targetDivID) {
        try {
            var myObj = document.getElementById(ctlobj);            
            var myTargetObj = document.getElementById(targetDivID);
            var pos = findPos(myObj);
            var browser = navigator.appName;            

            //alert(browser);
            if (browser != 'Netscape' && browser != 'Opera') {                
                myTargetObj.style.position = 'absolute';
                myTargetObj.style.posLeft = pos[0];                
                myTargetObj.style.posTop = pos[1] + myObj.clientHeight;
            }
            else {
                myTargetObj.style.position = 'absolute';
                myTargetObj.style.left = pos[0] + 'px';
                myTargetObj.style.top = pos[1] + myObj.clientHeight + 'px';
                menu2PosLeft = pos[0];
                menu2PosTop = pos[1] + myObj.clientHeight;
            }
        }
        catch (e) {
            //alert('FindControlDisplayVertical:' + e);
        }
        
    }

    //==================================for div visiability===========================================================

    function JJ_findObj(n, d) { //v4.0
        var p, i, x; if (!d) d = document; if ((p = n.indexOf("?")) > 0 && parent.frames.length) {
            d = parent.frames[n.substring(p + 1)].document; n = n.substring(0, p);
        }
        if (!(x = d[n]) && d.all) x = d.all[n]; for (i = 0; !x && i < d.forms.length; i++) x = d.forms[i][n];
        for (i = 0; !x && d.layers && i < d.layers.length; i++) x = MM_findObj(n, d.layers[i].document);
        if (!x && document.getElementById) x = document.getElementById(n); return x;
    }
    function MM_showHideLayers() { //v3.0
        var i, p, v, d, obj, args = MM_showHideLayers.arguments;
        for (i = 0; i < (args.length - 2); i += 3) if ((obj = JJ_findObj(args[i])) != null) {
        v = args[i + 2];
        if (obj.style) {
            obj = obj.style;
            //alert('...pause here...');
            
            if (v == 'show') {
                v = 'visible';
                d = "block";
            }
            else if (v = 'hide') {
            v = 'hidden';
            d = "none";
            }
            //v = (v == 'show') ? 'visible' : (v = 'hide') ? 'hidden' : v;
        }                            
            //obj.visibility = v;
            obj.display = d;
        }
    }
    //==================================for div visiability==================================================
</script>
