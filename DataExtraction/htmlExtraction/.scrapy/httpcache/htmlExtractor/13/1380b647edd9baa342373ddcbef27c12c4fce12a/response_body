function chgImg(theField,newImg) {



	if (document.images) {



		document[theField].src= eval(newImg + ".src")



		}



	}



var preloadFlag = false;



function preloadImages22() {



	if (document.images) {



		preloadImagesTmp();



		preloadFlag = true;



	}



}



preloadImages22();



