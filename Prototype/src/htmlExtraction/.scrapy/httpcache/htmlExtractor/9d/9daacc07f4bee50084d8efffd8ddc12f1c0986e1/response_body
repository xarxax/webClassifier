	<script type=text/javascript>
        function stylesheetToggle() {
            if ($(window).height() < 781) {
                $('#indexImage').attr('src', '/themes/default/img/images-index-2.jpg');
            } else {
                $('#indexImage').attr('src', '/themes/default/img/images-index.jpg');
            }
        }
        jQuery(window).bind('load', function() {
            var proportion = jQuery('#indexImage').width()/jQuery('#indexImage').height();

                var newHeight = jQuery(window).height() - 295;
                jQuery('.img-home').height( newHeight );
                jQuery('#indexImage').width(  (newHeight - 10) * proportion - (newHeight-10)*0.14 );
            jQuery(window).bind('resize', function() {

                var newHeight = jQuery(window).height() - 295;
                jQuery('.img-home').height( newHeight );
                jQuery('#indexImage').width(  (newHeight - 10) * proportion - (newHeight-10)*0.14 );

            });
        });

        stylesheetToggle();
        $(window).resize(stylesheetToggle);

	</script>
