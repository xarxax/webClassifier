    (function () {
        "use strict";

        if (document.getElementById("map")) {
            var locations = [
                ['<div class="map-info-box"><ul class="contact-info-list"><li><span><i class="fa fa-home fa-fw"></i></span> 1408 Stone health Ln. Lincoln, CA 95648, USA</li><li><span><i class="fa fa-phone fa-fw"></i></span>916-408-0920</li></ul></div>', 38.853585, -121.276720, 1]
            ];

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 13,
                center: new google.maps.LatLng(38.853585, -121.276720),
                scrollwheel: false,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                //styles: [{ "featureType": "landscape", "stylers": [{ "saturation": 0 }, { "lightness": -45 }, { "visibility": "on" }] },
