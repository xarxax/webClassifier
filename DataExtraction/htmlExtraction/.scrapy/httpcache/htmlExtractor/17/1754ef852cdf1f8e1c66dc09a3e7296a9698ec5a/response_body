	contentsource: "markup" //"markup" or ["container_id", "path_to_menu_file"]
  });


	$().ready(function() {
		$("[name=programDate]").datepicker();
		$("[name=mtgStartTime]").timepicker();
		
		$('input[type="text"]').css('width','200px');

		// Shorten the width of input boxes for these
		$("[name=mtgStartTime]").addClass('small');
		$("[name=mtgEndTime]").addClass('small');
		$("[name=UlrichStartTime]").addClass('small');
		$("[name=UlrichEndTime]").addClass('small');
		$("[name=airportToMtgMiles]").addClass('small');
		$("[name=airportToMtgMin]").addClass('small');
		$("[name=hotelToMtgMiles]").addClass('small');
		$("[name=hotelToMtgMin]").addClass('small');
		$("[name=numAttendees]").addClass('small');
		$("[name=percentFemale]").addClass('small');
		$("[name=percentMale]").addClass('small');
		$("[name=ageRangeFrom]").addClass('small');
		$("[name=ageRangeTo]").addClass('small');

		// Ensure only numbers are allowed to be input
		$("[name=airportToMtgMiles]").ForceNumericOnly();
		$("[name=airportToMtgMin]").ForceNumericOnly();
		$("[name=hotelToMtgMiles]").ForceNumericOnly();
		$("[name=hotelToMtgMiles]").ForceNumericOnly();
		$("[name=numAttendees]").ForceNumericOnly();
		$("[name=percentFemale]").ForceNumericOnly();
		$("[name=percentMale]").ForceNumericOnly();
		$("[name=ageRangeFrom]").ForceNumericOnly();
		$("[name=ageRangeTo]").ForceNumericOnly();
		
		
		
		

	});
	

// Numeric only control handler
jQuery.fn.ForceNumericOnly =
function()
{
    return this.each(function()
    {
        $(this).keydown(function(e)
        {
            var key = e.charCode || e.keyCode || 0;
            // allow backspace, tab, delete, arrows, numbers and keypad numbers ONLY
			
                     if (!e.shiftKey && !e.altKey && !e.ctrlKey &&
                     // numbers   
                         key >= 48 && key <= 57 ||
                     // Numeric keypad
                         key >= 96 && key <= 105 ||
                     // comma, period and minus, . on keypad
                        //key == 190 || key == 188 || key == 109 || key == 110 ||
                     // Backspace and Tab and Enter
                        key == 8 || key == 9 || key == 13 ||
                     // Home and End
                        key == 35 || key == 36 ||
                     // left and right arrows
                        key == 37 || key == 39 ||
                     // Del and Ins
                        key == 46 || key == 45)
                         return true;

                     return false;
        });
    });
};




jQuery.fn.maxLength = function(max){  
    this.each(function(){  
        //Get the type of the matched element  
        var type = this.tagName.toLowerCase();  
        //If the type property exists, save it in lower case  
        var inputType = this.type? this.type.toLowerCase() : null;  
        //Check if is a input type=text OR type=password  
        if(type == "input" && inputType == "text" || inputType == "password"){  
            //Apply the standard maxLength  
            this.maxLength = max;  
        }  
        //Check if the element is a textarea  
        else if(type == "textarea"){  
            //Add the key press event  
            this.onkeypress = function(e){  
                //Get the event object (for IE)  
                var ob = e || event;  
                //Get the code of key pressed  
                var keyCode = ob.keyCode;  
                //Check if it has a selected text  
                var hasSelection = document.selection? document.selection.createRange().text.length > 0 : this.selectionStart != this.selectionEnd;  
                //return false if can't write more  
                return !(this.value.length >= max && (keyCode > 50 || keyCode == 32 || keyCode == 0 || keyCode == 13) && !ob.ctrlKey && !ob.altKey && !hasSelection);  
            };  
            //Add the key up event  
            this.onkeyup = function(){  
                //If the keypress fail and allow write more text that required, this event will remove it  
                if(this.value.length > max){  
                    this.value = this.value.substring(0,max);  
                }  
            };  
        }  
    });  
};
</script>
