        <script type="text/javascript">
            var d = [];
            var d1 = [];
            var values = [];
            $(function(){
                fci();
                dci();
            });
            function dci(){
                try {
                    var oXHR = zXmlHttp.createRequest();
                    oXHR.open("get", "dciflot.php", true);
                    oXHR.onreadystatechange = function(){
                        try {
                            if (oXHR.readyState == 4) {
                                if (oXHR.status == 200) {
                                    var response=oXHR.responseXML;
                                    var oRoot = response.documentElement;
                                    var letsatsi = oRoot.getElementsByTagName("letsatsi");
                                    var dci = oRoot.getElementsByTagName("dci");
                                    for (var i = 0; i < letsatsi.length; i++) {
                                        d[i] =[letsatsi.item(i).firstChild.data,dci.item(i).firstChild.data];
                                    }
                                    getValues(d);
                                }
                                else {
                                }
                            }
                        }
                        catch (oException) {
                        }
                    };
                    oXHR.send(null);
                }
                catch (oException) {
                }
            }
            function fci(){
                try {
                    var oXHR = zXmlHttp.createRequest();
                    oXHR.open("get", "fciflot.php", true);
                    oXHR.onreadystatechange = function(){
                        try {
                            if (oXHR.readyState == 4) {
                                if (oXHR.status == 200) {
                                    var response=oXHR.responseXML;
                                    var oRoot = response.documentElement;
                                    var fletsatsi = oRoot.getElementsByTagName("fletsatsi");
                                    var fci = oRoot.getElementsByTagName("fci");
                                    for (var i = 0; i < fletsatsi.length; i++) {
                                        d1[i] =[fletsatsi.item(i).firstChild.data,fci.item(i).firstChild.data];
                                    }
                                    getFCIValues(d1);
                                }
                                else {
                                }
                            }
                        }
                        catch (oException) {
                        }
                    };
                    oXHR.send(null);
                }
                catch (oException) {
                }
            }
            function getFCIValues(value1){
                $.plot($("#fcigraph"), [value1], {
                    xaxis: {
                        mode: "time",
                        tickDecimals: 0
                    },lines: { show: true, lineWidth: 2, fill: true,fillColor:"#edc240" },grid: {
                        backgroundColor: "#fffaff"
                    }
                });
            }
            function getValues(value){
                $.plot($("#dciplaceholder"), [value], {
                    xaxis: {
                        mode: "time",
                        tickDecimals: 0
                    },lines: { show: true, lineWidth: 2,fill: true,fillColor:"#edc240" },grid: {
                        backgroundColor: "#fffaff"
                    }
                });
            }
        </script>
