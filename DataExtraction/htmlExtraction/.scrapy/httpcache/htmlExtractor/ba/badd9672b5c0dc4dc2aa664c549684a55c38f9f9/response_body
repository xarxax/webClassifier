    function SetupNewValidatorUpdateDisplay() {
        try {
            var functionBody, newFunction;
            functionBody = ValidatorUpdateDisplay.tstringWriter();
            functionBody = functionBody.substring(functionBody.indexOf('{') + 1, functionBody.lastIndexOf('}'));
