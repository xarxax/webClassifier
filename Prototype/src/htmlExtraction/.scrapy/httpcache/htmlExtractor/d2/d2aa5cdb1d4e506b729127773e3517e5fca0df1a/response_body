                            $("#ctl00_UcSide1_btnDistributorSubmit").click( function() {
                                if ($(".ddlCountry").val() != "USA" && $(".ddlCountry").val() != "CAN") {
                                    return true
                                }
                                else {
                                    if ($("#ctl00_UcSide1_txtZipPostal").val().length == 0) {
                                            alert('Please Enter a Zipcode');
                                            return false
                                        }
                                    if ($(".ddlCountry").val() == "USA" || $(".ddlCountry").val() == "CAN") {
                                        if ($(".ddlCountry").val() == "USA") {
                                            var regex = new RegExp("^\\d{5}$");
                                            var qs = regex.exec($("#ctl00_UcSide1_txtZipPostal").val());
                                            if (qs == null) {
                                                $(".dist-search-error").html("Invalid Zip Code");
                                                if ($("#ctl00_UcSide1_ddlState").val().length == 0) {
                                                    return false
                                                }
                                                else {
                                                    return true
                                                }
                                            }
                                            else {
                                                if (qs.length == 0 ) {
                                                    $(".dist-search-error").html("Invalid Zip Code");
                                                    return false
                                                }
                                                else {
                                                    return true
                                                }
                                            }
                                        } 
                                        else {
                                            var regex = new RegExp("^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]\\d[A-Za-z] \\d[A-Za-z]\\d$");
                                            var qs = regex.exec($("#ctl00_UcSide1_txtZipPostal").val());
                                                
                                            var regex2 = new RegExp("^[ABCEGHJKLMNPRSTVXYabceghjklmnprstvxy]\\d[A-Za-z]\\d[A-Za-z]\\d$");
                                            var qs2 = regex2.exec($("#ctl00_UcSide1_txtZipPostal").val());
                                                                                    
                                            if (qs == null && qs2 == null) {
                                                $(".dist-search-error").html("Invalid Postal Code");
                                                if ($("#ctl00_UcSide1_ddlState").val().length == 0) {
                                                    return false
                                                }
                                                else {
                                                    return true
                                                }
                                            }
                                            else {
                                                if (qs.length == 0 ) {
                                                    $(".dist-search-error").html("Invalid Postal Code");
                                                    return false
                                                }
                                                else {
                                                    return true
                                                }
                                            }
                                            
                                        }
                                        return false;
                                    }    
                                };
                            });
                            $("#ctl00_UcSide1_txtZipPostal").on('change keyup paste', function (event) {
                                if ($("#ctl00_UcSide1_txtZipPostal").val().length > 2) {
                                    if (isNaN($("#ctl00_UcSide1_txtZipPostal").val())) {
                                        $("#ctl00_UcSide1_ddlCountry").val("CAN");
                                    }
                                    else {
                                        $("#ctl00_UcSide1_ddlCountry").val("USA");
                                    }
                                };
                                //alert($("#ctl00_UcSide1_txtZipPostal").val().length);
                                if ($("#ctl00_UcSide1_txtZipPostal").val().length < 5) {
                                    //$("#ctl00_UcSide1_ddlState").prop('disabled', false).css('background-color', 'white');
                                    $("#ctl00_UcSide1_divZipPostal").show();
                                    $("#ctl00_UcSide1_hrDist").show();
                                    $("#ctl00_UcSide1_divState").show();
                                }
                                else
                                {
                                    //$("#ctl00_UcSide1_ddlState").hide('disabled', true).css('background-color', '#cccccc').css('border-color', '#cccccc');
                                    $("#ctl00_UcSide1_divZipPostal").show();
                                    $("#ctl00_UcSide1_hrDist").hide();
                                    $("#ctl00_UcSide1_divState").hide();
                                    event.preventDefault();
                                }
                            });
                            $("#ctl00_UcSide1_ddlState").change(function (event) {
                                //alert($("#ctl00_UcSide1_ddlState").val());
                                if ($("#ctl00_UcSide1_ddlState").val().length==0) {
                                    //$("#ctl00_UcSide1_txtZipPostal").prop('disabled', false).css('background-color', 'white');
                                    $("#ctl00_UcSide1_divZipPostal").show();
                                    $("#ctl00_UcSide1_hrDist").show();
                                    $("#ctl00_UcSide1_divState").show();
                                }
                                else {
                                    //$("#ctl00_UcSide1_txtZipPostal").prop('disabled', true).val("").css('background-color', '#cccccc').css('border-color', '#cccccc');
                                    $("#ctl00_UcSide1_divZipPostal").hide();
                                    $("#ctl00_UcSide1_hrDist").hide();
                                    $("#ctl00_UcSide1_divState").show();
                                    event.preventDefault();
                                }
                                var provinces = ["AB", "BC", "MB", "NB", "NL", "NT", "NS", "NU", "ON", "PE", "QC", "SK", "YT"];
                                //alert(provinces.indexOf($("#ctl00_UcSide1_ddlState").val()));
                                if (provinces.indexOf($("#ctl00_UcSide1_ddlState").val()) >=0) {
                                    //alert("We Are In Canada");
                                    $("#ctl00_UcSide1_ddlCountry").val("CAN");
                                }
                                else {
                                    //alert("We Are In USA");
                                    $("#ctl00_UcSide1_ddlCountry").val("USA");
                                }
                            });
                        </script>

