function Validate(theForm)
{

if (theForm.userid.value == "")
  {
    alert("Please enter your user ID.");
    theForm.userid.focus();
    return (false);
  }
  
if (theForm.password.value == "")
  {
    alert("Please enter your password.");
    theForm.password.focus();
    return (false);
  }
}  
  
