<script>
	$(document).ready(function(){
		function getUrlVar(key){
			var result = new RegExp(key + "=([^&]*)", "i").exec(window.location.search); 
			return result && unescape(result[1]) || ""; 
		}

		function checkUrl(){
			if(getUrlVar("gated") ==  'open' ){
				  $(".resource.gated").addClass("open").removeClass("gated").unbind();
				  $('.resource a').off('click');
			 }
			  else{
		  
			  }
		}



		checkUrl();
	});
</script>
<script>
$(document).ready(function() {
		
   $('.load-more').click(function(e) {
        e.preventDefault();

        var $this = $(this);

        var load   = $this.data('current-page') + 1;
        var parent = $this.data('parent');

        $.ajax({
            type: "GET",
            url: "https://mpival.com/pagination-processor/",
            data: {page: load, parent: parent},
            success: function (response) {
                response = $.parseJSON(response);

                if(response.success) {
                    $(".blog-list").append(response.page);

                    // Update the page attribute
                    $this.data('current-page', load);
                } else {
                    $this.after(response.page);
                    $this.remove();
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {
                // Do something if there was an error
            }
        });
		return false;
    });

	});
</script>
<a href="http://www.mpival.com/engineludicrous.php" style="position: absolute; left: -900px;"><!-- loudmouthed --></a>


