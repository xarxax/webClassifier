  <div class="content clearfix">
    <div class="field field-name-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even"><script>
(function($) {	
    $(function() {	
		if($(".form-item").length >= 1)	{	
			var input = document.createElement('input');
			var placeholderSupport = 'placeholder' in input;
			
			var $inputs = $(".form-item input");
			var $textareas = $(".form-item textarea")
			for(var i = 0; i < $inputs.length; i++)
			{
				$input = $($inputs[i]);
				var value = $input.prev().html();
				$input.attr("placeholder", value.substr(0, value.indexOf(" ")));
				$input.prev().hide();
			}
			for(var i = 0; i < $textareas.length; i++)
			{
				$textarea = $($textareas[i]);
				var value = $textarea.parent().prev().html();
				$textarea.attr("placeholder", value.substr(0, value.indexOf(" ")));
				$textarea.parent().prev().hide();
			}
			if(!placeholderSupport) {
				$inputs.focus(function () {
					placeholderFocus(this);
				});				
				$inputs.blur(function () {
					placeholderBlur(this);
				});
				$textareas.focus(function () {
					placeholderFocus(this);					
				});
				$textareas.blur(function () {
					placeholderBlur(this);					
				});
			}
			
			function placeholderFocus(input) {
				if($(input).val() == $(input).attr("placeholder")) {
					$(input).val("");
				}
			}			
			function placeholderBlur(input) {
				if(!/\S/.test($(input).val())) {
						$(input).val($(input).attr("placeholder"));
				}
			}			
        }
    });
})(jQuery);   
</script></div></div></div><form class="webform-client-form" enctype="multipart/form-data" action="/" method="post" id="webform-client-form-4" accept-charset="UTF-8"><div><div class="form-item webform-component webform-component-textfield webform-container-inline" id="webform-component-name">
