//<!--
var numb = '0123456789';
var lwr = 'abcdefghijklmnopqrstuvwxyz';
var upr = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ';

function isValid(parm,val) {
  if (parm == "") return true;
  for (i=0; i<parm.length; i++) {
    if (val.indexOf(parm.charAt(i),0) == -1) return false;
  }
  return true;
}


/* function calc cable price kurt schroeder, IKNOWTEK, Inc 20050210 */ 
function calcCable(base, pp, amt, formfld, pricelbl, id, skew, minqty, btn, excpt) {
	var num;
	var obj; // price label. this will be calculated
	var obj2; // the textbox field. the name will be changed using a generated skew. default value will be set
	var obj3; // the skew label. this will be generated using base skew + length
	var obj4; // the submit button. will be enabled when a skew is created
	var obj5; // the worker object for the length box to clear the value.
	var tnam; // worker variable
/* base = the base price
	pp = price per amout passed in
	amt = amount of feet or inches
	formfield = the textbox
	pricelbl = the price description
	id = the primary key of the item in the database
	skew = the base skew of the item to be changed
	minqty = a qty minium for this item should always be 1
	btn = the button that allows a submit
*/	
		num = base + (pp*amt);
		excpt2 = '*' + excpt + '*';
	//alert(amt + ', ' + excpt.replace(/,/g, "*").indexOf('*' + amt + '*'));
	if(excpt2.replace(/,/g, "*").indexOf('*' + amt + '*') != -1) {
		obj5 = document.getElementById('LEN_' + id + '_' + skew);
		obj5.value = '';
		obj2 = document.getElementById(formfld);
		obj2.value = '';
		obj2.disabled = true;
		obj4 = document.getElementById(btn);
		obj4.disabled = true;
		alert('The following cable sizes are not allowed: ' + excpt + '.\n Please select another size.');
	}
	else { 
	obj =  document.getElementById(pricelbl);
	obj.innerHTML = num.toFixed(2) + 'ea';
	obj.className = 'bld';	
	
	obj2 = document.getElementById(formfld);
	obj2.value = 1;
	tnam = obj2.name;
	obj2.setAttribute("name", "ITM_" + id + "_" + skew + amt + "_" + minqty);
	obj2.disabled = false;
	
	obj3 = document.getElementById(skew);
	obj3.innerHTML = skew + amt;
	obj3.className = 'bld';
	
	obj4 = document.getElementById(btn);
	obj4.disabled = false;
	}
}
//-->
</script>

