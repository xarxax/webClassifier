<script type="text/javascript">
    var milliseconds = 15000;
    var i = 2;
    var int;
    var old = 1;
    function rotate(spec) {
        if (spec != null) {
            i = spec;
            control(0);
        };
        if (old != -1) {
            document.getElementById(old).style.display = 'none';
        }
        else {
            document.getElementById(1).style.display = 'none';
            document.getElementById('one').style.backgroundColor = '#ffffff';
        }

        document.getElementById(i).style.display = 'block';
        document.getElementById('one').style.backgroundColor = '#ffffff';
        document.getElementById('one').style.color = '#f68c33';
        document.getElementById('two').style.backgroundColor = '#ffffff';
        document.getElementById('two').style.color = '#f68c33';
        document.getElementById('three').style.backgroundColor = '#ffffff';
        document.getElementById('three').style.color = '#f68c33';
        document.getElementById('four').style.backgroundColor = '#ffffff';
        document.getElementById('four').style.color = '#f68c33';
        //document.getElementById('five').style.backgroundColor = '#ffffff';
        //document.getElementById('five').style.color = '#f68c33';
        switch (i) {
            case 1:
                document.getElementById('one').style.backgroundColor = '#f68c33';
                document.getElementById('one').style.color = '#ffffff';
                break;
            case 2:
                document.getElementById('two').style.backgroundColor = '#f68c33';
                document.getElementById('two').style.color = '#ffffff';
                break;
            case 3:
                document.getElementById('three').style.backgroundColor = '#f68c33';
                document.getElementById('three').style.color = '#ffffff';
                break;
            case 4:
                document.getElementById('four').style.backgroundColor = '#f68c33';
                document.getElementById('four').style.color = '#ffffff';
                break;
            //case 5:
            //    document.getElementById('five').style.backgroundColor = '#f68c33';
            //    document.getElementById('five').style.color = '#ffffff';
                break;
        }

        old = i;
        i++;
        if (i > 4) i = 1;
    };
    function control(op) {
        if (!op) {
            document.getElementById('stop').style.display = 'none';
            document.getElementById('play').style.display = 'inline';
            clearInterval(int);
        }
        else {
            document.getElementById('stop').style.display = 'inline';
            document.getElementById('play').style.display = 'none';
            int = setInterval('rotate()', milliseconds);
        };
    };
    window.onload = function () {
        int = setInterval('rotate()', milliseconds);
    };
</script>
