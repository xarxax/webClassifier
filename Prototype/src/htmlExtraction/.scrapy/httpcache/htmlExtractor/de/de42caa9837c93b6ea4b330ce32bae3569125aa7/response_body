    <script>
        (function(){ // anonymous function as seemss to execute quicker than $(document).ready(function....
            $('#sliderinner').carouFredSel({
                responsive: true,
                width: '100%',
                scroll: (function(){
					var win_width = $(window).width();
					if (win_width > 847) {
						return 3;
					} else if (win_width <= 847 && win_width > 433) {
						return 2;
					} else {
						return 1;
					}
				}),
                prev : "#slider_prev",
                next : "#slider_next",
                items: {
                    width: 415,
                    height: 200, //	optionally resize item-height
                    visible: {
                        min: 1,
                        max: 3
                    }
                },
                auto: {
                    timeoutDuration: 8000
                },
                swipe: {
                    onTouch: true,
                    onMouse: true
                }
            });
			setScrollItems();
			
			var resizeTimeout;

			window.onresize = (function() {
				if (resizeTimeout) {
					clearTimeout(resizeTimeout);
				}
				resizeTimeout = setTimeout(function() {
					setScrollItems();
				}, 500);

			});
			
			function setScrollItems(){
				var win_width = $(window).width();
				if (win_width > 847) {
					count = 3;
				} else if (win_width <= 847 && win_width > 433) {
					count = 2;
				} else {
					count = 1;
				}
				$("#sliderinner").trigger("configuration", [ "scroll", count ]);
			}
			
        })();
		
    </script>
    
