<script>

	var secsMain;
	var timerIDMain = null;
	var timerRunningMain = false;
	var delayMain = 1000;

function InitializeTimerMain()
	{
    // Set the length of the timer, in seconds
    secsMain = 1;
    StopTheClockMain();
    StartTheTimerMain();
	}

function StopTheClockMain()
	{
    if(timerRunningMain) clearTimeout(timerIDMain);
    timerRunningMain = false;
	}

function StartTheTimerMain()
	{
    if (secsMain==0)
    	{
        StopTheClockMain();
		hidelayersMain();
    	}
    	else
    	{
        secsMain = secsMain - 1
        timerRunningMain = true
        timerIDMain = self.setTimeout("StartTheTimerMain()", delayMain)
    	}
	}
</script>
<script>

function hidelayersMain() {
StopTheClockMain();
document.getElementById("SMMain1").style.visibility="hidden";
document.getElementById("SMMain2").style.visibility="hidden";

}

</SCRIPT>



