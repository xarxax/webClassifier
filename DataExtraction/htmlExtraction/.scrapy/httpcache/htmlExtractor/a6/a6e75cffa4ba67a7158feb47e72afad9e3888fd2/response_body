	$(document).ready(function () {
		var width = $(window).width();
		if(width < 767)
		{
			$('.tabs-list a.opener.deeplink').removeAttr("href");
			$('.slide').css('max-width', width);
		}
	})
	function checkSponsorsBlocks() {
		//hides the left border for the first list item (in a row/line) in the sponsors image list
		var sponsors = $("div.sponsors-block .container .col-md-12 ul li");
		if (sponsors.length > 0) {
			var topOffsetNum = -10000;
			for (var i = 0; i < sponsors.length; i++) {
				var sponsor = $(sponsors.get(i));
				var top = sponsor.position().top;
				if (top > topOffsetNum) {
					sponsor.addClass("first-row-child");
					topOffsetNum = top;
				} else {
					sponsor.removeClass("first-row-child");
				}
			}
		}
	}
	$(window).load(function () {
		checkSponsorsBlocks();
	});
	$(window).resize(function () {
		checkSponsorsBlocks();
	});
</script>


</div>
<style type="text/css">
    .content-header-link {
        font-size: 34px;
        line-height: 54px;
    }
</style>

		
