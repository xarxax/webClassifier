    {{ if (summary.totalResult > summary.limit) { }}
        <a href="#" class="button js-show-more-link">
            Show all</a>
    {{ } }}
</script>
<script type="text/template" id="searchresult_pagination">
    {{ var pages = Math.ceil(summary.totalResult/summary.limit); }}

    {{ for (var i = 0; i < pages; i++) { }}
        {{ var cssClass = (summary.start/summary.limit === i) ? "b" : "b-secondary"; }}
        <li>
            <a href="#" data-start="{{=i * summary.limit}}" class="{{=cssClass}} js-page">{{=i + 1}}</a>
        </li>
    {{ } }}
</script>

