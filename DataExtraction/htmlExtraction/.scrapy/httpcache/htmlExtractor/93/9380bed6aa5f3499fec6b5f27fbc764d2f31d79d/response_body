function setNoticeCookie() {
  var days = 1;
  var date = new Date();
  date.setTime(date.getTime()+(days*24*60*60*1000));
  var expires = "; expires="+date.toGMTString();
  document.cookie = "notice; path=/100/202/300";
}

function fnRedirect() {
  var _link = document.getElementById('redir');
  if (_link.click)
  {
    _link.click();
  } else {
    window.location='http://epe.lac-bac.gc.ca/100/202/300/charlotte/2000/07-31/pages/interviews/authors/katherinegovier.htm';
  }
}
setNoticeCookie();
setTimeout('fnRedirect()', 15000);
</script>

