            <script type="text/javascript">
                jQuery(window).ready(function(){
                    var $togglers = jQuery('#mobile-togglers a');
                    var $hcontent = jQuery('.header .header-content');

                    $togglers.on('click',function(){
                        var rel = jQuery(this).attr('rel');
                        if ($hcontent.find('.'+rel).hasClass('active')) {
                            $hcontent.find('.'+rel).toggleClass('active');
                        } else {
                            $hcontent.children().removeClass('active');
                            $hcontent.find('.'+rel).addClass('active');
                        }
                    });
                });
            </script>
        </div>
