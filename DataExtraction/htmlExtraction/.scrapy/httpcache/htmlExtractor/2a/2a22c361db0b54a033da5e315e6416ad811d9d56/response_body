  window.onload = function() {
    $(document).ready(function() {
      $(".product-panel-h1").each(function() {
        var truncText = gracefulTruncate($(this).text(), 60);
        $(this).html(truncText);
      });

      $(".product-panel-mobile-h1").each(function() {
        var truncText = gracefulTruncate($(this).text(), 36);
        $(this).html(truncText);
      });

      $(".product-panel-h2").each(function() {
        var truncText = gracefulTruncate($(this).text(), 40);
        $(this).html(truncText);
      });

      $(".product-panel-carousel").show();
    });
  }
