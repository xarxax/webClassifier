		<script>
	var i_addonBlockAnimationSpeed = 600;
	var i_addonBlockDelaySpeed		= 5000;
	var i_addonBlockDelayAuto		= 15000;
	var b_addonBlockSwapRestart	= true;
	var a_addonBlockTimers			= new Array();
	var a_addonBlockHolder			= new Array();
	var a_addonBlockClicked		= new Array();
	function addonBlockAnimate(dv_block,b_next,b_auto){
		var s_id		= dv_block.attr('id');
		clearTimeout(a_addonBlockTimers[s_id]);
		if(!b_auto){
			a_addonBlockClicked[s_id] = true;
		}
		var dv_holder	= $('.blocksHolder',dv_block);
		var s_click		= dv_block.attr('blockClick');
		var i_moveBy	= $('.blocksItem:first-child',dv_holder).outerWidth();
		var i_total		= dv_block.attr('blockTotal')*1;
		var i_pos		= dv_block.attr('blockPos')*1;
		if(s_click == 'y'){
			dv_block.attr('blockClick','n');
			var i_newPos = i_pos;
			if(b_next){
				i_newPos += 1;
			} else {
				i_newPos -= 1;
			}
			var i_newPosSize = 0-(i_newPos*i_moveBy);
			dv_holder.animate({'left':i_newPosSize+'px'},i_addonBlockAnimationSpeed,function(){
				if(b_next){
					if(i_newPos == i_total+1){
						i_newPos = 1;
						i_newPosSize = 0-(i_newPos*i_moveBy);
						
						dv_block.attr('blockPos',i_newPos);
						dv_holder.css('left',i_newPosSize+'px');
					} else {
						dv_block.attr('blockPos',i_newPos);
					}
				} else{
					if(i_newPos == 0){
						i_newPos = i_total;
						i_newPosSize = 0-(i_newPos*i_moveBy);
						
						dv_block.attr('blockPos',i_newPos);
						dv_holder.css('left',i_newPosSize+'px');
					} else {
						dv_block.attr('blockPos',i_newPos);
					}
				}
				dv_block.attr('blockClick','y');
				
				if(b_addonBlockSwapRestart == true && b_auto == false || b_auto == true){
					a_addonBlockTimers[s_id] = setTimeout(function(){
						addonBlockAnimate(a_addonBlockHolder[s_id],true,true);
					},b_auto ? i_addonBlockDelaySpeed : i_addonBlockDelayAuto);
				}
			});
		}
	}
	$(document).ready(function(){
		$('.blocksCont').each(function(i_index){
			if($('.blocksHolder',this).length > 0){
								var dv_holder	= $('.blocksHolder',this);
				var i_total		= $('.blocksItem',dv_holder).length;
				var dv_first	= $('.blocksItem:first-child',dv_holder).clone();
				var dv_last		= $('.blocksItem:last-child',dv_holder).clone();
				var i_moveBy	= $('.blocksItem:first-child',dv_holder).outerWidth();
				dv_first.appendTo(dv_holder);
				dv_last.prependTo(dv_holder);
				dv_holder.width(i_moveBy*(i_total+2));
				dv_holder.css('left',(0-i_moveBy)+'px');
				$(this).attr({
					'blockClick':'y',
					'blockTotal':i_total,
					'blockPos':'1'
				});
				$('.left',this).click(function(){
					addonBlockAnimate($(this).parent(),false,false);
					return(false);
				});
				$('.right',this).click(function(){
					addonBlockAnimate($(this).parent(),true,false);
					return(false);
				});
			}
			$('div.partInside, a.partInsideLink',this).hover(function(){
				if($('.photo',this).attr('hasHover') == 'y'){
					$('.photo',this).css('background-position',
						//(0-parseInt($('.photo',this).innerWidth()))+'px 0px'
						'0px '+(0-parseInt($('.photo',this).innerHeight()))+'px'
					);
				}
			},function(){
				if($('.photo',this).attr('hasHover') == 'y'){
					$('.photo',this).css('background-position','0px 0px');
				}
			});
		});
	});
</script><div class="blocksFullWidth"><div class="blocksCont" id="blocksLeftColTop">&nbsp;<div class="blocksHolderWrap"><div class="blocksItem"><div class="inside"><div class="partHolder" style="width:25%;">
