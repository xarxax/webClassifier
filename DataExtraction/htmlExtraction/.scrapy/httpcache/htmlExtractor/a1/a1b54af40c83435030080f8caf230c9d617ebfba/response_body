<script>
jQuery( document ).ready(function() {
    
jQuery( '.md-modal' ).find( '.md-close' ).add('.md-overlay').click( function() {
  jQuery( '.md-modal' ).removeClass( 'md-show' );
  jQuery.cookie("showmodal", "true", { path: '/', expires: 30 });
} );
    
jQuery( '.md-trigger' ).click( function() {
	jQuery( '.md-modal' ).addClass( 'md-show' );
} ); 
jQuery( '.setcookie' ).click( function() {
	jQuery.cookie("showmodal", "true", { path: '/', expires: 30 });
  //alert('clearerd');
} );

    
jQuery( '.md-clearcookie' ).click( function() {
	jQuery.cookie("showmodal", "false", { path: '/', expires: 30 });
  //alert('clearerd');
} );

var my_cookie = jQuery.cookie("showmodal");
if (my_cookie && my_cookie == "true") {
	//alert('we do not need to show modal');
}
else{
	jQuery( '.md-modal' ).addClass( 'md-show' );
}
    
});
</script>

<link href="https://fonts.googleapis.com/css?family=Cabin:400,700" rel="stylesheet">
