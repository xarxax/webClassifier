  <script>
    $(document).foundation();

	    $(".menu").addClass("js").before('<div id="mob-menu">&#8801;</div>');
        $("#mob-menu").click(function(){
                $(".menu").toggle();
        });
        $(window).resize(function(){
                if(window.innerWidth > 768) {
                        $(".menu").removeAttr("style");
                }
        });

	$(window).resize(function () {
		
				var height = $(document).height();
				var headheight = $("header").height();
				var elementsheight = $(".elements").height();
				var footerheight = $("footer").height();
				var splashheight = height - headheight - footerheight - elementsheight;

					$(".splash").css("height", splashheight);

			}).resize();

			$('.splash.hp').backstretch([
			"http://mittmann.com/wp-content/themes/mitt/images/splash-01.jpg",
			"http://mittmann.com/wp-content/themes/mitt/images/splash-05.jpg",
			"http://mittmann.com/wp-content/themes/mitt/images/splash-02.jpg"

		], {duration: 3000, fade: 750});
	  </script>

