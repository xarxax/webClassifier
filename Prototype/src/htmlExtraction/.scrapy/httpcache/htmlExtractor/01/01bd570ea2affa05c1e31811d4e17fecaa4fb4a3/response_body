<script type="text/javascript" language ="javascript">

    $(function () {
        // Dialog			
        $('#dialogLWS').dialog({
            autoOpen: false,
            width: 201,
            height: 505,
            closeOnEscape: true,
            show: "fold",
            hide: "fold"
        });

        // Dialog Link
        $('#btn_dialogLWS').click(function () {
            if ($('#dialogLWS').dialog("isOpen")) {
                $('#dialogLWS').dialog('close');
            } else {
                var position = $("#topglobal").position();
                var fixLeft = position.left;
               //if ($("body").css("direction") == "rtl" && $.browser.msie) fixLeft = $("#wrapper").position().left + 210;
               // if (fixLeft >= 19576) fixLeft -= 19576;
                $("#dialogLWS").dialog("option", "position", [fixLeft+445, position.top + 35 - $(window).scrollTop()]);
                $('#dialogLWS').dialog('open');
                return false;
            }
        });

        $('#dialogLWS a[rel="external"]').click(function () {
            $(this).attr('target', '_blank');
        });

    });
</script>
