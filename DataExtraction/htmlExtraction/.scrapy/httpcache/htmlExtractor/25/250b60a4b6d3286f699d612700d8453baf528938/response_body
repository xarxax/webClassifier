<script type="text/javascript">
    function ValidatePasswordChange() {
        if ($(".usernamePassReset").val() == "") {
            $(".errorPassChange").show();
            $(".errorPassChange").text("Please enter your username.");
            return false;
        }
        else if ($(".currentPassReset").val() == "") {
            $(".errorPassChange").show();
            $(".errorPassChange").text("Please enter your password.");
            return false;
        }
        

        else if ($(".newPassReset").val() == "") {
        $(".errorPassChange").show();
        $(".errorPassChange").text("Please enter your new password.");
        return false;
    }
        else if ($(".newPassReset").val() != $(".confirmPassReset").val()) {
        $(".errorPassChange").show();
        $(".errorPassChange").text("The two passwords do not match.");
        return false;
    }
        else {
        $(".errorPassChange").hide();
        return true;
    }
    }
        function ValidatePasswordResetRequest() {
        var str = $(".emailPasswordReset").val();
        if ($(".emailPasswordReset").val() == "") {
        $(".errorPassResetRequest").show();
        return false;
    }
    //    else if (str.indexOf("@") < 0 || str.indexOf(".") < 0) {
    //    $(".errorPassResetRequest").text("That is not a valid email address.");
    //    $(".errorPassResetRequest").show();
    //    return false;
    //}
        else {
        $(".errorPassResetRequest").hide();
        return true;
    }
    }
        $(document).ready(function () {
        
        $(".errorPassChange").hide();
        $(".errorPassResetRequest").hide();

        // set Keyword Search text in javascript so users with javascript disabled don't have to clear it to search
        var tbSearch = $('#ctl00_tbSearch');
        if (tbSearch.val() == "") { tbSearch.val("Keyword Search"); }

        // initialize jQuery UI autocomplete
        $('#ctl00_tbSearch').autocomplete({
        source: 'search_autocomplete.aspx',
        select: function (event, ui) { return WebForm_FireDefaultButton(event, 'ctl00_ibSearch'); },
        search: function () { },
        minLength: 0,
        delay: 0
    });

        $("a[rel='largeImage']").colorbox({ width: "60%", height: "50%" });
		$(".youtube").colorbox({iframe:true, innerWidth:660, innerHeight:390});

    });
    var currentPopin = "";
    function OnClosePopin() {
        if ($("#q1000").length > 0) {
            OnCloseMaster();
        }
    }
    function OnCloseMaster() {
        $("#q1000").dialog("close");
    }

    function ResizePopin(width) {
        $("#" + currentPopin).dialog("width: 100");
    }
    function ShowQuotePopinMaster() {
        var width = "1000px";
        var popinElementId = "q1000";
        //var buttons = { Close: OnClosePopin };
        width = typeof width !== 'undefined' ? width : "1000px";
        var options = { modal: true, width: width, dialogClass: 'myui-dialog2', zIndex: 30000 }; // , buttons: buttons
        $("#" + popinElementId).dialog(options);
        currentPopin = popinElementId

        // $(".ui-dialog-titlebar").hide();
        $("#" + popinElementId).addClass("myui-dialog");
    }
</script>

