<script>
    if (getPolicyCookie('policy')) {
        $("#cookie").hide();
    }
    var magazineid = 10;
    if (magazineid == 16) {
        $("#cookie").hide();
    }

    function setPolicyCookie(target) {
//         var date = new Date();
//         date.setYear(date.getFullYear()+2)
        //console.log(date)

        var date = new Date();
        var dates = {year: date.getFullYear(), month: date.getMonth(), date: date.getDate()};
        var expiredate = new Date(dates.year + 1, dates.month, dates.date);
        var expirydate = expiredate.toUTCString();


        document.cookie = "policy=1; expires=" + expirydate + ";path=/";
        document.getElementById(target).style.display = 'none';
//         document.cookie = "username=John Doe; expires=Thu, 18 Dec 2024 12:00:00 UTC";
        return true;
    }
    function getPolicyCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

</script>

