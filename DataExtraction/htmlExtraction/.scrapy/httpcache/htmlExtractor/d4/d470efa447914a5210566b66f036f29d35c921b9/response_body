<script>
    jQuery(function($) { // DOM is now ready and jQuery's $ alias sandboxed
        $(document).ready(function() {
            $('.customer-tab-slider-wrapper').not('.slick-initialized').slick({
                slidesToShow: 5,
                slidesToScroll: 1,

                responsive: [
                    {
                        breakpoint: 768,
                        settings: {
                            slidesToShow: 4,
                            slidesToScroll: 1,
                            infinite: true
                        }
                    },
                    {
                        breakpoint: 480,
                        settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true
                        }
                    },
                    {
                        breakpoint: 375,
                        settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true
                        }
                    },
                    {
                        breakpoint: 320,
                        settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true
                        }
                    },
                ]
            });

            $('a[data-slider-toggle="customer"]').on('shown.bs.tab', function (e) {
                var id = $(e.target).data('tab_id');
                $('#' + id + ' .customer-tab-slider-wrapper' ).get(0).slick.setPosition(); //resize();
            });
        });
    });
