		var apiSuccess = 0;
		var userIsLoggedIn = 0;
		function gup( name, url ) {
			if (!url) url = location.href;
			name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
			var regexS = "[\\?&]"+name+"=([^&#]*)";
			var regex = new RegExp( regexS );
			var results = regex.exec( url );
			return results == null ? "" : results[1];
		}
		var testad = gup('testad', location);
		
		
		function delayedGoogletag(i) {
			
			if( apiSuccess == 1 || userIsLoggedIn == 1 || i == 0) {
				// alert( 'success' + apiSuccess + ' logged' + userIsLoggedIn + ' i' + i );
				console.log('userloggedin: ' + userIsLoggedIn);
				console.log('testad: ' + testad);
				  googletag.cmd.push(function() {
					googletag.defineSlot('/35094463/qrn/absco/jewelosco', [300, 250], 'div-gpt-ad-homeDesktop-1')
					  .addService(googletag.pubads())
					  .setTargeting("page", "17")
					  .setTargeting("abspage", "home")
					  .setTargeting("sdn", "jewelosco")
