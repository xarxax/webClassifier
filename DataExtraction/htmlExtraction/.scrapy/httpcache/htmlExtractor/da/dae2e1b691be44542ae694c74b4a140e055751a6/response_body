<script type="text/javascript" src="/assets/js/jquery.vaccordion.js"></script>
<script type="text/javascript">

	function initAccordion() {
		$('#va-accordion').vaccordion({
			accordionW      : $('#va-accordion').closest('.row').width(),
			accordionH      : 600,
		    animOpacity     : 0.8,
		    visibleSlices   : 4
		});
		
		window.setTimeout("$('.va-slice-1').click();", 10);
	}
	
	$(function() {

		
		initAccordion();
		
		$( window ).resize(function() {
			window.setTimeout("initAccordion()", 20);
			
		});
	});
	
	
</script>