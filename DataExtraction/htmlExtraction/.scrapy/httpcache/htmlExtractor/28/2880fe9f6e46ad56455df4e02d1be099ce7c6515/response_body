                                <script language="javascript">
									$(function(){
										$('#states-box').css({'min-height':$('#states-box').height()+'px'});
									});
									function toggleView(id){
										if($('#'+id).css('display') == 'none'){
											$('#'+id).fadeIn();
										}else{
											$('#'+id).fadeOut();
										}
									}
									function showBondsCount(urltitle,preapproved){
										if(preapproved == 'undefined'){
											preapproved = 0;	
										}
										url = 'loadbondsCount.cfm?urltitle='+urltitle+'&preapproved='+preapproved+'&uti='+getUTI();
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#states-box').html(data);
												$('#states-box').removeClass('states-box');
												$('#states-box').css({'padding':'10px','background-color':'#F5F5F5'});
												$('#statebonds').hide();		   
												$('#licensebonds').hide();		   
												$('#nonstatebonds').hide();		 
												$('#courtbonds').hide();
												$('#ajax-loader').hide();
											}
										});
									}
									function showBonds(urltitle,preapproved,bondtype){
										if(preapproved == 'undefined'){
											preapproved = 0;	
										}
										if(bondtype == 'undefined'){
											bondtype = 0;	
										}
										url = 'loadbonds.cfm?urltitle='+urltitle+'&preapproved='+preapproved+'&bondtype='+bondtype+'&uti='+getUTI();
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#states-box').html(data);
												$('#states-box').removeClass('states-box');
												$('#states-box').css({'padding':'10px','background-color':'#F5F5F5'});
												$('#statebonds').hide();		   
												$('#licensebonds').hide();		   
												$('#nonstatebonds').hide();		
												$('#courtbonds').hide(); 
												$('#ajax-loader').hide();
												if(preapproved == 1){
													$('#samedaybonds').show();		
												}
											}
										});
									}
									function showAllStates(preserFilter){
										url = 'loadStates.cfm?preservefilter='+preserFilter+'&uti='+getUTI();
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#states-box').html(data);
												if(!$('#states-box').hasClass('states-box')){
													$('#states-box').addClass('states-box');
												}
												$('#states-box').css({'padding':'0 0 13px'});
												$('#ajax-loader').hide();
											}
										});	
									}
									function showAllPreApprovedStates(){
										url = 'loadStates.cfm?preapproved=1&preservefilter=1&uti='+getUTI(); 
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#states-box').css({'padding':'0 0 13px'});
												if(!$('#states-box').hasClass('states-box')){
													$('#states-box').addClass('states-box');
												}
												$('#ajax-loader').hide();
												$('#states-box').html(data);
												$('#samedaybonds').show();	
											}
										});	
									}
									function showByBondType(){
										url = 'loadBondTypes.cfm?uti='+getUTI();
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#states-box').html(data);
												if(!$('#states-box').hasClass('states-box')){
													$('#states-box').addClass('states-box');
												}
												$('#states-box').css({'padding':'0 0 13px'});
												$('#ajax-loader').hide();
											}
										});	
									}
									function showSearchPage(ind){
										url = 'showHomeSearch.cfm?uti='+getUTI(); 
										$('#ajax-loader').show();
										$.ajax({
											url : url,
											success : function(data){
												$('#ajax-loader').hide();
												$('#states-box').html(data);
												$('#states-box').removeClass('states-box');
												$('#states-box').css({'padding':'10px','background-color':'#F5F5F5'});
											}
										});	
									}
									function showSearchResult(str){
										if(str != ''){
											url = 'showHomeSearch.cfm?act=search_act&searchTerm='+str+'&uti='+getUTI(); 
											$('#ajax-loader').show();
											$.ajax({
												url : url,
												success : function(data){
													$('#ajax-loader').hide();
													$('#states-box').html(data);
													$('#states-box').removeClass('states-box');
													$('#states-box').css({'padding':'10px','background-color':'#F5F5F5'});
												}
											});	
										}
									}
									function setTab(ind){
										$('.navigation-item').removeClass('navigation-active');
										$('#item'+ind).addClass('navigation-active');
									}
									function trim(stringToTrim) {
										return stringToTrim.replace(/^\s+|\s+$/g,"");
									}
									
									function getUTI(){
										var a = new Date();
										var uti = '' + a.getHours() + a.getMinutes() + a.getMilliseconds();
										return uti;
									}
								</script>
                                
