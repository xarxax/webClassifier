<script type="text/javascript">
$(document).ready(function(){
	var $mmenu = $("#main_nav").clone();
	$mmenu.find('ul').attr('id','').append($("#comfort_nav").html());
	$mmenu.attr('id','mobile_nav').removeClass('hidden-sm').removeClass('hidden-xs').removeClass('row');
	$mmenu.mmenu({  
					header:true,
					"offCanvas": {
                  		"position": "right"
              		 }
				});
				
	$("#mobile_nav").on( "opened.mm", function() {$("#mobile_menu_button").addClass('opened');}).on( "closed.mm", function() {$("#mobile_menu_button").removeClass('opened');});
	//$("#mm-main_nav").removeClass('hidden-sm').removeClass('hidden-xs').removeClass('row');
				
	$("#mobile_menu_button").click(function(e) {
         $("#mobile_nav").trigger("open.mm");
		 e.preventDefault();
      });										
	$("#mm-close").on('click',function(e) {
         $("#mobile_nav").trigger("close.mm");
		 e.preventDefault();
      });
	
	$('a[rel^="colorbox"]').colorbox();
	$("a.event_popup").colorbox({iframe:true,innerWidth:750,innerHeight:600});
	$("a.bio").colorbox({iframe:true,innerWidth:750,innerHeight:600});
	$('a.video').colorbox({iframe:true, innerWidth:640, innerHeight:390});	
	 
});



</script>
