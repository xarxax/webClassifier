<script>
	$( document ).ready(function() {
		var curyear = null;
		$(".sb-aquamail-archives-list p").each(function(i,e){
			var $p = $(this);
			var $a = $p.find("a");
			var $e = $p.clone();
			$e.find("a").remove();
			var thisyear = $e.text().match(/\d{4}/)[0];

			//year header
			if (thisyear != curyear) {
				$p.before($("<h2>" + thisyear + " News Releases</h2>"));
				curyear = thisyear;
			}

			//link only
			$p.html($a);
		});
	});
</script>
