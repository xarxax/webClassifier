$(function(){
	$('#top').click(function() {
		$("html, body").animate({ scrollTop: 0 }, "slow");
		return false;
	}).hide();
	$('nav,#filterBar').waypoint('sticky');
	$('#navMenu').hide();
	$('[href="#menu"]').hover(function(){
		$('#navMenu').stop(true,true).slideDown(300,function(){
			$(this).hover(function(){},function(){
				$('#navMenu').stop(true,false).slideUp(300);
			});
		});
	},function(){});	
});
