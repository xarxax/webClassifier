             newlayer.document.open();
             msgstring = (eval("document.form1.hdText"+p+".value"))
             newlayer.document.write("<b><font size='1' face='Arial, Helvetica, sans-serif'>"+msgstring+"</font></b><br>");
             newlayer.document.close();
             newlayer.moveTo(250, 130)
             }
	        }
	        //no introtext is used
	        //else{
	         //if (eval("window.document."+coll+"innertext"+p)){
		        //eval("window.document."+coll+"par1.innerHTML = document."+coll+"innertext"+p+".innerHTML");
	         //}
	        //} 
         pageurl = eval("document.form1.hdLink"+p+".value");
         
         var nextRecord = "DisplayRecords("+(p-1)+")";
         setTimeout(nextRecord, interval);
         
        }

