<script>
	
	(function() {
		var hamburgerholder = document.getElementById('mobile_menu_holder');
		var properties = window.getComputedStyle(hamburgerholder, null)
		var hamburgerwidth = properties.width;
		var mobilebarheight = ((parseFloat(hamburgerwidth)/.1090909090)/550)*70;
		
		document.getElementById('mobile_menu_holder').style.height = mobilebarheight+"px";
		document.getElementById('mobile_header_holder').style.height = mobilebarheight+"px";
		
		var submenunodes = document.getElementsByClassName('submenumobile');
                for(var i=0; i<submenunodes.length; i++)
                {
                    submenunodes[i].style.top = mobilebarheight-1+"px";
                }
	})();
	
	
	function opensubmobile(){
			console.log("menu pushed");
			if(document.getElementById('mobile_submenu').style.display == "block"){
				document.getElementById('mobile_submenu').style.display = "none";
			}else{
				document.getElementById('mobile_submenu').style.display = "block";
			}
	}

</script>
