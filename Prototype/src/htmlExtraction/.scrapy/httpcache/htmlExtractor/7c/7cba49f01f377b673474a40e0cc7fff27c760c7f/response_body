  $(function(){
	
    var slider = $("#slides").slidesjs({
	    pagination: { active: true, },
			navigation: false,
			play: { active: true, auto: true, interval: 5000, pauseOnHover: true, restartDelay: 3000 },
      width: 990,
      height: 355,
			start: 2, 
			callback: {
	      complete: function(number) {
					$('.is-column').removeClass("hovered");
					$('#is-column-' + number).addClass("hovered");
	      }
	    }
    });

		$(".is-column").hover( 
			function () {
				var rel = $(this).attr("rel");
				$('.is-column').removeClass("hovered");
				$(this).addClass("hovered");
				
				$('.slidesjs-pagination li a[data-slidesjs-item="' + rel + '"]').trigger('click');

			}, function () {

			}
		);
		
		$(".is-column").click( function () {
			
			var link = $(this).find('a').eq(0);
			if ( link.length ) {
				var url = link.attr('href');
				window.location.href = url;
			} else { return; }
			
		});
		
		$("#industries-selector").hover( 
			function () {
				$(".slidesjs-container").trigger("mouseenter");
			}, function () {
				$(".slidesjs-container").trigger("mouseleave");
			}
		);

  });
</script>


