    <script>
      // Fixed navigation
      $(window).on("scroll", function() {
        if ($(window).scrollTop() > $('.top-layer').height()) {
          $(".navbar").removeClass("large").addClass("small");
          $('.navbar-brand img').attr('src', 'img/logo.png');
        }
        else {
          $(".navbar").removeClass("small").addClass("large");
          $('.navbar-brand img').attr('src', 'img/logo.png');
        }
      });
    </script>

