jQuery(window).load(function() {
    var owlowlcarousel_5957 = jQuery('#g-owlcarousel-owlcarousel-5957');
    owlowlcarousel_5957.on('changed.owl.carousel', function(event) {
        var currentItem = "#g-owlcarousel-panel-owlcarousel-5957 #g-owlcarousel-panel-" + (event.item.index + 1);
        jQuery(currentItem).trigger("click");
    })
        jQuery('#g-owlcarousel-owlcarousel-5957-controls .owl-next').click(function() {
        owlowlcarousel_5957.trigger('next.owl.carousel');
    })
    jQuery('#g-owlcarousel-owlcarousel-5957-controls .owl-prev').click(function() {
        owlowlcarousel_5957.trigger('prev.owl.carousel');
    })
    });

var owlPanelItemsowlcarousel_5957 = jQuery('#g-owlcarousel-panel-owlcarousel-5957 .g-owlcarousel-panel');
jQuery("#g-owlcarousel-panel-owlcarousel-5957 #g-owlcarousel-panel-1").click(function(event) {
    var owlowlcarousel_5957 = jQuery('#g-owlcarousel-owlcarousel-5957');
    owlPanelItemsowlcarousel_5957.removeClass('selected');
    var toIndex = 1 - 1;
    owlowlcarousel_5957.trigger('to.owl.carousel', [toIndex, 1 - 1, true]);
    jQuery(this).addClass('selected');
});
