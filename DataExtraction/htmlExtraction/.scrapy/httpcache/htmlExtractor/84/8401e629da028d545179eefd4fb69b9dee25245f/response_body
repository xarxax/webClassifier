<script>
$(document).ready(function() {
    $('#RGVerticalSliderWidget576536 #fullpage').fullpage({
         verticalCentered: false,
         navigation: true,
         navigationPosition: 'right',
         autoScrolling: false,
         fitToSection: false
    });
    //Slide1 video, responsive height fix
    $(window).on('load resize', function () {
        var vidHeight = $('#fullpage').find('#Slide1').find('iframe').css('height');
        $('#fullpage').find('#Slide1').css('max-height',vidHeight);
    });
    //Slide2 slideshow
    $("#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel").slick({
        autoplay: true,          //Enables Autoplay
        autoplaySpeed: 10000,     //Autoplay Speed in milliseconds
        pauseOnHover: true,      //Pause Autoplay On Hover
        pauseOnDotsHover: true,  //Pause Autoplay when a dot is hovered
        arrows: false,           //Prev/Next Arrows
        dots: true,              //Show dot indicators
        lazyLoad: 'ondemand',    //lazyload
        fade: false,             //Enable fade
        cssEase: 'linear'        //CSS3 Animation easing for fade effect
    });
    //Slide2 slideshow, lazyload
    $('#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel').on('lazyLoaded', function (e, slick, image, imageSource) {
        image.attr('src',''); //remove source
        image.css('background-image','url("'+imageSource+'")'); //replace with background instead
        $('#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slickSlide').each(function() {
            var slideItem = $(this);
            var imgSrc = slideItem.find('img').css('background-image');
            slideItem.css('background-image', imgSrc);
            slideItem.find('img').addClass('hidden');
        });
    });
    //Slide2 slideshow, caption animations
    $('.RGVerticalSliderWidget576536 #Slide2').find('.Slide2-Slideshow1').find('.slideCaption').removeClass('hidden');
    $('.RGVerticalSliderWidget576536 #Slide2').find('.Slide2-Slideshow1').find('.slideCaption').addClass('animated fadeIn');
    $('.RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel').on('afterChange', function(event, slick, currentSlide){
        $('.slick-active .slideCaption').removeClass('hidden');
        $('.slick-active .slideCaption').addClass('animated fadeIn');
    });
    $('.RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel').on('beforeChange', function(event, slick, currentSlide){
        $('.slick-active .slideCaption').removeClass('animated fadeIn');
        $('.slick-active .slideCaption').addClass('hidden');
    });
    //Slide2, move photoCaption outside of slideCaption
    $('.RGVerticalSliderWidget576536 #Slide2 .photoCaption').insertBefore( $('.RGVerticalSliderWidget576536 #Slide2 .photoCaption').parent() );
    //Slide2 slideshow, full viewport on load
    var iHeight = $(window).height();
    $("#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slickSlide").height(iHeight);
    //Slide2 slideshow, keep full viewport on resize
    $(window).on('resize', function() {
        iHeight = $(window).height();
        $("#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slickSlide").height(iHeight);
    });    
    //Slide1, Slide3, Slide4, Slide5 LazyLoadAnything background images
    $("#RGVerticalSliderWidget576536 .section").lazyloadanything( {
        timeout: 0, /*fire onLoad as soon as something is seen*/
        "onLoad": function(e, LLobj) {
            var divBG = LLobj.$element;
            var src = divBG.attr('data-originalsrc');
            divBG.css('background-image', 'url("' + src + '")');
            divBG.addClass('animated fadeIn');
        }
    });
    //Compliance
    $(function() {
        $('#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slick-track').attr('aria-label', 'Hero Carousel');
        $('#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slick-dots li').removeAttr('aria-selected').removeAttr('aria-controls');
        $('#RGVerticalSliderWidget576536 #Slide2 .slickHeroCarousel .slick-dots button').removeAttr('role');
        $('#fp-nav').appendTo('#shell');
    });
    //Move anchor to first slide
    $("#RGVerticalSliderWidget576536 #Slide1 .gradient").prepend( $( "#RGVerticalSliderWidget576536 .anchor" ) );
    //Smooth scroll for anchor
    $('#RGVerticalSliderWidget576536 #Slide1 a.anchor').click(function() {
         if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
              var target = $(this.hash);
              target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
              if (target.length) {
                   $('html,body').animate({
                        scrollTop: target.offset().top
                   }, 1000);
                   return false;
              }
         }
    });
    //force the lazy load to evaluate elements on initial page load without waiting for scroll
    //event so any elements above the page fold will show
    $.fn.lazyloadanything('load');
}); //end doc ready
</script>
</div>
