<script language="JavaScript1.2" src="coolmenus1.js">

/*******************************************************************************

Copyright (c) 1999 Thomas Brattli (www.bratta.com)



eXperience DHTML coolMenus - Get it at www.bratta.com

Version 2.0

This script can be used freely as long as all copyright messages are

intact. Visit www.bratta.com/dhtml for the latest version of the script.



These are the varibles you have to set to customize the menu.

*******************************************************************************/

</script>

<script>

/********************************************************************************

Variables to set.



Remember that to set fontsize and fonttype you set that in the stylesheet

above!

********************************************************************************/



//Making a menu object

oMenu1=new menuObj('oMenu1') //Place a name for the menu in there. Must be uniqe for each menu



//Setting menu object variables



//Style variables NOTE: The stylesheet have been removed. Use this instead! (some styles are there by default, like position:absolute ++)

oMenu1.clMain='padding:5px; font-family:arial; font-size:12px; font-weight:bold' //The style for the main menus

oMenu1.clSub='padding:5px; font-family:arial; font-size:12px' //The style for the submenus

oMenu1.clSubSub='padding:5px; font-family:arial; font-size:12px' //The style for the subsubmenus

oMenu1.clAMain='text-decoration:none; color:ff6600' //The style for the main links

oMenu1.clASub='text-decoration:none; color:000000' //The style for the sub links

oMenu1.clASubSub='text-decoration:none; color:ffff00' //The style for the subsub links



//Background bar properties

oMenu1.backgroundbar=0 //Set to 0 if no backgroundbar

oMenu1.backgroundbarfromleft=-850 //The left placement of the backgroundbar in pixel or %

oMenu1.backgroundbarfromtop=60 //The top placement of the backgroundbar  in pixel or %

oMenu1.backgroundbarsize="100%" //The size of the bar in pixel or %

oMenu1.backgroundbarcolor="000000" //The backgroundcolor of the bar



oMenu1.mainheight=22 //The height of the main menuitems in pixel or %

oMenu1.mainwidth=95//The width of the main menuitems  in pixel or %



/*These are new variables. In this example they are set like the previous version*/

oMenu1.subwidth=170 // ** NEW ** The width of the submenus

oMenu1.subheight=20 //The height if the subitems in pixel or % 



oMenu1.subsubwidth=150// ** NEW ** The width of the subsubmenus in pixel or % 

oMenu1.subsubheight=oMenu1.subheight //** NEW ** The height if the subsubitems in pixel or % 





//Writing out the style for the menu (leave this line!)

oMenu1.makeStyle()



oMenu1.subplacement=oMenu1.mainheight //** NEW ** Relative to the main item

oMenu1.subsubXplacement=oMenu1.subwidth/1 //** NEW ** The X placement of the subsubmenus, relative to the sub item

oMenu1.subsubYplacement=3 //** NEW ** The Y placement of the subsubmenus, relative to the sub item



oMenu1.mainbgcoloroff='E6E6FF' //The backgroundcolor of the main menuitems

oMenu1.mainbgcoloron='#E6E6FF' //The backgroundcolor on mouseover of the main menuitems

oMenu1.subbgcoloroff='BDB5E1' //The backgroundcolor of the sub menuitems

oMenu1.subbgcoloron='#9966cc' //The backgroundcolor on mouseover of the sub menuitems

oMenu1.subsubbgcoloroff='#9966cc'//The backgroundcolor of the subsub menuitems

oMenu1.subsubbgcoloron='#000000' //The backgroundcolor on mouseover of the subsub menuitems

oMenu1.stayoncolor=0 //Do you want the menus to stay on the mouseovered color when clicked?




oMenu1.menuspeed=5 //The speed of the clipping in px

oMenu1.menusubspeed=2 //The speed of the submenus clipping in px



oMenu1.menurows=1 //Set to 0 if you want rows and to 1 if you want columns



oMenu1.menueventon="mouse" //Set this to "mouse" if you want the menus to appear onmouseover, set it to "click" if you want it to appear onclick

oMenu1.menueventoff="mouse" //Set this to "mouse" if you them to disappear onmouseout, if not set it to "click"



//Placement of the menuitems



//Example in %:

//oMenu1.menuplacement=new Array("20%","40%","60%","50%","65%") //Remember to make the arrays contain as many values as you have main menuitems



//Example in px: (remember to use the ' ' around the numbers)

//oMenu1.menuplacement=new Array(10,200,300,400,500)



//Example right beside eachother (only adding the pxbetween variable)

oMenu1.menuplacement=0



//If you use the "right beside eachother" you cant how many pixel there should be between each here

oMenu1.pxbetween=1 //in pixel or %



//And you can set where it should start from the left here

oMenu1.fromleft=65//in pixel or %



//This is how much from the top the menu should be.

oMenu1.fromtop=105//in pixel or %



/********************************************************************************

Construct your menus below

********************************************************************************/



//MAIN 0

//Main items:
// makeMain(MAIN_NUM,'TEXT','LINK','FRAME_TARGET') (set link to 0 if you want submenus of this menu item)

oMenu1.makeMain(0,'Services',0)
	//Sub items:
	// makeSub(MAIN_NUM,SUB_NUM,'TEXT','LINK',TOTAL,'FRAME_TARGET') (set link to 0 if you want submenus of this menu item)
	oMenu1.makeSub(0,0,'Industry News',"agrinews.jsp",2)
	oMenu1.makeSub(0,1,'Currency Converter',"phconverter.jsp",2)

	
		//SubSub items:
		// makeSubSub(MAIN_NUM,SUB_NUM,SUBSUB_NUM,'TEXT','LINK',TOTAL,'FRAME_TARGET')
		oMenu1.makeSubSub(0,0,0,'Agriculture',"agrinews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,1,'Chemicals',"chemnews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,2,'Bio-technology',"bionews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,3,'Trade News',"tradenews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,4,'Pharmaceuticals',"pharmanews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,5,'Health Care',"healthc.jsp",'#',11)
		oMenu1.makeSubSub(0,0,6,'Health Care Management',"healthcm.jsp",'#',11)
		oMenu1.makeSubSub(0,0,7,'Medical Research',"medicalres.jsp",'#',11)
		oMenu1.makeSubSub(0,0,8,'Medical Informatics',"medicalinfo.jsp",'#',11)
		oMenu1.makeSubSub(0,0,9,'Veterinary',"vetnews.jsp",'#',11)
		oMenu1.makeSubSub(0,0,10,'Pharmaceuticals',"Globallinks.jsp",11)








/********************************************************************************

End menu construction

********************************************************************************/

		

		

//When all the menus are written out we initiates the menu

oMenu1.construct()	

