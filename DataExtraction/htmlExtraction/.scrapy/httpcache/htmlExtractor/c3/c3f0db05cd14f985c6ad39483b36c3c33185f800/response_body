<script language="JavaScript" type="text/JavaScript">
function random_imglink(){
	var myimages=new Array()
	//specify random images below. You can have as many as you wish
	//just be sure to increment the number in 'myimages[#]'
	myimages[1]="header-rt2.jpg"
	myimages[2]="header-rt.jpg"
		
	//specify corresponding links below, if applicable
	//if not a link then just put = ""
	var imagelinks=new Array()
	imagelinks[1]=""
	imagelinks[2]=""
	imagelinks[3]=""
	imagelinks[4]=""
	imagelinks[5]=""
	imagelinks[6]=""
	
	var randomNumber = Math.floor(Math.random()*myimages.length)
	if (randomNumber==0){
		randomNumber = 1
		}
	
	if (imagelinks[randomNumber] != ""){
		document.write('<a href='+imagelinks[randomNumber]+'><img src=images/'+myimages[randomNumber]+' border=0></a>')
		}else{
		document.write('<img src=../images/'+myimages[randomNumber]+' border=0>')
		}
	}
//-->
</script>

