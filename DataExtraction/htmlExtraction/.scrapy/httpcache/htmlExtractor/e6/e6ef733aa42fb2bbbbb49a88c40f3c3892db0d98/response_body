<script>
jQuery( document ).ready(function( jQuery ) {
// make sure ".sticky" element exists
    var stickyTop = jQuery('.navbar').offset().top; // returns number 
    jQuery(window).scroll(function(){ // scroll event
      var windowTop = jQuery(window).scrollTop(); // returns number 
      if (stickyTop <= windowTop){
        jQuery('.navbar').css({ 'position':'fixed', 'margin-top':'0' });
      }
      else {
        jQuery('.navbar').css({ 'margin-top':'15px'});
      }
    });
});

</script>

<script>
jQuery(document).ready(function() {
	jQuery('#responsiveMenuToggle').click(function() {
		jQuery('nav').toggle();
			jQuery('nav > ul li').click(function() {
				jQuery('nav ul li ul').toggle();
			});
	});
	jQuery('.parentMenuItem').click(function(){
		jQuery(this).children('.sub-menu').toggleClass('subMenuOpen');
	});
	jQuery('#Cmenu li a').click(function(){
		jQuery('#Cmenu li > div').toggleClass('contactOpen');
	});
});
</script>
 
</div><!-- end of #header -->

 

