<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<script language="javascript" type="text/javascript">
    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function checkLanguage() {
        var cookieLang = getCookie("userLanguage");
        if (cookieLang != "") {
            window.open("/default.aspx?sc_lang=" + cookieLang, "_self");
        } else {
            var browserLang = navigator.language.substring(0, 2);
            var scLang = "";

            if (browserLang == "en") scLang = "en";
            if (browserLang == "es") scLang = "es-ES";
            if (browserLang == "zh") scLang = "zh-CN";
            if (browserLang == "de") scLang = "de-DE";
            if (browserLang == "ru") scLang = "ru-RU";
            if (browserLang == "ja") scLang = "ja-JP";
            if (browserLang == "ko") scLang = "ko-KR";

            if (scLang == "")
                window.open("/landing.html", "_self");
            else
                window.open("/default.aspx?sc_lang=" + scLang, "_self");
        }
    }

    checkLanguage();
</script>
<body>

</body>
</html>
