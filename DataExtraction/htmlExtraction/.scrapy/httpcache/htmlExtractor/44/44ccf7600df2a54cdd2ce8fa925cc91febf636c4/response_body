<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>

<script>
    $(window).scroll(function(){
        if ($(this).scrollTop() > 80) {
            $('.scrollicon').fadeIn();
        } else {
            $('.scrollicon').fadeOut();
        }
    });

    $('.scrollicon').click(function(){
        $("html, body").animate({ scrollTop: 0 }, 1200);
        return false;
    });
      </script>
    
