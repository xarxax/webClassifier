<div class="StorefrontWidget CustomWidget WrapClass1" id="b47217b2-e9f0-4cde-9fd5-58ce910f61c1">
	 <script>
    (function(settings) {
        $(function() {
            var serviceBaseURL = MrSite.Site.apiUrl;
            var extensionInPageId = settings.extensionInPageId;
            var $wrapper = $('#' + settings.wrapperId);
            var $textbox = $wrapper.find('.NewsletterTextBox');
            var $message = $wrapper.find('.NewsletterTextBoxMessage');
            var $thiz = $(this);
            var $radioButtons = $wrapper.find(".letUnsubscribe");
            if(settings.letUnsubscribe=="yes"){
                $radioButtons.show();
            }
            $wrapper.find('.NewsletterSignUpLabel').html(settings.labelHeader);
            $wrapper.find('.widgetTitle').html(settings.widgetTitle);
            $wrapper.find('.storefrontNewsletterSubmit').html(settings.buttonText);
            
            
            var showMessage = function(text, colour){
                $message.stop(true,true);
                $message.fadeOut("fast", function(){
                        $message.css('color', colour);
                        $message.html(text);
                    }).fadeIn("fast").delay(2000).fadeOut("fast");
            };
            
            var subscribe = function() {
                var email = $textbox.val();
                var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
                if (email.search(emailRegEx) == -1) {
                    showMessage("Wrong email format", "red");
                }else{
                    var action = $radioButtons.find("input:radio:checked").val();
                    $.ajax({
                        type: "GET",
                        url: serviceBaseURL + "webservices/newsLetters.ashx?action="+action+"&extensionInPageId="+extensionInPageId+"&email="+email+"&seed="+Math.random(),
                        dataType: "jsonp",
                        jsonp: 'jsonp_callback',
                        success: function (data) {
                            if(data.success){
                                $textbox.val("");
                                if(action=="unsubscribe"){
                                    $wrapper.find(".newsletterSubscribePanel").html(settings.unsubscribeSuccessText);
                                } else {
                                    $wrapper.find(".newsletterSubscribePanel").html(settings.subscribeSuccessText);
                                }
                            }else{
                                showMessage(data.message, "red");
                            }
                    	}
                    });
                }
            };
            
            $textbox.keypress(function(event) {
                if ( event.which == 13 ) {
                    $wrapper.find('.storefrontNewsletterSubmit').click();
                    event.preventDefault();
                }
            });
            
            $wrapper.find('.storefrontNewsletterSubmit').live("click", function(){
                subscribe();
            });
        });
    })(MrSite.Widget.getSettings());
</script>

