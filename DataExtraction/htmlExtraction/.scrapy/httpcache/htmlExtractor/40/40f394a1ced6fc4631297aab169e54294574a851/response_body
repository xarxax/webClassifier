theImages[0] = 'images/1.jpg'
theImages[1] = 'images/2.jpg'
theImages[1] = 'images/3.jpg'
theImages[1] = 'images/4.jpg'

var j = 0
var p = theImages.length;
var preBuffer = new Array()
for (i = 0; i < p; i++){
   preBuffer[i] = new Image()
   preBuffer[i].src = theImages[i]
}
