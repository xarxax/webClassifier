<script language="JavaScript" type="text/javascript">
	redirTime = "16000";        // 16 seconds
	redirURL = "http://www.shingle.com";
	
	function redirTimer() {
		self.setTimeout("self.location.href = redirURL;",redirTime);
	}
</script>

<style type="text/css">
<!--
