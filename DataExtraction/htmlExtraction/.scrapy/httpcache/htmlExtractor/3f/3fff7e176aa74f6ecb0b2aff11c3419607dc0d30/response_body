var heading_cur=0;
function showHeading(){
	$('#t-mony'+(heading_cur+1)).css({opacity: 0}).animate({opacity: 1.0,right: "50px"}, 1000);
	setTimeout(hideHeading, 8500);
}
function hideHeading(){
	$('#t-mony'+(heading_cur+1)).css({opacity: 1}).animate({opacity: 0,right: "-50px"}, 1000,function(){showHeading();});
	heading_cur=(heading_cur+1)%5;
}
