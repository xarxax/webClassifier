$(function(){
	$("#mobile-login").click(function(){
		if ($("#login-content").css('top') == '-200px')
		{
			$("#login-content").animate({ top:'40px' }, 600 ,'swing');
		}
	});
	$("#mobile-login").click(function(){
		if ($("#login-content").css('top') == '40px')
		{
			$("#login-content").animate({ top:'-200px' }, 600 ,'swing');
		}
	});
	$("#mobile-search").click(function(){
		if ($(".Search").css('right') == '-300px')
