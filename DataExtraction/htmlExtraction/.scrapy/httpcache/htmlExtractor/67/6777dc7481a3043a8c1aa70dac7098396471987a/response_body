$(document).ready(function()
{
	var reg_exp = new RegExp('^\/\~audio\/([0-9]+)[\/]*$');
	var result = window.location.pathname.match(reg_exp);
	var json_playlist = [{"mp3":"\/audio\/2_DONNA_-_HOPELESSLY_DEVOTED_TO_YOU__MASTER__hifi.mp3","title":"HOPELESSLY DEVOTED TO YOU","artist":"DONNA","shareURL":"http:\/\/donnamusic.com.au\/~audio\/52","postID":"52"},{"mp3":"\/audio\/3_DONNA_-_JOLENE__MASTER__hifi.mp3","title":"JOLENE","artist":"DONNA","shareURL":"http:\/\/donnamusic.com.au\/~audio\/53","postID":"53"},{"mp3":"\/audio\/1_DONNA_-_COUNTRY_ROADS__MASTER__hifi.mp3","title":"TAKE ME HOME, COUNTRY ROADS","artist":"DONNA","shareURL":"http:\/\/donnamusic.com.au\/~audio\/51","postID":"51"}];
	if (json_playlist.length > 0) {
		var theplaylist = new jPlayerPlaylist(
		{
			jPlayer: "#jplayer",
			cssSelectorAncestor: "#jplayer_container"
		},
		json_playlist,
		{
			swfPath: "/dashboard/js/jPlayer",
			supplied: "mp3",
			wmode: "window",
			active_track: (result != null && result.length > 0 ? result[1] : -1)
		});
	}
});
