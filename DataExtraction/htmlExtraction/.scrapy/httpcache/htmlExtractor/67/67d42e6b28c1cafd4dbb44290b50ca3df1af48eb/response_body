                var ua = navigator.userAgent, tem,
                    M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*([\d\.]+)/i) || [];
                if (/trident/i.test(M[1])) {
                    tem = /\brv[ :]+(\d+(\.\d+)?)/g.exec(ua) || [];
                    return 'IE ' + (tem[1] || '');
                }
                M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
                if ((tem = ua.match(/version\/([\.\d]+)/i)) != null) M[2] = tem[1];
                return M.join(' ');
            })();

            function DetermineBrowser() {
                var browser = '';
                if (navigator.browserVersion.indexOf('Firefox') != -1) {
                    browser = 'Firefox';
                } else if (navigator.browserVersion.indexOf('Chrome') != -1) {
                    browser = 'Chrome';
                } else if (navigator.browserVersion.indexOf('IE') != -1) {
                    browser = 'IE';
                } else {
                    browser = 'Other';
                }
                ShowWindow(browser);
            }
            function GetRadWindow() {
                var oWindow = null;
                if (window.radWindow)
                    oWindow = window.radWindow;
                else if (window.frameElement.radWindow)
                    oWindow = window.frameElement.radWindow;
                return oWindow;
            }
            function ShowWindow(browser) {
                var oManager = $find("ctl00_Header_hdrRwManager1");
                oManager.open(null, browser);
            }
            function CloseOnReload() {
                GetRadWindow().close();
            }
            function RefreshParentPage() {
                GetRadWindow().BrowserWindow.location.href = GetRadWindow().BrowserWindow.location.href;
            }
            function RefreshParentPage2() {
                document.location.reload();
            }
        </script>

