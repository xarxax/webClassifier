<script>
var destaques=new Array();
var act1 = act2 = 0;

$(function(){
		destaques[0] = '<a href="/pt/resultados_crescem_23_em_2017/">A Colep foi a empresa do Grupo RAR que mais contribuiu para as suas vendas globais</a>';
		destaques[1] = '<a href="/pt/resultados_crescem_23_em_2017/">EBITDA do Grupo RAR cresceu 6% em 2017</a>';
		destaques[2] = '<a href="/pt/resultados_crescem_23_em_2017/">Grupo RAR com EBITDA de 60 milhões de euros</a>';
		destaques[3] = '<a href="/pt/resultados_crescem_23_em_2017/">Resultados crescem 23% em 2017</a>';
		$('#destaque1').html(destaques[0]); 
	devolveRand();
});

function randomizador(){
	return Math.floor(Math.random()*destaques.length);
}

function devolveRand(){
	$('#destaque1').slideUp(1000,function(){ 
		$('#destaque1').html(destaques[act1]); 
		$(this).slideDown(1000); 
	});
	if((act1+1)==destaques.length)
		act1 = 0;
	else
		act1++;
	setTimeout('devolveRand()',8000);
	//$('#destaque2').html(destaques[act2]);
	/*$('#destaque1').slideDown(1000);
	$('#destaque2').slideDown(1000,function(){  act2=act1=0; setTimeout('devolveRand()',5000); });*/
}
</script>


<div class="titDestaques_pt"><h3>DESTAQUES</h3></div>
<div id="destaque1" class="divDestaque destHome"></div>

