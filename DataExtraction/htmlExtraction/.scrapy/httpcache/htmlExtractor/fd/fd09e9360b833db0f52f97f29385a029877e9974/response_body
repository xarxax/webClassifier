  <script>

    function init() {

      var canvas = document.getElementById("flowchart");
      var ctx = canvas.getContext("2d");

      draw(ctx);
    }

    function draw(ctx) {

      // layer1/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(212.8, 177.5);
      ctx.bezierCurveTo(212.8, 184.1, 207.4, 189.5, 200.8, 189.5);
      ctx.lineTo(23.8, 189.5);
      ctx.bezierCurveTo(17.1, 189.5, 11.8, 184.1, 11.8, 177.5);
      ctx.lineTo(11.8, 155.5);
      ctx.bezierCurveTo(11.8, 148.9, 17.1, 143.5, 23.8, 143.5);
      ctx.lineTo(200.8, 143.5);
      ctx.bezierCurveTo(207.4, 143.5, 212.8, 148.9, 212.8, 155.5);
      ctx.lineTo(212.8, 177.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(229, 229, 229)";
      ctx.fill();

      // layer1/Group

      // layer1/Group/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(205.8, 170.5);
      ctx.bezierCurveTo(205.8, 177.1, 200.4, 182.5, 193.8, 182.5);
      ctx.lineTo(16.8, 182.5);
      ctx.bezierCurveTo(10.1, 182.5, 4.8, 177.1, 4.8, 170.5);
      ctx.lineTo(4.8, 148.5);
      ctx.bezierCurveTo(4.8, 141.9, 10.1, 136.5, 16.8, 136.5);
      ctx.lineTo(193.8, 136.5);
      ctx.bezierCurveTo(200.4, 136.5, 205.8, 141.9, 205.8, 148.5);
      ctx.lineTo(205.8, 170.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(63, 153, 248)";
      ctx.fill();

      // layer1/Document
      ctx.restore();

      // layer1/Document/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(157.2, 65.0);
      ctx.lineTo(157.2, 0.0);
      ctx.lineTo(54.2, 0.0);
      ctx.lineTo(54.2, 65.0);
      ctx.bezierCurveTo(74.7, 72.0, 89.2, 69.7, 103.2, 60.0);
      ctx.bezierCurveTo(117.7, 50.0, 148.7, 51.0, 157.2, 65.0);
      ctx.closePath();
      ctx.fillStyle = "rgb(241, 241, 241)";
      ctx.fill();

      // layer1/Claims Data
      ctx.restore();
      ctx.font = "12.0px 'Myriad Pro'";
      ctx.fillStyle = "rgb(0, 0, 0)";
      ctx.fillText("Claims Data", 76.2, 38.0);

      // layer1/Path
      ctx.beginPath();
      ctx.moveTo(212.8, 382.5);
      ctx.bezierCurveTo(212.8, 389.1, 207.4, 394.5, 200.8, 394.5);
      ctx.lineTo(23.8, 394.5);
      ctx.bezierCurveTo(17.1, 394.5, 11.8, 389.1, 11.8, 382.5);
      ctx.lineTo(11.8, 360.5);
      ctx.bezierCurveTo(11.8, 353.9, 17.1, 348.5, 23.8, 348.5);
      ctx.lineTo(200.8, 348.5);
      ctx.bezierCurveTo(207.4, 348.5, 212.8, 353.9, 212.8, 360.5);
      ctx.lineTo(212.8, 382.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(229, 229, 229)";
      ctx.fill();

      // layer1/Group

      // layer1/Group/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(205.8, 375.5);
      ctx.bezierCurveTo(205.8, 382.1, 200.4, 387.5, 193.8, 387.5);
      ctx.lineTo(16.8, 387.5);
      ctx.bezierCurveTo(10.1, 387.5, 4.8, 382.1, 4.8, 375.5);
      ctx.lineTo(4.8, 353.5);
      ctx.bezierCurveTo(4.8, 346.9, 10.1, 341.5, 16.8, 341.5);
      ctx.lineTo(193.8, 341.5);
      ctx.bezierCurveTo(200.4, 341.5, 205.8, 346.9, 205.8, 353.5);
      ctx.lineTo(205.8, 375.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(63, 153, 248)";
      ctx.fill();

      // layer1/Patient EffectivenessGapsInCare
      ctx.restore();
      ctx.fillStyle = "rgb(0, 0, 0)";
      ctx.fillText("Patient Effectiveness", 0.0, 417.7);
      ctx.fillText("Gaps-In-Care", 18.9, 432.1);

      // layer1/PhysicianEffectiveness Score
      ctx.fillText("Physician", 140.6, 417.7);
      ctx.fillText("Effectiveness Score", 116.3, 432.1);

      // layer1/Direct Data

      // layer1/Direct Data/Group
      ctx.save();

      // layer1/Direct Data/Group/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(463.6, 186.7);
      ctx.lineTo(387.8, 186.7);
      ctx.bezierCurveTo(387.8, 186.7, 373.9, 173.5, 373.9, 157.9);
      ctx.bezierCurveTo(373.9, 142.9, 387.8, 132.3, 387.8, 132.3);
      ctx.lineTo(463.6, 132.3);
      ctx.bezierCurveTo(463.6, 132.3, 477.3, 140.6, 477.3, 158.4);
      ctx.bezierCurveTo(477.3, 178.5, 463.6, 186.7, 463.6, 186.7);
      ctx.closePath();
      ctx.fillStyle = "rgb(152, 152, 152)";
      ctx.fill();
      ctx.lineWidth = 2.5;
      ctx.strokeStyle = "rgb(255, 255, 255)";
      ctx.lineJoin = "miter";
      ctx.miterLimit = 4.0;
      ctx.stroke();

      // layer1/Direct Data/Group/Path
      ctx.beginPath();
      ctx.moveTo(463.6, 132.3);
      ctx.bezierCurveTo(463.6, 132.3, 451.9, 145.3, 451.9, 158.7);
      ctx.bezierCurveTo(451.9, 172.1, 463.6, 186.7, 463.6, 186.7);
      ctx.fill();
      ctx.stroke();

      // layer1/PhysicianEpisode Assign
      ctx.restore();
      ctx.restore();
      ctx.font = "10.0px 'Myriad Pro'";
      ctx.fillText("Physician", 397.1, 156.4);
      ctx.fillText("Episode Assign", 385.2, 168.4);

      // layer1/Path
      ctx.beginPath();
      ctx.moveTo(532.8, 382.5);
      ctx.bezierCurveTo(532.8, 389.1, 527.4, 394.5, 520.8, 394.5);
      ctx.lineTo(343.8, 394.5);
      ctx.bezierCurveTo(337.1, 394.5, 331.8, 389.1, 331.8, 382.5);
      ctx.lineTo(331.8, 360.5);
      ctx.bezierCurveTo(331.8, 353.9, 337.1, 348.5, 343.8, 348.5);
      ctx.lineTo(520.8, 348.5);
      ctx.bezierCurveTo(527.4, 348.5, 532.8, 353.9, 532.8, 360.5);
      ctx.lineTo(532.8, 382.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(229, 229, 229)";
      ctx.fill();

      // layer1/Group

      // layer1/Group/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(525.8, 375.5);
      ctx.bezierCurveTo(525.8, 382.1, 520.4, 387.5, 513.8, 387.5);
      ctx.lineTo(336.8, 387.5);
      ctx.bezierCurveTo(330.1, 387.5, 324.8, 382.1, 324.8, 375.5);
      ctx.lineTo(324.8, 353.5);
      ctx.bezierCurveTo(324.8, 346.9, 330.1, 341.5, 336.8, 341.5);
      ctx.lineTo(513.8, 341.5);
      ctx.bezierCurveTo(520.4, 341.5, 525.8, 346.9, 525.8, 353.5);
      ctx.lineTo(525.8, 375.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(63, 153, 248)";
      ctx.fill();

      // layer1/Patient HighCostPredictive Score
      ctx.restore();
      ctx.font = "12.0px 'Myriad Pro'";
      ctx.fillStyle = "rgb(0, 0, 0)";
      ctx.fillText("Patient High-Cost", 381.2, 417.7);
      ctx.fillText("Predictive Score", 385.2, 432.1);

      // layer1/Path
      ctx.beginPath();
      ctx.moveTo(846.8, 177.5);
      ctx.bezierCurveTo(846.8, 184.1, 841.4, 189.5, 834.8, 189.5);
      ctx.lineTo(657.8, 189.5);
      ctx.bezierCurveTo(651.1, 189.5, 645.8, 184.1, 645.8, 177.5);
      ctx.lineTo(645.8, 155.5);
      ctx.bezierCurveTo(645.8, 148.9, 651.1, 143.5, 657.8, 143.5);
      ctx.lineTo(834.8, 143.5);
      ctx.bezierCurveTo(841.4, 143.5, 846.8, 148.9, 846.8, 155.5);
      ctx.lineTo(846.8, 177.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(229, 229, 229)";
      ctx.fill();

      // layer1/Group

      // layer1/Group/Path
      ctx.save();
      ctx.beginPath();
      ctx.moveTo(839.8, 170.5);
      ctx.bezierCurveTo(839.8, 177.1, 834.4, 182.5, 827.8, 182.5);
      ctx.lineTo(650.8, 182.5);
      ctx.bezierCurveTo(644.1, 182.5, 638.8, 177.1, 638.8, 170.5);
      ctx.lineTo(638.8, 148.5);
      ctx.bezierCurveTo(638.8, 141.9, 644.1, 136.5, 650.8, 136.5);
      ctx.lineTo(827.8, 136.5);
      ctx.bezierCurveTo(834.4, 136.5, 839.8, 141.9, 839.8, 148.5);
      ctx.lineTo(839.8, 170.5);
      ctx.closePath();
      ctx.fillStyle = "rgb(63, 153, 248)";
      ctx.fill();

