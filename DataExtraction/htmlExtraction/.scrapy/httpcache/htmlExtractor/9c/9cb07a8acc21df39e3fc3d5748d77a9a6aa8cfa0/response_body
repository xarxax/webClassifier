<script>
 var company, results = [], request;
 var size = 10, minCharLength = 0, charlength = 0;
 var typeValue;
 var whatType;
 $( document ).ready(function() {
 $(".what-error-msg").hide();
 $(".where-error-msg").hide();
 });
 
 
 $(document).keydown(function(e) {
 switch(e.which) {
 
 
 case 38: // up
 removeHTMLTags();
 break;
 
 case 40: // down
 removeHTMLTags();
 break;
 
 default: return; // exit this handler for other keys
 }
 e.preventDefault(); // prevent the default action (scroll / move caret)
 }); 
 
 function removeHTMLTags(){
 var str = $("#"+whatType).val();
 $("#"+whatType).val(str.replace(/(<([^>]+)>)/ig,""));
 
 }
 
 $["ui"]["autocomplete"].prototype["_renderItem"] = function(ul,item){
 return $( "<li></li>" ).data("item.autocomplete", item ).append($( "<a></a>").html(item.label)).appendTo(ul);}; 
 function populate(type)
 {
 whatType=type;
 charLength = $("#"+type).val().length;
 typeValue = $("#"+type).val();
 if(charLength > minCharLength)
 {
 var action = "/sites/Satellite?pagename=CCH/Controller/Autosuggest/Action";
 var url = action + "&type=" + type + "&text=" + typeValue + "&size=" + size + "&company=" + company;
 getData(url,type);
 }
 }
 function getData(url,type){
 $("#"+type).autocomplete({
 source: function(request, response) {
 $.get(url, {
 q: request.term
 }, function(data) {
 var obj = JSON.parse(data);
 var errorMsg;
 if(type==="what")
 {
 errorMsg = type + "-error-msg";
 if(obj.hits.hits.length === 0)
 $("."+errorMsg).show();
 else
 $("."+errorMsg).hide();
 response($.map(obj.hits.hits, function (el) {
 return el.highlight.what_auto_suggest;
 }));
 }
 else
 {
 errorMsg = type + "-error-msg";
 if(obj.hits.hits.length === 0)
 $("."+errorMsg).show();
 else
 $("."+errorMsg).hide();
 response($.map(obj.hits.hits, function (el) {
 return el.highlight.where_auto_suggest;
 }));
 }
 });
 },
 
 close:function(e,ui)
 {
 removeHTMLTags();
 }
 
 
 })
 
 
 
 
 }
 
 var whatValidation = /^$|^(?!&)(?!.*--)[A-Za-z\s&/-]+$/;
 var whereValidation = /^$|^(?!')(?!.*--)(?!.*\\.\\.)[A-Za-z\d\s+.'-]+$/;
 
 function validateForm() 
 {
 var flag = false;
 if($('#what').val() == '' && $('#where').val() == '')
 {
 $('.error').html("Please fill What or Where field");
 $('.error').css('display','block');
 flag = false;
 }
 else
 {
 if (!whatValidation.test($('#what').val())) 
 {
 $('.error').css('display','block');
 $('.error').html('Illegal Characters Entered for What');
 flag = false;
 }
 else if (!whereValidation.test($('#where').val())) 
 {
 $('.error').css('display','block');
 $('.error').html("Illegal Characters Entered for Where");
 flag = false;
 }
 else
 {
 $('.error').css('display','none');
 flag = true;
 }
 }
 return flag;
 }
 
 
 // for getting company name from ICS and putting it in hidden textbox
 setTimeout(function(){
 company = $('#company').val().toLowerCase();
 }, 500);
 </script>
 
<script>
 $('map').imageMapResize();
 </script>
 





<!-- Start Google Analytics Button Code -->
<script>
/* Variables */
var form = document.getElementById("Apply_Now");
var eventCategory = "Leads";
var eventAction = "Lead_Form_Submitted";
var leadSource = "";
var match = /leadSource=([^&]+)/.exec(document.location.search); 
if (match != null && match.length == 2) 
{
leadSource = match[1];
}

function createFunctionWithTimeout(callback, opt_timeout) {
var called = false;
function fn() {
if (!called) {
called = true;
callback();
}
}
setTimeout(fn, opt_timeout || 2000);
return fn;
}

function myFunction(event)
{    
event.preventDefault();
//do ga
ga('set', 'dimension1', leadSource);

ga('send', 'event', eventCategory, eventAction, {
hitCallback: createFunctionWithTimeout(function() {
form.submit();
})
});
//end ga    
} 
form.addEventListener('submit', function(event){myFunction(event);});
</script>
<!-- End Google Analytics Button Code -->



		
