<script language="javascript" type="text/javascript">
var k = '1';
function over(i)
{
	document.getElementById("Layer"+k).style.display = "none";
	document.getElementById("Layer"+i).style.display = "";
	k=i;
	return;
}
function out(i)
{
	document.getElementById("Layer"+k).style.display = "none";
}
