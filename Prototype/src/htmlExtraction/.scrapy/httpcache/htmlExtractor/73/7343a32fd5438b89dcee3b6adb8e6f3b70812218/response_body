<script language="JavaScript" type="text/javascript"><!--
saveScreenSize();
function formValidation(oForm)
{
	if (!oForm.txtListingId.value.isListingId())
	{
		alert("Invalid Listing Id!\n\nUse this format: #######.####\n\ni.e. 1999001.0551")
		$Element.setFocus(oForm.txtListingId);
		return false;
	}
	return true;
}
function saveScreenSize()
{
	var sSize	= $JWP.Screen.getScreenSize();
	var oDate	= $JWP.Date.newDate();
	oDate.setTime(oDate.getTime() + (1000 * 60 * 60 * 24 * 90));
	"ScreenSize".setCookie(sSize, oDate);
}
// --></script>
