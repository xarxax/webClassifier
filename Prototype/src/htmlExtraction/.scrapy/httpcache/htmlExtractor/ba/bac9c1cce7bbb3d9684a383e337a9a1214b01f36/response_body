<script type="text/javascript" charset="utf-8">
	$(document).ready(function() {
		$('#nav-icon').bind('click',function() {
			$('#drop-mobile').toggleClass('hovering');
			$('#nav-icon').toggleClass('hovering');
			return false;
		});
		$('.mobile-nav-item').bind('click',function() {
			$('#drop-mobile').removeClass('hovering');
			$('#nav-icon').removeClass('hovering');
		});
	});
</script>



