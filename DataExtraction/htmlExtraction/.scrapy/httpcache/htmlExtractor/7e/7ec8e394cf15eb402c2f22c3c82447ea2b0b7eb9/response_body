<script type="text/javascript">
    jQuery(document).ready(function ($) {
			
			
				if("Click" == "Mouseover" ){
					
					$(".gbta-navigation-container .gbta-nav-item").children(".gbta-nav-link").on("mouseenter", function () {
							$(this).addClass("active");
							$(this).siblings(".gbta-submenu").addClass("active");
							$(this).parent().siblings().children(".gbta-nav-link,.gbta-submenu").removeClass("active");
					});
					$(".gbta-navigation-container .gbta-submenu-nav-item").children("a").on("mouseenter", function () {
							$(this).addClass("active");
							$(this).siblings(".gbta-submenu-subnav").addClass("active");
							$(this).parent().siblings().children("a,.gbta-submenu-subnav").removeClass("active");
					});
					
				}else{
				
					$(".gbta-navigation-container .gbta-nav-item.dir").children(".gbta-nav-link").attr("href","javascript:void(0);")
					$(".gbta-navigation-container .gbta-submenu-nav-item.dir").children("a").attr("href","javascript:void(0);");
					
					$(".gbta-header--mobile .dir .gbta-nav-link--mobile").attr("href","javascript:void(0);").on("click",function(){
					
							var $this = $(this);
							var $submenu = $this.siblings('[data-component="menu-mobile--child"]');
							var $closeButton = $submenu.find('[data-close="menu-mobile--child"]');
							 var $mobileMenuParent = $('[data-component="menu-mobile--parent"]');							
							$submenu.addClass('animating');
							setTimeout(function () {
									$submenu.addClass('active').removeClass('animating');
									$mobileMenuParent.addClass('has-active-child');
									$closeButton.focus();
							}, 10);

					})
					
					



				}




        $(".gbta-mobile-button").click(function (event) {
            event.preventDefault();
        });
    });
</script>
