<script>
	$(document).ready(function () {
		popupMessage();
	});
	function popupMessage() {
		var errorMessage = "";
		if (errorMessage != "" && sessionStorage.getItem("bso-message-displayed") != "true") {
			sessionStorage.setItem("bso-message-displayed", true);
			var dlg = $('#bso-message');
			dlg.appendTo("body").modal('show');
		}

		return true;
	}

	function showWaitMessage(msg) {
		var dlg = $('#pleaseWaitDialog');
		dlg.appendTo("body").modal('show');
		dlg.find('.modal-body h1').html(msg);
	}

	function hideWaitMessage() {
		var dlg = $('#pleaseWaitDialog');
		dlg.modal('hide');
	}

	function popupErrorMessage(msg, title) {
		var dlg = $('#general-error-dialog');
		dlg.appendTo("body").modal('show');
		dlg.find('.modal-title').html(title);
		dlg.find('.modal-body p').html(msg);
	}
	function IsErrorMessagePopupIsVisible() {
		return $('#general-error-dialog').hasClass("in");
	}
</script>

