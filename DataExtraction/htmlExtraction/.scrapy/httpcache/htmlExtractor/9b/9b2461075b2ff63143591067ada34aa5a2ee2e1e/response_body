<script type="text/javascript" src="https://www.exten.ch//jsPlugins/royalSlider/jquery.royalslider.min.js"></script>
<script type="text/javascript">

    $( document ).ready(function() {
        $('#full-width-slider').royalSlider({
            arrowsNav: true,
            keyboardNavEnabled: true,
            controlsInside: false,
            imageScaleMode: 'fill',
            arrowsNavAutoHide: false,
            autoScaleSlider: true,
            autoScaleSliderWidth: 2000,
            autoScaleSliderHeight: 700,
            autoPlay: {
                enabled: true,
                stopAtAction: false,
                pauseOnHover: false,
                delay: 2300,
                loopRewind:false,
                loop:true
            },
            controlNavigation: 'bullets',
            thumbsFitInViewport: false,
            navigateByClick: true,
            startSlideId: 0,
            transitionType:'fade',
            globalCaption: false,
            deeplinking: {
                enabled: true,
                change: false
            }
        });


        var tabChange = function () {
            var tabs = $('.menuServizi.nav-tabs > li');
            var active = tabs.filter('.active');
            var next = active.next('li').length ? active.next('li').find('a') : tabs.filter(':first-child').find('a');
            // Use the Bootsrap tab show method
            next.tab('show','fade');
        };
        // Tab Cycle function
        var tabCycle = setInterval(tabChange, 5000);

        // Tab click event handler
        $('.menuServizi.nav-tabs > li a').on('click', function (e) {
            e.preventDefault();
            clearInterval(tabCycle);
            $(this).tab('show','fade');
            setTimeout(function () {
            }, 10000);
        });
        
        $( ".tab-content" )
        .mouseenter(function() {
        clearInterval(tabCycle);
        })
        .mouseleave(function() {
         tabCycle = setInterval(tabChange, 5000);
    });



    });

</script>



