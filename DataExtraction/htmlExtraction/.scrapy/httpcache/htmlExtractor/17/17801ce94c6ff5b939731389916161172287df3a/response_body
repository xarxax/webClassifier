function simple_tooltip(target_items, name){

 jQuery(target_items).each(function(i){

 	

		if(jQuery(this).attr("ztooltip") != null){ // checks if there is a title

		

		jQuery("body").append("<div class='"+name+"' id='"+name+i+"'><p>"+jQuery(this).attr('ztooltip')+"</p></div>");

		var my_tooltip = jQuery("#"+name+i);



		jQuery(this).removeAttr("ztooltip").mouseover(function(){

				my_tooltip.css({opacity:0.9, display:"none"}).show();

		}).mousemove(function(kmouse){

				my_tooltip.css({left:kmouse.pageX+15, top:kmouse.pageY+15});

		}).mouseout(function(){

				my_tooltip.hide();

		});



		}

	});

}



function simple_tooltip2(target_items, name){ // FOR 2015 CMIA SYMPOSIUM EDUCATION & SKILLS DEVELOPMENT PAGE

 jQuery(target_items).each(function(i){

 	

		if(jQuery(this).attr("ztooltip") != null){ // checks if there is a title



		if (jQuery(this).attr("class") != null){

			var ltdclass = " " + jQuery(this).attr("class");

		} else {

			var ltdclass = "";

		}



		jQuery("body").append("<div class='"+name+ltdclass+"' id='"+name+i+"'><p>"+jQuery(this).attr('ztooltip')+"</p></div>");

		var my_tooltip = jQuery("#"+name+i);



		jQuery(this).removeAttr("ztooltip").mouseover(function(){

				my_tooltip.css({display:"none"}).show();

		}).mousemove(function(kmouse){

				if (my_tooltip.hasClass('lasttdclass')){

					my_tooltip.css({left:kmouse.pageX-615, top:kmouse.pageY+15});

				} else if (my_tooltip.hasClass('littletdclass')) {

					my_tooltip.css({left:kmouse.pageX+15, top:kmouse.pageY-200});

				} else {

					my_tooltip.css({left:kmouse.pageX+15, top:kmouse.pageY+15});

				}

		}).mouseout(function(){

				my_tooltip.hide();

		});



		}

	});

}



   function shuffle(tbl) {

   		tbl.find("td").each(function(){
   			var emptytd=jQuery(this).html();
   			console.log(jQuery.trim(emptytd));
   			if(jQuery.trim(emptytd)=='&nbsp;'){
   				jQuery(this).remove();
   			}
   		});
   		

        var arr = tbl.find("td");

        for(

          var j, x, i = arr.length; i;

          j = parseInt(Math.random() * i),

          x = arr[--i], arr[i] = arr[j], arr[j] = x

        );



        var tmp;

        var rows = tbl.find("tr").length



        var cols = tbl.find("tr:first td").length



        for (i = 0; i < rows; i++){

            tmp = tbl.find("tr").eq(i);

            tmp.html()

            for (j = 0; j < cols; j++)

            	tmp.append(arr[i*cols+j]);
            	
                
                

        }



        

        tbl.parent().css('visibility','visible'); // shows table after shuffled

  	}



jQuery(function(){

	simple_tooltip(".booth-taken","tooltip");

	simple_tooltip2(".educ-skills-tbl td","tooltip2"); // FOR 2015 CMIA SYMPOSIUM EDUCATION & SKILLS DEVELOPMENT PAGE

	

	shuffle(jQuery(".corporate-members-table tbody"));

	

	jQuery(".list-exhibitors-tbl").tablesorter();



	jQuery('.closeAUEbnr').click(function(){

		hideAUEbanner();

	})

	

	if (jQuery('.event_2014_CMIA_Vendor_Fair').length){

		jQuery('body').addClass('CMIA_Vendor_Fair_2014_registration');

	}

	

	jQuery('button[onclick="window.location=\'/sponsorships/new/cmia-supporter?format=html&layout=default\'"]').attr("onclick","window.location='/sponsor?prod=1'");

	jQuery('button[onclick="window.location=\'/sponsorships/new/appetizer-sponsor?format=html&layout=default\'"]').attr("onclick","window.location='/sponsor?prod=2'");

	jQuery('button[onclick="window.location=\'/sponsorships/new/coffee-and-water-sponsor?format=html&layout=default\'"]').attr("onclick","window.location='/sponsor?prod=3'");

	jQuery('button[onclick="window.location=\'/sponsorships/new/bag-sponsor?format=html&layout=default\'"]').attr("onclick","window.location='/sponsor?prod=4'");

	

	jQuery('div#stateField select#state option[value="CA"]').attr('selected','selected');

	jQuery('div.controls select#country option[value="US"]').attr('selected','selected');

	jQuery('div.controls select#country').parent().parent().hide();

	

	jQuery('select#member_direct>option:first-child').attr('disabled','disabled'); //disable select option in a reg form

	jQuery('tr.lvlBtn').clone().insertAfter('tr#suFee'); //copies the purchase buttons and inserted after subscription price

	

	jQuery('.icon-edit, .icon-print, .icon-envelope').parent('a').css('margin-right','10px');

	

	var input = document.createElement("input");

    if(('placeholder' in input)==false) { 

		jQuery('[placeholder]').focus(function() {

			var i = jQuery(this);

			if(i.val() == i.attr('placeholder')) {

				i.val('').removeClass('placeholder');

				if(i.hasClass('password')) {

					i.removeClass('password');

					this.type='password';

				}			

			}

		}).blur(function() {

			var i = jQuery(this);	

			if(i.val() == '' || i.val() == i.attr('placeholder')) {

				if(this.type=='password') {

					i.addClass('password');

					this.type='text';

				}

				i.addClass('placeholder').val(i.attr('placeholder'));

			}

		}).blur().parents('form').submit(function() {

			jQuery(this).find('[placeholder]').each(function() {

				var i = jQuery(this);

				if(i.val() == i.attr('placeholder'))

					i.val('');

			})

		});

	}

	

	var jQuerycarousel = jQuery('#carousel').carousel();

  

	jQuery('#carousel_prev').on('click', function(ev) {

	  jQuerycarousel.carousel('prev');

	});

	jQuery('#carousel_next').on('click', function(ev) {

	  jQuerycarousel.carousel('next');

	});

	

	function clickNext(){

		jQuery('a#carousel_next').click();

	}

		

	setInterval(clickNext,5000);



	//var str = jQuery('#member-profile #jform_name').val();

	//var res = str.split(' ');



	//var lname = res[res.length - 1]; //select last element in array

	//var fname = res.slice(0, -1); //select all in array except for last one



	//jQuery('#member-profile #jform_name').val(fname);

	//jQuery('#member-profile #jform_lastname').val(lname);



	//jQuery('#member-registration').attr('onsubmit','nameFields();');

	//jQuery('#member-profile').attr('onsubmit','nameFields();');

});



function nameFields(){

	var a = jQuery('#jform_name').val();

	var b = jQuery('#jform_lastname').val();

	jQuery('#jform_name').val(a + ' ' + b);

};



function loginFields(){

	var a = jQuery('#uk-uname').val();

	var b = jQuery('#uk-upass').val();

	jQuery('.login-box .uk-form input[name="username"]').val(a);

	jQuery('.login-box .uk-form input[name="password"]').val(b);

	jQuery('.login-box .uk-form button[name="Submit"]').click();

}



function corp(){

	if (jQuery('#corporate').val() == "CMIA Corporate Member"){

		if (jQuery('#payment_type').val() == "Full Payment"){

			jQuery('#price').val('450.00');

		} else {

			jQuery('#price').val('225.00');

		}

	} else {

		if (jQuery('#payment_type').val() == "Full Payment"){

			jQuery('#price').val('650.00');

		} else {

			jQuery('#price').val('325.00');

		}

	}

}



function pyment(){

	if (jQuery('#via_payment').val() == "PayPal"){

		jQuery('#booth-form').attr('action','/processing-payment');

		jQuery('#paymentTip').hide();

	} else {

		jQuery('#booth-form').attr('action','/simple-paypal/check.php');

		jQuery('#paymentTip').show();

	}

}



function pymentsponsor(){

	if (jQuery('#via_payment').val() == "PayPal"){

		jQuery('#sponsor-form').attr('action','/processing-payment');

		jQuery('#paymentTip').hide();

	} else {

		jQuery('#sponsor-form').attr('action','/simple-paypal/check-sponsor.php');

		jQuery('#paymentTip').show();

	}

}



function checkContact(){

	var a = jQuery('input[name="payer_phone"]').val();

	var b = jQuery('input[name="payer_cellphone"]').val();

	

	if (a == "" && b == "")

	{

		alert('Please enter any contact number');

		return false;

	}

}



function showTransDinner()

{

var str = jQuery('#transInput').val();

jQuery('#preloader').show();

setTimeout(hidePreloader,3000);

if (window.XMLHttpRequest)

  {// code for IE7+, Firefox, Chrome, Opera, Safari

  xmlhttp=new XMLHttpRequest();

  }

else

  {// code for IE6, IE5

  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");

  }

xmlhttp.onreadystatechange=function()

  {

  if (xmlhttp.readyState==4 && xmlhttp.status==200)

    {

    document.getElementById("transactions").innerHTML=xmlhttp.responseText;

    }

  }

xmlhttp.open("GET","/codes/searchTrans-Dinner.php?q="+str,true);

xmlhttp.send();

}



