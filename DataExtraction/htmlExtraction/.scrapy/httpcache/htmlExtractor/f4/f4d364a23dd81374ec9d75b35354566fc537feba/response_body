<script>
  jQuery(document).ready(function ($) {
    $(window).scroll(function () {
      var scroll = $(window).scrollTop();
      //>=, not <=
      if (scroll >= 300) {
        //clearHeader, not clearheader - caps H
        $("body").addClass("scrolled");
      } else {
        $("body").removeClass("scrolled");
      }
    });
  });
</script>
<script>
  jQuery(document).ready(function ($) {
    //Varie javascript
    $('.contenitore-box-home li a, .contenitore-pagina-analisi li a').html(function (i, html) {
      return html.replace(/(\w+\s\w+)/, '<b>$1</b>')
    });
    $("#sel").select2();
    $("#seluno").select2();
    $('table').addClass('table');
    $('#dropdownMenu2').hover(function () {
      $('#dropdownMenu1').removeClass('attivo');
      $('#dropdownMenu2').addClass('attivo');
      $('#dropdownMenu3').removeClass('attivo');
      $('.dropdown-menu.tre').css('display', 'none');
      $('.select2-drop').css('display', 'none');
      $('.dropdown-menu.due').css('display', 'block');
    });
    $('#dropdownMenu3').hover(function () {
      $('#dropdownMenu1').removeClass('attivo');
      $('#dropdownMenu3').addClass('attivo');
      $('#dropdownMenu2').removeClass('attivo');
      $('.dropdown-menu.tre').css('display', 'block');
      $('.dropdown-menu.due').css('display', 'none');
    });
    $('#dropdownMenu1').hover(function () {
      $('#dropdownMenu3').removeClass('attivo');
      $('#dropdownMenu2').removeClass('attivo');
      $('#dropdownMenu1').addClass('attivo');
      $('.dropdown-menu.tre').css('display', 'none');
      $('.select2-drop').css('display', 'none');
      $('.dropdown-menu.due').css('display', 'none');
    });
    $('.dropdown-menu.due .select2-selection').mouseleave(function () {
      $('.dropdown-menu.due').css('display', 'block');
    });
    $("img.logo-cdr").insertAfter(".menu-menu-footer-container");
    $("ul.mini-menu").insertAfter("img.logo-cdr");
    $("div.certificazioni").insertAfter("#nav_menu-2 ~ #text-3 .textwidget");
    $("div.certificazioni").insertAfter("#nav_menu-2 ~ #text-5 .textwidget");
  });
  jQuery(window).ready(function ($) {
      $("div.notizia").removeClass("hide");
    $("button.chiudi-notizia").click(function () {
      setCookie('visited', '1', 10);
      //alert ($.cookie('visited'));
      $("div.notizia").hide(500);
    });
  });
  function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires=" + d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
  }
  /*javascript per la gestione dello slider nella home page*/
  jQuery(document).ready(function ($) {
    if ($('.owl-slidetop-gallery').length) {
      $('.owl-slidetop-gallery').owlCarousel({
        loop: true,
        dots: false,
        animateIn: 'fadeIn',
        animateOut: 'fadeOut',
        autoplay: true,
        margin: 0,
        autoplayTimeout: 4000,
        responsive: {
          0: {
            items: 1
          },
          600: {
            items: 1
          },
          1000: {
            items: 1
          }
        }
      });
      $('.owl-slidetop-gallery .hide').removeClass('hide');
    }
  });
</script>
