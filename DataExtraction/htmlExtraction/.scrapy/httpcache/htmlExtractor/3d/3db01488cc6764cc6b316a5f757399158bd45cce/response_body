if (window.innerWidth <= 640){
  	$('.mapWrap').remove();
	$('.locationFinder').click(function(e){
		e.preventDefault;
		$('.myTW').toggleClass('open');
	});

}  else{

	$('.locationExpander').click(function(e){
	  e.preventDefault;
	  $('.mapWrap').toggleClass('mapShow');
	  $('#map-canvas').toggleClass('show');
	  initialize();
	});
}


