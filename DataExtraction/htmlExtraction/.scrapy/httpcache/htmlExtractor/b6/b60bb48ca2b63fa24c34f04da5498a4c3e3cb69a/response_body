<script type="text/javascript">
function showLogin() {
	var lP = document.getElementById("loginPanel");
	lP.style.visibility = "visible";
	lP.style.display = "";
	try {
		document.getElementById("sUser_Identity").focus();
	} catch (exception) {}
}
function login() {
	var f1 = document.forms[0];
	f1.action = "/Util_Login/default.aspx";
	f1.submit();
}
function doKeyPress(evt) {
	//handle user hitting ENTER when they're in a form field. -- used for PASSWORD box in Login fields.
	var kC;
	if (window.event) {kC = window.event.keyCode;} else {kC = evt.which;}
	if (kC == 13) {login();}
}
function openWindow(loc, h, w, flag) {
	//used to open RE Glossary
	var params = "location=no,toolbar=no,scrollbars=yes,directories=no,Status=yes,copyhistory=no,width=" + w + ",height=" + h + ",left=10,top=10,resizable=yes";
	ChildWin = window.open(loc, 'openWin', params);
	ChildWin.focus();
}
</script>	
