<script type="text/javascript">
	(function ($) {
		function togglePopout() {
			var popoutopen = $('.popout.open');
			var popoutclosed = $('.popout.closed');

			var effect = 'slide';
			var duration = '500';
			var options = { direction: 'left' };

			popoutopen.toggle(effect, options, duration);
			popoutclosed.toggle(effect, options, duration);
		}

		$(function () {

					if($('.popout.open').is(":visible")) {
            	togglePopout();
            	$('.popout.open').removeClass("visible");
            	$('.popout.closed').addClass("visible");
        }
		
			$(".trigger").click(function(){
				togglePopout();
			});

		});

	})(jQuery);
</script>
