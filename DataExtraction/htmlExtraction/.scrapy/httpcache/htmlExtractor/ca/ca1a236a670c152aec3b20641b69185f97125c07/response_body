    <script language="Javascript" type="text/javascript">
		geocoder = new google.maps.Geocoder();
		
		function getGeoFromHeader () {
			var geoAddress = "";
			if (document.getElementById('txtZip').value != "") {
				geoAddress = document.getElementById('txtZip').value;
			} else if (document.getElementById('txtCity').value) {
				geoAddress = document.getElementById('txtCity').value;
				if (document.getElementById('txtState').value != "") { geoAddress += geoAddress + ' ' + document.getElementById('txtState').value; }
			} else if (document.getElementById('txtState').value) {
				geoAddress = document.getElementById('txtState').value;
			}
			//alert(geoAddress);
			
			var Lat = 0;
			var Lon = 0;
			if (geoAddress != "") {		
				geocoder.geocode( 
					{ 
						'address': 
						geoAddress 
					}, 
					function(results, status) { 
						if (status == google.maps.GeocoderStatus.OK) { 
							//alert(results[0].geometry.location.lat());
							document.getElementById('geoLat').value = results[0].geometry.location.lat();
							document.getElementById('geoLon').value = results[0].geometry.location.lng();
							var message = 
							'Latitude: ' + results[0].geometry.location.lat()+ 
							'Longitude: ' + results[0].geometry.location.lng(); 
							 //alert(message); 
							document.getElementById('geoForm').submit();
						}
					} 
				);
			}
			
			return false;
		}  
		
		function getGeo () {
			var geoAddress = "";
			//alert(document.getElementById('txtZip2').value);
			if (document.getElementById('txtZip2').value != "") {
				geoAddress = document.getElementById('txtZip2').value;
			} else if (document.getElementById('txtCity2').value) {
				geoAddress = document.getElementById('txtCity2').value;
				if (document.getElementById('txtState2').value != "") { geoAddress += geoAddress + ' ' + document.getElementById('txtState2').value; }
			} else if (document.getElementById('txtState2').value) {
				geoAddress = document.getElementById('txtState2').value;
			}
			//alert(geoAddress);
			
			var Lat = 0;
			var Lon = 0;
			if (geoAddress != "") {		
				geocoder.geocode( 
					{ 
						'address': 
						geoAddress 
					}, 
					function(results, status) { 
						if (status == google.maps.GeocoderStatus.OK) { 
							//alert(results[0].geometry.location.lat());
							document.getElementById('geoLat2').value = results[0].geometry.location.lat();
							document.getElementById('geoLon2').value = results[0].geometry.location.lng();
							var message = 
							'Latitude: ' + results[0].geometry.location.lat()+ 
							'Longitude: ' + results[0].geometry.location.lng(); 
							 //alert(message); 
							document.getElementById('geoForm2').submit();
						}
					} 
				);
			}
			
			return false;
		}
		
		function getGeoMobile () {
			var geoAddress = "";
			if (document.getElementById('txtZip3').value != "") {
				geoAddress = document.getElementById('txtZip3').value;
			} else if (document.getElementById('txtCity3').value) {
