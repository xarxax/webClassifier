<script type="text/javascript">
    $(window).bind('load', function() {
        // setj the desired carousel width
        var iWidth = "100%";
    
        // check if max photo width is less than the width setting and shrink it if need be
        var iMaxWidth = 0;
        $('#carouselWrap1621 img').each(function(i, img) {
            var iImgWidth = $(img).width();
            iMaxWidth = (iImgWidth > iMaxWidth) ? iImgWidth : iMaxWidth;
        });
        iWidth = (iMaxWidth < iWidth) ? iMaxWidth : iWidth;

        $('#carouselWrap1621').width(iWidth);
        $('#carousel1621').carouFredSel({
            width: iWidth,
            //height: 'auto',
            items: {
                visible: 5,
                width: iWidth,
            },
            scroll: {
                duration: 500,
                pauseOnHover: true
            },
            auto: false, // 7000
            prev: {
                button: "#btnPrev1621",
                key: "left"
            },
            swipe: true,
            next: {
                button: "#btnNext1621",
                key: "right"
            }
        });

    });
    $(window).load(function () {
        var maxHeight = 0;
        $('#carouselWrap1621').find('.caroufredsel_wrapper > ul > li > a img').each(function (i, el) {
            maxHeight = Math.max(maxHeight, $(el).height());
        });
        var btnHeight = $('#carouselWrap1621').find('.btnNext').outerHeight();
        if (maxHeight > 0) {
            $('#carouselWrap1621').find('.btnPrev, .btnNext').css('top', (maxHeight / 2) - (btnHeight / 2));
        }
    });
</script> 
        
    </div>

                
