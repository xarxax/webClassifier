 $(document).ready(function() { 
            $("a").each(function() { 
                var ionFocus = $(this).attr("onFocus"); 
                if (ionFocus == null) { 
                    $(this).attr("onFocus", "this.blur();"); 
                } 
                else { 
                    $(this).attr("onFocus", ionFocus + ";this.blur();"); 
                } 
                $(this).attr("hideFocus", "hidefocus"); 
            }); 
            $('input[@type="image"]').each(function() { 
                var ionFocus = $(this).attr("onFocus"); 
                if (ionFocus == null) { 
                    $(this).attr("onFocus", "this.blur();"); 
                } 
                else { 
                    $(this).attr("onFocus", ionFocus + ";this.blur();"); 
                } 
                $(this).attr("hideFocus", "hidefocus"); 
            }); 
        }); 
		//結束
    </script>

