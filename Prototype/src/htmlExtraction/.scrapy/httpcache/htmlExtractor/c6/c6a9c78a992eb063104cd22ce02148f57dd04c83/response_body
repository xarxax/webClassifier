 jQuery(document).ready(function($){
 jQuery('.um-accordion dt').addClass('expand-plus');
 jQuery('.um-accordion dt').click(function(){
    if(!jQuery(this).hasClass('collapse-minus')){
     jQuery(this).removeClass('expand-plus');
     jQuery(this).addClass('collapse-minus');
    }else {
      jQuery(this).addClass('expand-plus');
      jQuery(this).removeClass('collapse-minus');
    }
    jQuery(this).next().toggle();
  });
  
  jQuery('.um-accordion .expand-all').addClass('expandall-plus');
  
  jQuery('.um-accordion .expand-all').click(function(){
   if(!jQuery(this).hasClass('collapseall-minus')){
   jQuery(this).removeClass('expandall-plus');
    jQuery(this).addClass('collapseall-minus');
    jQuery(this).find('a').html('Collapse All');
   }else{
    jQuery(this).removeClass('collapseall-minus');
   jQuery(this).addClass('expandall-plus');
    jQuery(this).find('a').html('Expand All');
   }
   jQuery(this).parent().find('dt').trigger('click');
  });
 });
</script>
