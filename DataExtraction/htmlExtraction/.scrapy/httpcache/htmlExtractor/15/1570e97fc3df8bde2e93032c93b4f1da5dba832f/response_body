    <script>
        $(document).ready(function () {            
            $('#AuthLoginButton').click(function () {
                if (ValidateInput()) {
                        $.ajax({
                            type: "POST",
                            url: "/Services/AuthenticationService/AuthenticationWebService.asmx/Authenticate",
                            data: '{userName: "' + $('.sfLoginFieldsWrp .sfTxt')[0].value + '", password: "' + $('.sfLoginFieldsWrp .sfTxt')[1].value + '", persistent: "' + $('.sfCheckBoxWrapper input').is(':checked') + '" }',
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function (data) {
                                if (data.d.Success == false) {
                                    // set error message, crosspoint should approve
                                    $('.sfError.authError').html(data.d.ErrorMessage);
                                    $('.sfError.authError').show();
                                }
                                else {
                                    // simulate login click
                                    // __doPostBack('ctl00$LoginPlaceholder$ctl02$ctl00$ctl00$LoginButton', '');
                                window.location = window.location.pathname;
                            }
                        },
                        failure: function (response) {
                            alert(response.d);
                        }
                    });
                }
            });
        });

        function ValidateInput() {
            result = true;
            // validate textboxes
            if ($('.sfLoginFieldsWrp .sfTxt').eq(0).val() == "") {
                $('#userNameError').show();
                result = false;
            }
            else {
                $('#userNameError').hide();
            }

            if ($('.sfLoginFieldsWrp .sfTxt').eq(1).val() == "") {
                $('#passwordError').show();
                result = false;
            }
            else {
                $('#passwordError').hide();
            }
            return result;
        }
    </script>


    
    
</fieldset>
</div>
        </div>
    </div>
</div>
        
        
