$(document).ready(function(){
     $(".panel-heading span.icon_minim,.minimize,.icon_minim").trigger('click');
});

$(document).on('click', '.panel-heading span.icon_minim,.minimize,.icon_minim', function (e) {
    var $this = $(this);

    if (!$('.xebec').hasClass('panel-collapsed')) {

        $this.parents('.panel').find('.panel-body').slideUp();

        $('.xebec').addClass('panel-collapsed');

        $('.xebec').removeClass('glyphicon-minus').addClass('glyphicon-plus');

    } else {

        $this.parents('.panel').find('.panel-body').slideDown();

        $('.xebec').removeClass('panel-collapsed');

        $('.xebec').removeClass('glyphicon-plus').addClass('glyphicon-minus');
    }
});
</script>
