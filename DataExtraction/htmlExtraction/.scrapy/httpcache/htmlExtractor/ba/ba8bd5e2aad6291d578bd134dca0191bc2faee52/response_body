				function PrepareTxt(w){
	var forret = " <style type='text/css'> .list_m { 	height: 111px; 	border-bottom: 1px solid #9E9E9E; font-size: 10px; } #list_header { 	background-color: #B1AFB0; 	height: 5px; } #list_image { 	width: 150px; 	float:left; } #list_image img{ 	border: 1px solid white; } #list_cont { 	width: 430px; 	float:left; 	font-size: 12px; } #list_mname { 	padding-top:3px; 	color:black; 	font-weight:bold; 	float: left; 	width: 400px; 	font-size: 13px; } #list_mname a, #list_mname a:hover { 	color:black; 	text-decoration: none; } #list_status { 	width:80px; 	float:left; 	padding-top: 5px; 	text-align: center; } #list_items { 	width:350px; 	float:left; } #list_pricelinks { 	border-top: 1px solid #797979; 	padding-top: 1px; 	height: 20px; } #list_price { 	color:#797979; 	font-weight:bold; 	float: left; 	width: 280px; 	font-size: 13px; } #list_full { 	width: 62px; 	font-weight:bold; 	float: left; 	font-size: 13px; 	text-align: right; } #list_full a { 	color:black; 	text-decoration: underline; } #list_full a:hover { 	text-decoration: none; } #list_links { 	float: right; } .small_icon {	 	float: left; 	width: 18px; 	height: 16px; 	border: none; } </style>  " + document.getElementById('lm'+w).innerHTML;
	var myhost = document.getElementById('myhost').lang;	

	var temp = forret.split('src="../');
	forret = temp.join('src="'+myhost);
	temp = forret.split('src="images/');
	forret = temp.join('src="'+myhost+'tpl/images/');
		
	var root = document.createElement('div');
	root.innerHTML = forret;

	forret = root.innerHTML;

		
	return forret;
}
function chID(wid,wlang){
	GLOBID = wlang;
    clip.destroy();
    clip.glue( wid );		
} 
				
				
					
