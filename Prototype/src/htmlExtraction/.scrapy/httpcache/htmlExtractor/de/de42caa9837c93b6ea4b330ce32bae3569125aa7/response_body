<script>
window.categories_id = "0";
window.section = "shop";
window.currentpage = "";
$(document).ready(function(e){
	$('body').click(function(e){
            if ($(e.target).parent().attr("id")!="basketlink"){
             $('#basketpreview').hide();
            }
        });
        $('#basketlink').click(function(e){
            $('#basketpreview').show();
            e.preventDefault();
            $.get("shop.php?basketpreview=true",function(data){
                var items = $.parseJSON(data);
                console.log(items);
                var prev = $('#basketpreview .items');
                prev.html("");
                for (var i=0; i<items.length; i++){
                    var html = "";
                    html = html+ '<div class="basketitem">';
                    html = html+ "<div class='col-xs-8'><div class='prevname'>"+items[i].quantity+" x "+items[i].name+"</div></div>";
                    html = html+ "<div class='col-xs-4' align='center'><img data-cid='"+items[i].cid+"' data-pid='"+items[i].id+"' data-picsrc='"+items[i].picsrc+"' src='images/"+items[i].image+"'></div>";
                    html = html+ "</div>";
                    prev.append(html);
                }
                if (items.length>0){
                    $('#basketpreview .checkout').show();
                    $('#basketpreview .empty').hide();
                }else{
                    $('#basketpreview .empty').show();
                    $('#basketpreview .checkout').hide();
                }
            });
        });
});

  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-65671476-1', 'auto');
  ga('send', 'pageview');

</script>

</script>