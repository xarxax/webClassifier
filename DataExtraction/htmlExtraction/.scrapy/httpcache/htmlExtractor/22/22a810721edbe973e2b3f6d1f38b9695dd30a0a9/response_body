	<script type="text/javascript">
    $(document).ready(function(){
        $("body").removeClass("preload");
    });
	
    var isTouch = "ontouchstart" in window;

    if(isTouch){

        var modals = document.querySelectorAll('.modal-block');

        var i=0;
        for(i;i<modals.length;i++){
            modals[i].addEventListener('touchstart', function(e){
                var mod = e.currentTarget;
                var cls = mod.className;
                var ACTIVE_CLASS = "active";

                if(cls.match('active')){
                    mod.className=mod.className.replace(new RegExp('(\\s|^)'+ACTIVE_CLASS+'(\\s|$)'),' ').replace(/^\s+|\s+$/g, '');
                } else {
                    mod.className += " "+ACTIVE_CLASS;
                }

            });
        }

    }


	</script>





