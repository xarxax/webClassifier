<script type="text/javascript">
dispDep2();
/* 1뎁 오버했을시 */
$("#nav>.select>li").hover(function () {
	$("#nav .sub").hide();
	$(this).find(".sub").show();
}, function() {
	$(this).find(".sub").hide();
	dispDep2();
});

/* 페이지별 네비2뎁표시 */
function dispDep2() {
	var dep1 = parseInt(""); //뎁1 인덱스
	var dep2 = parseInt("")-1; //뎁2 인덱스
	var pi = parseInt(""); //제품 카테고리
	var d1 = $("#nav>.select>li"); //뎁1

	/*
	switch (pi) { //제품페이지 일경우 카테고리 매칭
		case 1 : dep2 = 0; break;
		case 2 : dep2 = 1; break;
		case 4 : dep2 = 2; break;
		case 5 : dep2 = 3; break;
		case 6 : dep2 = 4; break;
		case 7 : dep2 = 5; break;
	}
	*/

	d1.eq(dep1).find(".sub").show(); //뎁2 보여라!
	if (dep2 != -1) {
		d1.eq(dep1).find(".sub a").eq(dep2).addClass("on"); //현재문서 표시해라!
	}
	//console.log(d1.eq(dep1).find(".sub").length);
}
</script>	<!--Container-->
