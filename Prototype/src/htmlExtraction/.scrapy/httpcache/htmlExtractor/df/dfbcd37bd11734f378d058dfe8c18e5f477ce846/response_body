        var oWnd = $find("ctl00_radWin");
            oWnd.setUrl("/tools/subscribe.aspx");
            oWnd.setSize(550, 300)
            oWnd.show();
        }

    function showDisclaimer(varID) {
        var oWnd = $find("ctl00_radWin");
