<script language = "javascript">
	//로그인 함수
	function fncLogin(form , tmp_form){
		
		if(form.str_mb_id.value == ""){alert("아이디를 입력하신 후 다시 시도해 주십시요!!"); form.str_mb_id.focus(); return;}
		form.str_mb_id.value = form.str_mb_id.value.toLowerCase();
		if(form.str_mb_password.value == ""){alert("비밀번호를 입력하신 후 다시 시도해 주십시요!!"); form.str_mb_password.focus(); return;}		

		tmp_form.str_mb_id.value = escape(form.str_mb_id.value);
		tmp_form.str_mb_password.value = escape(form.str_mb_password.value);

		fncSaveId(form.str_mb_id , form.str_mb_idsave, 30);	//아이디 저장 체크 => 쿠키 생성

		form.method = "post";
		form.target = "HiddeniFrame";
		//form.action = "http://www.urd.co.kr/~ftp_urd/eng_board/member/login_process.php?Type=login&str_page_name=/html/main";
		form.action = "https://www.urd.co.kr:46016/eng_board/member/login_process.php?Type=login&str_page_name=/html/main";
		form.submit();
	}

	//상단 제품 카테고리 레이어 관련 함수
	function fncCateLayer(url , layer){
		layer = document.getElementById(layer);
		var data = "";
		$.ajax({  
			type: "POST",   
			url: url ,
			data: data ,   
			success: function(msg){ 
				layer.innerHTML = msg;
			}
		});	
		setTimeout("fncCateLayer('./main_cate_data.php' , 'mainPro')",2000); 
	}


	$(document).ready(function(){
		//로그인 하지 않은 상황일 때 : 아이디 쿠키 적용
				fncGetId(document.frmLogin.str_mb_idsave , document.frmLogin.str_mb_id);
				

		fncCateLayer('./main_cate_data.php' , 'mainPro');
	});
</script>
