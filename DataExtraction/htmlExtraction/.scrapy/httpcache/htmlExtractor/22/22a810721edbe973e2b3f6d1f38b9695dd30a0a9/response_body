    <script type="text/javascript">
		var images = ['home-person-01.jpg', 'home-person-02.jpg', 'home-person-03.jpg', 'home-person-04.jpg', 'home-person-05.jpg', 'home-person-06.jpg', 'home-person-07.jpg', 'home-person-08.jpg', 'home-person-09.jpg', 'home-person-10.jpg'];
        $('<img class="fade-in" src="img/' + images[Math.floor(Math.random() * images.length)] + '">').appendTo('#person-bkg');
