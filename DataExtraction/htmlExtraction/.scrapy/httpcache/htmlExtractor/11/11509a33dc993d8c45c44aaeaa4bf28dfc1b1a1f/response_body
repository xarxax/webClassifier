// function to display NORMAL image
function imgNormal(imgName)
  {
  if (document.images)
    {
    document[imgName].src = eval(imgName + "Norm.src");
    }
  }

// function to display HIGHLIGHT image
function imgHiLite(imgName)
  {
  if (document.images)
    {
    document[imgName].src = eval(imgName + "Hi.src");
    }
  }

// function to display SELECTED image
function imgSel(imgName)
  {
  if (document.images)
    {
    document[imgName].src = eval(imgName + "Sel.src");
    }
  }
// function to launch a new remote window
function launch(windowURL,windowName,windowFeatures,orgName)
{
  var remote = open(windowURL,windowName,windowFeatures,orgName);
  if (remote.opener == null)
    remote.opener = self;
  remote.opener.name = orgName;
  return remote;
}
// function to open EMail Survey dialog box
function launchSurvey()
{
  targetURL = "email_survey.asp";
  winName = "UXRSurveyWin";
  myRemote = launch( targetURL,
                     winName,
                    "resizable=yes,menubar=no,status=no,scrollbars=yes,width=350,height=160",
                    "UXRSurvey");
}
