<script>
    var ua = navigator.userAgent.toLowerCase();
    if (ua.indexOf("msie") != -1 || ua.indexOf("opera") != -1) {
        jQuery('body').css('overflow', 'hidden');
        jQuery('html').css('overflow', 'hidden');
    }

    jQuery(function () {

        var sliderInterval = setInterval(function () {
            var length = $('.contentHolder .contentHolderUnit').length;

            if (length > 0) {
                $(".homepage-footer-wrapper").fadeIn();
                clearInterval(sliderInterval);
            }
        }, 100);

        ApplyLayerHacks();

        jQuery('.fullpage-slider.1c711d73-6115-48dc-96a1-62df0e93290a').bannerscollection_zoominout({
            skin: 'opportune',
            responsive: true,
            showCircleTimer: false,
            thumbsWrapperMarginTop: -55,
            enableTouchScreen: true,
            width100Proc: true,
            height100Proc: true,

            absUrl: '/Resources/',
            fadeSlides:  true,
            autoPlay: 10,
            width: 2880,
            height: 1800,
            initalZoom: 1,
            finalZoom: 1,
            myloaderTime : 3,
            pauseOnMouseOver : false,
            scrollSlideDuration : 0.5,
            scrollSlideEasing : 'swing',
            showAllControllers : true,
            showNavArrows : true,
            showBottomNav : true,
            showOnInitNavArrows : false,
            showOnInitBottomNav : false,
            autoHideNavArrows : true,
            autoHideBottomNav : true,
            loop : true,
            setAsBg : true,
            responsiveRelativeToBrowser: true,
            showPreviewThumbs: false
        });    
    });

    // Begin hack methods: to center the text and get the right line-height
    function ApplyLayerHacks() {

        jQuery(".bannerscollection_zoominout_text_line").each(function () {

            var container = jQuery(this);
            
            WrapCenterDivIfNeeded(container);

            SetLineHeight(container.children());

            SetPaddingWhenItalic(container.find("span"));
        });
    }

    function WrapCenterDivIfNeeded(container) {

        if (container.data("custom-centred") === true) {

            var width = 2880;
            var div = jQuery("<div id=\"center\">").css("width", width).css("margin-left", (width / 2) * -1);

            container.wrapInner(div);
        }
    }

    function SetLineHeight(children) {

        children.each(function () {

            var child = jQuery(this);
            if (child.attr("style") === undefined || child.attr("style").indexOf("line-height") === -1) {

                child.css("line-height", "1.2em");

                SetLineHeight(child.children());
            }
        });
    }

    function SetPaddingWhenItalic(elements) {

        elements.each(function () {

            var span = jQuery(this);
            if (span.has("em")) {

                var cssFontSize = span.css("font-size");
                var fontSize = parseInt(cssFontSize.replace("px", ""));
                var margin = (fontSize / 5);

                span.css("padding-right", margin + "px");
            }
        });
    }
    // End hack methods: to center the text and get the right line-height
</script>

