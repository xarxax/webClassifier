<script>
  $('.orderpartsdialog').parents('.modaldialogcontent').first().addClass('orderpartsdialogcontent');
  $(".modaldialogheader span.close").click(function(){ $(this).closest('.modaldialog').hide(); });
  window.onclick = function(event) {
    $('.modaldialog').each(function(){
      if ($(event.target).hasClass('modal-backdrop')) { $("#ModalDialogWrapper").modal('hide'); }
    });
  }
  function showModalDialog(id){
    // 
    if (window.location.pathname.startsWith("/APAC") || window.location.pathname.startsWith("/LATAM") || window.location.pathname.startsWith("/EMEA")) {
      $('.orderpartsdialogbuttonwrapper a').first()[0].click()
    } else {
      var dia = $('.modaldialog:has(#' + id + ')');
      dia.modal();
      dia.position({my:'center top',at:'bottom left',of:$(this),collision:'fit flip'});
    }
  }
</script>
