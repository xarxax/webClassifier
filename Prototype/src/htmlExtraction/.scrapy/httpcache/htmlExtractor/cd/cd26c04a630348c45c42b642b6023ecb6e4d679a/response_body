<!-- Script for CustomVariationsLabelMenu -->
<script type="text/javascript">
    if (typeof(console)=='undefined'){
        var console = {};
        console.log = function(){};
    }
    var PageVariations = [];
    
    if (typeof (jQuery) != 'undefined') {
        jQuery(document).ready(function() {
            $ = jQuery;
            var el = $('span.menu-item-text:contains(Language:)');
            if (el.length == 0) {
                console.log('could not find Language item');
                return; // couldnt find Language node
            }
                var l = PageVariations.length, v;
                for (var i = 0; i < l; i++) {
                    v = PageVariations[i];
                    el.after(BuildItem(v.NavigateUrl, v.DisplayText));
                }
          
		 $('#s4-searcharea').mouseover(function() { $('.varlangs').css('visibility','visible'); }); 
		 $('#s4-searcharea').mouseout(function() { setTimeout("$('.varlangs').css('visibility','hidden')", 2000);} ); 
            
        });
    }

    function BuildItem(url, name) {
	// use this one for hidding -- Dimitri klentos
	  //      return '&nbsp;<span style="margin-top:5px;margin-right:10px;visibility:hidden;"><a href="' + url + '" class="varlangs" style="font-weight:normal">' + name + '</a></span>';
	//  	// use this one for making it visible
	        return '&nbsp;<span style="margin-top:5px;margin-right:10px;"><a href="' + url + '" class="varlangs" style="font-weight:normal">' + name + '</a></span>';
    }
    
</script>



