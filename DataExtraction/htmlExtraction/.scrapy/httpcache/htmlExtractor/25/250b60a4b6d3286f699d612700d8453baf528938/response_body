<script type="text/javascript">
    $(document).ready(function () {
        $('#rotInner').cycle({
            fx: 'fade',
            timeout: 6000,
            pause: 1
        });

        $('#rotInner2').cycle({
            fx: 'fade',
            timeout: 4333,
            pause: 1
        });

        $('#rotInner3').cycle({
            fx: 'fade',
            timeout: 4333,
            pause: 1
        });

        $('#appsFooter').show();
        $('.lnkCirculation').addClass('activeLink');
        $('.app:not(#appCirculation)').hide();

        $('#appsFooter a').click(function (e) {
            e.preventDefault();
            $('#appsFooter a').removeClass('activeLink');
            $(this).addClass('activeLink');
            $('.app').hide();
            activeApp = $(this).attr('href');
            $(activeApp).show();
        });

    });

    var currentPopin = "";
    function OnClosePopin() {
        $("#" + currentPopin).dialog("close");
    }

    function ResizePopin(width) {
        $("#" + currentPopin).dialog("width: 100");
    }
    function ShowQuotePopin() {

        var width = "1000px";
        var popinElementId = "q0";
        var buttons = { Close: OnClosePopin };
        width = typeof width !== 'undefined' ? width : "1000px";
        var options = { modal: true, width: width, dialogClass: 'myui-dialog2', zIndex: 30000 }; // , buttons: buttons
        $("#" + popinElementId).dialog(options);
        currentPopin = popinElementId

        // $(".ui-dialog-titlebar").hide();
        $("#" + popinElementId).addClass("myui-dialog");
    }

</script>
