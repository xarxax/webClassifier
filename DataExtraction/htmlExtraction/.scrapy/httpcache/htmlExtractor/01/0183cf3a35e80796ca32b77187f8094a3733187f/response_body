function showpmenu() {
 if ($("#pmenu").length < 1) {
    // need to create div 
    makediv("pmenu");
  }
//Get
var p = $(".Psearch");
var offset = p.offset();
//set
$("#pmenu").offset({ top: offset.top + 40, left: offset.left -20 });
// $('#pmenu').css({
//    'position': 'fixed',
//     'top' : offset.top + 40,
//  });
// $("#pmenu").html($("#left_menucontent").html()) ;
//if ($('#pmenu').length <= 1) {  
    jQuery.ajax({
        type: "POST",
        url: MDSRnUrl,
        data: "ORDERNO=888888&PAGEID=31&LOGINPATH=DL&SUBMITVAR=menuContent.html",
        dataType: "html",
        success: function (data) {
            $('#pmenu').html(data);         
        }
    });
//}
// show existing side menu
//$(".STab").click()  ;
 $('#pmenu').css({
    'position': 'fixed',
    'display': 'none',
    'z-index': '9999',
     'top' : offset.top + 20,
  });
// $(element).is(":visible");   
//$(".STab").click()  ;
$('.Psearch').mouseover(function(){   $("#pmenu").fadeIn(); $(".STab").click()  ; });
$("#pmenu").click(function(){    $("#pmenu").fadeOut();   });
$("#pmenu").mouseout(function() {
 var isHovered = $('#pmenu').is(":hover");
 if (! isHovered ) {  $("#pmenu").fadeOut() ; }
});
}



function showcontacts() { 
$("#slide_holder").show();
$(".midtab_content").hide();
$("#midtab3").fadeIn() ;
}
function showcontact() {
 var t1 = setTimeout(function() { showcontacts()}, 1000);
} 
