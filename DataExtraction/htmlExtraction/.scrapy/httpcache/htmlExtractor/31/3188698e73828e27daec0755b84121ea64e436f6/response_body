	<script type='text/javascript'>
	
	function TimeOnLoad()
	{
		$(".eleLoader").show();
		$(".time-sel").hide();
		
		var sIdx = $('.loc-sel')[0].selectedIndex;
		var rstid = $('.loc-sel').val();
		var SelectedLoc=$('select[class=loc-sel]').val();
		
		var selDate= $("#hidSelDate").val(); //var date = $('.datepicker').val();
		
	   if(SelectedLoc!='' && SelectedLoc >0)
	   {
			var sIdx =  sIdx - 1;
			locationid  = SelectedLoc;	   
	   }
	   else
	   {
			var locationid  = RES_LOCATIONS[sIdx].restaurantID;   
	   }
			
		var splmsg=RES_LOCATIONS[sIdx].field_special_message;		

		
		
		$.ajax({
			type: "POST",
			url: "https://fogodechao.com/reservation_v2/ajaxprocess.php",
			
			data: "opflag=DATE_LOAD_HOME&locationid="+locationid+"&curtime="+SystemTime()+"&curdate="+$('#hidCurDate').val()+"&selDate="+selDate,			
			
			dataType: 'json',
			async: true,
			success:function(data){
				//alert(data.output);
				//$("#timepicker1").html(data.output);
				
				$(".eleLoader").hide();
				$(".time-sel").show();			
				$("#timepicker1").html(data.output);
				$("#timepicker1").selectOrDie("update");
				
				//$("#timepicker1").selectOrDie({tabindex: 1});
			}
		});
		
		if(splmsg=='')
		{
			$(".spl_msg").hide();
			$(".find-table").show();
		}
		else
		{
			$(".find-table").hide();
			$(".spl_msg").show();
			$(".spl_msg").html(splmsg);
		}
	}
	
	$(document).ready(function() {
		//console.log('Nil location :'+SEL_LOCATION);
		if($('.loc-sel').hasClass('loc-sel')){
			TimeOnLoad();
		}
	});
	
	
	
	function SystemTime()
	{
		  var date= new Date();
		  var hours = date.getHours();
		 
		  
		  var minutes = date.getMinutes();
		  var ampm = hours >= 12 ? 'PM' : 'AM';
		  hours = hours % 12;
		  hours = hours ? hours : 12; 
		  
		  hours = hours < 10 ? '0'+hours : hours;
		  
		  minutes = minutes < 10 ? '0'+minutes : minutes;
		  
		  if(minutes<30) { var minutes='00';  }  
		  else if(minutes<60) { var minutes='30'; } 
		  
		  var strTime = hours + ':' + minutes + ' ' + ampm;	
		  
		  return strTime;
	}
	

	
	
	$("select.time-sel").selectOrDie({
                customClass: "time-sel",
                customID:    "time-sel"
            });
	
	
	function changeTime()
	{
		//alert($("#hidSelDate").val())
		$(".time-sel").hide();
		$(".eleLoader").show();
		
		var sIdx = $('.loc-sel')[0].selectedIndex;
        
		if(sIdx > 0) { var sIdx =  sIdx - 1; }           
		
		var locationid  = RES_LOCATIONS[sIdx].restaurantID;
		var selDate= $("#hidSelDate").val();
        var time = $('#timepicker1').val();
		var splmsg=RES_LOCATIONS[sIdx].field_special_message;
		
			
					 
			$.ajax({
			type: "POST",
			url: "https://fogodechao.com/reservation_v2/ajaxprocess.php",
			
			data: "opflag=DATE_CHANGE_HOME&locationid="+locationid+"&selectedtime="+time+"&curtime="+SystemTime()+"&curdate="+$('#hidCurDate').val()+"&selDate="+selDate,			
			
			dataType: 'json',
			async: true,
			success:function(data){
				
				$("#timepicker1").html('');
				$("#timepicker1").html(data.output);
				$("#timepicker1").selectOrDie("update");
				
				$(".eleLoader").hide();
				$(".time-sel").show();
				
				/*
				var $input = $('.datepicker').pickadate();
				var picker = $input.pickadate('picker');
				picker.stop();
				*/
			}
		});
		
		if(splmsg=='')
		{
			$(".spl_msg").hide();
			$(".find-table").show();
		}
		else
		{
			$(".find-table").hide();
			$(".spl_msg").show();
			$(".spl_msg").html(splmsg);
		}
	}
	
	
	
		
			
		
		$( '.datepicker' ).pickadate({
			
				min: [2019,0,29],
				format: 'ddd,mmm dd, yyyy',
				onSet: function() {
					
					var abc=(this.get('select','ddd, mmm dd, yyyy'));
					//alert(abc);
					$("#hidSelDate").val(abc);
					changeTime();
				}
		});
		
	
	
	
	
	
	
	$(function(){
            $('.find-table').click(function(e) {
                e.preventDefault();
                var rstid = $('.loc-sel').val();
                if($('.loc-sel').val()=="")
				{
					alert('Please select a loction');
				}
				else
				{	
					var sIdx = $('.loc-sel')[0].selectedIndex,
					//sIdx = sIdx - m,
					sIdx = sIdx - 1,
					city = $('select.loc-sel option:selected').text(),
					date = $('.datepicker').val(),
					time = $('#timepicker1').val(),
					size = $('select.guest-sel').val(),
					rid  = RES_LOCATIONS[sIdx].restaurantID
						
					//window.location.href = 'http://fogo.pinkjacketclients.com/reservation_v2/index/front/'+encodeURIComponent(city)+'/12,feb,2014/12:00am/2/111333/';
					//window.location.href = 'WEB_ROOT/index/front/'+encodeURIComponent(city)+'/'+date+'/'+time+'/'+size+'/'+rid+'/';
					window.location.href = 'https://fogodechao.com/reservation_v2/index/front/'+encodeURIComponent(city)+'/'+date+'/'+time+'/'+size+'/'+rstid+'/'+'?utm_campaign=reservation_from_home&utm_medium=fogo_reservation&utm_source=home_center';
				}
            });
			
		}); 
		
		
		
	</script>
	<script type='text/javascript'>//<![CDATA[ 
		var presource;
		$(function(){
            var userAgent = window.navigator.userAgent.toLowerCase(),
            ipad = /ipad/.test( userAgent ),
            iphone = /iphone|ipod/.test( userAgent );
			$(".checkbox-dropdown").click(function () {
				$(this).toggleClass("is-active");
			});

			$(".checkbox-dropdown ul").click(function(e) {
				e.stopPropagation();
			});
            
			/*
            $('.find-table').click(function(e) {
                e.preventDefault();
                var sIdx = $('.loc-sel')[0].selectedIndex,
                    //sIdx = sIdx - m,
                    city = $('select.loc-sel option:selected').text(),
                    date = encodeURI($('.datepicker').val()),
                    time = $('.timepicker').val(),
                    size = $('select.guest-sel').val(),
                    rid  = RES_LOCATIONS[sIdx].restaurantID
                    
                window.location.href = "/reservation/#/front/" + city + "/" + date + "/" + time + "/" + size + "/" + sIdx + "/" + rid;
            });
            */
			
            $('select.loc-sel').change(function() {
				
				//alert($('select.loc-sel').val());
				changeTime();
				
                var max = 0,
                    options = "";
                
                RES_LOCATIONS.forEach(function(obj) {
                    if($('select.loc-sel').val() == obj.restaurantID) {
                        max = obj.max;
                    }
                });
                for(var i = 1; i <= max; i++) {
                    var selected = "";
                    if(i == $('select.guest-sel').val()) {
                        selected = " selected";
                    }
                    options += '<option value="' + i + '"' + selected + '>' + i + '</option>' + "\r\n";
                }
                $('select.guest-sel').html(options);
                $("select.guest-sel").selectOrDie("update");
                //$('select.loc-sel').selectOrDie("update");
            })
            
            $.ajax({
                url: "https://fogodechao.com/promotions-json",
                dataType: "json",
                success: function(data) {
                    //var regex = new RegExp( '(' + request.term + ')', 'gi' );
                    //console.log(data)
                    //var s_str = data.client_id_c.replace(regex, "<b style='color:#cc0000;font-size:110%;'>$1</b>");
                    //var html_str = '<div><div style=\'float: right;\'>' + el.name + '</div>' + s_str + '</div>';
                    var s = [];
                    if(data.results.length > 0) {
                        presource = $.map(data.results[0], function(item) {
                            //console.log(item);
                            var d = item.field_date.split(" to ");
                            return {
                                label: item.title,
                                value: item.field_date,
                                sDate: d[0],
                                eDate: d[1],
                                link: item.field_page_link,
                                showDate: item.field_show_date,
                                button: item.field_button_text
                            };
                        });


                        setTimeout( function() {
                            //console.log(presource);
                            // create the notification
                            if(presource[0].showDate == 1 || presource[0].showDate == '') {
                                var dateText = '- ' + presource[0].value;
                            } else {
                                var dateText = '';
                            }
                            if(presource[0].button != "") {
                                var buttonText = presource[0].button;
                            } else {
                                var buttonText = 'book today';
                            }
                            var notification = new NotificationFx({
                                message : ' <p> <span class="serif">' + presource[0].label + '</span> ' + dateText + ' <a href="' + presource[0].link + '" class="btn-home book-table book-table-white">' + buttonText + ' <em class="fa fa-caret-right"></em></a></p>',
                                layout : 'bar',
                                effect : 'slidetop', // actually slides from the bottom
                                type : 'error', // notice, warning or error
                                ttl : 600000
                                });

                            // show the notification
                            notification.show();

                        }, 1200 );
                    }
                    
                }
            });
            
            ///notify
            
		}); 
	</script>

