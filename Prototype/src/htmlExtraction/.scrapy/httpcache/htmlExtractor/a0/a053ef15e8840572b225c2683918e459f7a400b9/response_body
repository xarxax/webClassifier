<script>
var jbr = jQuery.noConflict();
jbr(function(jbr) {
	jbr('#slides').slidesjs({
		width: 975,
		height: 475,
		navigation: {
			active: false
		},
		pagination: {
			active: true
		},
		play: {
			active: false,
			effect: "fade",
			interval: 6000,
			auto: true,
			pauseOnHover: false
		}
	});
	
	jbr(".jLink").click(function(){
		 window.location = jbr(this).find("a").attr("href"); 
		 return false;
	});
	
	//for solutions rotation
	var s_rotation = function (){
		jbr(".s-gear").rotate({
			angle:0, 
			animateTo:360, 
			callback: s_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	}	
	var s_re_rotation = function (){
		jbr(".s-gear").rotate({
			angle:0, 
			animateTo:-360, 
			callback: s_re_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	};
	jbr('#i-col-1').hover(
		function() {
			s_rotation();
		}, function() {
			s_re_rotation();
			setTimeout(function(){
				jbr(".s-gear").stopRotate();
			}, 2000);
		}
	);
	//
	//for results rotation
	var r_rotation = function (){
		jbr(".r-gear").rotate({
			angle:0, 
			animateTo:360, 
			callback: r_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	};
	var r_re_rotation = function (){
		jbr(".r-gear").rotate({
			angle:0, 
			animateTo:-360, 
			callback: r_re_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	};
	jbr('#i-col-2').hover(
		function() {
			r_rotation();
		}, function() {
			r_re_rotation();
			setTimeout(function(){
				jbr(".r-gear").stopRotate();
			}, 2000);
		}
	);
	//
	//for videos rotation
	var v_rotation = function (){
		jbr(".v-gear").rotate({
			angle:0, 
			animateTo:360, 
			callback: v_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	};
	var v_re_rotation = function (){
		jbr(".v-gear").rotate({
			angle:0, 
			animateTo:-360, 
			callback: v_re_rotation,
			easing: function (x,t,b,c,d){    
			  return c*(t/d)+b;
			}
		});
	};
	jbr('#i-col-3').hover(
		function() {
			v_rotation();
		}, function() {
			v_re_rotation();
			setTimeout(function(){
				jbr(".v-gear").stopRotate();
			}, 2000);
		}
	);
	//
 });
</script>

