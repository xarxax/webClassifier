<script> 
	var getUrlParam = function(e){var t = new RegExp("[?&]" + e.replace(/[\[\]]/g, "\\$&") + "(=([^&#]*)|&|#|$)"),a = t.exec(window.location.href);return a && a[2] ? decodeURIComponent(a[2].replace(/\+/g, " ")) : ""};
	var sjUI = (function(w,d,x,a,e,s,c,r){s = [];var b=function(){s.push(arguments);},q="ui";b.arr=s;w[a]=w[a]||[];w[a][q]=w[a][q]||[];w[a][q].push(b);c=d.createElement(x);c.async=1;c.src=e;r=d.getElementsByTagName(x)[0];r.parentNode.insertBefore(c,r);return b;})(window, document, "script", "sajari", "//cdn.sajari.net/js/integrations/website-search-1.2.0.js");
 
	sjUI("config", {
		mode: "inline",
		project: sjCompany,       											// Set this to your project.
		collection: sjCollection, 											// Set this to your collection.
		pipeline: "website",     											// Run the website pipeline.
		attachSearchBox: document.getElementById("search-box"), 			// DOM element to render search box.
		attachSearchResponse: document.getElementById("search-response"), 	// DOM element to render search results.
		searchBoxPlaceHolder: "", 	
 		attachDynamicContent: document.getElementById("dynamic-content"),
		maxSuggestions: 5,										// Placeholder text for the search box.
		results: {
			"showImages": true
		}, 										// Configure the results
		values: {
			"resultsPerPage": "20",
			"q": getUrlParam("q"),
			"q.override": false
		}, // Set default values
		tabFilters: null, // Make user selectable filters
		overlay: false // Whether to render an overlay or in-page
	});
    sjUI("sub", "response-updated", function(event, response) { 
		$(".sj-result-title a").each(function(i) {
			var curLink = $(this);
			var temp = "";
			curPath = this.pathname;
			url = this.href;
			title = this.textContent; 
			 
			if (url.indexOf(".pdf") != -1) {
				if (curLink.text().indexOf("[PDF]") == -1) {
					temp = "[PDF] ";
				};
				$(this).click(function() {
					curLink.attr("target", "_blank");
				});
			} 
			curLink.text(temp + curLink.text());  
		}); 
    }); 

	function InputChanged(event) {
		if (event && event.keyCode === 13 /* Enter Key */) {
			Search();
		} 
	}
	function Search() { 
		//sjUI("pub", "overlay-show");
		sjUI("pub", "values-set", { q: document.getElementById("input-box").value });
		sjUI("pub", "search-send", { q: document.getElementById("search-response") });
	}
</script>

