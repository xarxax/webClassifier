<script src="http://cdn.jquerytools.org/1.2.7/all/jquery.tools.min.js"></script>
<script type="text/javascript">

function equalHeight(group) {
	var tallest = 0;
	group.each(function() { 
		var thisHeight = $(this).height();
		if(thisHeight > tallest) {
			tallest = thisHeight;
		}
	});
	group.height(tallest);
}
jQuery(document).ready(function() {
	equalHeight(jQuery(".formee-equal"));
	
	// supply the language
	$.tools.validator.localize("fr", {
		':email'  	: 'Entrer une adresse email valide',
		'[required]'	: 'Accepter notre politique'
	});
	$.tools.validator.localize("it", {
		':email'  	: 'Si prega di inserire un indirizzo email valido',
		'[required]'	: 'Accettare la nostra Privacy Policy'
	});
	
	$.tools.validator.localize("en", {
		':email'  	: 'Please enter a valid email address',
		'[required]'	: 'Please accept our Privacy Policy'
	});

	// use the language in the validator
	$("#newsletter").validator({ lang: 'it' });
	
});
jQuery(window).resize(function() {
	equalHeight(jQuery(".formee-equal"));
});


</script>
