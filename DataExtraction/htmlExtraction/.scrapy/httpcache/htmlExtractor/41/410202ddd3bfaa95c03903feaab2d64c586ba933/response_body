<script type="text/javascript">
		function FindTable() {
			var location = $('#ddlLocation').val();
			var guest = $('#ddlguest').val();
			var time = $('#ddltime').val();
			var date = $("[id$=txtDateTime]").val();

			//var url = "http://www.opentable.com/opentables.aspx?t=Single&restref=" + location + "&rid=" + location + "&p=" + guest + "&d=" + date + " " + time;
			//alert(url);

			var opentablelink = "http://www.opentable.com/opentables.aspx?t=Single&restref=" + location + "&rid=" + location + "&p=" + guest + "&d=" + date + " " + time;

			window.open(opentablelink, '_blank', 'height=500,width=850,scrollbars:false' )
		}
	</script>
