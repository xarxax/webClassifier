<script>
	jQuery(document).ready(function($){

		$("#scrollbar").mCustomScrollbar({
			scrollButtons:{ enable:true	}
		});
		$('.refreshClick').click(function(event) {

			event.preventDefault();
			var linkID = $(this).attr('id');

			url = "/404.php?c=" + linkID;
			//window.location.replace(url);
			window.location.href = url;

		});
		$('.toggleClickResults').click(function(event) {
			event.preventDefault();
			var linkID = $(this).attr('id');
			var boxID = '#'+linkID+'wrap';
			//			console.warn(boxID);
			if ($(boxID).is(":visible")) {
				//$(boxID).hide();
				$(boxID).slideUp('fast', function() {
					// Animation complete.
				});
			} else {
				$(boxID).slideDown(function() {
					// Animation complete.
					$("#scrollbar").mCustomScrollbar("update");
				});
			}
		});

	});
</script>

<script>
	jQuery(document).ready(function($){
		$('.resbutton').click(function(event) {
			event.preventDefault();
			if ($('#reswrap').is(":visible")) {
				$('html,body').animate({ scrollTop: $('#reswrap').offset().top }, 'slow');
			} else {
				$('#reswrap').slideDown('slow', function() {
					// Animation complete.
				});
				$('html,body').animate({ scrollTop: $('#reswrap').offset().top }, 'slow');
			}
		});
		var date = new Date();
		date.setDate(date.getDate());
		$('.resdate').datepicker({
			format: 'mm/dd/yyyy',
			startDate: date,
			todayHighlight: true
		});
	});
	$(function() {
		$('#RestaurantID').change(function() {
			var x = $(this).val();
			// update the hidden input's value
			$('#RestaurantReferralID').val(x);
		});
	});
	function checkrestable(sendform){
		//alert("i: "+rid +"j: "+resparty)
		if (sendform.RestaurantID.value == "" || sendform.RestaurantReferralID.value == ""){
			alert("No location selected!");
			return false;
		}
				trackReservation('desktop');
		$("#resModal").modal("show");
			}
</script>

<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

	ga('create', 'UA-25221513-1', 'auto');
	ga('send', 'pageview');

	/*** tracks a click on an outbound link ***/
	var trackReservation = function(mode){
		if (!ga.q) {
			ga('send', 'event', 'reservation', 'submit', mode, {'transport': 'beacon','hitCallback': function(){}});
		}
	};

	/*** tracks a click on an outbound link : onclick="trackOutboundLink('http://'); return false;" ***/
	var trackOutboundLink = function(url){
		if (ga.q) {
			// Google Analytics is blocked
			// document.location = url;
			window.open(url);
		} else {
			ga('send', 'event', 'outbound', 'click', url, {'transport': 'beacon','hitCallback': function(){}});
			window.open(url);
		}
	}
</script>

<script>
			window.onload = function ()
	{
		// get user's location
		GetLocation();
	};
		function GetLocation ()
	{
		// HTML5/W3C Geolocation
		if ( navigator.geolocation )
		{
			navigator.geolocation.getCurrentPosition( UserLocation, DefaultLocation, {timeout:10000,maximumAge:120000} );
		}
		else
			console.log("Geolocation is not supported by this browser.");
	}

	// Callback function for error
	function DefaultLocation()
	{
		/*DEFAULT: BURLINGAME*/
		//NearestCity( 37.591768, -122.342912 );
	}

	// Callback function for asynchronous call to HTML5 geolocation
	function UserLocation( position )
	{
		NearestCity( position.coords.latitude, position.coords.longitude );
	}

	// Convert Degress to Radians
	function Deg2Rad( deg ) {
		return deg * Math.PI / 180;
	}

	function PythagorasEquirectangular( lat1, lon1, lat2, lon2 )
	{
		lat1 = Deg2Rad(lat1);
		lat2 = Deg2Rad(lat2);
		lon1 = Deg2Rad(lon1);
		lon2 = Deg2Rad(lon2);
		var R = 6371; // km
		var x = (lon2-lon1) * Math.cos((lat1+lat2)/2);
		var y = (lat2-lat1);
		var d = Math.sqrt(x*x + y*y) * R;
		return d;
	}

	//var lat=20; // user's latitude
	//var lon=40; // user's longitude

	var cities= [
		["bloomington", 44.853160, -93.355625],
		["burlingame", 37.591768, -122.342912],

		["oakland", 37.793591, -122.276306],
		["redondobeach", 33.840701, -118.391978],
		["stpaul", 44.945583, -93.095756]
	];

	function NearestCity( latitude, longitude )
	{
		var mindif=99999;
		var closest;

		for (index = 0; index < cities.length; ++index) {
			var dif =  PythagorasEquirectangular( latitude, longitude, cities[ index ][ 1 ], cities[ index ][ 2 ] );
			if ( dif < mindif )
			{
				closest=index;
				mindif = dif;
			}
		}

		//		nearest location
		//      alert( cities[ closest ] );
		url = "/404.php?c=" + cities[ closest ][ 0 ];
		window.location.replace(url);
		console.log('geolocationset');

	}
</script>


