"s8000", "bada", "googlebot-mobile"
)

var ua = navigator.userAgent.toLowerCase();
var isMobile = false;
for (var i = 0; i < mobiles.length; i++) {
if (ua.indexOf(mobiles[i]) > 0) {
isMobile = true;
break;
}
}
