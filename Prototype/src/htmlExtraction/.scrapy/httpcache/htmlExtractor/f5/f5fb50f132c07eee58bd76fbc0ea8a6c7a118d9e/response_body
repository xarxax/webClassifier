//<![CDATA[
requirejs(['jquery','owlcarousel'], function(jQuery, owlcarousel){ //

    jQuery(function($) {
        
        var owl = $('#slideshow-50ac62956fd80560c0ce7f941b7512a3');
        owl.owlCarousel({

            navigationText: false
            , addClassActive: true

                    , singleItem: true

                            , transitionStyle: 'fade'
            
        
                    , slideSpeed: 500        
                    , paginationSpeed: 500        
                    , autoPlay: 15000        
                    , stopOnHover: true
        
                    , rewindNav: true
            , rewindSpeed: 600
        
                    , pagination: true
        
                    , navigation: true
        
        
            , beforeInit: function() {
                var firstSlide = owl.find('.item:eq(0)');
                firstSlide.find('[data-animate-in]').each(function() {
                    $(this).addClass($(this).data('animate-in') + ' animated');
                });
            }

                        , afterMove: function() {
                owl.find('.owl-item.active [data-animate-in]').each(function() {
                    $(this).addClass($(this).data('animate-in') + ' animated');
                });
            }

            , beforeMove: function() {
                owl.find('.owl-item.active [data-animate-in]').each(function() {
                    $(this).removeClass($(this).data('animate-in') + ' animated');
                });
            }

        
        }); // end: owl

    });

});
