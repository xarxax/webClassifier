<script>
    $('#slide-main').cycle({
        fx: 'fade',
        speed: 'slow',
        timeout: 10000,
        prev: '#prev',
        next: '#next',
    });

    $('#pause').click(function () {
        $('#slide-main').cycle('pause');
        return false;
        if (is_play) {
            is_play = false;
            $(this).attr("src", "home_files/images/pause.png");
        }
        else {
            $(this).attr("src", "home_files/images/play.png");
        }
    });
    $('#play').click(function () {
        $('#slide-main').cycle('resume');
        return false;
    });
</script>
        </div>
