<!-- MAIN CENTER Slideshow JAVA (uses M suffix on variables) -->
<script>
var slideIndexM = 0;
showSlidesM();

function showSlidesM() {
    var iM;
    var slidesM = document.getElementsByClassName("mySlidesM");
    var dotsM = document.getElementsByClassName("dotM");
    for (iM = 0; iM < slidesM.length; iM++) {
       slidesM[iM].style.display = "none";  
    }
    slideIndexM++;
    if (slideIndexM> slidesM.length) {slideIndexM = 1}    
    for (iM = 0; iM < dotsM.length; iM++) {
        dotsM[iM].className = dotsM[iM].className.replace(" active", "");
    }
    slidesM[slideIndexM-1].style.display = "block";  
    dotsM[slideIndexM-1].className += " active";
    if (slideIndexM>1) {
	setTimeout(showSlidesM, 3000); // Change all but image1 every 3 seconds
	}
    else {
	setTimeout(showSlidesM, 6000); // Change image1 after every 6 seconds
	}
}
</script>



