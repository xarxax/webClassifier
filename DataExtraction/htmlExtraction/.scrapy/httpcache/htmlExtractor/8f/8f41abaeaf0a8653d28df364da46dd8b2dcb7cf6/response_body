        .fancybox-inner {
            overflow: hidden !important;
          }
    </style>
    
    
    <script type="text/javascript">
        $('#da-slider').cslider({
            current     : 0,
            // index of current slide
            bgincrement : 15,  
            // increment the background position 
            // (parallax effect) when sliding
            autoplay    : true,
            // slideshow on / off    
            interval    : 5000  
            // time between transitions
        });
    </script>


    

