<script type="text/javascript">
	$( document ).ready(function() {
								 	purifyContent(2);
									
									var hasImageBanner = false;
									if(hasImageBanner)
										$(window).resize(function(){setBannerImage();});
										
									document.getElementById('SearchTextBox').onkeydown = function(e)
									{
										if(e.keyCode == 13)	
										{
											e.preventDefault();
											submitSearch();
										    return false;
										}
									};
								 });
	
	var smalImg = '/img/company/top_banner_small.jpg';
	var bigImg = '';
	var actImg = '';
		
	function setBannerImage()
	{		
		var w  = window,
			d  = w.document,
			de = d.documentElement,
			db = d.body || d.getElementsByTagName('body')[0],
			x  = w.innerWidth || de.clientWidth || db.clientWidth,
			y  = w.innerHeight|| de.clientHeight|| db.clientHeight;		
		
		var newImg = '';
		
		if(x < 630)
			newImg = '/img/company/top_banner_small.jpg';
		else
			newImg = '';
		
		if(newImg !== actImg)
		{
			document.getElementById('headerImage').style.backgroundImage="url('" + newImg + "')";
			actImg = newImg;
		}
	}
	
	
	function openEmail(mailType, optinalId, subject, message)
	{
		var obj = $('#obj');
		var textstatus = $('#textstatus');
		jQuery.ajax({
		type: "POST",
		url: '/phps/functions_ext',
		dataType: 'json',
		data: {functionname: 'getEmailAdress', arguments: [mailType, optinalId]},
	
		success: function (obj, textstatus)
					{
					  if( !('error' in obj) )
					  {
						var mailLists = obj.result;					
						var t = $('#example').DataTable();
						var mailTo = mailLists[0];
						
						var joinChar = '?';
						
						mailCc = '';
						if(mailLists[1] !== '')
						{
							mailCc = '?cc=' + mailLists[1];
							joinChar = '&';
						}
						
						var mailBcc = ''
						if(mailLists[2] !== '')
						{
							mailBcc = joinChar + 'bcc=' + mailLists[2];
							joinChar = '&';
						}
						
						var subjectText = '';
						if(subject !== '')
						{
							subjectText = joinChar + "subject=" + encodeURIComponent(subject);
							joinChar = '&';
						}
						
						var bodyText = '';
						if(message !== '')
							bodyText = joinChar + "body=" + encodeURIComponent(bodyText);
							
						document.location.href = "mailto:" + mailTo + mailCc + mailBcc + subjectText + bodyText;
						
						//If the browser respond to the mailto it will loose focus, then timer must be stopped
						$(window).blur(function(){clearTimeout(t);});
	
						//The browser did not lost focus after 500 miliseconds, the mailto url protocol is not set or not working
						//Send the user to the contact us form
						t = setTimeout(function(){document.location.href = '/contact_us';}, 500);
					  }
					  else
						  console.log(obj.error);
				}
			});	
	}
	
	function submitSearch()
	{
		var live = true;

		if(live)
		{
			var objPost = $('#obj');
			var textstatusPost = $('#textstatus');
			jQuery.ajax({
			type: "POST",
			url: '/phps/functions_ext',
			dataType: 'json',
			data: {functionname: 'pardotHandlerUrl', arguments: ['SearchForm']},
		
			success: function (objPost, textstatusPost)
						{
						  if( !('error' in objPost) )
						  {			
							var form = document.getElementById('searchForm');								
							form.method = 'POST';
							form.action = objPost.result;
							form.submit();
						  }
						  else
							  console.log(objPost.error);
						}
				});
		}
		else
		{
			var form = document.getElementById('searchForm');								
			form.method = 'POST';
			form.action = '/search';
			form.submit();
		}
	}
	
	function notify_optin(emailAdress, country)
	{
		var obj = $('#obj');
		var textstatus = $('#textstatus');
		jQuery.ajax({
		type: "POST",
		url: '/phps/functions_ext',
		dataType: 'json',
		data: {functionname: 'getEmailAdress', arguments: ['mk']},
	
		success: function (obj, textstatus)
					{
					  if( !('error' in obj) )
					  {
						var mailLists = obj.result;
						
						jQuery.ajax({
						type: "POST",
						url: '/phps/functions_ext',
						dataType: 'json',
						data: {functionname: 'notifyOptIn', arguments: 
																	[
																		mailLists[0],
																		mailLists[1],
																		mailLists[2],
																		emailAdress,
																		country === '' ? 'Unknown' : country
																	]}
							});					
					  }
					  else
						  console.log(obj.error);
				}
			});
	}
</script>

