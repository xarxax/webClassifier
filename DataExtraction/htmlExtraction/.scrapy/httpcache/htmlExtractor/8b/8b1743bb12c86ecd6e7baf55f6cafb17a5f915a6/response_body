<script type="text/javascript">
   
    jQuery.uaMatch = function( ua ) {
    ua = ua.toLowerCase();
    var match = /(chrome)[ \/]([\w.]+)/.exec( ua ) ||
        /(webkit)[ \/]([\w.]+)/.exec( ua ) ||
        /(opera)(?:.*version|)[ \/]([\w.]+)/.exec( ua ) ||
        /(msie) ([\w.]+)/.exec( ua ) ||
        ua.indexOf("compatible") < 0 && /(mozilla)(?:.*? rv:([\w.]+)|)/.exec( ua ) || [];
    return {
        browser: match[ 1 ] || "",
        version: match[ 2 ] || "0"
    };
};
if ( !jQuery.browser ) {
    var 
    matched = jQuery.uaMatch( navigator.userAgent ),
    browser = {};
    if ( matched.browser ) {
        browser[ matched.browser ] = true;
        browser.version = matched.version;
    }
    // Chrome is Webkit, but Webkit is also Safari.
    if ( browser.chrome ) {
        browser.webkit = true;
    } else if ( browser.webkit ) {
        browser.safari = true;
    }
    jQuery.browser = browser;
}
        jssor_1_slider_init = function() {

            var jssor_1_options = {
              $AutoPlay: true,
              $SlideDuration: 800,
              $SlideEasing: $Jease$.$OutQuint,
              $ArrowNavigatorOptions: {
                $Class: $JssorArrowNavigator$
              },
              $BulletNavigatorOptions: {
                $Class: $JssorBulletNavigator$
              }
            };

            var jssor_1_slider = new $JssorSlider$("jssor_1", jssor_1_options);

            /*responsive code begin*/
            /*you can remove responsive code if you don't want the slider scales while window resizing*/
            function ScaleSlider() {
                var refSize = jssor_1_slider.$Elmt.parentNode.clientWidth;
                if (refSize) {
                    refSize = Math.min(refSize, 1920);
                    jssor_1_slider.$ScaleWidth(refSize);
                }
                else {
                    window.setTimeout(ScaleSlider, 30);
                }
            }
            ScaleSlider();
            $Jssor$.$AddEvent(window, "load", ScaleSlider);
            $Jssor$.$AddEvent(window, "resize", ScaleSlider);
            $Jssor$.$AddEvent(window, "orientationchange", ScaleSlider);
            /*responsive code end*/
        };
		

                  
    </script>

<script type="text/javascript">


var phonePattern =/^[(]{0,1}[0-9]{3}[)]{0,1}[-\s\.]{0,1}[0-9]{3}[-\s\.]{0,1}[0-9]{4}$/;
    var emailPattern = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    function isNumeric(n) {
        return !isNaN(parseFloat(n)) && isFinite(n);
    }
    
    function checkPrice(){
    if($('#cprice').val()==''){
        $('#cprice').focus();
        $('#cprice').css({'outline-color':'red'});        
         return false;
    }
    else if(!isNumeric($('#cprice').val())){
        $('#cprice').focus();
        $('#cprice').css({'outline-color':'red'});        
         return false;
    }
    else if($('#cname').val()==''){
        $('#cname').focus();
        $('#cname').css({'outline-color':'red'});        
         return false;
    }
    else if($('#cwebsite').val()==''){
        $('#cwebsite').focus();
        $('#cwebsite').css({'outline-color':'red'});        
         return false;
    }
    else if($('#cufname').val()==''){
        $('#cufname').focus();
        $('#cufname').css({'outline-color':'red'});        
         return false;
    }
    else if($('#culname').val()==''){
        $('#culname').focus();
        $('#culname').css({'outline-color':'red'});        
         return false;
    }
    else if($('#cuphone').val()==''){
        $('#cuphone').focus();
        $('#cuphone').css({'outline-color':'red'});        
         return false;
    }
    else if(!phonePattern.test($('#cuphone').val())){
        $('#cuphone').focus();
        $('#cuphone').css({'outline-color':'red'});        
         return false;
    }
    else if($('#cumail').val()==''){
        $('#cumail').focus();
        $('#cumail').css({'outline-color':'red'});        
         return false;
    }
    else if(!emailPattern.test($('#cumail').val())){
        $('#cumail').focus();
        $('#cumail').css({'outline-color':'red'});        
         return false;
    }
    else{
        
        var formData= $("#check-price").serialize();
       	$.ajax({
		type: "POST",
		url: "https://accucarecanada.com/home/check_price",
		data: formData,
		dataType    : 'json',
		success: function(data){
             $("#check-price").trigger('reset');
             if(data.status=='1'){
                $('#price-status').html(data.msg);
			}
			else{
				$('#price-status').html(data.msg);		
			}
		}
        });
        
    }

    return false;
}
  

function _send2Friend(){
    
    if($('#name').val()==''){
        $('#name').focus();
        $('#name').css({'outline-color':'red'});        
         return false;
    }
    else if($('#email').val()==''){
        $('#email').focus();
        $('#email').css({'outline-color':'red'});        
         return false;
    }
    else if(!emailPattern.test($('#email').val())){
        $('#email').focus();
        $('#email').css({'outline-color':'red'});        
         return false;
    }
    else if($('#fname').val()==''){
        $('#fname').focus();
        $('#fname').css({'outline-color':'red'});        
         return false;
    }
    else if($('#femail').val()==''){
        $('#femail').focus();
        $('#femail').css({'outline-color':'red'});        
         return false;
    }
    else if(!emailPattern.test($('#femail').val())){
        $('#femail').focus();
        $('#femail').css({'outline-color':'red'});        
         return false;
    }
    else if($('#description').val()==''){
        $('#description').focus();
        $('#description').css({'outline-color':'red'});        
         return false;
    }
    else{
        
        var formData= $("#send-2-friend").serialize();
       	$.ajax({
		type: "POST",
		url: "https://accucarecanada.com/home/send_to_friend",
		data: formData,
		dataType    : 'json',
		success: function(resp){
             $("#send-2-friend").trigger('reset');
             if(resp.status=='1'){
                $('#f-status').html(resp.msg);
			}
			else{
				$('#f-status').html(resp.msg);		
			}
		}
        });
        
    }

    return false;
    
}





function checkField(x){
    return $('#'+x).length;
} 
 
 
function isEmpty(val){
         return (val === undefined || val == null || val.length <= 0) ? false : true;
}

function _addToCart(x){
    
    var _form=true;
    
    if(checkField('color')){
        var c=isEmpty($('#color').val())
        if(!isEmpty($('#color').val())){
           $('#status').html('<strong>Select the required fields</strong>');
           _form=false;
        }
        else{
            _form=true;
        }
        
    }
    
    if(checkField('size')){
        if(!isEmpty($('#size').val())){
           $('#status').html('<strong>Select the required fields</strong>');
           _form=false;
        }
        else{
            _form=true;
        }
               
    }
   // alert(_form);
    if(_form==true){
    
    
            $.ajax({
            type: "POST",
            url: "https://accucarecanada.com/home/add/",
            data: $('.cart-form').serialize(),
            dataType: 'json',
            success: function (response) {
                if(response.status==1){
                    $('.count').html(response.count);
                    // $('#status').html('<div class="alert alert-success"><strong>'  +response.msg.replace(/\\/g, '') +  ' Added!</strong></div>');
                    window.location = "https://accucarecanada.com/cart";
                    //$('#status').html(response.msg);
                    
                }
                else{
                    $('.count').html(response.count);
                    $('#status').html(response.msg);
                    
                }
            }
        }); 
        
    
    } 
       
    
    return false;
}    


function _coupon(){
    
    var c=$('#coupon_code').val();
    if(c==''){
        $('#coupon-status').html('Please enter the coupon code');
    }
    else{
        
        $.ajax({
            type: "POST",
            url: "https://accucarecanada.com/home/coupon/",
            data:{c:c},
            dataType: 'json',
            success: function (response) {
                if(response.status==1){
                    
                    $('#coupon-status').html(response.msg);
                }
                else if(response.status==2){
                    $('#coupon-status').html(response.msg);
                }
                else if(response.status==3){
                    $('#coupon-status').html(response.msg);
                }
                
            }
        }); 
    }
    return false;
}

function _updateToCart(x)
{
  var qty= document.getElementById("q"+x).value;
        $.ajax({
            type: 'POST',
            url: "https://accucarecanada.com/home/update/",
            data: { id:x,qty:qty},
            dataType: 'json', 
            success:function(response){
                if(response.status==1){
                    window.location.reload(true);
                }
            }
        });

}
    
function _add(){
	//$('#price'+y).html(x);
	var _Quan=$('#quantity').val();
	if(1 <= _Quan){
            $('#quantity').val(++_Quan);
	}
	else if(_Quan==0){
            $('#quantity').val(1);	
	}
	else if(_Quan==1){
            $('#quantity').val(1);	
	}
	
	
	
}

function _search(){
    var _search=$('#term').val();
    if(!_search){
        return false;
    }
    else{
        return true;
    }
    
}


function _msearch(){
    var _search=$('#mterm').val();
    if(!_search){
        return false;
    }
    else{
        return true;
    }
    
}

function _minus(){
	var _Quan=$('#quantity').val();
	if(1 <= _Quan){
            if(_Quan==1){
		$('#quantity').val(1);	
            }
            else{
		$('#quantity').val(--_Quan);
            }
	}
	else if(_Quan==0){
            $('#quantity').val(1);	
	}
	else if(_Quan==1){
            $('#quantity').val(1);	
	}
	
}
$(document).ready(function(){
    
    
    
    /*
    var cProvince='';
    
    if(cProvince!=''){
        $('#pDiv').hide();
    }
    */
    
    $('.fancybox').fancybox();
    
    $(document).on("change", "#upload_prescription", function(e){
            $('#loading').show();
            var file = this.files[0];
            var fd = new FormData();
            fd.append("file", file);
            
            // These extra params aren't necessary but show that you can include other data.
            var xhr = new XMLHttpRequest();
            xhr.open('POST', 'https://accucarecanada.com/home/upload', true);
            xhr.upload.onprogress = function(e) {

                    };
            xhr.onload = function() {
                $("#upload_prescription").replaceWith($("#upload_prescription").val('').clone(true));
                $('#pre-status').html('');
                if (this.status == 200) {
                    var resp = JSON.parse(this.response);
                    if(resp.Status==1){
                        $("#files").append(resp.url);
                    }else{
                        $('#pre-status').html(resp.Msg);
                    }
                }else{
                        $('#pre-status').html(this.status);
				}
                $('#loading').hide();
            };
            xhr.send(fd);
            return false;
        });
        
        
        $(document).on("click", ".dPIBtn", function(){
            
            var _id=$(this).attr('data-id');
            
            $('#myModal .btn-primary').addClass('DBtn');
            $('#myModal .modal-title').html('Delete');
                                                        
            $('#myModal #modal-content').html('Are you delete this image');
            $('.DBtn').attr('id',_id);
            $('#myModal').modal('toggle');
            
            
        });
        
        
         $(document).on("click", ".DBtn", function(e){
            //alert(1);
            var _i=$(this).attr('id');
            
            if(_i!=''){
               
                    $('#files').each(function(i, items_list){
                                var myText = "";
                                $(items_list).find('li').each(function(j, li){
                                    if(_i==$(this).attr('id')){
                                        $(this).remove();
                                    }
                                })
                    });    
               
                
                $.ajax({
                        type: "POST",
                        url: 'https://accucarecanada.com/home/deleteFile/',
                        data: {i:_i},
                        dataType: 'json',
                        cache: false,
                        success: function(resp){
                           if(resp.Status==1){
                             //alert(1);
                             $('#'+_i).remove(); // when referencing by id
                             $('#myModal').modal('hide');
                           } 
                           else{
                               $('#myModal').modal('hide');
                           }
                        }
                });
            }
        });
        
        
    
});
</script>
