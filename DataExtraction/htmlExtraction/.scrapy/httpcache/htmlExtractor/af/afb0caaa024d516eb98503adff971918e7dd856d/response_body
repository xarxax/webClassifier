    $(document).ready(function() {
        // camera slider輪播設定(API設定參考：http://www.pixedelic.com/plugins/camera/)
        jQuery('#camera_wrap').camera({
            loader: false,
            pagination: false,
            thumbnails: false,
            height: '40%',
            minHeight: '200',
            caption: false,
            navigation: true,
            //預設1.5秒自動切換
            time: 1500,
            //場景切換速度
            transPeriod: 1500,
            fx: 'mosaic'
        });
