    if (this._cachedActiveTabIndex != -1) {
        this.set_activeTabIndex(this._cachedActiveTabIndex);
        this._cachedActiveTabIndex = -1;

        var activeTab = this.get_tabs()[this._activeTabIndex];
        if (activeTab) {
            activeTab._wasLoaded = true;
            /*** CMS ***/
