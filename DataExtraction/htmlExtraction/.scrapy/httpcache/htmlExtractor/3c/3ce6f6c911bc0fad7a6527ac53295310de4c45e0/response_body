function NoFramesAllowed()
	{
	if (this.top != this)
		{
		top.location.href=this.location.href;
		return;
		}
	}
function popUp ()
	{
	var popUp = window.open("check/index.html", "popupWin",'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width=450,height=510');
	self.name = "mainWin";
	}
