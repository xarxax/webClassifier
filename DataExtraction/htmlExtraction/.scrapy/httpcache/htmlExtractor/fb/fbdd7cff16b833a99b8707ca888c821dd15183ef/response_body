$(document).ready(function() {

    datePick(); //Jquery datpepicker

   $(".hero").backstretch([{
        url: "images/homeslideshow/2017/Benvenuti-1-10.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'La Tua Famiglia- Join us for an Italian Thanksgiving Feast'  
    }, {
        
        url: "festa/2019/Umbria-carousel-2.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'Festa Regionale Umbria February 4-17'
    }, { 
        url: "images/homeslideshow/2016/App-Italy-carousel-1-117.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'Win a trip for two to Italy. Download our app and collect all 6 stamps on your culinary journey.'
    }, {
        url: "images/homeslideshow/2018/New-App-carousel-1-7.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'Start Earning Credit towards rewarding rewards every time you Dine.'
    }, {
        url: "images/homeslideshow/2017/GIFT-CARD-carousel-8.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'Il Fornaio Gift Cards-Carta Regalo'
    }, {
        url: "images/homeslideshow/2018/WS-carousel-7.jpg",
        duration: 5000,
        fade: 500,
        fadeFirstImage: false,
        alt: 'Il Fornaio is Honored to Receive The Wine Spectator Award of Excellence Tutti i Ristoranti'

    }]);

    //Variabale to make whole slider swipeable
    var next = document.getElementById("backHeroHome");

    // create a simple instance
    // by default, it only adds horizontal recognizers
    var mc = new Hammer(next);

    // Swipe left event
    mc.on("swipeleft", function(ev) {
       $(".hero").data('backstretch').next();
    });

    // Swipe right event
    mc.on("swiperight", function(ev) {
        $(".hero").data('backstretch').prev();

    });
    /*END NEW SLIDER CONTROLS*/

    actions();

    var forms = document.getElementsByTagName("form");
    for (var i = 0; i < forms.length; i++) {
        forms[i].noValidate = true;

        forms[i].addEventListener("submit", function(event) {
            //Prevent submission if checkValidity on the form returns false.
            if (!event.target.checkValidity()) {
                event.preventDefault();
                alert("Please fill out all fields.");
            }

        }, false);
    }
});



$(window).on("backstretch.before", function(e, instance, index) {
    if (index == 0) {
        $(".slide1").toggleClass("slide_reveal").children().toggleClass("slide_link");
        $("div").not(".slide1").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");

    } else if (index == 1) {
        $(".slide2").addClass("slide_reveal").children().addClass("slide_link");
        $("div").not(".slide2").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "url(images/homeslideshow/app-slide12302015.jpg) center no-repeat");

    } else if (index == 2) {
        $(".slide3").addClass("slide_reveal").children().addClass("slide_link");
        $("div").not(".slide3").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");

    } else if (index == 3) {
        $(".slide4").addClass("slide_reveal").children().addClass("slide_link");
        $("div").not(".slide4").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");

    } else if (index == 4) {
        $(".slide5").addClass("slide_reveal").children().addClass("slide_link");
        $("div").not(".slide5").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");    

    } else if (index == 5) {
        $(".slide6").addClass("slide_reveal").children().addClass("slide_link");
        $("div").not(".slide6").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");      

    } else {
        $("div").removeClass("slide_reveal").children().removeClass("slide_link");
        $(".backstretch").css("background", "#000");
    };
});
</script>
