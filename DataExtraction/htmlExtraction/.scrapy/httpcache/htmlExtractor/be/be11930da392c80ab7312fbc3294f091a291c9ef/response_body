				  <param name="SCALE" value="exactfit">
				  <embed src="patented.swf" quality=high pluginspage="http://www.macromedia.com/shockwave/download/index.cgi?P1_Prod_Version=ShockwaveFlash" type="application/x-shockwave-flash" scale="exactfit" width="228" height="32" bgcolor="">
				  </embed> 
				</object></b></font><br>
				<a href="products.htm"><img src="images/combo.jpg" width="210" height="200" border="0"></a> 
			  <form method="POST" name="goform">
                <select name="Products" size="1" onChange="window.location=document.goform.Products.options[document.goform.Products.selectedIndex].value">
				  <option value="products.htm" selected>Select Product</option>
				  <option value="deli_bag.htm">Deli Bag</option>
