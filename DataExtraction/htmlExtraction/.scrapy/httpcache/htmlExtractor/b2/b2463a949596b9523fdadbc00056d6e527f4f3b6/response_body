    <script lang="JavaScript">
        var preloadFlag = false;

        function Init() {
            Preload_Images("images/main_menu/About_Us-over.gif",
                           "images/main_menu/Services-over.gif",
                           "images/main_menu/Products-over.gif",
                           "images/main_menu/Ordering-over.gif",
                           "images/main_menu/Contact_Us-over.gif")
            preloadFlag = true
        }

        function Preload_Images() {
            imgTemp = new Image()

            for (i = 0; i < Preload_Images.arguments.length; i++) {
                imgTemp.src = Preload_Images.arguments[i]
            }
        }

        function Swap(imgObj, imgSrc) {
            if (preloadFlag)
                imgObj.src = imgSrc
        }

</script>

   
    
