<script language="JavaScript">
function image_logo_on_change()
{
  var fedex_logo = "images/fedex.jpg";
  var dhl_logo = "images/dhl.jpg";
  var ups_logo = "images/ups.jpg";
  var tnt_logo = "images/tnt.jpg";
  
  var theform = document.myform;
  var express_company = theform.sltExpressCompany.value;
  
  if (express_company == "fedex")
  {
	document.getElementById("imgLogo").src = fedex_logo;
  }
  else if (express_company == "dhl")
  {
	document.getElementById("imgLogo").src = dhl_logo;
  }
  else if (express_company == "ups")
  {
	document.getElementById("imgLogo").src = ups_logo;
  }
  else if (express_company == "tnt")
  {
	document.getElementById("imgLogo").src = tnt_logo;
  }
}
</script>

