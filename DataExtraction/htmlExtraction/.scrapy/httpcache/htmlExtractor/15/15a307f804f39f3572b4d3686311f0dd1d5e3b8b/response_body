  <script type="text/javascript">
    function sendemail(token) {
      let name      = document.getElementById('name').value;
      let email     = document.getElementById('email').value;
      let telephone = document.getElementById('telephone').value;
      let comment   = document.getElementById('comment').value;
      let sw        = true;
      if(name != "") {
        if(email != "") {
          let tammail = document.getElementById('email').value.split('@');
          if(tammail.length != 2) { sw = false; alert("Email Incorrect."); }
          else {
            let tammail2 = tammail[1].split('.');
            if(tammail2.length != 2) { sw = false; alert("Email Incorrect."); }
          }
          if(sw == true) {
            if(comment != "") {
              new Ajax.Request('https://ndtsupply.com/email/email.php',
                {
                  method:'post',
                  parameters: {name: name,email: email,telephone: telephone,comment: comment },
                  onSuccess: function(transport) {
                    let response = transport.responseText || "no response text";
                    location.href="/";
                  },
                  onFailure: function(){ alert('Something went wrong...'); }
                });
            } else { alert("Comment is mandatory"); }
          }
        } else { alert("Email is mandatory"); }
      } else { alert("Name is mandatory"); }
    }
  </script>
