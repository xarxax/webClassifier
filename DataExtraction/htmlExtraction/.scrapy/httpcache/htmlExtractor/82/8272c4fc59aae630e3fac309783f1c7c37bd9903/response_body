<script>

	

	$(function(){
	
		$( "header .navbar-nav > li.dropdown .accordionEl" ).click(function() {
			var parent = $( this ).parent();
			if (parent.hasClass('active')) {
				$( "header li.dropdown" ).removeClass('active');
			} else {
				$( "header li.dropdown" ).removeClass('active');
				parent.addClass('active');
			}
		});

        $(".swipebox").swipebox({
            hideBarsDelay: 0
        });
            $('#slider').flexslider({
                animation:"fade",
                animationLoop:true,
                slideshow:true,
                controlNav:false,
                prevText:"",
                nextText:"",
                start:function(){
                     $('#mainSlider, span, div').removeClass("hideLoadingSlider");
                }
            });
        
         $( "header .dropdown-toggle" ).click(function() {
			window.location = $(this).attr('href');
		});
        if (!isBrowserMobile()) {	       
	        $( "header .dropdown-toggle" ).mouseenter(function() {
	        	if ($(window).width() >= 1024)
					$(this).parents('li.dropdown').find( ".dropdown-menu" ).show();
			});
			$( "header .dropdown-toggle" ).mouseleave(function() {
				if ($(window).width() >= 1024)
					$(this).parents('li.dropdown').find( ".dropdown-menu" ).hide();
			});
			$( "header .dropdown-menu" ).mouseleave(function() {
				if ($(window).width() >= 1024) {
					$(this).parents('li.dropdown').removeClass('open');
					$(this).hide();
				}
			});
			$( "header .dropdown-menu" ).mouseenter(function() {
				if ($(window).width() >= 1024) {
					$(this).parents('li.dropdown').addClass('open');
					$(this).show();
				}
			});
		}
		
		 $('header a.linkSearch').click(function() {
		 	if ($('form.formSearch').hasClass('show')) {
		 		$('form.formSearch').removeClass('show');
		 	} else {
				$('form.formSearch').addClass('show');
			}
		});
		
		var showLeftPush = document.getElementById( 'showLeftPush' ),
		siteLogo = document.getElementById( 'site-logo' ),
		body = document.body;
		$( "#showLeftPush" ).click(function() {
			classie.toggle( this, 'active' );
			classie.toggle( body, 'cbp-spmenu-push-toright' );
			classie.toggle( siteLogo, 'hidden' );
			classie.toggle( menuLeft, 'cbp-spmenu-open' );
			if ($('header .navbar-collapse').hasClass('in')) {
				$('header .navbar-collapse').removeClass('in');
				$('header .navbar-collapse').css('height', 1);
			}
		});
		
		
		$( window ).resize(function() {
    		resetMenu();
    	});
    	$(window).on("orientationchange",function(){
			resetMenu();
		});
		
    })

	function resetMenu() {
		if ($( window ).width() > 768) {
			$( "#menuLeft" ).removeClass( 'cbp-spmenu-open' );
			$( "#showLeftPush" ).removeClass( 'active' );
			$( "body" ).removeClass( 'cbp-spmenu-push-toright' );
			$( "#site-logo" ).removeClass( 'hidden' );
		}
	}
	
	function isBrowserMobile() {
		var mobile = (/android|iphone|ipad|ipod|blackberry|mini|iemobile|opera mobi|opera mobile|palmos|webos|googlebot-mobile|windows phone|windows\sce|palm/i.test(navigator.userAgent.toLowerCase()));
	    return mobile;
	}

	function getInternetExplorerVersion() {
		var rv = -1; // Return value assumes failure.
		if (navigator.appName == 'Microsoft Internet Explorer'){
			 var ua = navigator.userAgent;
			 var re  = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
			 if (re.exec(ua) != null)
			   rv = parseFloat( RegExp.$1 );
		}
		return rv;
	}
	
	function isInternetExplorer() {
		var browserName=navigator.appName; 
		if (browserName=="Microsoft Internet Explorer")
			return true;
		else
			return false;
	}
	
	var ie8 = false;
	if (isInternetExplorer() && getInternetExplorerVersion() == 8) {
		ie8 = true;
	}
		
</script>	
