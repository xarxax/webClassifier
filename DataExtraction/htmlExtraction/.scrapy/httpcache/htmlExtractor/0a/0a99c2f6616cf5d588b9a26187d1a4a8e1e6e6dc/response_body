<script>
$(function(){
	
	$('.hover-thumb').mouseover(function(){
		var hoverimg = $(this).attr('data-hoversrc');
		var thumbimg = $(this).attr('src');
		
		$(this).attr('src', function(i,e){
			return e.replace(thumbimg, hoverimg);
		});
		$(this).mouseleave(function(){
			$(this).attr('src', function(i,e){
				return e.replace(hoverimg, thumbimg);
			});
		});
	});
	
	/*$("#sub-nav > li.show-cats").mouseenter(function(e){if($(this).hasClass('active')){}else{
			$(this).children("ul").delay(0).slideDown(200);
			$(this).stop();
			$(this).mouseleave(function(){$(this).children("ul").delay(20000).slideUp(200);});
		}
  	});
	$('.hide-this').click(function(){$(this).next('ul').slideUp(350);});*/
});
</script>



