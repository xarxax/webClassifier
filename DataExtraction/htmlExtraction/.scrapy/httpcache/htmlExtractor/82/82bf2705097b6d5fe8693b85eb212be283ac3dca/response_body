function toggleClamShellMenu(objectID) {
	if (isAll || isID) {
	   domStyle = findDOM(objectID,1);
	   if (domStyle.display == 'block') domStyle.display='none';
	   else domStyle.display='block';
	}
	else {
	   destination = 'upgradebrowser.html';
	   self.location = destination;
	}

