<script>
	$("a[id|='video']").click(function() {
		var targetID = $(this).attr("id").replace(/[^0-9]+/g, ""); 
		$("div[id^='videobox-0']").hide();
		$("div#videobox-" + targetID).show();
		$("a[id^='video-'][id!='video-" + targetID + "-th']").removeClass("active");
		$("a#video-" + targetID + "-th").addClass("active");
		$("h3[id^='video-category'][id!='video-" + targetID + " h3']").removeClass("active");
		$("a#video-" + targetID + " h3").addClass("active");
		$("iframe[id^='player'][id!='player" + targetID + "']").each(function() { 
			$(this).attr("src", $(this).attr("src").replace("?autoplay=1", "") ); 
		}); 
	});	
 
	
</script>



