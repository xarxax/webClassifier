<script>
    function selectProduct(val) {
      window.location = val;
    }
    var xhr;
    function auto_sudgest(){

    var ctxt= jQuery('.product_search').val();
       jQuery('.suggesstion-box').hide();
       jQuery('.suggesstion-box li').remove();
    jQuery('.header_search').addClass("loading");
         var params = {"ctxt":ctxt,action:"get_sudgets"}
                 if(xhr){
                    xhr.abort();
                }
        		xhr = jQuery.post("https://www.venturemedical.com/wp-admin/admin-ajax.php",params,function(data){


                      if(data){
                       jQuery('.suggesstion-box').append(data);
                        jQuery('.suggesstion-box').show();
                        jQuery('.header_search').removeClass("loading");

                       }else{
                        jQuery('.header_search').removeClass("loading");
                        jQuery('.suggesstion-box').hide();
                       }
                });
    }
    jQuery(document).ready(function($){
    var count = 0;
      jQuery(".product_search").keyup(function(){
                var keyword = jQuery(this).val();
                if(keyword.length >= 5  && count == 0){
                    jQuery('.suggesstion-box').html('');
                    auto_sudgest();
               }else{
                jQuery('.suggesstion-box').hide();
               }
    	});
       $('body').not('.suggesstion-box').click(function(){
            $(".suggesstion-box").hide();
        });
    })
</script>

