						<script>
			
		var excludedbrowsers = "";
		var previewid = "";	
		var modalcolor = "#FFFFFF";
		var modalopacity = 0.5;
		var fadeoutduration = .5;
		var fadeinduration = .5;
		var modalclickclose = 0;
		var loadpopupurl = "https://www.narescue.com/promotionalpopup/index/loadpopup/";
