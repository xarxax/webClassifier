    function loadMenu(id){
        $.ajax({
            url:"https://accucarecanada.com/home/menu",
            method:"POST",
            cache: true,
            dataType : 'html',
            data:{id:id},
            success:function(resp){
                $('#custom-menu').html(resp);
            }
        });
    }
    
