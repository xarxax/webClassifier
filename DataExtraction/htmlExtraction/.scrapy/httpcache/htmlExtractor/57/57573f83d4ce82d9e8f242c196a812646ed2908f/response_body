    <script type="text/javascript">
        $(function(){
            $('.btn').click(function(e){
                e.preventDefault();
                var country_code = $(this).attr('id');
                $.cookie('country_code', country_code, {
                    domain: '.cenetwork.com',
                    expires: 30
                });
                window.location.replace('https://'+country_code+website);
            });
        });

    </script>
