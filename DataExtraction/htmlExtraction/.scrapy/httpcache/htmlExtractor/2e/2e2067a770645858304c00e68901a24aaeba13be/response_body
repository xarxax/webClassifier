			<button id = "seeMoreButton2" class = "see-more" onclick="viewMoreOnSale();">
			SEE MORE
		</button>
	

	<script>
	var more;
	var count = 0;
	
		function viewMoreOnSale()
		{	
			if(!more)
			{
				count = 0;
				var temp = document.getElementById("blockSpecialsHidden");
				
				if(temp)
				{
					while(temp.id == "blockSpecialsHidden")
					{
						temp.id = "blockSpecialsShown";
						if(document.getElementById("blockSpecialsHidden"))
							temp = document.getElementById("blockSpecialsHidden");
							
						count++;
					}
				}

				document.getElementById("seeMoreButton2").innerHTML = "SEE LESS";
				
				more = true;
			}
			else
			{
				var temp = document.getElementById("blockSpecialsShown");
				var i = 0;
				
				if(temp)
				{
					while(i < count)
					{
						temp.id = "blockSpecialsHidden";
						if(document.getElementById("blockSpecialsShown"))
							temp = document.getElementById("blockSpecialsShown");
							
						i++;		
					}
				}
				
				document.getElementById("seeMoreButton2").innerHTML = "SEE MORE";
				
				more = false;
			}
		}	
	</script>


