<script type="text/javascript" src="http://www.tradeeastspices.com/wp-content/themes/TradeEastSpices/js/ui/jquery-ui-1.8.16.custom.min.js"></script>
<script type="text/javascript">
	var sub_on = [false, false, false, false, false, false, false, false, false, false, false, false];
	
	function validateLen (fld) {

	    if (fld.value.length == 0 || fld.value.indexOf(" *") > 0) {
//	    	fld.value = "* Required";
	    	return true;
	    }
	    return false;
	}
	
	function validatePhone(fld) {
		var alph = /^[0-9]{1,100}$/;
		
		if ((err = validateLen (fld))) {
			return true;
		} else if (!fld.value.match(alph)) {
//	    	fld.value = "* May contain only numbers";
	    	return true;
	    }
	    return false;
	}


	function validateEmail(fld) {

   		var tfld = fld.value.replace(/^\s+|\s+$/, '');
	    var emailFilter = /^[^@]+@[^@.]+\.[^@]*\w\w$/ ;
	    var illegalChars= /[\(\)\<\>\,\;\:\\\"\[\]]/ ;
    
		if ((err = validateLen (fld))) {
			return true;
	    } else if (!emailFilter.test(tfld)) {
//	    	fld.value = "* Invalid email address format";
	    	return true;
	    } else if (fld.value.match(illegalChars)) {
//	    	fld.value = "* Contain invalid characters";
	    	return true;
	    }
	    return false;
	}

	function validateDrop (fld) {
	    if (fld.selectedIndex == 0) {
	    	return true;
	    }
	    return false;
	}


	function validateRadio (fld) {
		for (i = 0; i < fld.length; i++) {
			if (fld[i].checked) return false;
		}
		return true;
	}
	
	
	

	function sub_on_off (id) {
		sub_on[id] = !sub_on[id];
		if (sub_on[id]) {
			document.getElementById('sub_span_' + id).className = 'sub_on';
			document.getElementById('sub_ul_' + id).className = 'sub_on';			
		} else {
			document.getElementById('sub_span_' + id).className = 'sub_off';
			document.getElementById('sub_ul_' + id).className = 'sub_off';			
		}
	}

	
	
	
	function adjust_height() {
		//if ( is_post('754') ) {

			h1 = document.getElementById('content').clientHeight;
			e = document.getElementById('side-menu');
//			r = document.getElementById('page').getElementsByClassName('Recipes');
		
		//	if (r == null) {
				if (e == null) e = document.getElementById('side-bar')
				if (e != null) {
					h2 = 610;
					if (h1 < h2) h1 = h2;
					e.style.height = (h1 - 60) + "px";
				}
		//	}
		// }
	}
	

	function on_off (on, ev, txt) {
		if (on && ev.value == txt)
			ev.value = '';
		else if (!on && ev.value == '')
			ev.value = txt;
	}
	
	if (location.hash != '') location.hash = '';

	$(document).ready(function(){

		$('.signup-modal').dialog({autoOpen: false, modal: true, width: 500});

		$('.signup-btn').click(function(e)
		{
			e.preventDefault();
			$('.signup-modal').dialog('open');	
		});
	});	
</script>


