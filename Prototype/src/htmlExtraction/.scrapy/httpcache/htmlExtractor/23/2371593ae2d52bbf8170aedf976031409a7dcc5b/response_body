<script type="text/javascript">
    var timer;

    function lookup(dado){
        if(dado.length <= 1){
            setTimeout("$('#suggestions').slideUp();", 200);
            setTimeout("$('#closeBusca').fadeOut('slow');", 200);
        }else{
            clearInterval(timer);

            timer = setTimeout(function(){
                $.post(
                    "rpc.php",
                    {
                        busca: ""+dado+""
                        // page: "/site/pt/index.php"
                    },
                    function(data){
                        if(data.length > 0){
                            $('#suggestions').show();
                            $('#closeBusca').fadeIn('slow');
                            $('#autoSuggestionsList').html(data);
                        }
                    }
                );
            }, 1000);
        }
    }

    function fill(thisValue){
        $('#Busca').val(thisValue);
        setTimeout("$('#suggestions').slideUp();", 200);
        setTimeout("$('#closeBusca').fadeOut('slow');", 200);
    }

    function alturaFooter(){
        var hFooter = $("#footer").height();
        var hFooter = hFooter + 10;
        var pad = $("#main, #mainIn").css({ 'padding-bottom': ''+hFooter+'px' });
    }

    alturaFooter();
    $(window).resize(function(){
        alturaFooter();
    });

    var buscas = {
            getCidades:function(id){
                $.ajax({
                    type:"POST",
                    url:"cidades.php",
                    data:"id="+id,
                    dataType:"html",
                    success:function(dados){
                        $("#cidade").html($.trim(dados));
                    },
                    error:function(dados){
                        alert("Erro:"+dados);
                    }
                });
            },

            getRepresentantes:function(cidade,estado,tipo){
                $.ajax({
                    type:"POST",
                    url:"representantes_assistencias.php",
                    data:{
                        "cidade":cidade,
                        "estado":estado,
                        "tipo":tipo
                    },
                    dataType:"html",
                    success:function(dados){
                        $("#Representantes").html($.trim(dados));
                    }
                });
            },
            getRepresentantesInternacionais:function(pais,tipo){
                $.ajax({
                    type:"POST",
                    url:"representantes_assistencias_int.php",
                    data:{
                        "pais":pais,
                        "tipo":tipo
                    },
                    dataType:"html",
                    success:function(dados){
                        $("#Representantes").html($.trim(dados));
                    }
                });
            }

        };

    $(function(){
        if($("#containerAreaNobre").length){
            $('#containerAreaNobre').nivoSlider({
                controlNav: false
            });
        }

        $("#estado").change(function(){
            buscas.getCidades($(this).val());
            $("#pais").val("");
        });

        $("#cidade").change(function(){
            buscas.getRepresentantes($(this).val(),$("#estado").val(),$(this).attr("rel"));
            $("#pais").val("");
        });
        $("#pais").change(function(){
            buscas.getRepresentantesInternacionais($(this).val(),$(this).attr("rel"));
            buscas.getCidades("");
            $("#cidade, #estado").val("");
        });
    })
</script>
