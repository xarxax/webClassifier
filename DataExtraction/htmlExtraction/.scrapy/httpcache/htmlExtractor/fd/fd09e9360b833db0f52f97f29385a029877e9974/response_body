	 function disableCTRL(e)
	{
		var allow_input_textarea = true;
		var key; isCtrl = false;
		if(window.event)
			{ key = window.event.keyCode;if(window.event.ctrlKey)isCtrl = true; if( (window.event.srcElement.nodeName =='INPUT' || window.event.srcElement.nodeName=='TEXTAREA') && isCtrl && ())
					return true; }
		else
			{ key = e.which; if(e.ctrlKey) isCtrl = true; if( (e.target.nodeName =='INPUT' || e.target.nodeName =='TEXTAREA') && isCtrl && ()) 
					return true;  }
	        
	     if(isCtrl && ( key == 83 ))
	          return false;
	          else
	          return true;}  
	document.onkeydown= disableCTRL; 
