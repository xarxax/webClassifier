<script>

testimonials = "";

function playtestimonials() {
	next = $(".testimonial-controls span").index($(".testimonial-controls span.active")) + 1;
	if (next == $(".testimonial-controls span").length) {
		next = 0;
	}
	$(".testimonial-controls span").eq(next).click();
	testimonials = setTimeout("playtestimonials();",5000);
}


$(document).ready(function() {


     $(".capabilities-icons a").hover(function(e) {
          $(this).children("img").addClass("animated flip");
     }, function(e) {
          $(this).children("img").removeClass("animated flip")
     });

     if ($(".testimonial-controls span").length > 1) {
		$(".testimonial-controls span").click(function() {
			if (testimonials) clearTimeout(testimonials);
			$(".testimonial-controls span").removeClass("active");
			clicked = $(".testimonial-controls span").index(this);
			$(".testimonial-controls span").eq(clicked).addClass("active");
			$(".testimonial:visible").fadeOut(500,function() {
				$(this).hide();
				$(".testimonial").eq(clicked).fadeIn(500,function() {
					$(this).show();
				});
			});
		});
		testimonials = setTimeout("playtestimonials();",5000);
	}

});
</script>

