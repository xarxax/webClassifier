        function myCallBack() {
            var recaptcha1;
            var recaptcha2;
            recaptcha1 = grecaptcha.render('recaptcha1', {
                'sitekey': '6LdSEgcUAAAAANgETRN5kTqpbnDIWNqVBNYwlBsG', //Replace this with your Site key
                'theme': 'light'
            });

            recaptcha2 = grecaptcha.render('recaptcha2', {
                'sitekey': '6LdSEgcUAAAAANgETRN5kTqpbnDIWNqVBNYwlBsG', //Replace this with your Site key
                'theme': 'light'
            });
        };



        function SearchKeyword() {
            var keyword = document.getElementById("Keyword");
            if ((keyword.value.length < 2) || (keyword.value == 'Search Parts & Accessories')) {
                alert('Please, enter a keyword (2 characters or more) to search.');
                return false;
            } else {
                var url = "/Items/Search" + "?keywrd=" + keyword.value;
                window.location = url;
                return false;
                // onclick="return CheckKeyword();"
            }
        }
    </script>
