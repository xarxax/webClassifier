		<script type="text/javascript" src="//maps.google.com/maps/api/js?key=AIzaSyDH42b6AGy-p07OwSBl8HbiuGeO3fTzKd0"></script>
		<script type="text/javascript" src="/events/map/downloadxml.js"></script>
		<script type="text/javascript">
			//<![CDATA[
			// global "map" variable
			var map = null;
			
			// A function to create the marker and set up the event window function
			function createMarker(latlng, name, id, dates, loc, status, type) {
				var marker = new google.maps.Marker({
					position: latlng,
					map: map,
					zIndex: Math.round(latlng.lat()*-100000)<<5
				});
				
				if (type == 'Anime') {
					name = '<i class="fa fa-eye"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Comic') {
					name = '<i class="fa fa-comment"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Costume') {
					name = '<i class="fa fa-user-secret"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Fantasy') {
					name = '<i class="fa fa-bolt"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Fetish') {
					name = '<i class="fa fa-glass"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Film') {
					name = '<i class="fa fa-movie"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Furry') {
					name = '<i class="fa fa-paw"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Gaming') {
					name = '<i class="fa fa-map"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Horror') {
					name = '<i class="fa fa-tint"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'SciFi') {
					name = '<i class="fa fa-rocket"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Steampunk') {
					name = '<i class="fa fa-cogs"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'Toys') {
					name = '<i class="fa fa-child"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'TV') {
					name = '<i class="fa fa-television"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else if (type == 'VGames') {
					name = '<i class="fa fa-gamepad"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				} else {
					name = '<i class="fa fa-asterisk"></i>' + ' <a href="/events/info/' + id + '/"><b>' + name + '</b></a><br />' + dates + '<br />' + loc;
				}
				var contentString = name;
				
				google.maps.event.addListener(marker, 'click', function() {
					infowindow.setContent(name); 
					infowindow.open(map,marker);
				});
			}
			
			function initialize() {
				// create the map
				var myOptions = {
					zoom: 3,
					center: new google.maps.LatLng(40,-100),
					mapTypeControl: false,
					navigationControl: true,
					mapTypeId: google.maps.MapTypeId.ROADMAP
				}
				map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
				
				// Try HTML5 geolocation.
				if (navigator.geolocation) {
					navigator.geolocation.getCurrentPosition(function(position) {
						var pos = {
							lat: position.coords.latitude,
							lng: position.coords.longitude
						};
						
						map.setZoom(7);
						map.setCenter(pos);
					}, function() {
						handleLocationError(true, infowindow, map.getCenter());
					});
				} else {
					// Browser doesn't support Geolocation
					handleLocationError(false, infowindow, map.getCenter());
				}
				
				google.maps.event.addListener(map, 'click', function() {
					infowindow.close();
				});
				
				// Read the data from xml
downloadUrl("events/map/ac-maps/map-upcoming.xml", function(doc) {
					var xmlDoc = xmlParse(doc);
					var markers = xmlDoc.documentElement.getElementsByTagName("marker");
					for (var i = 0; i < markers.length; i++) {
						// obtain the attribues of each marker
						var lat = parseFloat(markers[i].getAttribute("lat"));
						var lng = parseFloat(markers[i].getAttribute("lng"));
						var point = new google.maps.LatLng(lat,lng);
						var name = markers[i].getAttribute("name");
						var id = markers[i].getAttribute("id");
						var dates = markers[i].getAttribute("dates");
						var loc = markers[i].getAttribute("loc");
						var status = markers[i].getAttribute("status");
						var type = markers[i].getAttribute("type");
						// create the marker
						var marker = createMarker(point,name,id,dates,loc,status,type);
					}
				});
			}
			var infowindow = new google.maps.InfoWindow ({ 
				size: new google.maps.Size(150,50)
			});
			//]]>
		</script>
