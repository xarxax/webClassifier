<script language="Javascript"> 
 
function ChkTemplate()
   {
     if ( document.search.site.value == "kaufman_collection" )
	{
	  document.search.client.value = "kaufman_frontend_rev3";
	  document.search.proxystylesheet.value = "kaufman_frontend_rev3";
	}
      else if ( document.search.site.value == "library_collection" )
	{
	  document.search.client.value = "library_frontend";
	  document.search.proxystylesheet.value = "library_frontend";
	}
      else
	{
	  document.search.client.value = "default_frontend";
	  document.search.proxystylesheet.value = "default_frontend";
	}
 
     return true;
   }
 
</script> 

