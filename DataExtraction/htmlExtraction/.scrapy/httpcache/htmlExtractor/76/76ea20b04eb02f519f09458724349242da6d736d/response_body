	<script type="text/javascript">
	$(document).ready(function(){
		var er=0;
		var foc ='';
		$("#guestEmail").hide();
		$("#guestEmailCart").hide();
		$('#loginForm').submit(function(e){
			e.preventDefault();
			jQuery("#header_email").html("");
			var email = $("#user_email").val();
			var pass = $("#user_pass").val();
			
			if(CheckValidEmail('user_email')==false)
			{ 
				if(er==0)
				foc = 'user_email';
				er=1;
				jQuery("#header_email").html("Enter valid email");
				
			}
		  
		  if(selectcon('user_email')==false) { 
				if(er==0)
				foc = 'user_email';
				er=1;
			}
			if(selectcon('user_pass')==false) { 
				if(er==0)
				foc = 'user_pass';
				er=1;
			}
			if(er==1)
			{
				jQuery('.'+foc).focus();
				return false;
			}
			
			if(CheckValidEmail('user_email')==true && selectcon('user_pass')==true)
			{
				var data1 = "email="+email+"&pass="+pass;
				$.ajax({
						  type: "POST",
						  url: "login.php",
						  data: data1,
						  success:function(data2){
							if(data2!='error'){
								 if(data2==''){
									location.href='myaccount';
								  }else{								
									location.href=data2;
								  }
							}else{
								$(".message").html("Incorrect Username or Password..Please check whether your account has been activated.");
								
							}  
						  }
				});
			}
		});
	});
	
		function btn_click(id){
				
				window.location.href=id;
				  
			}
			function DropDown(el) {
				this.dd = el;
				this.initEvents();
			}
			DropDown.prototype = {
				initEvents : function() {
					var obj = this;

					obj.dd.on('click', function(event){
						$(this).toggleClass('active');
						event.stopPropagation();
					});	
				}
			}
					
			$(function() {

				var dd = new DropDown( $('#dd') );

				$(document).click(function() {
					
					$('.wrapper-dropdown-2').removeClass('active');
				});

			});
			
		<!-- pop-window image-->
			
			$(function($){
				var addToAll = false;
				var gallery = true;
				var titlePosition = 'inside';
				$(addToAll ? 'img' : 'img.fancybox').each(function(){
					var $this = $(this);
					
					var src = $this.attr('data-big') || $this.attr('src');
					
					var a = $('<a href="#" class="fancybox"></a>').attr('href', src);
					$this.wrap(a);
				});
				if (gallery)
					$('a.fancybox').attr('rel', 'fancyboxgallery');
				$('a.fancybox').fancybox({
					titlePosition: titlePosition
				});
				
				/* image 2 popup */
				var addToAll1 = false;
				var gallery_img = true;
				var titlePosition1 = 'inside';
				$(addToAll1 ? 'img' : 'img.fancybox_back').each(function(){
					var $this = $(this);
					var src_back = $this.attr('data-big') || $this.attr('src');
					var a = $('<a href="#" class="fancybox_back"></a>').attr('href', src_back);
					$this.wrap(a);
				});
				if (gallery_img)
					$('a.fancybox_back').attr('rel', 'fancyboxgallery');
				$('a.fancybox_back').fancybox({
					titlePosition1: titlePosition1
				});
				
				
			});
   
	<!-- pop-window image-->
	
	
	</script>
	<!-- pop-window -->

