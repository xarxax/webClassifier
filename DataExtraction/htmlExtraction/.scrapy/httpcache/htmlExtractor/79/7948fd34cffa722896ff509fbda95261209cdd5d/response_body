                              <script>
function empty_state_city()
{
	var drop	=	'<select class="select-city" size="1" name="data[Search][state]">';
    var countryName	=	$('#VenueCountry').val();
	if($.trim(countryName)	==	'United States')
	{
		drop	+=	$("#EEUU_states").html();
		drop	+=	'</select>'
	}
	else if($.trim(countryName)	==	'Canada')
	{
		//
		drop	+=	$("#CANADA_states").html();
		drop	+=	'</select>'
	}
	else
	{
		drop	=	'<input type="text" style="width:122px;" name="data[Search][city]" class="textinput venueSearchCity" id="data[Search][city]" value="">';
	}
	
	$("#states_div").html(drop);
/*    $('input[name="data[Search][city]"]').attr('value','');
    $('[name="data[Search][state]"]').attr('value','');
    $('select[name="data[Search][city]"]').attr('value','');
    $('#searchConferenceCategoryForm').submit();*/
}
function submit_form_search()
{
    var url='http://www.allvenues.com/venues/index';
    country=$('select[name="data[Search][country]"]').val();
    state=$('[name="data[Search][state]"]').val();
    city=$('input[name="data[Search][city]"]').val();
    name=$('input[name="data[Search][name]"]').val();

    if(country != ''){
        url=url+'/country:'+country;
    }
    if(state != '' && typeof  state != 'undefined'){
        url=url+'/state:'+state;
    }
    if(city != '' && typeof  state != 'undefined'){
        url=url+'/city:'+city;
    }
    $('#searchConferenceCategoryForm').attr('action',url);
}
</script>
