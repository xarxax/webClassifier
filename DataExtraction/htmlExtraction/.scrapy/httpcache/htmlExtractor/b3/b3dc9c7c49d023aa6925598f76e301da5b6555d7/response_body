			}
		} else {
			for(i=0; i< document.getElementsByTagName("li").length; i++) {
				if(document.getElementsByTagName("li")[i].className.indexOf("lockOff") !== -1) {
					if(window.location.href.indexOf("private-myarc/fraud/alerts/") !== -1 && document.getElementsByTagName("li")[i].innerHTML.indexOf("fraud-alerts-incidents.jsp") !== -1) {
						var oldInnerHTML = document.getElementsByTagName("li")[i].innerHTML;
						var oldInnerIdx1 = oldInnerHTML.indexOf("arrowOff");
						var newInnerHTML = oldInnerHTML.substring(0,oldInnerIdx1) + "arrowOff leftnavOn" + oldInnerHTML.substring(oldInnerIdx1+8,oldInnerHTML.length);
						document.getElementsByTagName("li")[i].innerHTML = newInnerHTML;
					}
				}
			}
		}
	//-->
</script>
