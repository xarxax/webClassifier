	MrSite.API = {
		ajax: function (urlService, callBackFunction) {
			$.ajax({
				type: "GET",
				url: urlService + "&seed=" + Math.random(),
				dataType: "jsonp",
				jsonp: 'jsonp_callback',
				success: function (data) {
					callBackFunction(data);
				}
			});
		},
		Shopping: {
			getCart: function (callBackFunction) {
				var url = MrSite.Site.apiUrl + "webservices/Shopping.ashx?action=getCart"
				MrSite.API.ajax(url, callBackFunction);
			}
		}
	};
