<script>
function select_page(pagenumber) {
    document.getElementById("Page").value = pagenumber;
    document.search_results.action = "engine_search.cfm";
    document.search_results.submit();
}
function engine_details(make,model,listingid) {
var cleanmake = make.replace(/([^a-z0-9]+)/gi, '_');
var cleanmodel = model.replace(/([^a-z0-9]+)/gi, '_');
    document.search_results.action = "engines/"+cleanmake+"/"+cleanmodel+"/"+listingid;
    document.search_results.submit();
}
</script>
