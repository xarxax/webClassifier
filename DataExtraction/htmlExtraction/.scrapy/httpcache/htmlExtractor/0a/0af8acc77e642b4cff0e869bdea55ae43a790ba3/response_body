	$(document).ready(function(){
		$(".mobile-hover").click(function(e){
			
			var id = $(this).attr("data-id");
			
			$('.mobile-hover-show.mobile-hover-expand[data-id!='+id+']').removeClass('mobile-hover-expand');
			
			
			$('.mobile-hover-show[data-id='+id+']').toggleClass("mobile-hover-expand");
			
			return false;
		});
