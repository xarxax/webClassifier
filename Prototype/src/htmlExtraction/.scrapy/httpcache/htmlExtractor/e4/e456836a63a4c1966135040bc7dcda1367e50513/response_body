<script>
 	var start = 0;
	var product_size = 0;
	var prev_loaded = 0;
	var next_loaded = 0;


    var t = jQuery(".pScroll ul > li").size();  
    var c = jQuery(".pScroll ul").children();
    var s = c.size();	
  
    var counter = 0;
    var cl = 0;
 	
    //scroll function
    function moveGallery(dir){
			
			
			c = jQuery(".pScroll ul").children();
			
			//alert(jQuery(".pScroll ul").width())
			jump = 7;
			dw = 137 * jump;
			
			
			if(	jQuery(".pScroll ul").width() != (Number(dw)*t + Number(dw))){
  			jQuery(".pScroll ul").width(Number(dw)*t*10);	
			}
			
	  
      if(cl==0){
  		
        cl=1;
  		
        var p = jQuery(".pScroll ul").position();
	//alert(p.left);	
        if(dir==1){ //previous 
					if(p.left==0){
          			for(i=c.length;i>c.length-jump;i--){
							jQuery(c[i-1]).clone(true).prependTo(jQuery(".pScroll ul"));  
							jQuery(c[i-1]).remove();
						}
         		jQuery(".pScroll ul").css('left', p.left-Number(dw));
          }
          jQuery(".pScroll ul").animate({"left": "+="+Number(dw)+"px"}, "slow", function() {cl=0;jQuery(".pScroll ul").css('left', 0);});
					
					if(start == 0){
						start = product_size;
					}
					
					start -= jump;
					if(start < prev_loaded && next_loaded < start){
						prev_loaded = start;						
					}

        }else if(dir==-1){ //next
					if(p.left<0){
  					for(i=0;i<jump;i++){
							jQuery(c[i]).clone(true).appendTo(jQuery(".pScroll ul"));  
							jQuery(c[i]).remove();
						}
  
            jQuery(".pScroll ul").css('left', p.left+Number(dw));
  
          }
		
          jQuery(".pScroll ul").animate({"left": "-="+Number(dw)+"px"}, "slow", function() {cl=0;});
		
					
					start += jump;
					if(start == product_size){ start = 0;}

					if(next_loaded < start && start < prev_loaded){
						next_loaded += jump;
					}
					
        }
  
        c = jQuery(".pScroll ul").children();
  
      }
  
    }
    //setMiddle(1);
    jQuery(".prv").click(function(){
      moveGallery(1)
    });
    jQuery(".nxt").click(function(){
      moveGallery(-1)
    });
	
	
	ajaxCall('/cart/order/call/process/getProductGroups/',null, 'iniScroll();');
	
	var ajaxLoad = 0;
	function setProductSet(p){
	  ajaxCall('/cart/order/call/process/getProductBySet/product/'+p,null, 'ajaxLoad = 0;');
	}
	
	function iniScroll(){
		t = jQuery(".pScroll ul > li").size();  
		c = jQuery(".pScroll ul").children();
		s = c.size();
	
		jQuery(".pScroll ul li").click(function(){
			if(ajaxLoad == 0){
				jQuery(".pScroll ul li.sel").removeClass('sel');
				jQuery(this).addClass('sel');	  
				jQuery(".pSet ul").html("<li><img style='padding-top:50px;' src='/images/spinningLoader2.gif' /></li>")
				ajaxLoad = 1;
				setProductSet(jQuery(".pScroll ul li.sel a").html());
			}
		});
		
		jQuery(".pScroll ul li.sel").removeClass('sel');
		jQuery(c[0]).addClass('sel');
		setProductSet("Business Cards");
	}
	
		
	var dw = jQuery(".pScroll ul").width();	
  </script>  <!-- FOOTER -->
