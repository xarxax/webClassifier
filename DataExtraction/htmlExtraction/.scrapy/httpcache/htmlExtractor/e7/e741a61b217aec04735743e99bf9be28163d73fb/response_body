<script type="text/javascript">
jQuery('.toggle').click(function(e) {
    e.preventDefault();
  
    var $this = jQuery(this);
  
    if ($this.next().hasClass('show1')) {
        // alert('show');
        $this.next().removeClass('show1');
        //$this.next().hide();
        $this.next().slideToggle(350);
    } else {
        $this.parent().parent().find('li .inner').removeClass('show1');
        // $this.parent().parent().find('li .inner').slideUp(350);
        $this.next().toggleClass('show1');
        $this.next().slideToggle(350);
    }

});
jQuery(".industry-dropdown a.dropdown-link").click(function() {
        console.log('clicked');
        // Hide all item spans
        jQuery(".industry-dropdown .dropdown-container").each(function(){
            if(!jQuery(this).hasClass('show1'))
            {
                jQuery(this).slideUp(350);
            }else{
                //$(this).next().slideDown(350);
            }
        });
        });
</script>
<script type="text/javascript">

    var windowScroll_t;

    jQuery(window).scroll(function(){

        clearTimeout(windowScroll_t);

        windowScroll_t = setTimeout(function(){

            if(jQuery(this).scrollTop() > 100){

                jQuery('#totop').fadeIn();

            }else{

                jQuery('#totop').fadeOut();

            }

        }, 500);

    });

    jQuery('#totop').click(function(){

        jQuery('html, body').animate({scrollTop: 0}, 600);

        return false;

    });

    jQuery(function($){

        $(".cms-index-index .footer-container.fixed-position .footer-top,.cms-index-index .footer-container.fixed-position .footer-middle").remove();

    });

</script>
