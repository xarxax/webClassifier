<script type="text/javascript">
	jQuery(".tpl-social").each(function( i, obj ) {

		var twitter = jQuery(this).find("a[title='twitter']");
		var facebook = jQuery(this).find("a[title='facebook']");
		var linkedin = jQuery(this).find("a[title='linkedin']");
		if((twitter.length) && (facebook.length)  && (linkedin.length) ){
			 twitter.insertAfter( linkedin);
			 facebook.insertAfter( twitter );
		}
		if((twitter.length) && (linkedin.length) ){
			 twitter.insertAfter( linkedin);
		}
	});

	jQuery('#menu-toggle').on('click', function(){
	    jQuery('#site-navigation').slideToggle('slow');
	});

	jQuery(window).scroll(function() {    
	    var scroll = jQuery(window).scrollTop();

	    if (scroll >= 500) {
	        jQuery(".main-header").addClass("sticky");
	    } else {
	        jQuery(".main-header").removeClass("sticky");
	    }
	});

	jQuery(window).bind("load", function() {
