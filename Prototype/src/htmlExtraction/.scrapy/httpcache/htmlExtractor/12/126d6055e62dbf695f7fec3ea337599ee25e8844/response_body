<script type="text/javascript">
 var counterref = document.referrer;
if (counterref.length <3){ counterref = "noref";}
var haveholder = false;
var id = "noid";
var dir="help";
var holderparam= ""; 
var regex=/^([a-zA-Z0-9_\.]+)$/;
var blurbvisible = true;
if(regex.test(holderparam) && holderparam != ""){haveholder=true;}
	var childpgmsg ="hidescrollbarandsendsize";	
window.onload=function() {
	// The following block is to fix the home page height problem in Google's cached version
	if(document.body.childNodes.length <= 2) {
		tps_util.addClass(document.documentElement,"kill-overflow");
	}

	window.name = "";
	var mopt = document.getElementById('moboptions');
	var fheader = document.getElementById('mobfixedheader');
	var sbobj = document.getElementById('scrollable');
	var mbobj = document.getElementById('mainbubble');
	var ifrmholder = document.getElementById('holder');
	var mainsec = document.getElementById('mainsection');
	var fmparamobj = {};
	var csbparamobj = {};
	fmparamobj.scollable = sbobj;
	fmparamobj.mainbubble = mbobj;
	fmparamobj.ifrm = ifrmholder;	
	fmparamobj.ifrmwrapper = document.getElementById('iframewrapper');
	fmparamobj.mainsec = mainsec;
	fmparamobj.loadedcallback = childPgLoaded;
	csbparamobj.scrollable = sbobj;
	csbparamobj.mainscreen = document.getElementById('mainscreen');
	csbparamobj.niosmainscreen = "non-ios-mainscreen";
	csbparamobj.scroller = document.getElementById('scroller');
	csbparamobj.scrollpane = document.getElementById('scrollpane');
	csbparamobj.scrolloverlay = document.getElementById('scrolloverlay');
	csbparamobj.msscroller = 'ms-scroller';
	csbparamobj.msscrollpane = 'ms-scroll-pane';
	csbparamobj.iosscrollable = 'ios-scrollable';
	csbparamobj.iosscrollpane = 'ios-scroll-pane';
	csbparamobj.niosscrollable = 'non-ios-scrollable';
	csbparamobj.visibility = 'set-visibility';
	
	tps_customscrollbar.init(csbparamobj);	
	tps_frmmsg.outer.init(fmparamobj);
		initNavHeader();
		initUtilDialog();
	if(haveholder)
	{	
		showblurb('iframe');
		initHolder();
		tps_frmmsg.outer.hideScrollbar(ifrmholder,childpgmsg);
	}
	document.getElementById("blurbmenuitem").addEventListener("click",function(e) {
		ifrmholder.src="/help/refreshhome.htm";
		tps_navigate.header.hideOptions(sbobj);
		e.preventDefault();
		e.stopPropagation();
		return false;
	});	

	document.getElementById("popup").addEventListener("load",function() {
	var pbox = document.getElementById('popupbox');
    var ptext = document.getElementById('popuptext');
    var spopup = document.getElementById('showpopup');
    var poverlay = document.getElementById('popupoverlay');
    var clspopup = document.getElementById('closepopup');
    var mainsec = document.getElementById('mainsection');
    var fixheader = document.getElementById('mobfixedheader');
    var popparam = {};
    popparam.popupfrm = this;
    popparam.popupbox = pbox;
    popparam.showpopup = spopup;
    popparam.popuptext = ptext;
    popparam.popupoverlay = poverlay;
    popparam.closepopup = clspopup;
    popparam.pospopup = "popup-pos--fixedsize";
    popparam.fixpopup = "popup--fixedsize";
    popparam.hideprintbtn = "dummypopup-print--hide";
    popparam.visibility = "set-visibility";
    popparam.mainsec = mainsec;
    popparam.fixheader = fixheader;
    popparam.ovrlapsbcls = "popup-overlap-sb";
    tps_navigate.popup.init(popparam);
	pbox.focus();
	});

 document.getElementById("closepopup").addEventListener("click",function() {
 	tps_navigate.popup.hide();
    tps_navigate.popup.isVisible= false;
    tps_util.addClass(document.getElementById('showpopup'),"popup-print--hide");
    tps_util.removeClass(document.getElementById('mobfixedheader'),'popup-overlap-sb');
  },false);
  document.getElementById("popupoverlay").addEventListener("click",function() {
  	tps_navigate.popup.hide();
    tps_navigate.popup.isVisible= false;
    tps_util.addClass(document.getElementById('showpopup'),"popup-print--hide");
    tps_util.removeClass(document.getElementById('mobfixedheader'),'popup-overlap-sb'); 
  },false);
document.addEventListener("keydown",function(event) {tps_navigate.popup.handleKey(event);},false);
  document.getElementById('dialogform').addEventListener("keydown",function(event) {
    var keycode = event.keyCode; 
    if (keycode == 27) { 
      event.stopPropagation();
      event.preventDefault();
      tps_util.closeDialog(false);
    }
    tps_util.checkDialogKeys(event);
  },false);
	var arrExpandList =['services','aboutus'];
	for(var i=0;i<arrExpandList.length;i++) {
		var explist = document.getElementById(arrExpandList[i]);
		explist.addEventListener("transitionend",function(e) {
	    	tps_frmmsg.outer.resizeIframe();
		},false);
	}
    if(tps_util.iphoneSafari()) {
        window.addEventListener("scroll",function() {
        	tps_navigate.header.iosFixHeader();
        });
    }
    var opt = document.getElementById('options');
    var callusid = document.getElementById('callus');
    if(tps_util.ipadSafari() || tps_util.iphoneSafari()) {
    	document.getElementById('movetotop').addEventListener("click",function() {
			document.body.scrollTop = document.documentElement.scrollTop = 0;
		});
		window.addEventListener("scroll",function() {
			tps_navigate.showTopBubble(document.body,opt,callusid,'sticky-section');
		});
	} else {
		document.getElementById('movetotop').addEventListener("click",function() {
			sbobj.scrollTop = document.documentElement.scrollTop = 0;
		});
		document.getElementById("scrollable").addEventListener("scroll", function() {
			tps_customscrollbar.scroll(fheader,'mob-header__options--bg');
			tps_navigate.showTopBubble(sbobj,opt,callusid,'sticky-section');
		});
	}
};
function initNavHeader() {
	var navinitobj = {}; 
	navinitobj.uparrow="up-arrow";
	navinitobj.downarrow="down-arrow";
	tps_navigate.init(navinitobj);
	var navheaderobj = {};
	navheaderobj.options = document.getElementById('options');
	navheaderobj.mainsec = document.getElementById('mainsection');
	navheaderobj.moboptions = document.getElementById('moboptions');
	navheaderobj.fixedheader = document.getElementById('mobfixedheader');
	navheaderobj.showoptionscls = 'sidenav--active';
	navheaderobj.hideelecls = 'hide';
	navheaderobj.headerbgcls = 'mob-header__options--bg';
	navheaderobj.headerhide = 'mob-header__options--hide';
	navheaderobj.headeranim = 'mob-header__options--anim';
	tps_navigate.header.init(navheaderobj);
}
function initUtilDialog() {
	var initobj = {};
	initobj.dialogwrapper = document.getElementById('showdialog');
	initobj.dialogbody = document.getElementById('dialogmsg');
	initobj.alertcls = "dialog--alert";
	initobj.confirmcls = "dialog--confirm";
	initobj.visibilitycls = "set-visibility";
	initobj.dialogok = document.getElementById('dialogok');
  	initobj.dialogcancel = document.getElementById('dialogcancel');
	tps_util.initDialog(initobj);
}
function resetScrollbar() {
	tps_frmmsg.outer.ifrmWidth=0;
		tps_frmmsg.outer.ifrmHeight=0;
		document.getElementById('mainbubble').removeAttribute("style"); //remove the width of the main bubble if set
		showblurb('blurb');	
}
function resetHome() {
	// to solve the browser back button issue
	try {
    var ifrm = document.getElementById('holder');
	if(ifrm.contentWindow.document.body.childElementCount==0)
	{			
		resetScrollbar();
		tps_customscrollbar.show();
	}
	}
	catch(err) {}
}
tps_navigate.init = function(navobj) {
  tps_navigate.uparrowcls = navobj.uparrow;
  tps_navigate.downarrowcls = navobj.downarrow;
};
function childPgLoaded() {
	var ifrm = document.getElementById('holder');
	    showblurb('iframe');
        initHolder();
        tps_frmmsg.outer.hideScrollbar(ifrm,childpgmsg);
}
function showblurb(arg)
{
var incont = document.getElementById('innercontent');
var hifrm = document.getElementById('holder');
if (arg == "blurb")
{
	tps_util.removeClass(incont,'hide-element');
	tps_util.removeClass(incont,'hide-flash');	
	tps_util.addClass(hifrm,'hide-iframe');
	document.getElementById('holder').removeAttribute("style");
	setblurbinmenu(true);
	blurbvisible =true;
}
else
{
	tps_util.addClass(incont,'hide-element');
	tps_util.removeClass(hifrm,'hide-iframe');
	setblurbinmenu(false);
	blurbvisible =false;
}
}
function setblurbinmenu(isblurb)
{
if (isblurb)
{
	document.getElementById('mainsection').focus();
var elem = document.getElementById('options').getElementsByTagName('a');
for(var i = 0; i < elem.length; i++)
{
tps_util.removeClass(elem[i], 'navoptions__list--selected');
tps_util.addClass(document.getElementById('blurbmenuitem'), 'navoptions__list--selected');
}
}
else
{
tps_util.removeClass(document.getElementById('blurbmenuitem'), 'navoptions__list--selected');
}
}
function initHolder() {
var obj = document.getElementById("holder");
	if(!blurbvisible) {
	var nli =document.getElementById('navlist');
	var selected = {};
	selected.highlight="navoptions__list--selected";
	tps_navigate.markmenu(obj,nli,selected);
	tps_navigate.header.hideOptions(document.getElementById('scrollable'));
	}
}
