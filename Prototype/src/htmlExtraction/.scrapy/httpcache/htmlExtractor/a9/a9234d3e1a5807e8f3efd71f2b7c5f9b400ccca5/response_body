<script language="javascript">
function handlechange(e)
{
    //alert(e.value);
    if (e.value.toLowerCase().match("@y.com") || e.value.match("test") || e.value.match("brad") ) {		    
	var lst = document.getElementById("pnlcode1");
	lst.style.display = "block";
	lst = document.getElementById("pnlcode2");
	lst.style.display = "block";
    }	
    else {
   	var lst = document.getElementById("pnlcode1");
	lst.style.display = "none";
	lst = document.getElementById("pnlcode2");
	lst.style.display = "none";
    }	
}	
function popitup(url) {
	//newwindow=window.open(url,'name','height=300,width=450');
	newwindow=window.open(url, "null",'left=450,top=300,width=550,height=300,titlebar=0,location=0,resizable=0,toolbar=0,status=0,menubar=0,scrollbars=0', false);

	if (window.focus) {newwindow.focus()}
	return false;
} 

</script>

