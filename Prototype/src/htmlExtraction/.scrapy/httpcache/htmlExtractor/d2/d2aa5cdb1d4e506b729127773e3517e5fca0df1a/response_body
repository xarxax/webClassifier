<script language="javascript" type="text/javascript">
    $(".send-AaeForm").click(function() {
        $.ajax({
            url: "/responses/ctc.ashx?_=" + $(".txtCaptcha").val(),
            success: function(d) {
                if (d == "") {
                    var btn = document.getElementById('ctl00_UcAskAnExpert1_btnhSubmit');
                    if (btn) { btn.click(); }
                }
                else {
                    $(".send-AaeForm-msg").css("color", "red");
                    $(".send-AaeForm-msg").html("<br />The text did not match the image.");
                    $(".txtCaptcha").val("");
                    $(".send-AaeForm-img").attr("src", "/images/c.ashx?v=" + new Date());
                }
            }
        });

        return false;
    });
</script>
