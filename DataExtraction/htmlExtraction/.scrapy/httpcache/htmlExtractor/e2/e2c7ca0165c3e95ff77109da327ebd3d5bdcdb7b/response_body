<script>
var tag = document.createElement('script');
tag.id = 'video-api';
tag.src = 'https://www.youtube.com/player_api';
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

var player;
var player_ready = false;

(function($) {
    $(document).ready(function() {

        $("#voted-best-tasting-burger .click-here, #play_area").on('click',function(){
            ga('send', 'event', 'play-video', 'click', 'Golden Chicken');
            $("#voted-best-tasting-burger .click-here, #play_area").removeClass('show');
            $("#habit_video").addClass('show');

            if(player_ready){
                player.playVideo();
            }
        });

        $('#voted-best-tasting-burger .exit').on('click',function (){
            player.stopVideo();
        });
    });
})(jQuery);

function onYouTubePlayerAPIReady() {
    player = new YT.Player('habit_video', {
        playerVars: {'controls': 1},
        events: {
            'onReady': onPlayerReady
        }
    });
}

function onPlayerReady(){
   player_ready = true; 
}
</script>

    
		


