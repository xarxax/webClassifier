function createRequestObject() {
    var ro;
    var browser = navigator.appName;
    if(browser == 'Microsoft Internet Explorer'){
        ro = new ActiveXObject('Microsoft.XMLHTTP');
    }else{
        ro = new XMLHttpRequest();
    }
    return ro;
}

var http = createRequestObject();

function copy_to_clipboard(text)  {  
	if(window.clipboardData) { window.clipboardData.setData('text',text); }  
	else { var clipboarddiv=document.getElementById('divclipboardswf');  
		if(clipboarddiv==null)  {  
			clipboarddiv=document.createElement('div');  
			clipboarddiv.setAttribute("name", "divclipboardswf");  
			clipboarddiv.setAttribute("id", "divclipboardswf");  
			document.body.appendChild(clipboarddiv);  
		}  
		clipboarddiv.innerHTML='<embed src="clipboard.swf" FlashVars="clipboard='+  
		encodeURIComponent(text)+'" width="0" height="0" type="application/x-shockwave-flash"></embed>';  
	}  
	alert('The text is copied to your clipboard...');  
	return false;  
}
function menusend(who){
	var mylng = document.getElementById('mylang').lang;
	if (who=='b1_t1') { window.location.href='index.php?whosend=list&menuitem=b1_t1&lng='+mylng; }
	else if (who=='b1_t22') { window.location.href='index.php?whosend=list&menuitem=b1_t22&send=ref&lng='+mylng; }	
	else { window.location.href='index.php?whosend=menu&menuitem='+who+'&lng='+mylng; }
}
function gotopage(myurl) {
	window.location.href=myurl;	
}
function gotoback() {
	window.history.back();;
}
function ShowFav() {
	var mycount = document.getElementById('favcount').innerHTML;
	var mylng = document.getElementById('mylang').lang;
	if (mycount != '0' ) {
		gotopage("index.php?whosend=list&send=fav&lng="+mylng);		
	}
}
function photos_scroll(wh) { // 0-no, 1-up 2-down
	if (wh==1) {
		if (document.getElementById('hid2').style.display == 'inline') {
			document.getElementById('hid2').style.display = 'none';
			document.getElementById('moveup').lang = "0";
			document.getElementById('moveup').style.cursor = "default";
			document.getElementById('moveupimg').src = "images/move_no.jpg";
			document.getElementById('movedown').lang = "2";
			document.getElementById('movedown').style.cursor = "pointer";
			document.getElementById('movedownimg').src = "images/move_down.jpg";
			document.getElementById('hid1').style.display = 'inline';		
		}
		else if (document.getElementById('hid3').style.display == 'inline') {
			document.getElementById('hid3').style.display = 'none';
			document.getElementById('moveup').lang = "1";
			document.getElementById('moveup').style.cursor = "pointer";
			document.getElementById('moveupimg').src = "images/move_up.jpg";
			document.getElementById('movedown').lang = "2";
			document.getElementById('movedown').style.cursor = "pointer";
			document.getElementById('movedownimg').src = "images/move_down.jpg";
			document.getElementById('hid2').style.display = 'inline';		
		}		
	}
	else if (wh==2) {
		if (document.getElementById('hid1').style.display == 'inline') {
			document.getElementById('hid1').style.display = 'none';
			document.getElementById('moveup').lang = "1";
			document.getElementById('moveup').style.cursor = "pointer";
			document.getElementById('moveupimg').src = "images/move_up.jpg";
			if (document.getElementById('hid3')==null) {
				document.getElementById('movedown').lang = "0";
				document.getElementById('movedown').style.cursor = "default";
				document.getElementById('movedownimg').src = "images/move_no.jpg";
			}
			else {
				document.getElementById('movedown').lang = "2";
				document.getElementById('movedown').style.cursor = "pointer";
				document.getElementById('movedownimg').src = "images/move_down.jpg";
			}
			document.getElementById('hid2').style.display = 'inline';		
		}
		else if (document.getElementById('hid2').style.display == 'inline') {
			document.getElementById('hid2').style.display = 'none';
			document.getElementById('moveup').lang = "1";
			document.getElementById('moveup').style.cursor = "pointer";
			document.getElementById('moveupimg').src = "images/move_up.jpg";
			document.getElementById('movedown').lang = "0";
			document.getElementById('movedown').style.cursor = "default";
			document.getElementById('movedownimg').src = "images/move_no.jpg";
			document.getElementById('hid3').style.display = 'inline';		
		}		
	}
}
function GoSUBSCRIBE(mess,who,myfurl) {
	var str;
	var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z])?)$/i;
	if (who == 'sub') {  
		str=document.getElementById('youremail').value;
		if (!(filter.test(str))) { 
			document.getElementById('youremail').focus();
			alert(mess); 
  		    return false; 
		}
	}
	else if (who == 'emash') {  
		str=document.getElementById('emash_mail').value;
		if (!(filter.test(str))) { 
			document.getElementById('emash_mail').focus();
			alert(mess); 
  		    return false; 
		}
	}
	else if (who == 'sendym') {
	    var contnm=document.getElementById('Contactname').value;
        var compnm=document.getElementById('Compname').value;
		str=document.getElementById('email').value;

        if (trim(contnm) == '') { 
			document.getElementById('Contactname').focus();
			alert("Incorrect Contact Name!"); 
  		    return false;
		}
        else if (trim(compnm) == '') { 
			document.getElementById('Compname').focus();
			alert("Incorrect Company Name!"); 
  		    return false;
		}
        else if (!(filter.test(str))) { 
			document.getElementById('email').focus();
			alert(mess); 
  		    return false;
		}
        myfurl = '';
        str=document.getElementById('Contactname').value;   // 1
        myfurl = str;
        str=document.getElementById('Compname').value;      // 2
        myfurl = myfurl + ';'+ str;
        str=document.getElementById('email').value;         // 3
        myfurl = myfurl + ';'+ str;  
        str = document.getElementById('Address').value;   // 4
        myfurl = myfurl + ';'+ str;       
        str = document.getElementById('Country').value;   // 5
        myfurl = myfurl + ';'+ str;
 
        str = document.getElementById('phones').value;   // 6
        myfurl = myfurl + ';'+ str;       
        
        str = document.getElementById('MachineName').value;   // 7
        myfurl = myfurl + ';'+ str;
        str = document.getElementById('MachineLoc').value;   // 8
        myfurl = myfurl + ';'+ str;
        
        str = document.getElementById('MachineDescr').value;   // 9
        myfurl = myfurl + ';'+ str;
        
        myfurl = encodeURIComponent(myfurl);

	}
	sendemail(who,myfurl);
}
function trim(newstr) {
	return newstr.replace(/^\s\s*/, '').replace(/\s\s*$/, '');
}
function sendemail(who,myfurl) {
    if (who == 'sendym') {
        myloader('emash_answer');
   	    var mylng = document.getElementById('mylang').lang;
        http.open('post', 'sendmail.php?who='+who+'&myfurl='+myfurl+'&lng='+mylng);    
        http.onreadystatechange = handleResponse;    
        http.send(null);
    }
    else {
    	var email='';
    	var emess;
    	var ephone;
    	var deal;
    	var mycopy;
    	var mash_id;
    	var	mash_name;
    	if (who=='sub') {
    		email=document.getElementById('youremail').value;
    		document.getElementById('myanswer').innerHTML = '';
    	}
    	else if (who=='emash') {
    		myfurl = encodeURIComponent(myfurl);
    		email=document.getElementById('emash_mail').value;
    		ephone=document.getElementById('emash_phone').value;
    		ephone=encodeURIComponent(ephone);
    		emess=document.getElementById('emash_mess').value;
    		emess=encodeURIComponent(emess);
    		deal=document.getElementById('emash_dealer');
    		if (deal.checked) { deal = 'dealer'; }
    		else { deal = 'printer'; }
    		mycopy=document.getElementById('emash_copy');
    		if (mycopy.checked) { mycopy = '1'; }
    		else { mycopy = '0'; }
    		
    		mash_id=document.getElementById('emash_inp').name;
    		mash_name=document.getElementById('emash_inp').value;
    		mash_name=encodeURIComponent(mash_name);
    
            myloader('emash_answer');
    	}	
    	var mylng = document.getElementById('mylang').lang;
        http.open('post', 'sendmail.php?who='+who+'&email='+email+'&ephone='+ephone+'&emess='+emess+'&deal='+deal+'&mycopy='+mycopy+'&mash_id='+mash_id+'&mash_name='+mash_name+'&myfurl='+myfurl+'&lng='+mylng);    
        http.onreadystatechange = handleResponse;    
        http.send(null);
     }
}
function myloader(myid) {
     var pb = document.getElementById(myid);
    pb.innerHTML = "<img src='images/ajax-loader.gif' width='160' height='20' alt='Please wait...' title='Please wait...' >";
    pb.style.display = '';
//    document.getElementById(myid).style.display = "inline";
}
function ViewSend(mylink) {
	var who = document.getElementById('sel_view').value;
	var zasend = 'fsend=view&mylink='+mylink+'&who='+who;
	http.open('post', 'fav.php?'+zasend);       
	http.onreadystatechange = handleResponse;    
	http.send(null);		
}
function HidePhotos() {
	var allfhotos = document.getElementById('allfotos').style.display;
	if ( allfhotos == 'none') { document.getElementById('allfotos').style.display = 'inline'; }
	else { document.getElementById('allfotos').style.display = 'none'; }		
}
function ShowMail() {
	var w = document.getElementById('det_m_mail').style.display;
	if ( w == 'none') { document.getElementById('det_m_mail').style.display = 'inline'; }
	else { document.getElementById('det_m_mail').style.display = 'none'; }		
}
function showallpicts(snord) {
		var tit = document.getElementById('shallpic').name;
		tit = encodeURIComponent(tit);
		var sn = document.getElementById('shallpic').value;
		var mylng = document.getElementById('mylang').lang;
		var forsend = "photos.php?tit="+tit+"&sn="+sn+"&snord="+snord+"&lng="+mylng;
		
    	var left = parseInt((screen.availWidth/2) - (800/2));
    	var top = parseInt((screen.availHeight/2) - (754/2)); // center of screen
 		
      	window.open(forsend,'','width=800,height=754,toolbar=0,menubar=0,scrollbars=1,status=0,resizable=1,left='+left+',top='+top+',screenX='+left+',screenY='+top);
}
function addToFav(myid,mess,who) {
	var mycount = document.getElementById('favcount').innerHTML;
	var myfav_id = document.getElementById('favcount').lang;	
	mycount = parseInt(mycount);	
	var mystr=myfav_id.split(";");
	if (mystr.in_array(myid) == false ) { 	
		mycount = mycount + 1;
		if ( mycount == 1 ) {	myfav_id = myid; }
		else {	myfav_id = myfav_id + ';'+myid; }
			if (who == 'list') {	
				document.getElementById('favimg'+myid).style.cursor = "default";
				document.getElementById('favimg'+myid).src = "images/buttons/favorites_template_grey.png";
				document.getElementById('favimg'+myid).onclick = "";
			}
			var zasend = 'fsend=addToFav&fav_count='+mycount+'&fav_id='+myfav_id;
		    http.open('post', 'fav.php?'+zasend);       
		    http.onreadystatechange = handleResponse;    
			http.send(null);

			alert(mess);	
	}	
}
Array.prototype.in_array = function(p_val) {
	for(var i = 0, l = this.length; i < l; i++) {
		if(this[i] == p_val) {
			return true;
		}
	}
	return false;
}
function SkypeMe(myid) {
    var mylng = document.getElementById('mylang').lang;
    http.open('post', 'sendmail.php?who=SkypeMe&id='+myid+'&lng='+mylng);    
    http.onreadystatechange = handleResponse;    
    http.send(null);
}
function handleResponse() {
    if(http.readyState == 4){
        var response = http.responseText;
        var update = new Array();
        if(response.indexOf('|' != -1)) {                    
            update = response.split('|');
            if (update[1] == 'Ok') {             	
               if (update[3] == 'addToFav') {             	
					document.getElementById('favcount').innerHTML = update[4];	
					document.getElementById('favcount').lang = update[5];
					document.getElementById('favcount').style.cursor = "pointer";
					document.getElementById('favcount').style.color = "#B71A37";
					document.getElementById('favcount').style.textDecoration="underline";
					document.getElementById('parentfav').style.cursor = "pointer";
					document.getElementById('parentfav').style.textDecoration="underline";  
			   }
			   else if (update[3] == 'sub') { 
			   		document.getElementById('myanswer').innerHTML = update[4];           	
					document.getElementById('youremail').value = '';
			   }
			   else if (update[3] == 'emash') { 
			   		document.getElementById('emash_answer').innerHTML = update[2];          	
					document.emashform.reset();
			   }
               else if (update[3] == 'view') { 
			   		window.location.href='index.php?'+update[4];
			   }
               else if (update[3] == 'SkypeMe') {
                    var myhref = document.getElementById('lnkSkype').lang;
                    window.location.href=myhref;              
			   }               			   			                    
            }
            else if (update[1] == 'No') {
                  alert(update[2]);                              		
			}			
			else {
				alert('PHP ERROR!!!');
			}
        }
    }
}
