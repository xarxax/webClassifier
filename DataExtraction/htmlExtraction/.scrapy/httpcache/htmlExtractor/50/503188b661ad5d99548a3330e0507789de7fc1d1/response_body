	$("#menu_wrapper").css("width",middlewidth-doodlewidth + "px");
	$("#menu_wrapper").css("visibility", "visible");
	$("#doodletitle").css("left", realleft+"px");
	$("#doodletitle").css("top", doodleheight+17+"px");
	$("#searchform").css("width",sidebarwidth);
