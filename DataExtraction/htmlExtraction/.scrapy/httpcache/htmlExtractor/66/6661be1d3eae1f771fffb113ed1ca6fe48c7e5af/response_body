         <div class="Button clearfix grpelem shared_content" id="buttonu5213" data-visibility="changed" style="visibility:hidden" data-content-guid="buttonu5213_content"><!-- container box -->
          <!-- m_editable region-id="editable-static-tag-U5217-BP_960" template="index.html" data-type="html" data-ice-options="disableImageResize,link,clickable,txtStyleTarget" data-ice-editable="link" -->
          <a class="nonblock nontext clearfix colelem shared_content" id="u5217-4" href="cutsmart.html" data-href="page:U151" data-muse-uid="U5217" data-muse-type="txt_frame" data-IBE-flags="txtStyleSrc" data-sizePolicy="fixed" data-pintopage="page_fixedCenter" data-content-guid="u5217-4_content"><!-- content --><p id="u5217-2">CUT SMART</p></a>
          <!-- /m_editable -->
