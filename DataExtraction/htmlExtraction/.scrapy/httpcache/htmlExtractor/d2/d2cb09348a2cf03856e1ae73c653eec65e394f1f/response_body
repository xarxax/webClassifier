        var url = "/m";
        var mobileBrowser = "False";

        $(function () {            
            if ($(window).width() < 700)
                openMobileDialog();
        });

        function setViewCookie(viewType)
        {
            if (viewType == "mobile" || viewType == "full")
            {
                document.cookie = 'view=' + viewType + '; path=/';
                if (viewType == "mobile") {
                    window.location = url;
                }
            }
        }

        function openMobileDialog() {
            Sys.Application.remove_load(openMobileDialog);
            window.setTimeout("showDialogBox('divMobileSiteDialog');", 1000);
        }


        if ($(window).width() < 700) {
            Sys.Application.add_load(openMobileDialog); 
        }

        //if (mobileBrowser=="True") {
        //    setViewCookie("mobile");
        //}


    </script>

    

