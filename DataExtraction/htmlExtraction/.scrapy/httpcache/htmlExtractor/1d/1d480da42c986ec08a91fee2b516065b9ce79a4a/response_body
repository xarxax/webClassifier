<script type="text/javascript">
	totalImages = $("#Slide ul li").length;
	currentImage = 0;
	
	strBullets = "";

	counter = 0;
 	while (counter<totalImages) {
         var imgSrc = $("#Slide ul li img:eq("+counter+")").attr("src");				
 		$("#Slide ul li img:eq("+counter+")").css("filter","progid:DXImageTransform.Microsoft.AlphaImageLoader(enabled='true',sizingMethod='crop',src='"+imgSrc+"')");
 		if($("#Slide ul li img:eq("+counter+")").css("filter")!="none"){
 			//$("#Slide ul li img:eq("+counter+")").attr("src","/images/dotblank.gif");
 		}
 		counter+=1;
 		strBullets += "<div class=\"BannerBullet\"></div>";
     }

	$("#BannerBullets").html(strBullets);
	
	$("#Slide ul li").css("opacity","0").css("display","inline").hide();
	$("#Slide ul li:first").css("opacity","1").show();
	$("#BannerBullets div:first").css("backgroundPosition","0px 0px");
	
	bannerTimer = window.setInterval(changeImage, 7000);
	
	function changeImage() {
		if(!$("#Slide ul li:eq("+currentImage+")").is(":animated")){
			$("#Slide ul li:eq("+currentImage+")").animate({opacity:"0"},400).hide();
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","-26px 0px");
			
			currentImage += 1;
			if(currentImage >= totalImages) {
				currentImage = 0;
			}
			
			$("#Slide ul li:eq("+currentImage+")").show().animate({opacity:"1"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","0px 0px");
		}
	}
	
	$(".BannerBullet").click(function(){
		if(!$("#Slide ul li:eq("+currentImage+")").is(":animated")){
			window.clearInterval(bannerTimer);
			
			$("#Slide ul li:eq("+currentImage+")").animate({opacity:"0"},400).hide();
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","-26px 0px");
			
			currentImage = $(this).index();
			
			$("#Slide ul li:eq("+currentImage+")").show().animate({opacity:"1"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","0px 0px");
			
			bannerTimer = window.setInterval(changeImage, 7000);
		}
	});

</script>			
	</div>
</div>

