    <script language="javascript" type="text/javascript">

        $(document).ready(function () {

            $('ul.sf-menu li').each(function () {
                if ($(this).children('ul').length) {
                    var t = $(this).children('a').html();
                    $(this).children('a').html(t + '<span class="sf-sub-indicator"> »</span>');
                }

                $(this).children(".inActiveLeftImage").click(function () { window.location = $(this).next().attr('href'); });
                $(this).children(".inActiveRightImage").click(function () { window.location = $(this).prev().attr('href'); });
            });



        });

      
    </script>
    <script type="text/javascript" language="javascript">
        function pageLoad() {
            var current = false;
            $("#navigation a").each(function () {
                if (window.location.href == this.href) {
                    current = true;
                    if (this.parentNode.tagName == "LI" && this.parentNode.className == "") {
                        this.parentNode.className = "current";
                    }
                    else {
                        $($(this).parents().closest('ul.level-2')).parents().closest('li')[0].className = "current";
                    }
                }
            });           
        }
    </script>
    
    <script type="text/javascript" language="javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js"></script>
