<script type="text/javascript">

  var currentTab;

  $(function() {
    initializeHandlers();
    initializeUI();

    // deprecated?
   /* $('.updatedGallery').each(function() {
      $(this).qtip({
        content: { url: 'galleryUpdate.aspx', data: { id: $(this).attr('rel')} },
        show: { when: { event: 'mouseover' }, effect: { length: 0} },
        hide: 'mouseout',
        position: {
          corner: { target: 'bottomLeft', tooltip: 'topLeft' },
          adjust: { x: 20, y: 5 }
        },
        style: { width: 200 }
      });
    });*/
  });

  function initializeUI() {
    currentTab = "galleryTab";
    var cookie = $.cookie('home_thumbnails');

    if (cookie != null && cookie == "hide") {
      $("#showThumbs").attr("checked", false);
      $(".thumbnails").hide();
      $(".seriesList").show();
      $(".updatedGallery").unbind('mousemove');
      $(".updatedGallery").unbind('mouseover');
    }
  }

  function initializeHandlers() {
    $("#showThumbs").click(function() {
      if ($(this).is(":checked")) {
        $(".thumbnails").show();
        $.cookie('home_thumbnails', 'show', { expires: 10000, path: '/' });
        $(".seriesList").hide();
        setUpdatedGalleryHandler();
      }
      else {
        $(".thumbnails").hide();
        $.cookie('home_thumbnails', 'hide', { expires: 10000, path: '/' });
        $(".seriesList").stop(true, true);
        $(".seriesList").show();
        $(".updatedGallery").unbind('mousemove');
        $(".updatedGallery").unbind('mouseover');
      }
    });


    $("#updateNav a").click(function() {
      showTab($(this).attr("id"));
    });

    setUpdatedGalleryHandler();

  }

  function setUpdatedGalleryHandler() {
    var config = {
      over: function() {
        $(this).find(".seriesList").slideDown(200);
      },
      timeout: 200,
      out: function() {
        $(this).find(".seriesList").slideUp(200);
      }
    }
    $(".updatedGallery").hoverIntent(config);
  }

  function showTab(tabName) {
    if (tabName != currentTab) {
      $("#" + currentTab).removeClass("selected");
      $("#" + currentTab + "Arrow").hide();
      $("#" + currentTab + "Content").hide();
      currentTab = tabName;
      $("#" + currentTab).addClass("selected");
      $("#" + currentTab + "Arrow").show();
      $("#" + currentTab + "Content").show();
    }
  }
  

</script>


