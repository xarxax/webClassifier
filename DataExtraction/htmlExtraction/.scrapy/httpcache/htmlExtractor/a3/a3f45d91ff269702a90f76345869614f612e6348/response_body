           function f1(data) {
               $("#forgotpwd-popup").show();
               $("#ForgotPassword1_msg").html(data);


           }

           function close_forgotpwd_popup() {
               $("#forgotpwd-popup").hide();
           }
           $(document).keydown(function (e) {
               // ESCAPE key pressed
               if (e.keyCode == 27) {
                   $("#forgotpwd-popup").hide();
               }
           });
                     </script>
    <script language="javascript" type="text/javascript">
        function openDemo(IEurl, NNurl, Width, Height) {
            // hide javascript from all non-enabled browsers.
            if (parseInt(navigator.appVersion) >= 4) {
                var IE4 = (navigator.appVersion.indexOf("MSIE") > 0);
                var NN4 = (navigator.appName == "Netscape");
                var OS;
                if (navigator.appVersion.indexOf("Win") > 0) OS = "Windows";
                switch (OS) {
                    case "Windows":
                        if (IE4 > 0) eval("window.open('" + IEurl + "','','toolbar=no,location=no,status=no,menubar=no,scrollbars=no,resizable=no,width=" + Width + ",height=" + Height + "')");
                        if (NN4 > 0) eval("window.open('" + NNurl + "','','toolbar=no,location=no,status=no,menubar=no,scrollbars=no,resizable=no,width=" + Width + ",height=" + Height + "')");
                        break;
                }
            }
        }
    </script>
   
   
	</script>


