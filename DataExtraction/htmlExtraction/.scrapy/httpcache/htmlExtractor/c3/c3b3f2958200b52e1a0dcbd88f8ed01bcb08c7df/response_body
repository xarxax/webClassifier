<script>
function placeBannerText(){
    var bannerHeight = $('#banner').outerHeight();
    var headerHeight = $('#header').outerHeight();
    var leftOffset = 0;

    if($(window).width() < 965){headerHeight = 0;leftOffset = 20;}

    var textBox = $('#banner .banner-info');

    textBox.css({"top":(((bannerHeight-headerHeight)-textBox.outerHeight())/2)+headerHeight, "left":$('#logo').offset().left+leftOffset});

    textBox.fadeIn("slow");
}

$(window).on('load',function(){
    setTimeout(placeBannerText(), 100);
});

$(window).on('resize',function(){
    placeBannerText();
});
</script>

