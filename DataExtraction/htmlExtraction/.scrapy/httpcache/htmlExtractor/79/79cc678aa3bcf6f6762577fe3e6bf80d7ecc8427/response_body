            function displayWindow()
            {
                var w, h, l, t;
                w = 400;
                h = 320;
                l = screen.width/2-w/2;
                t = screen.height/6;

                // no title
                // displayFloatingDiv('windowcontent', '', w, h, l, t);

                // with title
				//alert('test');
                displayFloatingDiv('windowcontent', 'Floating and Dimming Div', w, h, l, t);
