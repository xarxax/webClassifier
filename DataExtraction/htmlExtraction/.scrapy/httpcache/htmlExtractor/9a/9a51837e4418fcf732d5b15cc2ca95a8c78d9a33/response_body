	</div><script type="text/javascript">
	function changeForm(selectionValue) {
		if (selectionValue == "DonationRequests") {
			$("#country").val("USA");
			$("#CountrOtherRow").hide();
			$("#StateRow").hide();
			$(".content-contactform").children().hide();
			$("[for='contactReason']").show();
			$("#contactReason").parent().show();
			$("#LocalRestaurantDonation").show();
		} else if (selectionValue == "LocationSuggestion") {
			$("#LocalRestaurantDonation").hide();
			$(".content-contactform").children().show();
			$(".locationSuggestionHide").hide();
      $(".locationSuggestionHide").children().removeClass("required");
			$(".locationSuggestionShow").show();
			$(".locationSuggestionShow").children().addClass("required");
			$("[for='city']").html("<span class='required'>*</span>Suggested City");
			$("[for='country']").html("Suggested Country");
			$("#contactUsForm").attr("action", "/LocationSuggestionSubmit.aspx");
			$("#country").val("USA");
			$("#country").trigger("change");
			if ($("#stateLabel").html() === "State") {
				$("#stateLabel").html("Suggested State");
			}
			$("#CountrOtherRow").hide();
			$("#countryOther").attr("placeholder", "Enter suggested country name");
			$("#city").addClass("required");
			$("#city").parent("div").attr("data-error", "Please enter a city.");
			$("#state").parent("div").attr("data-error", "Please enter a state.");
		} else {
			$("#LocalRestaurantDonation").hide();
			$(".content-contactform").children().show();
			$(".content-contactform").show();
      $(".locationSuggestionHide").show();
			$(".locationSuggestionHide").children().addClass("required");
			$(".locationSuggestionShow").hide();
			$(".locationSuggestionShow").children().removeClass("required");
			$("[for='city']").html("<span class='required'>*</span>City");
			$("[for='country']").html("<span class='required'>*</span>Country");
			$("#contactUsForm").attr("action", "/Mailer/ContactUs");
			$("#country").val("USA");
			$("#country").trigger("change");
			$("#CountrOtherRow").hide();
			$("#countryOther").attr("placeholder", "Enter country name");
			//$("#city").removeClass("required");
			$("#city").parent("div").attr("data-error", "Please enter your city.");
			$("#state").parent("div").attr("data-error", "Please enter your state.");
		}
	}
	require(["jquery"], function () {
		changeForm($("#contactReason").val());
		$("#contactReason").change(function () {
			changeForm($(this).val());
		});
	});
</script>

