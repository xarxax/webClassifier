<script>
    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }

    var md = new MobileDetect(window.navigator.userAgent);
    
    var pcview = location.search.split('pcview=')[1];
    var hasSetPC = readCookie("pcview");
    if(pcview){
        var cookieName = 'pcview';
        var cookieValue = '1';
        var myDate = new Date();
        myDate.setDate(myDate.getDate() + 1);
        var domain = window.document.domain;
        document.cookie = cookieName +"=" + cookieValue + ";expires=" + myDate 
                          + ";domain="+domain+";path=/";
    }else if(!hasSetPC && md.mobile()){
        window.location.href="http://3g.gxnews.com.cn/formwww.html?ref="+encodeURIComponent(window.document.location.href);
    }

</script>
