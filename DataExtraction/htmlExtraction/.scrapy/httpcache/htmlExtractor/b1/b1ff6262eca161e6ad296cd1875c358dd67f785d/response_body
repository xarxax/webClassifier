                    $(document).ready(function(){
                    $("#cookie").animate({bottom:0}, 1000);
                    $("#cookie a.agree").click(function(event){
                        event.preventDefault();
                        var cookieurl = $(this).attr("href");
                        $.ajax({url: cookieurl});
                        $("#cookie").fadeOut();
                    });

                    setTimeout(function() {
                        $("#cookie").hide("fade", {}, 500)
                    }, 60000);


                    });
                    </script><style>
