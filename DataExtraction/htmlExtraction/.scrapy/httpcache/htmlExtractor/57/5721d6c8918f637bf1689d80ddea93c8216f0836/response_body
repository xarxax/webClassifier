    <script>
    function equalHeight(group) {
       tallest = 0;
       group.each(function() {
          thisHeight = $(this).height();
          if(thisHeight > tallest) {
             tallest = thisHeight;
          }
       });
       group.height(tallest);
    }
    $(document).ready(function() {
       equalHeight($(".equal"));
    });
    </script>
    
    
<script type="text/javascript">
(function($){
  $(document).ready(function() {
      //init the cycle plugin
      $('#featured').find('ul.features').cycle({
        fx:      'scrollLeft',
        speed:    1200,
        timeout:  7000,
        fit:    false,
        prev:     "#prev2",
        next:     "#next2",
        pager:    '#features-nav',
        cleartypeNoBg: true,
        
        pagerAnchorBuilder: function(idx, slide) {
          var $slide = $(slide);
          var $num = idx + 1;
          return '<li><a href="#">' + $num + '</a></li>';
        },
        
        //callback to update background on slide change
        before: function(currSlideElement, nextSlideElement, options, forwardFlag) {
          //if this is the first slide then use currSlide to display the first background, else use nextSlide to transition
          $('#nav li:eq(' + (currSlideElement == nextSlideElement ? options.currSlide : options.nextSlide) + ') a').click();
        }
      });
    
    //pause on hover
    $('.feature').hover(
      function() {$('#featured').find('ul.features').cycle('pause');},
      function() {$('#featured').find('ul.features').cycle('resume');});
    
    //get the array of background images for the bgStretcher
      function getImages() {
        var images = [];
        $('div.summary').each(function() {
          images.push($(this).attr('data-src'));
        });
        return images;
      }
      
      //init the bgstretcher plugin    
      $('BODY').bgStretcher({
        images: getImages(),
        imageWidth: 1200,
        imageHeight: 910,
        slideDirection: 'N',
        slideShowSpeed: 1200,
        transitionEffect: 'fade',
        sequenceMode: 'normal',
        buttonPrev: '#prev',
        buttonNext: '#next',
        pagination: '#nav',
        anchoring: 'left center',
        anchoringImg: 'left center',
        resizeProportionally: 'true',
        //disable the slideshow
        slideShow: false,
        callbackfunction: function(){$('BODY').bgStretcher.pause()}
      });
      
      fixFooter(); 
  });
})(jQuery);
</script>

