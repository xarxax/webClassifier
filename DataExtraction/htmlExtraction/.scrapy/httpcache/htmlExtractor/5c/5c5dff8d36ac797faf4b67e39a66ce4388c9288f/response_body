<script type="text/javascript">
//$(document).ready(function() {
$(function() {
	
	function runCarousel() {
		$("#mainThumb").carouFredSel({
			auto: 4000
	/*		width: 438	*/
			, responsive: false
			, circular: true
			, infinite: true	// ????
			, prev: { button: "#btnPrev" }
			, next: { button: "#btnNext" }
	/*		, items: { width: 438, visible: 1 }	*/
			, scroll: { 
				pauseOnHover: true
				, onAfter: function(data) {
					var idx = $(this).triggerHandler("currentPage");	// ?? ???? ?? ???? ??
					$('.mainslideList > li').removeClass('current');
					$('.mainslideList > li').eq(idx).addClass('current');
					$('#mainBannerText > div').css('display','none');
					$('#mainBannerText > div').eq(idx).css('display','block');
	//				if(idx == 3) popDiscoverFoam();
				}
			}
/*
			, pagination: {	// paging custom
				container: '.mainslideList'
				, anchorBuilder: false
			}
*/

		});
	}												
	$("#mainThumb").imagesLoaded(runCarousel);	
	
	Shadowbox.init();	// shadowbox init

	// ie8 ???? ??? ???? ?? //
	if(checkVersion() != null && checkVersion() < 8) {
		Shadowbox.open({
			content: '#discover-container1'
			, player: 'inline'
			, width: 890
			, height: 370
			, title: $('#discover-title-inner1').text()
		});
	}
});

// discover foatm //
function popDiscoverFoam() {
    Shadowbox.open({
        content: '#discover-container'
		, player: 'inline'
		, width: 890
		, height: 370
		, title: $('#discover-title-inner').text()
    });
}

function getInternetExplorerVersion() {
	var rv = -1;
	if (navigator.appName == 'Microsoft Internet Explorer') {
		var ua = navigator.userAgent;
		var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
		if (re.exec(ua) != null)
		rv = parseFloat(RegExp.$1);
	}
	return rv;
} 

function checkVersion() {
	var ver = getInternetExplorerVersion();
//	if (ver > -1) msg = "You are using Internet Explorer " + ver;
//	else msg = "You are not using Internet Explorer";
	if(ver > -1) return ver;
	else return null;
}
</script>

