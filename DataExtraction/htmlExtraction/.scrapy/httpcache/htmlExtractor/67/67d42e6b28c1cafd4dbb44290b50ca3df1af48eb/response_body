<script type="text/javascript">
    function fixPostbackLink() {
        topicsObj = $('#ctl00_ContentPlaceHolder1_NavigatorBrowse_lbTopics');
    categoriesObj = $('#ctl00_ContentPlaceHolder1_NavigatorBrowse_lbCategories');
    typeObj = $('#ctl00_ContentPlaceHolder1_NavigatorBrowse_lbTimeSavers');
    stateObj = $('#ddlActiveStates');
    page = '';

    topicId = GetTopicId(topicsObj.val());

    topicId = (topicId == "") ? 0 : topicId;

    alias_id = GetAliasId(topicsObj.val());

    if (topicsObj.val() != "") {
        if (typeObj.val() == "55" || typeObj.val() == "112" || typeObj.val() == "111") {
            document.getElementById('aspnetForm').action = '/training.aspx?topic=' + topicId + '&category=' + categoriesObj.val();
        } else {
            document.getElementById('aspnetForm').action = '/index.aspx?topic=' + topicId + '&category=' + categoriesObj.val();
        }
        if (stateObj != undefined) {
            document.getElementById('aspnetForm').action += '&juris=' + stateObj.val();
        }
        if (typeObj != undefined) {
            if (typeObj.val() == "112") {
                typeObj.val("0");
            }
            document.getElementById('aspnetForm').action += '&type=' + typeObj.val();
        }
        if (alias_id != "" && alias_id != null) {
            document.getElementById('aspnetForm').action += '&alias=' + alias_id;
        }
    }
}
function GetTopicId(name) {
    var ret = '';
    ServiceCallSync("Services/TopicService.asmx", "GetTopicIdByName", "topicName:\"{0}\"".format(name),
    function (m) {
        ret = m;
    }, FailedCallback);
    return ret;
}
function GetAliasId(name) {
    var ret = '';
    ServiceCallSync("Services/TopicService.asmx", "GetAliasIdByName", "topicName:\"{0}\"".format(name),
    function (m) {
        ret = m;
    }, FailedCallback);
    return ret;
}
</script>
