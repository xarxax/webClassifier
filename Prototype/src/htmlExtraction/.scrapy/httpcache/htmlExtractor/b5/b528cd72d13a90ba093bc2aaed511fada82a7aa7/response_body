function sendshout(event, thisobject)
{
 event.preventDefault(); // stop form from submitting normally 
 var $form = $(thisobject), term = $form.find(":input").serializeArray(), url = $form.attr("action");
 $("#shoutbutton").attr("disabled", "disabled");
 $("[name=shoutform]").find("[name=message]").val("").attr("disabled", "disabled");
 $.post(url, term, function(data) {
  if (data == "OK") updatelivetoplist(53);
  else alert(data);
  $("#shoutbutton").removeAttr("disabled");
  $("[name=shoutform]").find("[name=message]").removeAttr("disabled");
 });
}

function deleteshout(idnum)
{
 var spanid = "#shout" + idnum;
 $(spanid).html("");
 $.post("ajax.php", { userid: 0, userauthcode: "", id: idnum, action: "deleteshout" }, function(data) {
 if (data == "OK") updatelivetoplist(53);
 else alert(data);
});
}


function updatelivetoplist(idnum)
{
 var divid = "#livetoplist"+idnum;
 $.post("http://www.top-properties.com/property-for-sale/ajax.php", { action: "updatelivetoplist", userid: 0, userauthcode: "", templatename: "templates/default/custom/front.tpl", id: idnum }, function(data) { 
 var parts = data.split("[|||]");
 divid = "#livetoplist"+parts[0];
 content = parts[1];
 $(divid).html(content);
 });
}

self.setInterval(function() { updatelivetoplist(46011); }, 3600000);
self.setInterval(function() { updatelivetoplist(1793); }, 4000000);
self.setInterval(function() { updatelivetoplist(53); }, 9999000);
self.setInterval(function() { updatelivetoplist(56343); }, 3600000);

$().ready(function() {

$(".datepicker").datepicker({ dateFormat: "yy/mm/dd", dayNamesMin: ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"], monthNames: ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"] });
$("[name=shoutform]").submit(function(event) { sendshout(event, this); });

if(typeof(initialize) == 'function') initialize();

        if ($("#zipsourceauto").length) $("#zipsourceauto").autocomplete({ source: "/property-for-sale/autocomplete.php?action=zipsourceauto" });
        else $("[name=zipsourceauto]").autocomplete({ source: "/property-for-sale/autocomplete.php?action=zipsourceauto" });

        if ($("#zipsource").length) $("#zipsource").autocomplete({ source: "/property-for-sale/autocomplete.php?action=zipsource" });
        else $("[name=zipsource]").autocomplete({ source: "/property-for-sale/autocomplete.php?action=zipsource" });

$('.stripeme tr:nth-child(even)').addClass('evenrow');
$('.stripeme tr:nth-child(odd)').addClass('oddrow');
$('a.ajaxlink').bind('click',function(event)
{
 event.preventDefault();
 $.get(this.href,{},function(response)
 { 
  var parts = response.split('[|||]');
  $(parts[0]).html(parts[1])
 })	
});
$('a.ajaxdelete').bind('click',function(event)
{
 event.preventDefault();
 var id = '#row'+this.id;
 $(id).hide(500);
 $.get(this.href,{},function(response) {  })	
});
 
});

