<script>
    $(function () {
        $('#change-toggle').click(function () {
            // Show/hide close button for menu collapse.
            $('#navbar-hamburger').toggleClass('hidden');
            $('#navbar-close').toggleClass('hidden');

            // When collapse menu is open, add styles.
            if ($(".navbar-collapse").hasClass("navbar-collapse collapse")) {
                $(".container-fluid:first-child").css({
                    'border-bottom': '1px solid #e6e6e6',
                    'padding-bottom': '20px'
                });
            } else {
                $(".container-fluid:first-child").css({
                    'border-bottom': 'none',
                    'padding-bottom': 'initial'
                });
            }
        });
    });
</script>

