<script type="text/javascript">

(function(){
	var nav_imgs = document.getElementById('navas').getElementsByTagName('img');
    for(var i=0 in nav_imgs ){
	    var img=navas[i]; 
	    var src1 = img.src;
		var src2 = src1.replace('.jpg','-ov.jpg');
		(new Image()).src=src2;
		img.setAttribute('data-src1',src1);
		img.setAttribute('data-src2',src2);
		img.onmouseover = function(){
			this.src=this.getAttribute('data-src2'); 
		} 
		img.onmouseout = function(){
			this.src=this.getAttribute('data-src1');
		}
    }
})()
</script></td>
