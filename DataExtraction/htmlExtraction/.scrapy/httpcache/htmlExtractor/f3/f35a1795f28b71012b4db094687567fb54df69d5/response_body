				paddingBottom: 0
			}, 300, function() {
				$portfolioContent.toggleClass('hide-content');
				$hideShow.removeClass('rotateToOpen').addClass('rotateToClose');
				$portfolioContent.animate({
					width: 30,
					padding: 0
				}, 200);
			});
		} else {
			$portfolioContent.animate({
				width: '100%',
				paddingLeft: 20,
				paddingRight: 20
			}, 200, function() {
				$portfolioContent.animate({
					height: 379,
					paddingBottom: 40,
					paddingTop: 40
				}, 300, function() {
					$portfolioContent.toggleClass('hide-content');
					$hideShow.addClass('rotateToOpen').removeClass('rotateToClose');
				});
			});
		}
	 	
	 	event.preventDefault();
	 	
	});
	
	$( ".header-show-hide-content" ).click(function() {
		
		var $portfolioContent = $(this).parent(),
		portfolioHeight = $portfolioContent.outerHeight(),
		$hideShow = $(this);
	 	
	 	var currentHeight = $portfolioContent.outerHeight();

		if( currentHeight > 30 ) { //if current height is less than 30
			$portfolioContent.animate({
				height: 30,
				paddingTop: 0,
				paddingBottom: 0
			}, 300, function() {
				$portfolioContent.toggleClass('hide-content');
				$hideShow.removeClass('rotateToOpen').addClass('rotateToClose');
				$portfolioContent.animate({
					width: 163,
					padding: 0
				}, 200);
			});
		} else {
			$portfolioContent.animate({
				width: 450,
				paddingLeft: 40,
				paddingRight: 40
			}, 200, function() {
				$portfolioContent.animate({
					height: 500,
					paddingBottom: 40,
					paddingTop: 40
				}, 300, function() {
					$portfolioContent.toggleClass('hide-content');
					$hideShow.addClass('rotateToOpen').removeClass('rotateToClose');
				});
			});
		}
	 	
	 	event.preventDefault();
	 	
	});
	
