$(function() {
    var bc = $('#buttonContainer');

    var $container = $('#container').cycle({
        fx: 'scrollHorz',
        speed: 300,
        timeout: 2000,
    });

    $container.children().each(function(i) {
        $('<a href="'+(i+1)+'.htm"><input type="button" class="buttonclass" id="button_'+(i+1)+'" /></a>')
            .appendTo(bc).mouseover(function() {
                $container.cycle(i);
                return false;
            });
    });

});
