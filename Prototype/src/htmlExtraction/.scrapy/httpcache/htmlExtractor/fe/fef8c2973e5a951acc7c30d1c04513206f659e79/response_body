<script>
      $(document).on('click', '.panel-heading span.clickable', function (e) {
          var $this = $(this);        
          if (!$this.hasClass('panel-collapsed')) {
              $this.parents('.panel').find('.panel-body').slideUp();
              $this.addClass('panel-collapsed');
              $this.find('i').removeClass('glyphicon-chevron-up').addClass('glyphicon-chevron-down');
          } else {
              $this.parents('.panel').find('.panel-body').slideDown();
              $this.removeClass('panel-collapsed');
              $this.find('i').removeClass('glyphicon-chevron-down').addClass('glyphicon-chevron-up');
          }
      })
  </script>
<script>
        function validate(evt) {
            if (evt.keyCode != 8) {
                var theEvent = evt || window.event;
                var key = theEvent.keyCode || theEvent.which;
                key = String.fromCharCode(key);
                var regex = /[0-9]|\./;
                if (!regex.test(key)) {
                    theEvent.returnValue = false;

                    if (theEvent.preventDefault)
                        theEvent.preventDefault();
                }
            }
        }
    </script>
<script type='text/javascript'>
$(window).load(function(){
      var i=1;
     $("#add_row").click(function(){
         $('#addr' + i).html("<td width:10px>" + (i + 1) + "</td><td width:300px><input name='part" + i + "' type='text' placeholder='part' onblur='checkTextField(this);' class='form-control input-md' style='text-transform: uppercase'/></td><td width:100px><input name='qty" + i + "' type='text' placeholder='qty' onblur='checkTextField(this);' onkeypress='validate(event)' class='form-control input-md' style='text-transform: uppercase'></td><td width:30px><input name='remove" + i + "' type='button' onclick='deleteRow(this)' value='Remove' class='form-control input-md'></td>");
         $('#tab_logic').append('<tr id="addr'+(i+1)+'"></tr>');
               
      i++; 
  });
     $("#delete_row").click(function(){
         if(i>1){
         $("#addr"+(i-1)).html('');
         i--;
         }
     });

     $('#add_to_cart').click(function () {
                  
         var part0 = document.getElementById("txtpart").value;
         var qty0 = document.getElementById("txtqty").value;
                            
         var x = document.getElementById('tab_logic').rows.length;
                
         s = (x - 1);
         

         if (x == 2) {            
             document.getElementById("txtpart").value = "";
             document.getElementById("txtqty").value = "";
         }
         // check cookie first ********
         var item = getCookie("ItemsCount");         
         if (item != null && parseInt(item) > 0) {
             item = parseInt(item) + s;
             s++;
         }
         else
         {
             item = parseInt(s);
             s++;            
         }
         
              // ********** remove for reading session from code behind *********//
             //document.getElementById("txtcount").value = parseInt(item);
             setCookie("ItemsCount", parseInt(item));
         
         // add first row to shopping cart cookie                                      
         var firstpart = part0 + "@" + qty0 + "@" ;
                                
       
         // Get table object
         var myTable = document.getElementById('tab_logic').tBodies[0];

         // first loop for each row	
         for (var r = 0, n = myTable.rows.length; r < n; r++) {
             // this loop is getting each colomn/cells
             for (var c = 0, m = myTable.rows[r].cells.length; c < m; c++) {

                 if (myTable.rows[r].cells[c].childNodes[0].value) {
                     // get student names
                     //var StudentName = myTable.rows[r].cells[1].innerHTML;
                     //alert(StudentName);
                     // get student marks in each text boxes  
                     
                         var parts = parts + myTable.rows[r].cells[c].childNodes[0].value + "@";                    
                     
                 }
             }
         }

         var shoppingCart = getCookie("shoppingCart");
         var cart = firstpart + parts

         if (shoppingCart != null && cart != null) {
             shoppingCart = shoppingCart + cart;
         }
         else {
             shoppingCart = cart;
         }
                
             document.getElementById("txtcart").value = shoppingCart;
             setCookie("shoppingCart", shoppingCart);
         
         //setCookie("shoppingCart", cart.join('@'));
         alert("Item has been add to shopping cart ");

         if (x > 2) { 

             var mytbl = document.getElementById("tab_logic");
              mytbl.getElementsByTagName("tbody")[0].innerHTML = mytbl.rows[0].innerHTML;
              document.getElementById("txtpart").value = "";
              document.getElementById("txtqty").value = "";
         }

         // end table elemen loop 

 
     });

});

</script>
<script>
    function deleteRow(row) {
        var i = row.parentNode.parentNode.rowIndex;
        document.getElementById('tab_logic').deleteRow(i);
    }
</script>  
