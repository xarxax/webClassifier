	<script>
	var more;
	var count = 0;
	
		function viewMore()
		{	
			if(!more)
			{
				count = 0;
				var temp = document.getElementById("blockNewProductsHidden");
				
				if(temp)
				{
					while(temp.id == "blockNewProductsHidden")
					{
						temp.id = "blockNewProductsShown";
						if(document.getElementById("blockNewProductsHidden"))
							temp = document.getElementById("blockNewProductsHidden");
							
						count++;
					}
				}
				
				document.getElementById("seeMoreButton").innerHTML = "SEE LESS";
				
				more = true;
			}
			else
			{
				var temp = document.getElementById("blockNewProductsShown");
				var i = 0;
				
				if(temp)
				{
					while(i < count)
					{
						temp.id = "blockNewProductsHidden";
						if(document.getElementById("blockNewProductsShown"))
							temp = document.getElementById("blockNewProductsShown");
							
						i++;		
					}
				}
				
				document.getElementById("seeMoreButton").innerHTML = "SEE MORE";
				
				more = false;
			}
		}	
	</script>

