	    <script type="text/javascript">
		
		
	
	// thumbs maker
	var gg_lb_thumb = function(src) {
					return 'https://northernscientific.com/wp/wp-content/plugins/global-gallery/classes/easy_wp_thumbs.php?src='+ encodeURIComponent(src) +'&w=100&h=100';
			
	};
	
	
	// show lightbox 
	gg_throw_lb = function(gall_obj, rel, clicked_index, no_deeplink) {
		if(!Object.keys(gall_obj).length) {return false;}

		if(jQuery('#gg_lb_gall').length) {jQuery('#gg_lb_gall').empty();}
		else {jQuery('body').append('<div id="gg_lb_gall"></div>');}
		
			
				
				jQuery.each(Object.keys(gall_obj), function(i, v) {
					var obj = gall_obj[v];
					var txt = (obj.descr) ? '<p style="margin-bottom: 10px;"><strong>'+obj.title+'</strong></p>'+obj.descr : obj.title;
					
					jQuery('#gg_lb_gall').append('<a href="'+obj.img+'"><img src="" title="'+ gg_lb_html_fix(txt) +'"></a>');
				});

				jQuery('#gg_lb_gall > a').simpleLightbox({
					widthRatio: 0.85,
					heightRatio: 0.85,
					animationSpeed: 500,
					animationSlide: false,
					disableRightClick: false,
					showCounter: false,
					className: 'gg_simplelb'
				});
				
				jQuery('#gg_lb_gall > a:eq('+ clicked_index +')').trigger('click');
				jQuery('.sl-overlay').addClass('gg_simplelb');
		
		
			};
	</script>
	</body>
