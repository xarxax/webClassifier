    <script type="text/javascript" src="/public/plugins/owlCarousel/owl.carousel.js"></script> 
 <!--- <script type="text/javascript" src="/public/plugins/owlCarousel/owlcarousel2-a11ylayer.js"></script>  -->
    <script type="text/javascript">
        $(document).ready(function () {


            var sync1 = $("#sync1");
            var sync2 = $("#sync2");

            sync1.owlCarousel({
                loop: false,
                nav: true,
                dots: false,
                center: true,
                items: 1,
                navText: ['<p class="screen-reader">previous</p>', '<p class="screen-reader">next</p>'],
                onTranslated: syncPosition,
                margin: 0,
                autoplay: true,
                autoplayTimeout: 4500,
                autoplayHoverPause: false,
                smartSpeed: 500,
                dotsSpeed: 500,
                onInitialized: function () {
                    //$(".owl-dot").click(function() {PlayCarousel();});
                    $("#sync1 .owl-dots").after("<div class=\"playPause\" aria-live=\"off\">" +
                                                        "<span class=\"carouselPlayLink\" onclick=\"PlayCarousel();\" style=\"display:none;\"><button aria-label=\"Play Slider\" class=\"carouselPlay fa fa-play-circle-o\"></button></span>" +
                                                        "<span class=\"carouselPauseLink\" onclick=\"PauseCarousel();\" ><button aria-label=\"Pause Slider\" class=\"carouselPause fa fa-pause-circle-o\"></button></span>" +
                                                    "</div>")
                    $('.owl-prev, .owl-next').attr("tabindex", "0");
                    $('.owl-prev .screen-reader, .owl-next .screen-reader').attr("role", "button");
                }
            });
            sync1.on('changed.owl.carousel', function (event) {
                PlayCarousel();
            });
            sync1.on('changed.owl.carousel', function (event) { PlayCarousel(); });

            var itemCount = $('#sync2 .item').length;

            sync2.owlCarousel({
                //items: 4,
                responsive: { 0: { items: 0 }, 479: { items: itemCount }, 1000: { items: itemCount} },
                loop: false,
                mouseDrag: false,
                touchDrag: false,
                autoplay: false,
                pullDrag: false,
                dots: false,
                //                pagination: false,
                //                responsiveRefreshRate: 100,
                afterInit: function (el) {
                    el.find(".owl-item").eq(0).addClass("synced");
                }
            });

            function syncPosition(el) {
                var current = el.item;
                $("#sync2")
                      .find(".owl-item")
                      .removeClass("synced")
                      .eq(current)
                      .addClass("synced")
                //if($("#sync2").data("owlCarousel") !== undefined){
                center(current)
                // }
            }
            $(".owl-nav").on("click", function (e) {
                $(".carouselPauseLink").css("display", "none");
                $(".carouselPlayLink").css("display", "inline");
                $("#sync1").owlCarousel().trigger("stop.owl.autoplay");
            });


            $(".owl-nav").on('keydown', function (e) {

                if (e.which == 13) {

                    if ($(e.target).hasClass('owl-prev')) action = 'prev.owl.carousel';
                    else if ($(e.target).hasClass('owl-next')) action = 'next.owl.carousel';

                    $("#sync1").owlCarousel().trigger(action);

                    setTimeout(function () {
                        $(".carouselPauseLink").css("display", "none"); $(".carouselPlayLink").css("display", "inline"); $("#sync1").owlCarousel().trigger("stop.owl.autoplay");
                    }, 1000);


                }
            });
            $("#sync2").on("click", ".owl-item", function (e) {
                e.preventDefault();

                var number = $(this).data("owlItem");
                sync1.trigger("to.owl.carousel", $(this).index());

                $(".carouselPauseLink").css("display", "none");
                $(".carouselPlayLink").css("display", "inline");
                $("#sync1").owlCarousel().trigger("stop.owl.autoplay");


                $(this).find(".owl-item").eq(0).focus();
            });

            function center(number) {
                var sync2visible = $("#sync2 .owl-item.active").length;
                var num = number;

                var found = false;
                for (var i in sync2visible) {
                    if (num === sync2visible[i]) {
                        var found = true;
                    }
                }

                if (found === false) {
                    if (num > sync2visible[sync2visible.length - 1]) {
                        sync2.trigger("to.owl.carousel", num - sync2visible.length + 2)
                    } else {
                        if (num - 1 === -1) {
                            num = 0;
                        }
                        sync2.trigger("to.owl.carousel", num);
                    }
                } else if (num === sync2visible[sync2visible.length - 1]) {
                    sync2.trigger("to.owl.carousel", sync2visible[1])
                } else if (num === sync2visible[0]) {
                    sync2.trigger("to.owl.carousel", num - 1)
                }

            }


        });

        function PlayCarousel() {
           
            $(".carouselPauseLink").css("display","inline");
            $(".carouselPlayLink").css("display","none");
          
            $("#sync1").owlCarousel().trigger("play.owl.autoplay",[4000]);
           
        };


        function PauseCarousel() {
       
            $(".carouselPauseLink").css("display","none");
            $(".carouselPlayLink").css("display","inline");
           
            $("#sync1").owlCarousel().trigger("stop.owl.autoplay");
        };
    </script> 

