        <script>
            $(function(){
                $(window).scroll(function(){
                    var _top = $(window).scrollTop();
                    if(_top>300){
                        $('.back_top').fadeIn(600);
                    }else{
                        $('.back_top').fadeOut(600);
                    }
                });
                $(".back_top").click(function(){
                    $("html,body").animate({scrollTop:0},500);
                });
            });
        </script>
