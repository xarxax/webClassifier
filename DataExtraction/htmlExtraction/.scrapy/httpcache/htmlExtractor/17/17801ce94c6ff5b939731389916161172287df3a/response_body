/* ---------- SIDEKICK AD BANNERS INTERVAL SCRIPT ---------- */



var switchAd1 = 15000; // how long an ad will be displayed

var delayAd1 = 1000; // time delay to show next ad



function showfirstSidekickbanner(){

	jQuery('.sidekick-small-banners').hide();

	jQuery('.medimizer-sidekick-small-banner').show();

	setTimeout(function(){

		if (jQuery('.medimizer-expanded-form').css('width') == '0px'){

			jQuery('.medimizer-sidekick-small-banner').hide();

			secondSidekicktimeout();

		}

	}, switchAd1)

	function secondSidekicktimeout(){

	showsecondSidekickbanner();
		// setTimeout(showsecondSidekickbanner, delayAd1);

	}

}



function showsecondSidekickbanner(){

	jQuery('.sidekick-small-banners').hide();

	jQuery('.medimizer-sidekick-small-banner2').show();

	setTimeout(function(){

		if (jQuery('.medimizer-expanded-form').css('width') == '0px'){

			jQuery('.medimizer-sidekick-small-banner2').hide();

			thirdSidekicktimeout();

		}

	}, switchAd1)

	function thirdSidekicktimeout(){

		// setTimeout(showfirstSidekickbanner, delayAd1);
		showfirstSidekickbanner();

	}

}



function showthirdSidekickbanner(){

	jQuery('.sidekick-small-banners').hide();

	jQuery('.ats-sidekick-small-banner').show();

	setTimeout(function(){

		if (jQuery('.ats-expanded-form').css('width') == '0px'){

			jQuery('.ats-sidekick-small-banner').hide();

			fourthSidekicktimeout();

		}

	}, switchAd1)

	function fourthSidekicktimeout(){

		setTimeout(showfourthSidekickbanner, delayAd1);

	}

}



function showfourthSidekickbanner(){

	jQuery('.sidekick-small-banners').hide();

	jQuery('.stanburns-sidekick-small-banner').show();

	setTimeout(function(){

		if (jQuery('.stanburns-expanded-form').css('width') == '0px') {

			jQuery('.stanburns-sidekick-small-banner').hide();

			fifthSidekicktimeout();

		}

	}, switchAd1)

	function fifthSidekicktimeout(){

		setTimeout(showfifthSidekickbanner, delayAd1);

	}

}



function showfifthSidekickbanner(){

	jQuery('.sidekick-small-banners').hide();

	jQuery('.hettich-sidekick-small-banner').show();

	setTimeout(function(){

		if (jQuery('.hettich-expanded-form').css('width') == '0px') {

			jQuery('.hettich-sidekick-small-banner').hide();

			firstSidekicktimeout();

		}

	}, switchAd1)

	function firstSidekicktimeout(){

		// setTimeout(showfirstSidekickbanner, delayAd1);

		setTimeout(showsecondSidekickbanner, delayAd1);



	}

}



// var adArray1 = ['showfirstSidekickbanner', 'showsecondSidekickbanner', 'showthirdSidekickbanner', 'showfourthSidekickbanner', 'showfifthSidekickbanner']; // list ad functions in array

var adArray1 = ['showfirstSidekickbanner','showsecondSidekickbanner']; // list ad functions in array

var randAd1 = adArray1[Math.floor(Math.random() * adArray1.length)] + '()';

setTimeout(randAd1 , 2000); // show first ad randomly with 2 second delay



/*-------------------------------------*/





