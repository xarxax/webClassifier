		<script type="text/javascript">
			function prependTokenToUsername()
			{
				document.login.username.value = "" + document.login.faux_username.value;
			}
		</script>
		<script type="text/javascript">
		function buildUserName() {			
			document.login.username.value = "" + document.login.faux_username.value;
		}
		</script>
		<script type="text/javascript">
			function isiRedirect() {
				var userName=document.login.faux_username.value;
				if(userName.indexOf("@") >= 0) {
					document.login.action = 'https://select.adp.com/nslogin/index.html';
				}
			}
		</script>

		<script type="text/javascript">
			function login_submit() {
				console.log("login_submit");
				if(loginForm.validate()){
					isiRedirect();
					buildUserName();
					document.login.submit();
				}
				else{ return false;}
			}
			
			function login(){
			isiRedirect();
				prependTokenToUsername();
				dojoLogin(Dialog,'login');
			}
		</script>
