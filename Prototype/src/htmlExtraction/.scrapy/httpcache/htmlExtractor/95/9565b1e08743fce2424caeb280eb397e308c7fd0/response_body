function array_unique(array) {
    var unique = [];
    for ( var i = 0 ; i < array.length ; ++i ) {
        if ( unique.indexOf(array[i]) == -1 )
            unique.push(array[i]);
    }
    return unique;
}
if (!Array.prototype.remove) {
  Array.prototype.remove = function(vals, all) {
    var i, removedItems = [];
    if (!Array.isArray(vals)) vals = [vals];
    for (var j=0;j<vals.length; j++) {
      if (all) {
        for(i = this.length; i--;){
          if (this[i] === vals[j]) removedItems.push(this.splice(i, 1));
        }
      }
      else {
        i = this.indexOf(vals[j]);
        if(i>-1) removedItems.push(this.splice(i, 1));
      }
    }
    return removedItems;
  };
}
$(document).ready(function() {

         $('#stock').keypress(function (e) {
             var key = e.which;
             if(key == 13){
                $(".stockbtn").trigger( "click" );
             }

         });
  
	 $(".newsemail").on('click',function(){
		var newsTitle = $(this).attr('id');
		newsLink =	newsTitle.split('##');
		dataString = '?'+newsLink[0]+'='+newsLink[1];
		reqUrl = url+"general/loadNews"+dataString;
		if (reqUrl !='') {
			$('#analysis-modal').foundation('reveal', 'open');
			$('#load-analysis-content').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
			$.get(reqUrl, {}, function(response){
			if(response) {
					$('#load-analysis-content').html(response);
				}
			});
	  }	

	 });

	 $('#login_form').on('submit', function(e) { //use on if jQuery 1.7+
        e.preventDefault();  //prevent form from submitting
        var data = $("#login_form :input").serializeArray();
        console.log(data); //use the console for debugging, F12 in Chrome, not alerts
    });
    $("#searchTxt").on("click", function () {
	if (delFlag)
	{
	  $("#searchTxt").val('');
	  delFlag =false;
	}
   })
   $("#searchTxt").on("focusout", function () {
	delFlag =true;
   })
   $("#searchTxt").on('input', function () {
      var val = this.value;
      if($('#newsTopics').find('option').filter(function(){
          return this.value.toUpperCase() === val.toUpperCase();        
      }).length) {
         $('#searchfrm').trigger('submit');
      }
   });

   $('#searchfrm').on('submit',function (e) {
		e.preventDefault();
		var data = $("#searchfrm :input").serializeArray();
		dataString = $.param(data);
		reqUrl = url+"general/news?"+dataString;
		if (reqUrl !='') {
			$('#newslist').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
			$.get(reqUrl, {}, function(response){
			if(response) {
					$('#newslist').html(response).fadeIn();
				}
			});
	  }	
	});
	/*$("#news").on("input", function () {
		var options = {};
		options.url = "/general/getlist";
		options.type = "GET";
		options.data = { "criteria": $("#news").val() };
		options.dataType = "json";
		options.success = function (data) {
				//alert(data);
				$("#news").empty();
				for(var i=0;i<data.length;i++)
				{
						$("#newsTopics").append("<option value='" + 
						data[i]+ "'></option>");
				}
		};
		$.ajax(options);
  });*/
	$(".stockbtn").on('click',function(){
		var stock  =  $("#stock").val();
		window.location.href ="https://members.stocktradersdaily.com/reports/focusStock/stock/"+stock
	});

	$(".more .buton").on('click',function(){
		var favorite = new Array();
		var sources  =  $("#sources").val();
		if(sources != '') {
			var arr = new Array();
		        arr = 	sources.split(',');
			for(var i=0; i< arr.length;i++){
				$.each($("input[name='chksource']"), function(){
				  if($(this).val() == arr[i]) {
					$(this).attr("checked",true);
				  }
				});
			}
		}
		$.each($("input[name='chksource']"), function(){
			if($(this).is(":checked")){
				favorite.push($(this).val());
			}	else {
				favorite.remove($(this).val());
			}
		});
		if(favorite.length)	{
			favorite = array_unique(favorite) ;
			var req = favorite.join(",");
			$("#sources").val(req);
		 	var dataString = '';
    			var data = $("#searchfrm :input").serializeArray();
      			dataString = $.param(data);
			/*dataString = '?newsId='+req;*/
			reqUrl = url+"general/news?"+dataString;
			if (reqUrl !='') {
			     $('#newslist').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
			     $.get(reqUrl, {cache: false}, function(response){
				if(response) {
				  //$('.newscls').removeClass('active');
				  $('#newslist').html(response).fadeIn();
				}
			     });
			}	
		}
	});

	$(".newscls").on('click',function() {
		var req = $(this).attr('title');
		/* get the existing and push the new sources*/
		//var favorite = [];
		//var sources  =  $("#sources").val();
		//if(sources != '')	{
		//	favorite =	sources.split(',');
		//}
		$('.small-block-grid-6 li a').removeClass('active');
		$(this).addClass('active')	;
		/* if($(this).hasClass("active")){
			$(this).removeClass('active')	;
			favorite = jQuery.grep(favorite, function(value) {
        		return value != req;
      			});
		} else {
			$(this).addClass('active')	;
			favorite.push(req);
		}
		favorite = array_unique(favorite) ;
		sources  = favorite.join(","); 
		$("#sources").val(sources);		 
		var dataString = '';
		dataString = '?newsId='+sources; */
		var sources  = req;
		$("#sources").val(sources);
		var data = $("#searchfrm :input").serializeArray();
		dataString = $.param(data);
		reqUrl = url+"general/news?"+dataString;
		if (reqUrl !='') {
		     $('#newslist').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
		     $.get(reqUrl, {cache: false}, function(response){
		     if(response) {
			$('#newslist').html(response).fadeIn();
		     }
		   });
	        }	
	});

	$('.find-stocks ul.small-block-grid-2 li a').on('click',function() {
		
		var reqId  = this.id;
		var dataString;
		dataString = '?category='+reqId;
		reqUrl = url+"general/MarketAnalysis"+dataString;
		if (reqUrl !='') {
		        $('#analysis-modal').foundation('reveal', 'open');
			$('#load-analysis-content').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
			$.get(reqUrl, {}, function(response){
			       if(response) {
				$('#load-analysis-content').html(response);
				}
			});
	  }	
	}); 

	$('.sentimentCls').on('click',function() {
		$('#sentiment').foundation('reveal', 'open');;
		var dataString;
		var opt  = this.id.split('##');
		var term = opt[0];
		var basket = opt[1];
		dataString =  '?theterm='+term+'&thebasket='+basket;
		$('#contentData').html('<p align="center" style="font-color:#4783b4;">Loading...</p>').fadeIn();
		$.get(url+'general/sentimentData'+dataString, { }, function(response){
			if(response) {
				$('#contentData').html(response);
			}
		});
	});	
});
