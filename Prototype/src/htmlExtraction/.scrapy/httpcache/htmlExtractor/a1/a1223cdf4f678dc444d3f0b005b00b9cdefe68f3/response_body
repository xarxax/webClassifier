        $(function() {
            $("#ddLForLanguage").change(function() {

                var culture = $(this).val();
                var actionUrl = '/Home/ChangeLanguage?selectedCulture=-0-&amp;url=-1-';
                var url = actionUrl
                    .replace(new RegExp('\\-0\\-', 'gm'), culture)
                    .replace(new RegExp('\\-1\\-', 'gm'));
                window.location.href = url;

            });

        });

            $(document).ready(function() {
                // Set default menu to look "active"
                var activeControllerName = 'Home';
                if (activeControllerName == 'Home') {
                    activeControllerName = 'Home';
                }
                else if (
                    activeControllerName == 'Msds') {
                    activeControllerName = 'Msds';
                }
                else if (activeControllerName == 'Product') {
                    activeControllerName = 'Product';
                }

                else if (activeControllerName == 'Account') {
                    activeControllerName = 'Account';
                }

                $("#" + activeControllerName + "Menu").attr('class', 'active');

            });

    </script>
