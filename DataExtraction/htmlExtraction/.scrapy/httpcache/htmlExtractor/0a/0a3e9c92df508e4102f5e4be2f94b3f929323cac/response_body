        <script>
jQuery(document).ready(function(){

    addOpenNavClass($(window).width());

    $(window).resize(function(){
        addOpenNavClass($(window).width());
    });

    var campsChatBtn = $("#btn-camps-live-chat");
    $("#btn-camps-live-chat").remove();
    $("body").prepend(campsChatBtn);
    $("#btn-camps-live-chat").css({"display":"block"});

    });
    function addOpenNavClass(winWidth) {

    if(winWidth > 979) {
        $('.nav').find(".dropdown-toggle").parent().hover(function(){
                $(this).addClass("open");
            },
            function(){
                $(this).removeClass("open");
            }
        );
    }

    }

    </script>
