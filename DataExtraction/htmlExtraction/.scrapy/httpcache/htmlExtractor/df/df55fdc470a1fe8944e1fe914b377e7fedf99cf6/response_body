}


function AddToBasket(buid, itemid, itemcode, itemname, qty, price, listprice, discpc) {
	var vAddToBURL='/SiteSphereHandler.ashx?action=basketadditem&norecalc=1&basketuid=' + buid + '&itemcode=' + escape(itemcode) + '&itemprice=' + price + '&listprice=' + listprice + '&discpc=' + discpc + '&itemname=' + escape(itemname) + '&itemid=' + itemid + '&itemquantity=' + qty + '&pageurl=' + escape(document.location.href) + "&accountuid=" + escape('') + "&accountdscode=" + escape('') + "&accountcode=" + escape('') + "&currency=GBP";
    AjaxGetSSData(vAddToBURL, (((page_url == "/cart/") || (page_url == "/cart")) ? SSAjaxBasketResultHandler : SSAjaxRefreshMiniBasketHandler) );
}

function AddToBasketNow(buid, id, code, name, qty, price, listprice, discpc) {
    AjaxGetSSData('/SiteSphereHandler.ashx?action=basketadditem&norecalc=1&basketuid=' + buid + '&itemcode=' + escape(code) + '&itemprice=' + price + '&listprice=' + listprice + '&discpc=' + discpc + '&itemname=' + escape(name) + '&itemid=' + id + '&itemquantity=' + qty + '&pageurl=' + escape(document.location.href) + "&accountuid=" + escape('') + "&accountdscode=" + escape('') + "&accountcode=" + escape('') + "&currency=GBP", 
    	(((page_url == "/cart/") || (page_url == "/cart")) ? SSAjaxBasketResultHandler : SSAjaxRefreshMiniBasketHandler));
}

