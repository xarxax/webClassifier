<script language="javascript" type="text/javascript">
	var speedx=1 ;
	var speed=40 ;
	var upordown=2;
	var MyMar;
	
// ready start
	window.readyHandlers = [];
	window.ready = function ready(handler) {
	  window.readyHandlers.push(handler);
	  handleState();
	};

	window.handleState = function handleState () {
	  if (['interactive', 'complete'].indexOf(document.readyState) > -1) {
	    while(window.readyHandlers.length > 0) {
	      (window.readyHandlers.shift())();
	    }
	  }
	};

	document.onreadystatechange = window.handleState;
	ready(function () {
	  downshow();
	});
	// ready end

	document.getElementById("pic2").innerHTML=document.getElementById("pic1").innerHTML 
	document.getElementById("pic").scrollLeft=document.getElementById("pic").scrollWidth 
	function Marquee(){ 
		if(document.getElementById("pic").scrollLeft<=0) 
			document.getElementById("pic").scrollLeft+=document.getElementById("pic2").offsetWidth 
		else{ 
			document.getElementById("pic").scrollLeft-- 
		} 
	} 
	var speed3=40;//速度數值越大速度越慢
	document.getElementById("pic2").innerHTML=document.getElementById("pic1").innerHTML 
	function Marquee2(){ 
		if(document.getElementById("pic2").offsetWidth-document.getElementById("pic").scrollLeft<=0) 
			document.getElementById("pic").scrollLeft-=document.getElementById("pic1").offsetWidth 
		else{ 
			document.getElementById("pic").scrollLeft++ 
		} 
	} 
	upshow(); 
	document.getElementById("pic").onmouseover=function() 
	{
		clearInterval(MyMar); 
	} 
	document.getElementById("pic").onmouseout=function() 
	{
		if(upordown==1)
		{
			upordown=2;
			downshow();
		}
		else
		{
			upordown=2;
			upshow();
		}
	}
	function upshow()
	{
		if(upordown==0)
		{
			clearInterval(MyMar);MyMar=setInterval(Marquee2,speedx)
		}
		else
		{
			upordown=0;
			clearInterval(MyMar);MyMar=setInterval(Marquee2,speed3)
		}
	}
	function downshow()
	{
		if(upordown==1)
		{
			clearInterval(MyMar);MyMar=setInterval(Marquee,speedx)
		}
		else
		{
			upordown=1;
			clearInterval(MyMar);MyMar=setInterval(Marquee,speed)
		}
	}
</script>
