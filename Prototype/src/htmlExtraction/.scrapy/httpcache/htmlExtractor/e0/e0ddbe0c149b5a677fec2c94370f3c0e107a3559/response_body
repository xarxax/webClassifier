		var myclose = false;

		function ConfirmClose()
		{
			if (event.clientY < 0)
			{
				
				myclose=true;
			}
		}

		function HandleOnClose()
		{
			if (myclose==true) 
			window.open('pop.html','POP','width=560,height=405,scrollbars=no,resizable=no',0,0);
		}

	</script>

<script language="JavaScript">
	function show()
	{
  	window.open('pop.html','POP','width=560,height=405,scrollbars=no,resizable=no',0,0);
	}
