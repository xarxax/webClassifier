<script language=javascript>
this.id = "chi";
selLoc = parent.location.hash;
var selPage = "0";
function actElement(pee, el) {
    clearElement(pee);
    pee.style.color = "#C9302B";
    if (flashI.window.chi) flashI.window.chi.SetVariable("/:jsElement", el);

}
function clearElement(pee) {
    pee.style.color = "#000000";
    if (flashI.window.chi)  flashI.window.chi.SetVariable("/:jsClear", "1");

}
function  setScroll(scrollie) {
  if (!scrollie) fS = flashI.document.body.style;
  else fS = scrollie.style;
  fS.scrollbarFaceColor = "ffffff";
  fS.scrollbarShadowColor = "ffffff"; 
  fS.scrollbarDarkShadowColor = "C9302B"; 
  fS.scrollbarHighlightColor = "ffffff"; 
  fS.scrollbar3dlightColor = "DFEBF2"; 
  fS.scrollbarDarkshadowColor = "FFECCF"; 
  fS.scrollbarTrackColor = "FFECCF"; 
  fS.scrollbarArrowColor = "C9302B";
  fS.overflow = "auto";
}

function goto(page, barOnly) {
  if (page != selPage) {
    switch (page) {
      case "0":
        flashI.location = "./flash_iframe.html";
        kav.style.visibility = "hidden";
        selPage = "0";
        break;
      case "1":
        flashI.location = "./about.html";
        kav.style.top = about.offsetTop + 100;
        kav.style.visibility = "visible";
        break;
      case "2":
        flashI.location = "./medicine.html";
        kav.style.top = medicine.offsetTop + 100;
        kav.style.visibility = "visible";
        break;
      case "3":
        flashI.location = "./products.html";
        kav.style.top = products.offsetTop + 100;
        kav.style.visibility = "visible";
        break;
      case "4":
        flashI.location = "./test.html";
        kav.style.top = test.offsetTop + 100;
        kav.style.visibility = "visible";
        break;
      case "5":
        flashI.location = "./contact.html";
        kav.style.top = contact.offsetTop + 100;
        kav.style.visibility = "visible";
        break;
      case "6":
        flashI.location = "./faq.html";
        kav.style.visibility = "hidden";
        break;
      case "7":
        flashI.location = "./members.php";
        kav.style.visibility = "hidden";
        break;
      case "8":
        flashI.location = "./links.html";
        kav.style.visibility = "hidden";
        break;
      case "9":
        if (!barOnly) flashI.location = "./disclaimer.html";
        kav.style.visibility = "hidden";
        break;
      case "10":
        flashI.location = "./privacy.html";
        kav.style.visibility = "hidden";
        break;
    }
    selPage = page;
  }
}

function insertSeperator() {
  for (i=0;i<3;i++) {
    flashI.document.write('<hr color=#C9302B style="');
    if (Math.random() > 0.5) flashI.document.write('border-style:dashed;');
    flashI.document.write('position:relative;top:-'+(12*i)+';" width=10>');
  }
}

function checkInner() {
	if (selLoc != "") {
		goto(selLoc.substring(1));
	}
}
</script>
