        $( "#id-analytics-3" ).addClass( "div-services-name1" );
        $( "#id-analytics-4" ).removeClass( "side-nav-link2" );
    });
    
    $("#id-public-relations-4").mouseover(function(){
        $( "#id-public-relations-3" ).removeClass( "div-services-name1" );
        $( "#id-public-relations-3" ).addClass( "div-services-name2" );
        $( "#id-public-relations-4" ).addClass( "side-nav-link2" );
    });
    $("#id-public-relations-4").mouseout(function(){
        $( "#id-public-relations-3" ).removeClass( "div-services-name2" );
        $( "#id-public-relations-3" ).addClass( "div-services-name1" );
        $( "#id-public-relations-4" ).removeClass( "side-nav-link2" );
    });
    
    $("#id-social-media-4").mouseover(function(){
        $( "#id-social-media-3" ).removeClass( "div-services-name1" );
        $( "#id-social-media-3" ).addClass( "div-services-name2" );
        $( "#id-social-media-4" ).addClass( "side-nav-link2" );
    });
    $("#id-social-media-4").mouseout(function(){
        $( "#id-social-media-3" ).removeClass( "div-services-name2" );
        $( "#id-social-media-3" ).addClass( "div-services-name1" );
        $( "#id-social-media-4" ).removeClass( "side-nav-link2" );
    });
    
    $("#id-email-marketing-4").mouseover(function(){
        $( "#id-email-marketing-3" ).removeClass( "div-services-name1" );
        $( "#id-email-marketing-3" ).addClass( "div-services-name2" );
        $( "#id-email-marketing-4" ).addClass( "side-nav-link2" );
    });
