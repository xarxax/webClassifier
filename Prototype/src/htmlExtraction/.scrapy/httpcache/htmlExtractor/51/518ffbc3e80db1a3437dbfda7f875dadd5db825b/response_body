	<div class="header__drawer-dimmer header__drawer-dimmer--hide"></div>

	<a href="https://www.narescue.com/" title="North American Rescue logo" class="header__logo"></a>


	<div class="searchForm header__search-area" id="search_mini_form" action="https://www.narescue.com/catalogsearch/result/">
	 <!--  <input id="search" type="text" name="" value="" class="input-text required-entry header__search-field" maxlength="" placeholder="Search our rescue products" />-->
	  
		<input name="q" id="searchInput" type="text" value="" class="input-text required-entry header__search-field" placeholder="Search our rescue products" autocomplete="off"/>
		<i class="icon-magnifying-glass header__search-icon"></i>
		<button class="header__search-go-btn">GO</button>
		<!--  <div id="resultsContainer" class="search-autocomplete" style="display: none; background-color: white; padding:10px;">
		<table id="resultsTable"></table>
		</div> -->

		<div id="resultsContainer" class="tt-dropdown-menu" style="display:none;">
			<div id="resultsTable" class="tt-dataset-a1"></div>
			<!-- <div class="tt-dataset-a2"></div>
