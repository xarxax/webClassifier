    document.location.href = "https://jabscoshop.com/checkout/default.aspx?brid=6&sitesphere-basket=4c2551b6-cff0-4345-b5cd-e60a9b665693&sitesphere-visitor=684afe7c-3f67-4c7a-898b-4e2b3e625471&uuid=684afe7c-3f67-4c7a-898b-4e2b3e625471";
}


