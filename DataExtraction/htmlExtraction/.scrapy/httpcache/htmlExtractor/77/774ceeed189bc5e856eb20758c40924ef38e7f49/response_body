<script>
    jQuery(function($){
        var calloutWidth = 0,
            $callouts = $('#home-callouts li');
        $callouts.each(function(){
            calloutWidth += $(this).width();
        });
        $callouts.css('margin-right', (980-calloutWidth)/($callouts.length-1));
        $callouts.last().css('margin', 0);
    });
</script>

