<script language="javascript" type="text/javascript">
    function getDimensions($img) {
        var windowHeight = $(window).height(),
            width = windowHeight / ($img.attr('data-height') / $img.attr('data-width')),
            documentWidth = $(document).width(),					    
            percentage = windowHeight / width,
            maxWidth;

        if (documentWidth >= width) {
            maxWidth = width;
        } else {
            maxWidth = windowHeight / percentage;
        }
                    
        var newHeight = percentage * maxWidth;                    
        var obj = {};
        obj.w = maxWidth;
        obj.h = newHeight;
        return obj;
    }

    function indexOfImg($children, value) {
        var index = -1;
        for (var i = 0; i < $children.length; i++) {
            var dataUrl = $($children[i]).attr('data-url');
            if (value === dataUrl) {
                index = i;
                return index;
            }
        }
        return index;
    }

    function doRollOver(value) {
        
        var $maxImage = $('#ctl00_ctl00_maxImage'),
            $children = $('#ctl00_ctl00_siteImageBg').children(),
            index = indexOfImg($children, value),
            $img = index >= 0 ? $($children[index]) : null;

        if (!$maxImage || !$img) {
            return;
        }

        var d = getDimensions($img);
        $img.width(d.w).height(d.h);
        $img.show();

        $maxImage.hide();
         
    }

    function doRollOut(value) {
        
        var $maxImage = $('#ctl00_ctl00_maxImage'),
            $children = $('#ctl00_ctl00_siteImageBg').children(),
            index = indexOfImg($children, value),
            $img = index >= 0 ? $($children[index]) : null;

        if (!$maxImage || !$img) {
            return;
        }

        $img.hide();
        $maxImage.show();
        
    }    

    function initMaxImage() {
        var $horizon = $('#horizon'),
            elementPosition = $horizon.offset();
        $(window).scroll(function () {
            if ($(window).scrollTop() > elementPosition.top) {
                $horizon.css('position', 'fixed').css('top', '0');
            } else {
                $horizon.css('position', 'static');
            }
        });
        
        var $maxImage = $('#ctl00_ctl00_maxImage');
        if (!$maxImage) {
            return;
        }

        $maxImage.maximage({
            cycleOptions: {
                fx: 'fade', speedId: 2000, speedOut: 2000, timeout: 5000, sync: 1
            }
        });
        
    }

    $(document).ready(function() {
        initMaxImage();
    });
</script>                  
        
