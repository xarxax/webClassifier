var flashvars = {};
var params = {scale:"noscale", salign:"lt", menu:"false",  allowfullscreen:"true", allowscriptaccess:"always", wmode:"transparent"};
var attributes = {};
swfobject.embedSWF("/skin/frontend/default/seiko/media/main.swf?" + new Date().getTime(), "swf-content", "950", "300", "10.0.0", "/common/util/expressInstall.swf", flashvars, params, attributes);
function mycarousel_initCallback(carousel) {
carousel.buttonNext.bind('click', function(){ carousel.startAuto(); });
carousel.buttonPrev.bind('click', function(){ carousel.startAuto(); });
carousel.clip.hover(function(){ carousel.stopAuto(); }, function(){ carousel.startAuto(); });
};
$(document).ready(function() {
$('#mycarousel').jcarousel({ auto:12, wrap:'circular', scroll:1, animation:1200, easing:'easeOutExpo', initCallback:mycarousel_initCallback });
$('.imager').attr('href',$('#myGallery .active').attr('alt'));
});
</script>


<!-- new carousel for new product-->
<script>
    function swapImages(){
      var $active = $('#myGallery .active');
      var $next = ($('#myGallery .active').next().length > 0) ? $('#myGallery .active').next() : $('#myGallery img:first');
      $active.fadeOut(function(){
      $active.removeClass('active');
      $next.fadeIn().addClass('active');
      $('.imager').attr('href',$('#myGallery .active').attr('alt'));
      });
    }

    $(document).ready(function(){
      // Run our swapImages() function every 5secs
      setInterval('swapImages()', 5000);
    });
</script>




