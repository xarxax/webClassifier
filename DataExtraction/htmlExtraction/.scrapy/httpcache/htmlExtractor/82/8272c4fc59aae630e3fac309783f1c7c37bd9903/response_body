$(function() {
	
	$("#formPortfolio input").each(function(){
		val = $(this).val();
		if (val != "") {
			$dropdownValue = $(".portfolio .dropdown-menu a[data-value='"+val+"']");		
			$dropdownValue.parents("div.dropdown").find("a.dropdown-toggle").html($dropdownValue.html()+"<span class=\"arrow\"></span>");
		}
	});
	$(".portfolio .dropdown-menu a").click(function(){
		filterName = $(this).parents("ul").attr("aria-labelledby");
		$(this).parents("div.dropdown").find("a.dropdown-toggle").html($(this).html()+"<span class=\"arrow\"></span>");
		filterValue = $(this).attr("data-value");
		$("input[name='"+filterName+"']").val(filterValue);
		$("#formPortfolio").submit();
	});
	$('.portfolio label.checkbox').click(function(e){
		e.preventDefault();
		filterValue = $(this).children().first().attr("data-value");
		var isChecked = $("#check"+filterValue).prop('checked');
		if (isChecked){
			$("#check"+filterValue).prop('checked', false);
		} else {
			$("#check"+filterValue).prop('checked', true);
		}
		$("#formPortfolio").submit();
	});
	
});
