<script>
function init() {
var imgDefer = document.getElementsByTagName('img');
for (var z=0; z<imgDefer.length; z++) {
if(imgDefer[z].getAttribute('data-src')) {
imgDefer[z].setAttribute('src',imgDefer[z].getAttribute('data-src'));
} } }

window.onload = init;

</script>

	
