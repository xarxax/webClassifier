        $(document).ready(function () {
            $('.show-login').click(function (e) {
                $('.login-box').hide();
                $($(this).attr('href')).show();
            });

            $('.close-login').click(function (e) {
                $(this).closest('.login-box').hide();
            });

            $('#payrollLoginSelect').change(function (e) {
                $('#payrollLoginBtn').removeClass('disabled');
            });

            $('.nav-expander').click(function (e) {
                $('.top-nav-outer').toggle();
            });

            $('.notification-close').click(function(e) {
                $('.notification-bar').hide();
            });

            if (!document.implementation.hasFeature('http://www.w3.org/TR/SVG11/feature#Image', '1.1')) {
                $('html').addClass('no-svg');
            }
            
        });

        function goSearch() {
            var ok = true;
            if ($.trim($("#SearchQuery").val() ).length == 0){
                ok = false;
            }
            return ok;
        }

    </script>
