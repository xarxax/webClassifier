    {{ if (summary.totalResult > 0) { }}
        {{ var shownResults = items.length; }}
        <strong>
            Showing {{=shownResults}} of <a href="#" class="b js-show-more-link">{{=summary.totalResult}} results</a></strong>
    {{ } }}
</script>
