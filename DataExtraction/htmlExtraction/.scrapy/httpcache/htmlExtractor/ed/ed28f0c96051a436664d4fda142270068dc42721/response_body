    <script type="text/javascript" src="/Scripts/slick.min.js"></script>
    <script>
        // Adds focus styles to currently selected accordion
        $('.panel-default').on('show.bs.collapse', function () {
            $(this).addClass('active');
        });

        $('.panel-default').on('hide.bs.collapse', function () {
            $(this).removeClass('active');
        });

        noWrap(".panel-title .collapsed");

        // Wraps element inner html with span tag which has a class that prevents
        // word wrapping. This is used to prevent the caret icon from becoming an orphan.
        function noWrap(elementSelector) {
            $(elementSelector).each(function() {
                var innerHtml = $(this).html();
                var subString = innerHtml.match(/\S+\s<span class=\"caret\"><\/span>/g);
                var newString = '<span class="no-wrap">' + subString + '</span>';
                var result = innerHtml.replace(/\S+\s<span class=\"caret\"><\/span>/g, newString);
                $(this).html(result);
            });            
        }
    </script>    
    <script>
        $(document).on('ready', function () {
            $(".survey-slider").slick({
                infinite: true,
                slidesToShow: 5,
                slidesToScroll: 4,
                autoplay: true,
                autoplaySpeed: 6000,
                responsive: [
                    {
                        breakpoint: 1275,
                        settings: {
                            slidesToShow: 4,
                            slidesToScroll: 4,
                            infinite: true
                        }
                    },
                    {
                        breakpoint: 1159,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            infinite: true
                        }
                    },
                    {
                        breakpoint: 920,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            infinite: true
                        }
                    }
                ]
            });
        });
    </script>
    <script>
    // Adds current page styles to corresponding nav bar link.
    $(".navbar-left li:contains('Home')").addClass("current-page");
    </script>
