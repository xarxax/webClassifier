<script type="text/javascript">
    $(document).ready(function() {
        setTimeout(function() {
            var expandMenu = $('#expand-menu .menu');
            if(expandMenu.length <=0) {
                return;
            }
            var currentMenuItem;
            // have current is in sub menu
            currentMenuItem =  expandMenu.find('ul li.current');
            if(currentMenuItem.length > 0) {
                currentMenuItem.prepend('<img alt="" src="../Media/UnitedTexasCorp/Image/arrow-left-icon.png">');
            }
            else {
                currentMenuItem = expandMenu.children('li.current');
                currentMenuItem.prepend('<img alt="" src="../Media/UnitedTexasCorp/Image/arrow-down-icon.png">');
            }
        }, 1000); 
     
        if(typeof($("#main-menu")) != 'undefined') {
            $("#main-menu").show();
			
            $("#main-menu ul").addClass('sf-menu');
			
            $("#main-menu ul").addClass("sf-navbar");
            $("#main-menu ul").superfish({
                    delay: 200,
                    speed: 'fast',
                    autoArrows: 1,
                    dropShadows: 1
                });
        }
     
    });
</script>

