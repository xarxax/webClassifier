<script type="text/javascript">
    $(document).ready(function () {
        var pathname = window.location.pathname.substring(0, window.location.pathname.length - 1); //remove last slash
        if (pathname.length = 0) {
            $("#selected").removeAttr("id");
            $('[name="divMain"]').attr("id", "selected");
        }
        if (pathname.search('Store') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divStore"]').attr("id", "selected");
        }
        if (pathname.search('Articles') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divArticles"]').attr("id", "selected");
        }
        if (pathname.search('Offers') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divOffers"]').attr("id", "selected");
        }
        if (pathname.search('News') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divNews"]').attr("id", "selected");
        }
        if (pathname.search('Contact') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divContact"]').attr("id", "selected");
        }
        if (pathname.search('About') > 0) {
            $("#selected").removeAttr("id");
            $('[name="divAbout"]').attr("id", "selected");
        }
    });
</script>
