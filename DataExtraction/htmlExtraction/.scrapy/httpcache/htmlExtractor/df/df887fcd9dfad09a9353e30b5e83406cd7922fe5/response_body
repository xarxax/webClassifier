);
NewImg3 = new Array (
"_images/App_11.gif",
"_images/App_4.gif",
"_images/App_3.gif",
"_images/App_2.gif"
);
var ImgNum = 0;
var ImgLength = NewImg.length - 1;

//Time delay between Slides in milliseconds
var delay = 4000;

var lock = true;
var run;
function chgImg(direction) {
if (document.images) {
ImgNum = ImgNum + direction;
if (ImgNum > ImgLength) {
ImgNum = 0;
}
if (ImgNum < 0) {
ImgNum = ImgLength;
}
document.slideshow.src = NewImg[ImgNum]; document.slideshow1.src = NewImg1[ImgNum]; document.slideshow2.src = NewImg2[ImgNum]; document.slideshow3.src = NewImg3[ImgNum];
   }
}
function auto() {
if (lock == true) {
lock = false;
window.clearInterval(run);
}
else if (lock == false) {
lock = true;
run = setInterval("chgImg(1)", delay);
   }
}
//  End -->
