    changeMainimage("linkimage340", "HidingBehindWall" + sa0, a0, a1, topMargin);
    changeMainimage("linkimage341", "IceCave" + sa0, a2, a1, topMargin);
    changeMainimage("linkimage342", "KatlynAndKyleInFrontFalls" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage347", "PanningOutConcetratedSap" + sa0, a0, a1, topMargin);
    changeMainimage("linkimage351", "SocksOnTable" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage332", "BoilingDownInSmaller" + sa1, a1, a0, topMargin);
    changeMainimage("linkimage355", "Thermometer" + sa0, a0, a1, topMargin);
    changeMainimage("linkimage337", "FilteringIntoBucket" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage352", "SocksOnTable" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage348", "PotOnStove" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage334", "BottlesOfSyrup" + sa0, a2, a1, topMargin);

    theRules[0].style.margin = topMargin + "px " + fullMargin + "px 0px 0px";
    theRules[1].style.margin = topMargin + "px " + onePortraitMargin + "px 0px 0px";
    theRules[2].style.margin = topMargin + "px " + oneLandscapeMargin + "px 0px 0px";
    theRules[3].style.margin = topMargin + "px " + twoPortraitMargin + "px 0px 0px";
    theRules[4].style.fontSize = fontSize + "%";
    theRules[4].style.margin = topMargin + "px " + tableCellMargin + "px 0px 0px";
    theRules[5].style.fontSize = fontSize + "%";
    theRules[6].style.fontSize = (fontSize * 1.15) + "%";

}

window.onresize = resizeMainimageForBrowserSize;


