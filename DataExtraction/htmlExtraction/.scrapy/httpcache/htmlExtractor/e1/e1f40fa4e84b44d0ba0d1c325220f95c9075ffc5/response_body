var pcs_shopstate=0;
function pcs_addtocart(prodid,priceid,quantity)
{
	if (pcs_shopstate==0)
	{
		var rnd = Math.round(Math.random()*10000);
		var url = "/updateCart.php?prodid="+escape(prodid) +"&priceid="+escape(priceid)+"&quantity="+escape(quantity)+"&abasketid=3ed568084ed5239464303ee4900511801548720301&rand="+rnd;
		xmlHttp.open("GET", url, true);
		xmlHttp.onreadystatechange = pcs_updatecart;
		xmlHttp.send(null);
				pcs_shopstate=1;
	}
	return false;
}	
function pcs_updatecart() {
  if (xmlHttp.readyState == 4) {
    var response = xmlHttp.responseText;
		pcs_shopstate=0;
		
				document.getElementById("basket").innerHTML = response;
							document.getElementById("basket").style.display = "block";
			var ScrollTop = document.body.scrollTop;
			if (ScrollTop == 0)
			{
					if (window.pageYOffset)
							ScrollTop = window.pageYOffset;
					else
							ScrollTop = (document.body.parentElement) ? document.body.parentElement.scrollTop : 0;
			}
			document.getElementById("basket").style.top = ScrollTop+'px';
		  }
}
