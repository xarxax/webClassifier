				<script type='text/javascript'>
				function init_map(){
					var myOptions = {
						zoom:16,
						center:new google.maps.LatLng(25.031211,121.51019100000008),
						mapTypeId: google.maps.MapTypeId.ROADMAP
					};
					map = new google.maps.Map(document.getElementById('gmap_canvas'), myOptions);
					marker = new google.maps.Marker({
						map: map,
						position: new google.maps.LatLng(25.031211,121.51019100000008)
					});
					
					infowindow = new google.maps.InfoWindow({
						content:'<strong>農委會林業試驗所</strong><br>100台北市中正區南海路53號',
					});
					
					google.maps.event.addListener(marker, 'click', function(){
						infowindow.open(map,marker);
					});
					
					
					//加tabindex
					google.maps.event.addListener(map, "tilesloaded", function(){
						[].slice.apply(document.querySelectorAll('#gmap_canvas a')).forEach(function(item) { 
						   item.setAttribute('tabindex','-1'); 
						});
						
						[].slice.apply(document.querySelectorAll('.gm-style-mtc div')).forEach(function(item) { 
						   item.setAttribute('tabindex','26'); 
						});
						
						[].slice.apply(document.querySelectorAll('.gm-style button')).forEach(function(item) { 
						   item.setAttribute('tabindex','27'); 
						});
						
						[].slice.apply(document.querySelectorAll('.gmnoprint div button')).forEach(function(item) { 
						   item.setAttribute('tabindex','27'); 
						});
					})
					
					infowindow.open(map,marker);
				}
					
				google.maps.event.addDomListener(window, 'load', init_map);
				</script>
				<div class="google_map" style='overflow:hidden;height:400px;width:100%;'>
					<div id='gmap_canvas' tabinde"26" style='height:400px;width:100%;'></div>
					<style>#gmap_canvas img{max-width:none!important;background:none!important}</style>
				</div> 
				<div class='sr-only'><a href='https://mapgenerator.org/'>Lageplan erstellen</a></div>
				 
			
