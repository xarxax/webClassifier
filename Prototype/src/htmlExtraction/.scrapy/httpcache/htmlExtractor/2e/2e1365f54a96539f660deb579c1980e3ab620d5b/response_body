var schermbreedte = getBrowser("innerW")

var max_breedte = 1400
var wit_ruimte = 100

function laadpagina()

{	breedte = getBrowser("innerW") - wit_ruimte

	if (breedte < 900)

{document.getElementById("container_normaal").style.display="none";
 document.getElementById("container_mobiel").style.display="block";}

 else

{	if (breedte > max_breedte - wit_ruimte)
	{breedte = max_breedte - wit_ruimte}

document.getElementById("logo").style.width = breedte + "px";
document.getElementById("slogan").style.width = breedte + "px";
