<script language='JavaScript'> 
<!--풍선 도움말 
function bhelp(form, show) { 
   if (show) { 
      form.style.pixelLeft = event.x -5; //이벤트가 발생된 가로축
      form.style.pixelTop = event.y +20 + document.body.scrollTop; //이벤트가 발생된 세로축
      form.style.visibility = 'visible'; //객체를 보여줍니다.
      //alert(document.body.scrollTop);
      
   } 
   else form.style.visibility = 'hidden'; //false인 경우 객체를 보이지 않게 합니다.
} 
//스크립트 끝--> 
</script> 
