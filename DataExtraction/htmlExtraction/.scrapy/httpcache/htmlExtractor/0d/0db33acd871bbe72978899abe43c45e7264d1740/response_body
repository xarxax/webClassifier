<script  type="text/javascript">
function getCookie(name) {
	if(document.cookie.length > 0) {
		start = document.cookie.indexOf(name);
		if(start == -1) return "";
		start = start + name.length;
		end = document.cookie.indexOf(";", start);
		if(end == -1) end = document.cookie.length;
		return unescape(document.cookie.substring(start,end));
	}
	else return "";
}
function popup_winopen(width,height,top,left,popupid) {
	var pop_uri = "popup/popup_view.php?popupid="+popupid+"&width="+width+"&height="+height+"&ptop="+top+"&pleft="+left;
	height2=parseInt(height)+40;
	window.open(pop_uri,"pop"+popupid,"toolbar=no, location=0, status=0, menubar=0, scrollbars=yes,top=0,left="+left+",width="+width+", height="+height2+", resizable=0");
}
</script>


