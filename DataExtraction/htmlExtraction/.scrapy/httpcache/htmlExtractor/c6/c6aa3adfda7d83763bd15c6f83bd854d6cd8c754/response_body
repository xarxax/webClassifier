<script>
jQuery(document).ready(function($){
	$('.homeslider').bxSlider({
		infiniteLoop: false,
		hideControlOnEnd: true,
		pager: false,
		captions: true
	});
	$('.scrollDown').click(function() {
		$('html,body').animate({ scrollTop: $(this).offset().top }, 'slow');
	});

	var $grid = $('.grid').masonry({
		itemSelector: '.grid-item',
		columnWidth: 290,
		isFitWidth: true,
		gutter: 10
	});
	// manually trigger initial layout
	$grid.masonry();

	$('.btn-restaurant').click(function(event) {
		event.preventDefault();
		$('.btn-restaurant').removeClass('active');
		$(this).addClass('active');
		var linkID = $(this).attr('id');
		var boxID = '.gItem-'+linkID;
		$('#masonWrap').slideUp('fast', function() {
			// Animation complete.
			$('.grid-item').hide();
			$(boxID).show();
			$grid.masonry('layout');
		});
		$('#masonWrap').show(function() {
			// Animation complete.
		});
	});

	
});
</script>


