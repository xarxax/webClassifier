var vDistributorSearchPartID="";
var vDistributorSearchPartState='';
function AjaxGetDistributorLocator() 
{ 
    if ($("#"+vDistributorSearchPartID) != null)
    {
	    $("#"+vDistributorSearchPartID).html(vAjaxLoadingImage);
	    var ctryindx = document.getElementById("destination-dropdown").selectedIndex;
	    var ctry = document.getElementById("destination-dropdown").options[ctryindx].text;
	    if ((String(ctry).toLowerCase() != "united states of america") && (String(ctry).toLowerCase() != "usa"))
	    {
		vDistributorSearchPartState="";
	    }
	    
	    //alert(ctry);
	    var urlin = '/SiteSphereHandler.ashx?query=Store7 Distributor Search&brandid=1&country='+ctry+'&state='+vDistributorSearchPartState;
		$.ajax( {
		    cache:true,
		    url:urlin,
		    error: function(request, error) { if(typeof console == 'object') { console.log('AjaxGetDistributorLocator ajax error: ' + error); } },
		    success: function(rdata) {
		    
			    if (rdata.indexOf('Error in SiteSphere') == -1)
			    {
				$("#"+vDistributorSearchPartID).html(rdata);
			    }
			    else
			    {
			    	$("#"+vDistributorSearchPartID).html("");
			    }
		    }
		  })
    }
}

var tmrTabMenu;

function hideTabMenuNow()
{
	$("#tabddmenu").hide();
}


function hideTabMenu()
{
	tmrTabMenu = setTimeout( 'hideTabMenuNow();' , 500);
	
}

function showTabMenu(tabId, fromobj)
{
	$("#tabddmenu").hide();
	if (!($.browser.msie && (parseFloat($.browser.version) < 8))) {
	
		$.ajax( {
		    cache:true,
		    url:'/SiteSphereHandler.ashx?query=store7%20Tab%20Menu%20Dropdown&menuid=' + escape(tabId),
		    error: function(request, error) { if(typeof console == 'object') { console.log('showTabMenu ajax error: ' + error); } },
		    success: function(rdata) {
			if (rdata.length == 0) 
			{ 
				$("#tabddmenu").hide(); 
			} else {
				$("#suggestions").hide(); 
				$("#search-suggestions-results").empty();
				$("#search-suggestions-results").removeClass("loadingminibasket");
				$("#suggestions").removeClass("loadingminibasket");


				$("#tabddmenu").css('left', $("#"+fromobj.id).position().left+$("#"+fromobj.id).parent().position().left);
				$("#tabddmenu").html(rdata.replace(/&amp;/, "&").replace(/&amp;/, "&"));
				$("#tabddmenu").show();
				$("#tabddmenu").css('height', $("#tabmenucontents").height()+10);
			}
		    }
		});
	}
}

       
var typingTimer;
var doneTypingInterval=350;

function doneTyping()
{
    var search; 
    search = $("#q").val();
    hideTabMenuNow();

    if (!($.browser.msie && (parseFloat($.browser.version) < 8))) {

	    if ((search.length > 0) && (search != "Search by part or keyword"))
	    { 
		$("#suggestions").addClass("loadingminibasket");
		$("#search-suggestions-results").addClass("loadingminibasket");
		$.ajax( {
		    cache:true,
		    url:'/SiteSphereHandler.ashx?query=store7%20search%20suggestions&searchphrase=' + escape(search) + '&fullsearchresult=false',
		    error: function(request, error) { if(typeof console == 'object') { console.log('search keyup ajax error: ' + error); } },
		    success: function(rdata) {
		    	hideTabMenuNow();
			if (rdata.length == 0) 
			{ 
				$("#suggestions").hide(); 
				$("#search-suggestions-results").empty();
				$("#search-suggestions-results").removeClass("loadingminibasket");
				$("#suggestions").removeClass("loadingminibasket");
			} else {
				
				$("#suggestions").show(); 
				$("#suggestions").html(rdata.replace(/&amp;/, "&").replace(/&amp;/, "&"));
				$("#suggestions").removeClass("loadingminibasket");
				$("#search-suggestions-results").removeClass("loadingminibasket");
			}
		    }
		});
	    } 
	    else 
	    { 
		$("#suggestions").hide(); 
		$("#search-suggestions-results").empty();
		$("#search-suggestions-results").removeClass("loadingminibasket");
		$("#suggestions").removeClass("loadingminibasket");
	    } 
    }
}


$(document).ready(function() 
	{	
		$("#q").keyup(function() 
		{ 
			typingTimer = setTimeout(doneTyping, doneTypingInterval);
			$("#search-suggestions-results").addClass("loadingminibasket");
			$("#suggestions").addClass("loadingminibasket");

			$("#search-suggestions-results").addClass("loadingminibasket");
		});
		$("#q").keydown(function () 
		{
			clearTimeout(typingTimer);
			//$("#search-suggestions-results").removeClass("loadingminibasket");
        	});
	});	

function IntOptAutoSetup()
{
	$.ajax( {
	    cache:true,
	    dataType: "xml",
	    url:'/SiteSphere/handlers/ip2location/default.ashx',
	    error: function(request, error) { if(typeof console == 'object') { console.log('IntOptAutoSetup ajax error: ' + error); } },
	    success: function(rdata) {
	    if (rdata != null) {
		    var ccde = $(rdata).find('countrycode').text();
		    if (ccde != null)
		    {
			if (ccde.length == 2)
			{
				//alert(ccde); 
				$("#right-column-international-options-choice").show("slow");
				$("#right-column-international-options-selected").hide("slow");
				
				$.ajax({
					  url: '/SiteSphereHandler.ashx?query=Store7%20Country%20Options',
					  success: function( data ) {
						$("#destination-dropdown").html(data);
						$("#invoice-dropdown").html(data);

						$("#destination-dropdown option[isocode='"+ccde+"']:first").attr("selected","selected");
						$("#invoice-dropdown option[isocode='"+ccde+"']:first").attr("selected","selected");
						$("#different-country").attr("checked", true);
						IntOptSelectDefaultCurrency();

						var vctr = document.getElementById('destination-dropdown').options[document.getElementById('destination-dropdown').selectedIndex].value;
						var vctr2 = document.getElementById('invoice-dropdown').options[document.getElementById('invoice-dropdown').selectedIndex].value;
						var vSame=$("#different-country").is(':checked');
						if (vSame) { vctr2=vctr; }
						var ccyc = document.getElementById('currency-dropdown').options[document.getElementById('currency-dropdown').selectedIndex].value;

						SetBasketCurrencyCountries(ccyc,vctr, vctr2);
					  }
				});
				
			}
		    }
	    	}
	    }
	  })
}

function showIntOpts()
{
	$("#right-column-international-options-choice").show("slow");
	populateIntOptCountryDropdowns();
	$("#right-column-international-options-selected").hide("slow");
}

function IntOptSelectDefaultCurrency()
{
	var vDest = $("#destination-dropdown option:selected").attr("defaultccy");
	var vInv = $("#invoice-dropdown option:selected").attr("defaultccy");
	//alert("Selected dest currency:" + vDest);
	//alert("Selected inv currency:" + vInv);
	
	if ($("#different-country").attr("checked") != "checked")
	{
		$("#currency-dropdown").val(vInv);
	}
	else
	{
		$("#currency-dropdown").val(vDest);
	}
}

function rvPopulateList(divid) {
    if (rvGetCount() == 0) {
        $('#' + divid).hide();
    }
    else {
        var vRVMax = 3;
        var newrvlist = rvGetList(vRVMax);
        
	$.ajax({
	  url: '/SiteSphereHandler.ashx?query=Store7%20Recently%20Viewed&columns=3&currentcurrencycode=GBP&visitoruid=684afe7c-3f67-4c7a-898b-4e2b3e625471&rvitemlist=' + newrvlist,
	  success: function( rdata ) {
		$('#' + divid).show();
		$('#' + divid).html(rdata);
		setupFlyToBasket();
	  }
	});        
    }
}

function rvPopulateList4Column(divid) {
    if (rvGetCount() == 0) {
        $('#' + divid).hide();
    }
    else {
        var vRVMax = 8;
        var newrvlist = rvGetList(vRVMax);
        
	$.ajax({
	  url: '/SiteSphereHandler.ashx?query=Store7%20Recently%20Viewed&columns=4&currentcurrencycode=GBP&visitoruid=684afe7c-3f67-4c7a-898b-4e2b3e625471&rvitemlist=' + newrvlist,
	  success: function( rdata ) {
		$('#' + divid).show();
		$('#' + divid).html(rdata);
	  }
	});        
    }
}


function PopulateCountriesAndSelect(ctryid, ictryid)
{
	$("#destination-dropdown").addClass("loadingminibasket");
	$("#invoice-dropdown").addClass("loadingminibasket");
	$.ajax({
	  url: '/SiteSphereHandler.ashx?query=Store7%20Country%20Options',
	  success: function( data ) {
	  	$("#destination-dropdown").html(data);
	  	if (ctryid=='') { ctryid = '1'; }
	  	$("#destination-dropdown").val(ctryid);
	  	$("#destination-dropdown").removeClass("loadingminibasket");
	  	$("#invoice-dropdown").html(data);
		if (ictryid=='') { ictryid = '1'; }
		$("#invoice-dropdown").val(ictryid);
	  	$("#invoice-dropdown").removeClass("loadingminibasket");
	  	try { AjaxGetDistributorLocator(); } catch (ex) { }
	  }
	});
	
}

function MultiAddCheckItemCode(numb)
{
	var icode = $("#multiadditemcode" + numb).val();
	if (icode != "")
	{
		$.ajax({
		  url: '/SiteSphereHandler.ashx?currentcurrencycode=GBP&query=Store7 Quick Order Item&itemcode=' + escape(icode),
		  success: function( rdata ) { populateMultiAddStockDivHandler(rdata, numb);
		  },
		  error: function( err, erri ) {
			$("#multiaddrowinfo" + numb).html("<span style='color:red;'>Product not found.</span>");
			$("#multiaddrowinfo" + numb).show();
			$("#multiaddrowitemimage" + numb).val("");
			$("#multiaddrowitemprice" + numb).val("");
			$("#multiaddrowitemstock" + numb).val("");
			$("#multiaddrowitemid" + numb).val("");
		  }
		});
	}
	else
	{
		
		$("#multiaddrowinfo" + numb).html("");
		$("#multiaddrowinfo" + numb).hide();
	}
}
 
function populateMultiAddStockDivHandler(rdata, pos)
{ 
 
	if (rdata == null)
	{
		$("#multiaddrowinfo" + pos).html("<span style='color:red;'>Product not found.</span>");
		$("#multiaddrowinfo" + pos).show();
		$("#multiaddrowitemimage" + pos).val("");
		$("#multiaddrowitemprice" + pos).val("");
		$("#multiaddrowitemstock" + pos).val("");
		$("#multiaddrowitemid" + pos).val("");
		return;
	}
	if (rdata == "")
	{
		$("#multiaddrowinfo" + pos).html("<span style='color:red;'>Product not found.</span>");
		$("#multiaddrowinfo" + pos).show();
		$("#multiaddrowitemimage" + pos).val("");
		$("#multiaddrowitemprice" + pos).val("");
		$("#multiaddrowitemstock" + pos).val("");
		$("#multiaddrowitemid" + pos).val("");
		return;
	}
	rdata=String(rdata);
	
	var resp_array2=rdata.split("##");
	var sActualItem ="";
	var sActualPrice ="";
	var sActualPriceRaw ="";
	var sActualImage ="";
	var sActualID ="";
	
	if (resp_array2.length > 0)
	{
		sActualStock=resp_array2[0];
	}
	else
	{
		$("#multiaddrowinfo" + pos).html("<span style='color:red;'>Product not found.</span>");
		$("#multiaddrowinfo" + pos).show();
		$("#multiaddrowitemimage" + pos).val("");
		$("#multiaddrowitemprice" + pos).val("");
		$("#multiaddrowitemstock" + pos).val("");
		$("#multiaddrowitemid" + pos).val("");
	}

	if (resp_array2.length > 1)
	{
		sActualImage=resp_array2[1];

	}
	if (resp_array2.length > 2)
	{
		sActualPrice=resp_array2[2];
		sActualPriceRaw=resp_array2[2];
	}
	if (resp_array2.length > 3)
	{
		sActualID=resp_array2[3];
		$("#multiaddrowitemid" + pos).val(resp_array2[3]);
	}
	
	if (resp_array2.length > 3)
	{
		sActualID=resp_array2[3];
		$("#multiaddrowitemid" + pos).val(resp_array2[3]);
	}

	if (resp_array2.length > 4)
	{
		sActualPriceRaw=resp_array2[4];
	}

	$("#multiaddrowitemimage" + pos).val(sActualImage);
	$("#multiaddrowitemprice" + pos).val(sActualPriceRaw);
	$("#multiaddrowitemstock" + pos).val(sActualStock);
	$("#multiaddrowitemid" + pos).val(sActualID);

	if (sActualID == "")
	{
		$("#multiaddrowinfo" + pos).html("<span style='color:red;'>Product not found.</span>");
		$("#multiaddrowinfo" + pos).show();
		$("#multiaddrowitemimage" + pos).val("");
		$("#multiaddrowitemprice" + pos).val("");
		$("#multiaddrowitemstock" + pos).val("");
		$("#multiaddrowitemid" + pos).val("");
	}
	else
	{
		$("#multiaddrowinfo" + pos).html(sActualImage + " " + sActualStock + " " + sActualPrice);
		$("#multiaddrowinfo" + pos).show();
	}
}

function ClearAddMultipleItemsMessage()
{
	$("#multiaddmessage").html("");
	$("#multiaddmessage").hide();
}


function AddMultipleItemsToBasket()
{
	var vToAdd=0;
	for (var pos=1; pos <= 10; pos++)
	{
		var vItemCode=$("#multiadditemcode" + pos).val();
		var vItemId=$("#multiaddrowitemid" + pos).val();
		var vItemQty=$("#multiadditemquantity" + pos).val();
		if ((vItemCode != "") && (vItemId != "") && (parseInt(vItemQty) > 0))
		{
			vToAdd=vToAdd+1;
		}
	}

	if (vToAdd == 0)
	{
		$("#multiaddmessage").show();
		$("#multiaddmessage").html("<span style='color:red;'>No products to add.</span>");
		setTimeout('ClearAddMultipleItemsMessage()', 2500);
	}
	else
	{
		// OK to add...
		$("#multiaddmessage").hide();
		$(".right-column-btn-add-multiple-items-to-basket").html(vAjaxLoadingImage);
		setTimeout('AddMultipleItemsToBasketNow()', 500);
	}
}

function AddMultipleItemsToBasketNow()
{
	for (var pos=1; pos <= 10; pos++)
	{
		var vItemCode=$("#multiadditemcode" + pos).val();
		var vItemId=$("#multiaddrowitemid" + pos).val();
		var vItemQty=$("#multiadditemquantity" + pos).val();
		var vItemPrice=$("#multiaddrowitemprice" + pos).val();
		if ((vItemCode != "") && (vItemId != ""))
		{
			AddItemToBasketJQ(vItemId, "'" + vItemCode + "'", '', vItemQty, vItemPrice, vItemPrice, 0);
		}
	}
	document.location.href='/cart/';
}	


