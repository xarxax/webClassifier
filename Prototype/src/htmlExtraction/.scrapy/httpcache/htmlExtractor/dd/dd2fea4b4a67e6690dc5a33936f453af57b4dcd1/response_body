			$('#advpsslideshow_5').bxSlider({
				useCSS:1,
				slideMargin: 0,
				speed: 4000,				
								minSlides: 5,
  				maxSlides: 5,
  				slideWidth: 360,
												auto: 1,
				autoHover: 1,
				pause: 4000,
				easing: 'linear',
				controls: 1,
				pager: 0,
  				autoControls: 1							});
							$("#advpsslideshow_5 .advs-title a").hover(function(){
					$(this).css('color','#c9c9c9');
				},function(){
					$(this).css('color','#FFFFFF');
				});
						$("#advpsslideshow_5 .advps-slide").hover(function(){
				  				  if(!$(this).find(".advps-excerpt-one").is(":animated")){					  
					  $(this).find(".advps-excerpt-one").fadeIn(200,'linear',function(){});
				  }
				  			},function(){
								$(this).find(".advps-excerpt-one").fadeOut(200,'linear',function(){});
							});
		});
	</script>
