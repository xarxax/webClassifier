	<script type="text/javascript">
	function jsvar(){ return _webroot; }
	(function($){

		a = (document.location.pathname.replace(/\/[^\/]*$/i, '\/') + './').replace(/^\/|\/$/g, '').split('/');
		b = [];
		for (var i = 0, i_max = a.length; i < i_max; i++)
		{
			if (a[i] == '.') continue;
			if (b.length && a[i] == '..')
			{
				b.pop();
				continue;
			}
			b.push(a[i]);
		}
		_webroot = document.location.protocol + '//' + document.location.host + '/' + b.join('/') + '/';

		$(function(){

			$('.jastips').hover(function(){
				$(this).toggleClass('hover');
			});

			// disabled right-click image
			$('img').mousedown(function(e){ if (e.which == 3) return false; });

			// disable selecting text
			$('div,span,p,td').disableTextSelect(); // .addClass('disableTextSelect');

			// disabled right-click
			$(document).bind('contextmenu', function(e){ return false; });

			// searchbox behavior
			$('#s_w').bind({
				'mousedown mouseenter click': function(e){
					if ($(this).val() == 'Enter product number') $(this).val('');
					$(this).focus();
					return true;
				},
				blur: function(){
					if ($(this).val() == '') $(this).val('Enter product number');
				}
			});

		});

	})(jQuery);
	</script>
