            <script>
            $(".mobile_search").click(function(e){
            e.preventDefault();
            hidden = $("#search_text2").hasClass("hidden");
            if(hidden){
            showMobSearch();
            }
            else{
            hideMobSearch();
            }
            });
            function showMobSearch(){
            $("#search_text2").removeClass("hidden");
            $(".mobile_search").removeClass("icon-search");
            $(".mobile_search").addClass("icon-cross");
            }
            function hideMobSearch(){
            $("#search_text2").addClass("hidden");
            $(".mobile_search").removeClass("icon-cross");
            $(".mobile_search").addClass("icon-search");
            }
            </script>
