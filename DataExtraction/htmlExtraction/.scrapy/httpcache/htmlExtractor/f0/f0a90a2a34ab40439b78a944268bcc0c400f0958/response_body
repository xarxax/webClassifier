<script>  

$(function(){

$('<select />').appendTo('nav');

$('<option />', {

'selected': 'selected',

'value' : '',

'text': ''

}).appendTo('nav select');



$('nav ul li a').each(function(){

var target = $(this);



$('<option />', {

'value' : target.attr('href'),

'text': target.text()

}).appendTo('nav select');



});

$('nav select').on('change',function(){

window.location = $(this).find('option:selected').val();

});

});

</script>

