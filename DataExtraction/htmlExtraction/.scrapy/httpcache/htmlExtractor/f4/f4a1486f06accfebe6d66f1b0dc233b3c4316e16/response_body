        //Control for foot nav on mobile display
        $(document).ready(function () {
            $('.footer h3').on("click keydown", function () {
                if ($(window).width() < 650)
                    $(this).next().slideToggle("fast");
            });
            $(".form input").keypress(function (e) {
                var keycode = e.charCode || e.keyCode;
                if (keycode == '13') {
                    e.preventDefault();
                    $(".primary").click();
                }
            });
        });
    </script>
