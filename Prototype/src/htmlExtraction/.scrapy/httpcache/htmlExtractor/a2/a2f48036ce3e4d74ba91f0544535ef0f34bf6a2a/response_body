<script type="text/javascript" src="/js/popup-gallery.js?v=2"></script> 
	
<script type="text/javascript">

	$( document ).ready(function() {
								 		var isSet = false;
										if(isSet)
											alert("Thank you for subscribing to our newsletter.");
								   });
	function submitOptIn()
    {
		if(!isEmailValid(document.getElementById('signup_email').value))
		{
			formOk = false;
			alert('Please enter a valid email adress');
		}
		else
		{
			var signup_optin = document.getElementById('signup_optin');
			signup_optin.value = 'I would like to receive relevant communications from PROCECO';
							  
			var objPost = $('#obj');
			var textstatusPost = $('#textstatus');
			jQuery.ajax({
			type: "POST",
			url: '/phps/functions_ext',
			dataType: 'json',
			data: {functionname: 'pardotHandlerUrl', arguments: ['OptIn', document.getElementById('signup_email').value, document.getElementById('signup_first_name').value, document.getElementById('signup_last_name').value, signup_optin.value]},
			
			success: function (objPost, textstatusPost)
						{
						  if( !('error' in objPost) )
						  {
							  notify_optin(document.getElementById('signup_email').value, '');
							  
							  var signup_optin = document.getElementById('signup_optin');
				  			  signup_optin.value = 'I would like to receive relevant communications from PROCECO';
																	  							  
							  var form = document.getElementById('frmOptIn');
							  form.action = objPost.result;
							  form.submit();
						  }
						  else
							  console.log(objPost.error);
					}
				});		    	
		}
    }	
</script>
