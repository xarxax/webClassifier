    <td width="143" colspan="2"><script language="JavaScript" type="text/javascript"> //By Paul Davis - www.kaosweaver.com
var j,l="",m="",p="",q="",z="",list= new Array()
list[list.length]='http://www.matrixfans.net/images/design/random/mfn_circle.gif';
list[list.length]='http://www.matrixfans.net/images/design/random/mfn_circle2.gif';
list[list.length]='http://www.matrixfans.net/images/design/random/mfn_circle3.gif';
list[list.length]='http://www.matrixfans.net/images/design/random/mfn_circle4.gif';
j=parseInt(Math.random()*list.length);
j=(isNaN(j))?0:j;
if (list[j].indexOf('?')==-1) {
document.write("<img src='"+list[j]+"'>");
}
else {
nvp=list[j].substring(list[j].indexOf('?')+2).split('&');
for(var i=0;i<nvp.length;i++) {
sub=nvp[i].split('=');
switch(sub[0]) {
case 'link':
l="<a href='"+unescape(sub[1])+"'>";
p="</a>";
break;
case 'target':
q=" target='"+unescape(sub[1])+"'";
break;
default:
m+=" "+sub[0]+"='"+unescape(sub[1])+"'";
break;
}
}
z=(l!="")?((q!="")?l.substring(0,l.length-1)+q+">":l):"";
z+="<img src='"+list[j].substring(0,list[j].indexOf('?'))+"'"+m+">"+p;
document.write(z);
}
</script></td>
