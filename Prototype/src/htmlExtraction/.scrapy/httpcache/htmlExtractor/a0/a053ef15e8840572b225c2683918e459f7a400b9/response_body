<!-- START SCRIPT FOR CALCULATOR -->
<script type='text/javascript'>
function docomputation(form) {

	var result ;
	var val = form.Input.value ;
	var numval = val.replace(/\$|\,/g,'') ;
	if (numval.length==0 || isNaN(numval)) {
		result = 0 ;
	} else {
		result = 50 * numval ;
	}

	form.Input2.value = formatCurrency(result.toString());
	form.Input.value = formatCurrency(val) ;
	return true ;

}


function formatCurrency(num) {
	num = num.replace(/\$|\,/g,'') ;
	if (isNaN(num)) {
		num = "0" ;
	}
	sign = (num == (num = Math.abs(num))) ;
	num = Math.floor(num*100+0.50000000001) ;
	num = Math.floor(num/100).toString() ;
	for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++) {
		num = num.substring(0,num.length-(4*i+3))+','+ num.substring(num.length-(4*i+3)) ;
	}
	
	return (((sign)?'':'-') + '$' + num);
}
</script>
<!-- END SCRIPT FOR CALCULATOR -->


