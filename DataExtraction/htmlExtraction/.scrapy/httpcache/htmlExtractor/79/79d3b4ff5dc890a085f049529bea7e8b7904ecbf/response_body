        window.Prerender.prerenderReady = false;
        window.markUserTime = function (l) {
            try {
                //console.log('logging markusertime: ' + l);
                if (!window.performance) { return; }
                var raf = window['requestAnimationFrame'] ||
                  (function (callback) { setTimeout(callback, 0); });
                raf(function () {
                    if (!!l) { //"nt undefined" cond'l check
                        if (l.indexOf('link_') != -1) {
                            window.performance.clearMarks();
                        }
                        if (!!window.performance && typeof window.performance.mark == "function") { window.performance.mark(l); }
                    }
                });
            } catch (e) { }
        };

        window.requestIdleCallback =
            window.requestIdleCallback ||
          function (cb) {
              return setTimeout(function () {
                  cb({
                      didTimeout: false,
                      timeRemaining: function () {
                          return 1; //always return 1 for browsers that dont support request idlecallback.
                      }
                  });
              }, 5000);
          };

