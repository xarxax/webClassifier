	<script>
		if(window.location.href.indexOf("BakeToSchool") > -1) {
			var userL = false;
			if(userL){
				window.location = 'https://www.chelsea.co.nz/baking-club';
			} else {
				$('.login-modal').modal('toggle');
			}
		}
	</script>

    
    <script type="text/javascript">
	    $(function () {

	        if ('ontouchstart' in window) {
				$( ".cc-optin-newchelsea input" ).focus(function() {
					console.log('touchstart');
					$('.fixed-header').css('position' , 'absolute');
				});
				$( ".cc-optin-newchelsea input" ).blur(function() {
					console.log('input blur');
					$('.fixed-header').css('position' , 'fixed');
				});
	        }

		});

	    var captchaCompleted = function(){
	        $('.subscribe').data("recaptcha-verified", true).on("submit", function(e) {
	        	e.preventDefault();
	            $.getJSON(this.action + "?callback=?", $(this).serialize(), function(data) {
	                if (data.Status === 400) {} else {
	                    $('#newsletter-success-desktop,#newsletter-success,#success-container').show();
	                    $('#newsletter-form-desktop,#newsletter-form,#newsletter').hide();
	                }
	            });
	        });
	    };

		var newsLetterCaptchaCompleted = function (){
			$('#newsletter-popup').data("recaptcha-verified", true).submit();
		};

		var chelseabayCaptchaCompleted = function (){
			$('#chelseabay_subscribe').data("recaptcha-verified", true);
		};

		var chelseabayCaptchaCompleted = function (){
			$('#chelseabay_subscribe_new').data("recaptcha-verified", true);
		};

		var recipeCaptchaCompleted = function (){
			
			$('#newsletter').data("recaptcha-verified", true);

		};

		$("#chelseabay-submit").on("click", function(){
			if ($("#chelseabay_subscribe").data("recaptcha-verified") && $('#fieldName').val().length !== 0 && $('#fieldEmail').val().length !== 0){
				$.ajax({
			        url: 'https://nzsugar.createsend.com/t/r/s/ujktjjk/',
			        type: 'post',
			        dataType: 'json',
			        data: $('#chelseabay_subscribe').serialize(),
			        complete: function() {
		        		$("#chelseabay_subscribe").hide();
						$("#chelseabay-success-desktop").show();
	                }
			    });
			}
		})

		$("#chelseabay-submit").on("click", function(){
			if ($("#chelseabay_subscribe_new").data("recaptcha-verified") && $('#fieldName').val().length !== 0 && $('#fieldEmail').val().length !== 0){
				$.ajax({
			        url: 'https://nzsugar.createsend.com/t/r/s/ujktjjk/',
			        type: 'post',
			        dataType: 'json',
			        data: $('#chelseabay_subscribe_new').serialize(),
			        complete: function() {
		        		$("#chelseabay_subscribe_new").hide();
						$("#chelseabay-success-desktop").show();
	                }
			    });
			}
		})

		$("#newsletter-submit").on("click", function(){
			if ($("#newsletter").data("recaptcha-verified") && $('#fieldName').val().length !== 0 && !$('#fieldEmail').val().length == 0){
				// $('#newsletter').submit();
				$.ajax({
			        url: '/index.php/tools/packages/cc_optin_pro/subscribe_controller?cID=1&id=4',
			        type: 'post',
			        dataType: 'json',
			        data: $('#newsletter').serialize(),
			        success: function(data) {
			        	if(data.closeAfter){
			        		$("#newsletter-form-desktop").hide();
							$("#newsletter-success-desktop").show();
			        	}
	                }
			    });
			}
		})

	    var onloadCallback = function(){
	        $('.g-recaptcha').each(function(index, el) {
	            var attributes = {
	                'sitekey'  : $(el).data('sitekey'),
	                'size'     : $(el).data('size'),
	                'badge'    : $(el).data('badge'),
	                'callback' : $(el).data('callback')
	            };

	            $(el).data('recaptcha-widget-id', grecaptcha.render(el, attributes));
	        });
	    };

	    $('#newsletter,#newsletter-mobile,#newsletter-popup').on("click focus", function(e){ 
	        var $form = $(this);
	        if ($form.data("recaptcha-verified")) return;
	        e.preventDefault();
	        grecaptcha.execute($form.find(".g-recaptcha").data("recaptcha-widget-id"));
	    });
	</script>

