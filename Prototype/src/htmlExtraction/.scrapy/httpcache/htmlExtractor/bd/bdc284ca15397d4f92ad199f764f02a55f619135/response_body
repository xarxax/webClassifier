		function addAboveBelowSubmenuClass719( IDToChange ) {
			var element = document.getElementById( IDToChange );
			element.classList.add("menu719submenuAbove");
		}
		function removeAboveBelowSubmenuClass719( IDToChange ) {
			var element = document.getElementById( IDToChange );
			element.classList.remove("menu719submenuAbove");
		}
