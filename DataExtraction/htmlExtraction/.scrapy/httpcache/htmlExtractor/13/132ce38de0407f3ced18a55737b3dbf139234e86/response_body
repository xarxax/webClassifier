<div class="uk-width-1-1"><div class="uk-panel"><h3 class="uk-panel-title">Looking for something specific?</h3>
<script type="text/javascript">
	var moduleId = 174;
	var RSFSearch174;
	var results_box = document.getElementById('search_suggest174');
	var generateResultsTimer = 0;

function searchSuggest174() {
	if (generateResultsTimer > 0) {
		clearTimeout(generateResultsTimer);
	}
	
	generateResultsTimer = window.setTimeout(function () {
		var str =  encodeURIComponent(document.getElementById('rsf_inp174').value);
		var ss = document.getElementById('search_suggest174');
	
		if (str.length == 0) {
			closeSearch174();
			return;
		}
	
		var req = new Request.JSON({
			method: 'post',
			url: 'http://www.fuchsna.com/'+'index.php?option=com_rssearch',
			data: 'view=results&layout=ajax&module_id=174&search='+str,
			onSuccess: function(responseText, responseXML) {
				var json = responseText;
				ss.innerHTML = '';
				
				var suggest = '';
				var results = json.length;
				var limit	= 5;
				var i		= 0;
				
				if (results > 0) {
					if (results < limit) {
						var max = results;
					} else {
						var max = limit;
					}
					
					suggest += '<div class="rssearch_close"><a href="javascript:void(0)" onclick="javascript:closeSearch174();"><img src="http://www.fuchsna.com/modules/mod_rssearch/assets/images/close.png" alt="" /></a></div>';
					
					json.each(function (el) {
						if (i < max) {
							suggest += '<div onmouseover="javascript:suggestOver174(this);" onmouseout="javascript:suggestOut174(this);" class="suggest_link">';
							suggest += '<a style="color: #FFFFFF" href="'+ el.link +'">'+el.title+'</a>';
							suggest += '</div>';
						}
						
						i++;
					});
					
					if (results > limit){
						suggest += '<div class="suggest_link suggest_link_all" onclick="javascript:setSearch174(this.innerHTML);"><b>View all results</b></a></div>';
					}
				}
				
				ss.set('html',suggest);
				RSFSearch174.slideIn();
			}
		});
		req.send();
	}, 800);
}

function suggestOver174(div_value){
	div_value.className = 'suggest_link_over';
}

function suggestOut174(div_value){
	div_value.className = 'suggest_link';
}

function setSearch174(value){
	document.getElementById('search_suggest174').innerHTML = '';
	document.getElementById('frmSearch'+174).submit();
}

function closeSearch174(){
	RSFSearch174.slideOut();
}

window.addEvent('domready', function() {
	$('search_suggest174').setStyle('height','auto');
	RSFSearch174 = new Fx.Slide('search_suggest174').hide();
	var parent = $('search_suggest174').getParent('div').setStyle('width','120px');
});
</script>

<div id="rssearch" class="rssearch_box">
	<form id="frmSearch174" action="/component/rssearch/view-results/" method="post" accept-charset="utf-8" class="rsf_form">
		
		<div class="input-append">
			<input type="text" id="rsf_inp174" name="search"  onkeyup="searchSuggest174();" autocomplete="off" style="width:120px;" />
			 <button type="submit" id="cmdSearch" class="btn">Search</button>		</div>
		<div id="search_suggest174" class="rsfsuggestions" style="width:120px"></div>

		<input type="hidden" name="task" value="search" />
		<input type="hidden" name="module_id" value="174" />
			</form>
</div></div></div>
                </section>

