jQuery(document).ready(function(){
   jQuery('div div div div.accordion div.wpb_accordion_wrapper div.wpb_accordion_section h3#ui-id-3').click(function(event){
       event.preventDefault();      
      window.location.href = "http://www.supercart.com/gallery/";     
   });
});

jQuery(document).ready(function(){
   jQuery('div div div div.accordion div.wpb_accordion_wrapper div.wpb_accordion_section h3#ui-id-5').click(function(event){
       event.preventDefault();      
      window.location.href = "http://www.supercart.com/contact-us/";     
   });
    jQuery( "#menu-item-66431" ).hover(function() {
        jQuery( "#cg-page-wrap" ).css("opacity","0.1");
        jQuery( "#cg-page-wrap" ).css("-webkit-transition-duration","2s"); /* Safari */
        jQuery( "#cg-page-wrap" ).css("transition-duration","2s");
    }, function() {
        jQuery( "#cg-page-wrap" ).css("opacity","1");
    });
    jQuery( "div#imageContainer div.grow" ).hover(function() {
        jQuery( "#textbelowgrid" ).css("opacity","0.1");
        jQuery( "#textbelowgrid" ).css("-webkit-transition-duration","2s"); /* Safari */
        jQuery( "#textbelowgrid" ).css("transition-duration","2s");
    }, function() {
        jQuery( "#textbelowgrid" ).css("opacity","1");
    });
});

</script>

