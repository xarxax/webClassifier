<script>
$(".newspaginate").click(function(){
     var rows = $(this).attr('data-attr');
      $('.newspaginate').removeClass('activepage');
     $('.number-'+rows).addClass('activepage');     
     $('.newsdetails').css('display', 'none');
     $('.newsdisplay-'+rows).css('display','block');
     
})	

$(".presspaginate").click(function(){
     var rows = $(this).attr('data-attr');
      $('.presspaginate').removeClass('activepress');
     $('.pressnumber-'+rows).addClass('activepress');     
     $('.pressdetails').css('display', 'none');
     $('.pressdisplay-'+rows).css('display','block');
     
})		

$(".one_open").bind('click', function(){				
                          $(".onehidden_wrap").empty();
                          var id = $(this).attr("data-attr");
                          var rowsid = $(this).attr("rows-attr");       
                          $( ".hiddennews-"+id).clone(true).appendTo(".news-"+rowsid);
                           $(".onehidden_wrap .hiddennews-"+id).css('display','block');

                         $(".onehidden_wrap").slideDown("slow","swing");
				
				 var self = $(".first_news_block");
				 var top = $(".second_news_block")
				 
				 if (self.hasClass("slide_margin_bottom")) {
                     self.removeClass("slide_margin_bottom");
                 } else {
                     self.removeClass("slide_margin_bottom").addClass("slide_margin_bottom");
                 }
				 
				 if (top.hasClass("slide_margin_top")) {
                     top.removeClass("slide_margin_top");
                 } else {
                     top.removeClass("slide_margin_top").addClass("slide_margin_top");
                 }
			});
			$(".onehidden_wrap_close").bind('click', function(){
				$(".onehidden_wrap").slideUp("slow","swing");
				
				var self = $(".first_news_block");
				 var top = $(".second_news_block")
				 
				 if (self.hasClass("slide_margin_bottom")) {
                     self.removeClass("slide_margin_bottom");
                 } else {
                     self.removeClass("slide_margin_bottom").addClass("slide_margin_bottom");
                 }
				 
				 if (top.hasClass("slide_margin_top")) {
                     top.removeClass("slide_margin_top");
                 } else {
                     top.removeClass("slide_margin_top").addClass("slide_margin_top");
                 }
			});
			
