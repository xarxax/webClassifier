<script language="javascript" type="text/javascript">
	window.IMG_URL = '/images';
	
	function doOnLoad() {
		preload('btn_about_on.png');
		preload('btn_home_on.png');
		preload('btn_technology_on.png');
		preload('btn_products_on.png');
		preload('btn_markets_on.png');
		preload('btn_news_on.png');
		//preload('btn_investor_on.png');
		preload('btn_contact_on.png');
	}
	
	function preload(imgname) {
		if(typeof(window.preloadedimages) == 'undefined') window.preloadedimages = new Array();
		var index = window.preloadedimages.length;
		window.preloadedimages[index] = new Image();
		window.preloadedimages[index].src = window.IMG_URL + '/' + imgname;
	}
	
	function buttonover(that) {
		var imgname = that.name;
		that.src = window.IMG_URL + '/' + imgname + '_on.png';
	}
	
	function buttonout(that) {
		var imgname = that.name;
		that.src = window.IMG_URL + '/' + imgname + '_up.png';
	}
</script>


</head>

<body onload="doOnLoad()">


