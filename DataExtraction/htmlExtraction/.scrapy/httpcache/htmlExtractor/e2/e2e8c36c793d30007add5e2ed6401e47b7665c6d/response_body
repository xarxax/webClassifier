<script type="text/javascript">	 
jq(document).ready(function()
{
	
	jq('#nav_capab').bind(
	{
		mouseenter: function()
		{
			jq(this).children('.js_menu').stop(true, true).fadeIn();
		},
		mouseleave: function()
		{
			jq('.js_menu').stop(true, true).fadeOut();
		}
	});
	
	jq('.js_menu_item').bind(
	{
		click: function()
		{
			var url = jq(this).children('.js_menu_item_link').text();
			document.location = url;
		}
	})
	
	
});
</script>
