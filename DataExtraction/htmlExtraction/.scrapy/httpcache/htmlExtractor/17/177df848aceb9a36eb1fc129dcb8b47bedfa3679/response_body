<script>
    $(function () {
        setUpFormUtilities();
        var destinationInput = $("#destination-input");
        setUpDestinationAutocomplete(destinationInput);
        destinationInput.focus();
        setupHomepageFakeSearchbar(destinationInput);
    });

    function setupHomepageFakeSearchbar(destinationInput){
        $('.fake-searchbar, .find-an-apartment3-close').click(function(){
            if($('body').hasClass('open-searchbar')){
                $('body').removeClass('open-searchbar');
            }else{
                $('body').addClass('open-searchbar');
                destinationInput.focus();
            }
        });
    }
