	<script type="text/javascript">

    function onYouTubePlayerAPIReady() {
    	if(ytp.YTAPIReady)
		    return;
	    ytp.YTAPIReady=true;
	    jQuery(document).trigger("YTAPIReady");
    }

    jQuery.mbYTPlayer.rasterImg ="https://www.napco.com/wp-content/plugins/wpmbytplayer/images/raster.png";
	jQuery.mbYTPlayer.rasterImgRetina ="https://www.napco.com/wp-content/plugins/wpmbytplayer/images/raster@2x.png";

	jQuery(function(){
        jQuery(".mbYTPMovie").mb_YTPlayer()
	});

	</script>
