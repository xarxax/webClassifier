<script language="JavaScript">
function doJob(){
if (document.specialty.emptype[0].checked==1){
//Go to the per diem search

window.location=('emp/empperdiem_assignments.asp?specialties='+document.specialty.specialties.value+'&locs='+document.specialty.locs.value+'&sRow=1&eRow=15');
}else{
window.location=('emp/emptravel_assignments.asp?specialties='+document.specialty.specialties.value+'&locs='+document.specialty.locs.value+'&sRow=1&eRow=15');
}
}

function opencert(){
var leftX=((screen.width/2)-390);
var topX=((screen.height/2)-300);
window.open('http://surgicalstaffinc.net/about/cert.asp','cert','width=780,left='+leftX+',top='+topX+',height=600,scrollbars=yes');

}

function jobposting(){
var leftX=((screen.width/2)-255);
var topX=((screen.height/2)-210);
window.open('http://surgicalstaffinc.net/job_posting.asp','jobposting','width=510,left='+leftX+',top='+topX+',height=420,scrollbars=yes');

}
</script>
