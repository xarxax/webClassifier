<script>
function presenter_next(){
	if ( presenterPaused ) return false;

	var $thisPresenter = $('.presenter');
	var $thisPresenterSlides = $('.presenter-slide',$thisPresenter);
	var $currentSlide = $thisPresenterSlides.filter('.active');
	$thisPresenterSlides.removeClass('active');

	var $nextSlide = $currentSlide.next();

	if ( $nextSlide.length > 0 ) {
		$nextSlide.addClass('active');
	} else {
		$thisPresenterSlides.first().addClass('active');
	}

	return true;
}

function presenter_prev(){
	if ( presenterPaused ) return false;

	var $thisPresenter = $('.presenter');
	var $thisPresenterSlides = $('.presenter-slide',$thisPresenter);
	var $currentSlide = $thisPresenterSlides.filter('.active');
	$thisPresenterSlides.removeClass('active');

	var $nextSlide = $currentSlide.prev();

	if ( $nextSlide.length > 0 ) {
		$nextSlide.addClass('active');
	} else {
		$thisPresenterSlides.last().addClass('active');
	}

	return true;
}


var presenterInterval;
var presenterPaused = false;
var presenterTime = 8000;
$(document).ready(function(){

	$('.presenter').bind('mouseover',function(){
		presenterPaused = true;
	})
	
	$('.presenter').bind('mouseout',function(){
		presenterPaused = false;
	})

	presenterInterval = setInterval(presenter_next,presenterTime)

});


</script>

