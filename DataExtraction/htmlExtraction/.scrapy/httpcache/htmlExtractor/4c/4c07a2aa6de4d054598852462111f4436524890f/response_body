		var p;

		function popup(img, width, height,comment)
		{
			popWindow=window.open ('', '_blank','menubar=no,status=no,scrollbars=no,width='+width+',height='+height);
			popWindow.document.write ('<HTML>\n\n<HEAD>\n<TITLE>Kompani</TITLE>\n</HEAD>\n\n');
			popWindow.document.write ('<BODY MARGINWIDTH=0 MARGINHEIGHT=0 TOPMARGIN=0 LEFTMARGIN=0 BGCOLOR=#000000>\n\n<IMG SRC="'+img+'" HSPACE=0 VSPACE=0 BORDER=0 ALT="'+comment+'"></BODY>\n\n</HTML>\n');
			popWindow.document.close ();
		};
