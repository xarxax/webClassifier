<script type="text/javascript">
    /* 354 is used here to have UNIQUE variable. If you need only one slider per page, no need to use 354 */
    var NEXT_ELEMENT_STRING_ID_354 = null;
    var PREV_ELEMENT_STRING_ID_354 = null;
    var CONTROL_ELEMENT_STRING_ID_354 = '#content_slider_lateral_controls_354';
    // Don't remove: Used by init_javascript.php
    var BLOCK_ID_354 = 354;
    var GALLERY_WIDTH_354 = 740;
    var GALLERY_HEIGHT_354 = 300;
    
        // run when the slider is ready
    $.fn.onUltimateSliderReady_354 = function(opts) {        
         $ul_354 = $('#content_slider_lateral_controls_354' );
         $height_354 = $('li:first-child', $ul_354).height();
        //console.log($height);
         $animation_nav_354 = $('<li></li>').appendTo($ul_354).addClass('nav_animation');
         jQuery.easing.def = "easeInOutSine";
    }
    // When slide start to change (click or diaporama)
    var ON_START_SLIDE_FUNCTION_354 = function (n) {
        $animation_nav_354.stop(true).animate({top:n * $height_354});
    }
    
</script>
	
