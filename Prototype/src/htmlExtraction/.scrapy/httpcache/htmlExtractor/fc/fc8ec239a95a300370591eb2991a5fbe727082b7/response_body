$(document).ready(function() {
  var owl_carousel_expertise = {
			items: 4,
			loop: false,
			nav: false,
			dots: false,
			navigation: false,
			// responsive: true
			responsive: {
				0: {
					items: 1,
					mouseDrag: true,
				},
