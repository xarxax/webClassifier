<script type="text/javascript">
    $('#customersCarousel').carousel({
        interval: 5000
    })
    $("#blrCustomers .carousel .item").first().addClass("active");

    $('#blrCustomers .carousel .item').each(function () {
        var next = $(this).next();
        if (!next.length) {
            next = $(this).siblings(':first');
        }
        next.children(':first-child').clone().appendTo($(this));

        var next2 = next.next();
        if (!next2.length) {
            next2 = $(this).siblings(':first');
        }
        next2.children(':first-child').clone().appendTo($(this));

        var next3 = next2.next();
        if (!next3.length) {
            next3 = $(this).siblings(':first');
        }
        next3.children(':first-child').clone().appendTo($(this));
    });
</script>

