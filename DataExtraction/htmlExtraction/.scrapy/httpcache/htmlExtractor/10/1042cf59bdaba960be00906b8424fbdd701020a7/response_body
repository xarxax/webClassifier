$(document).ready(function(){
	$('.letras:not(#letraa)').hide();
	$('.indiceAlfabetico a').click(function(){
		// Encontra e mostra o conteúdo com a letra certa
		var letraClicada = $(this).text();
		var letraClicada = letraClicada.toLowerCase();
		var idSelecionado = '#letra'+letraClicada;
		$('.letras').hide();
		$(idSelecionado).show();

		// Colore a letra selecionada
		$('.indiceAlfabetico a').removeClass('inicialSelecionada');
		$(this).addClass('inicialSelecionada');
		// alert(idSelecionado);
	});
	$('#top').click(function() {
		$.scrollTo('.header', 400);
	});
