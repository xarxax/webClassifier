	<script>
		var time = 10, counter = document.querySelector('#counter');

		var timer = setInterval(function() {
			time--;
			counter.innerText = time;

			if (time == 0) {
				clearInterval(timer);
				window.location.reload();
			}
		}, 1000);
	</script>
