    <script>
        //polyfill for conatains
        Array.prototype.contains = function (needle) {
            for (i in this) {
                if (this[i] === needle) return true;
            }
            return false;
        }

        function getRandomIntInclusive(min, max) {
            min = Math.ceil(min);
            max = Math.floor(max);
            return Math.floor(Math.random() * (max - min + 1)) + min; //The maximum is inclusive and the minimum is inclusive 
        }

        var slideIndex = 0;//getRandomIntInclusive(0,5);
        var timer;
        var stop = 0;
        var start = 0;
        var curId = "";
        sliderMain();
        function sliderMain(dir)
        {
            var x = document.getElementsByClassName("slide");
            var y = document.getElementsByClassName("pos");

            //if (dir == null)
            //{
                if (start == 1)
                {
                    for (var i = 0; i < x.length; i++)
                    {
                        if (!x[i].classList.contains("hide") && !x[i].classList.contains("hidden"))
                        {
                            x[i].className = "slide hide";
                            x[i].childNodes[1].className = "hide";
                        }
                        if (y[i].className == "pos black")
                            y[i].className = "pos";
                    }
                }
                
                start = 1;
            //}
            //else
            //{
            //    if(curId != "")
            //    {
            //        var banner = document.getElementById(curId);
                   
            //        if (dir == "left") {
            //            banner.className = "slide hide";
            //            banner.childNodes[1].className = "hide";
            //            y[banner.id.substring(6, 7) - 1].className = "pos";
            //        }
            //        else {
            //            banner.className = "slide hide";
            //            banner.childNodes[1].className = "hide";
            //            y[banner.id.substring(6, 7) - 1].className = "pos";
            //        }
            //    }
                
            //}
         
            slideIndex++;
            if (slideIndex > x.length) { slideIndex = 1 }
            if (dir == null)
            {
                if (slideIndex == 1) {
                    x[slideIndex - 1].className = "slide fadeIn";
                    x[slideIndex - 1].childNodes[1].className = "delayFadeIn";
                }
                else if (slideIndex == 2 || slideIndex == 4) {
                    x[slideIndex - 1].className = "slide fadeIn";
                    x[slideIndex - 1].childNodes[1].className = "delayFadeIn";
                }
                else {
                    x[slideIndex - 1].className = "slide fadeIn";
                    x[slideIndex - 1].childNodes[1].className = "delayFadeIn";
                }
            }
            else
            {
                if (dir == "left")
                {
                    x[slideIndex - 1].className = "slide fadeIn";
                    x[slideIndex - 1].childNodes[1].className = "delayFadeIn";
                }
                else
                {
                    x[slideIndex - 1].className = "slide fadeIn";
                    x[slideIndex - 1].childNodes[1].className = "delayFadeIn";
                }
            }
          
            curId = x[slideIndex - 1].id;
            y[slideIndex - 1].className = "pos black";
                
            if (stop == 0)
                timer = setTimeout(sliderMain, 10000);
        }
        function moveRight()
        {
            stop = 1;
            clearTimeout(timer);
            sliderMain("right");
        }

        function moveLeft()
        {
            stop = 1;
            slideIndex -= 2;
            if (slideIndex < 0)
                slideIndex = 2;
            clearTimeout(timer);
            sliderMain("left");
        }

        function moveToPos(index)
        {
            stop = 1;
            slideIndex = index;
            clearTimeout(timer);
            sliderMain();
        }

        document.getElementById("banner-holder").onmouseenter = function (event) {
            clearTimeout(timer);
        };
        document.getElementById("banner-holder").onmouseleave = function (event) {
            clearTimeout(timer);
            if(stop == 0)
                timer = setTimeout(sliderMain, 5000);
        }
    </script>

        
