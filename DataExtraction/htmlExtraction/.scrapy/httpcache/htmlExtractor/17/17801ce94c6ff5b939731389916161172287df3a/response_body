function openMedimizerAd() { // show sidekick medimizer expanded

	jQuery('.medimizer-expanded-form').animate({width:'850px'},1500);

	slideContainerLeft();

}



function hideMedimizerAd() { // hide sidekick medimizer expanded

	jQuery('.medimizer-expanded-form').animate({width:'0px'},1500);

	slideContainerRight();

	setTimeout(showthirdSidekickbanner,2000);
		setTimeout(showsecondSidekickbanner,2000);

}



function openATSbanner() { // show sidekick medimizer expanded

	jQuery('.ats-expanded-form').animate({width:'850px'},1500);

	slideContainerLeft();

}



function hideATSbanner() { // hide sidekick medimizer expanded

	jQuery('.ats-expanded-form').animate({width:'0px'},1500);

	slideContainerRight();

	setTimeout(showfourthSidekickbanner,2000);

}



function showStanbanner(){

	jQuery('.stanburns-expanded-form').animate({width:'850px'},1500);

	slideContainerLeft();

}



function hideStanbanner(){

	jQuery('.stanburns-expanded-form').animate({width:'0px'},1500);

	slideContainerRight();

	setTimeout(showfifthSidekickbanner,2000);

}



function openHettichbanner(){

	jQuery('.hettich-expanded-form').animate({width:'850px'},1500);

	slideContainerLeft();

}



function hideHettichbanner(){

	jQuery('.hettich-expanded-form').animate({width:'0px'},1500);

	slideContainerRight();

	setTimeout(showfirstSidekickbanner,2000);

}



function slideContainerLeft() { // when sidekick expands

	jQuery('.uk-container').animate({marginLeft:'-510px'},1500);

	jQuery('.menu-shadow').animate({marginLeft:'-1px'},1500);

}



function slideContainerRight() { // when sidekick collapses

	jQuery('.uk-container').animate({marginLeft:'190px'},1500);

	jQuery('.menu-shadow').animate({marginLeft:'0px'},1500);

}



