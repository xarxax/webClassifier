var fw_html_top = 107;


if (document.layers){
	visible = 'show';
  	hidden = 'hide';
}
else if (document.all){
	visible = 'visible';
	hidden = 'hidden';
}


function mainmenu_DoFSCommand(command, args)
{ 
	if (command == "showAd")
	{
		if (document.layers){
			daMenu = document.layers[args];
		}
		else if (document.all){
	    	daMenu = document.all(args).style;
		}
		daMenu.visibility = visible;
  	}
	
	if (command == "hideAd")
	{
		if (document.layers){
			daMenu = document.layers[args];
		}
		else if (document.all){
	    	daMenu = document.all(args).style;
		}
		daMenu.visibility = hidden;
  	}
}

</script>
