<script type="text/javascript" src="/includes/js/jquery.cookie.js"></script>

<script type="text/javascript">

	function registerClick(aid){

		$.get('/ads/click.php?id='+aid);

	}

	// load up all of our ads into arrays

	prodAds=new Array();

	
		prodAds.push('<a href="http://www.bordercrossingsmag.com" target="_new" onclick="if (registerClick(24)) return true;"><img src="/ads/images/24.jpg" alt="ad image" /></a>');

	
	moreAds=new Array();

	
	moreAds.push('<a href="http://www.publicjournal.ca" target="_new" onclick="if (registerClick(118)) return true;"><img src="/ads/images/118.jpg" alt="ad image" /></a>');

	
	moreAds.push('<a href="http://instantplaces.ca/pod-dreamers-radio-home/" target="_new" onclick="if (registerClick(275)) return true;"><img src="/ads/images/275.jpg" alt="ad image" /></a>');

	
	$(function() {

		rotateAds();

		var adInterval = 10; //adInterval in seconds

		setInterval("rotateAds()",(adInterval * 1000));

	});

	function rotateAds(){

	/* Prod ads start */

	// check for cookie, if it's not there create it

	if (!$.cookie("akimboPadstep")){

		$.cookie("akimboPadstep",0,{ path: '/' });

	}

	var prodAdsDisplay=1;

	// get previous adStep from cookie, if available...

	var pAdStep = parseInt($.cookie("akimboPadstep"));

	var outputAds = "";

	var whichAd=0;

	var i=0;

	while (i<prodAdsDisplay){

		whichAd=pAdStep+i;

		if(whichAd >= prodAds.length){

			whichAd = whichAd - prodAds.length;

		}

		outputAds +=prodAds[whichAd];

		i++;

	}

	pAdStep++;

	if(pAdStep >= prodAds.length){

		// reset to step zero if we've hit the end

		pAdStep=0;

	}

	// write the current step value back to our cookie

	$.cookie("akimboPadstep",pAdStep,{ path: '/' });

	$("#ads-prod").html(outputAds);

	/* prod ads end */



	/* More ads start */

	if (!$.cookie("akimboMadstep")){

		$.cookie("akimboMadstep",0,{ path: '/' });

	}

	var moreAdsDisplay=14;

	// get previous adStep from cookie, if available...

	var mAdStep = parseInt($.cookie("akimboMadstep"));

	var outputAds = "";

	var whichAd=0;

	var i=0;

	while (i<moreAdsDisplay){

		whichAd=mAdStep+i;

		if(whichAd >= moreAds.length){

			whichAd = whichAd - moreAds.length;

		}

		outputAds +=moreAds[whichAd];

		i++;

		}

		mAdStep++;

		if(mAdStep >= moreAds.length){

			// reset to step zero

			mAdStep=0;

		}

		$.cookie("akimboMadstep",mAdStep,{ path: '/' });

		$("#ads-more").html(outputAds);
		$("#ads-more2").html(outputAds);

		/* More ads end */

		}

</script>











<script type="text/javascript">

     $(document).ready(function(){

	// EMAIL THIS PAGE LINK

		$('#email_link').hide();

		$('a#email_page').click(function(){

			$('#email_link').show('slow');

		});

			

		$('a#close_email_page').click(function(){

			$('#email_link').hide('slow');

			$('#email_link2').hide('slow');

		})

		

	// EMAIL THIS PAGE LINK NONPAID CONTENT

		$('#email_link2').hide();

		$('a#email_page2').click(function(){

			$('#email_link2').show('slow');

		});

			
	// LOGIN FORM

		$('#login_link').hide();

		$('a#login_page').click(function(){

			$('#login_link').show('slow');

		});

		$('a#close_login_page').click(function(){

			$('#login_link').hide('slow');

		})

	

	// GET THE LINK OF THIS PAGE		

		$('#link_link').hide();

		$('a#link_page').click(function(){

			$('#link_link').show('slow');

		});

		

// GET THE LINK OF THIS PAGE NONPAID		

		$('#link_link2').hide();

		$('a#link_page2').click(function(){

			$('#link_link2').show('slow');

		});		

			

		$('a#close_link_page').click(function(){

			$('#link_link').hide('slow');

			$('#link_link2').hide('slow');

		})

 

	// PRINT THIS PAGE	 

 		$('#printpageButton').click(function() {

			$('#black_bar_left').hide();

			$('#black_bar_right').hide();

			$('#header_left').hide();

			$('#grey_bar_right').hide();

			$('#header_right_two').hide();

			$('#header_right_one').hide();

			$('#calendar_holder').hide();

			$('#next_seven_days').hide();

			$('#akimbos_filter').hide();

			$('#akimbos_filter_titles').hide();

			$('.tooltip').hide();

			$('#chanel_one').hide();

			$('#chanel_four').hide();

			$('#sm_sm_icons').hide();

			$('#email_link').hide();

			$('#link_link').hide();

			$('#akimbos_left_keyline').hide();

			$('#non_paid_ads').hide();

		window.print();

			$('#black_bar_left').show();

			$('#black_bar_right').show();

			$('#header_left').show();

			$('#grey_bar_right').show();

			$('#header_right_two').show();

			$('#header_right_one').show();

			$('#calendar_holder').show();

			$('#next_seven_days').show();

			$('#akimbos_filter').show();

			$('#akimbos_filter_titles').show();

			$('#chanel_one').show();

			$('#chanel_four').show();

			$('#sm_sm_icons').show();

			$('#akimbos_left_keyline').show();

			$('#non_paid_ads').show();

			return false;

		});





	//  HOMEPAGE CAROUSEL

		$("#slider3").mbSlider({ 	

			controlsLocation: 'external',

			easing: 'easeOutExpo',

			slideDuration: 7000,

			slideSpeed: 0,

			pauseOnHover: true,

			showControls: {

				next: true,

				prev: true,

				play: true,

				pause: true,

				first: false,

				last: false

			},

			controlsText: {

				next: '',

				prev: '',

				play: '',

				pause: '',

				first: '',

				last: ''

			},		

		});

 

		$('.blog_city').click(function(){

			$("#akimblog_homepage").load("/includes/akimblog_homepage.php?ab_c_id=" + $(this).attr('id'));

		});





		$('.hood_over').mouseover(function(){

			$("#hood_info").load("/includes/hood_info.php?gh_id=" + $(this).attr('id'));

		});





// CITY / VENUE DROPDOWN SWICHER

 		jQuery('#mycarousel').jcarousel({

			start: 33
		});

 

		$('#city').change(function(){

			$('#venue').html('<option value="">Loading</option>');	

			$.get("/includes/ajax_venue.php", {

				func: "city",

				drop_var: $('#city').val()

			}, function(response){

				$('#venue').fadeOut();

				setTimeout("finishAjax('venue', '"+escape(response)+"')", 40);

			});

			return false;

		}); 

		



		$('#city2').change(function(){

			$('#h_hood').html('<option value="">Loading</option>');	

			$.get("/includes/ajax_hoods.php", {

				func: "city2",

				drop_var: $('#city2').val()

			}, function(response){

				$('#h_hood').fadeOut();

				setTimeout("finishAjax('h_hood', '"+escape(response)+"')", 40);

			});

			return false;

		}); 
		
		

		$('#city3').change(function(){

			$('#sub_type').html('<option value="">Loading</option>');	

			$.get("/includes/ajax_directory.php", {

				func: "city3",

				drop_var: $('#city3').val()

			}, function(response){

				$('#sub_type').fadeOut();

				setTimeout("finishAjax('sub_type', '"+escape(response)+"')", 40);

			});

			return false;

		}); 				

 

 

	});	  

	

	function finishAjax(id, response) {

	  $('#wait_1').hide();

	  $('#'+id).html(unescape(response));

	  $('#'+id).fadeIn();

	}	

	

</script>



<!-- CONTROLS FOR CAROUSEL -->

<script type="text/javascript">

		var nav = function() { 

			$("#mycarousel .btn-down").click(); // next move 

			$("#mycarousel").data( 

				'hover',  

				 window.setTimeout(nav, 1000); // continue in 1000 ms 

			); 

		}; 

		$("#mycarousel .btn-down").hover( 

  			nav, 

  			function() { 

   				window.cancelTimeout ($("#mycarousel").data('hover')); // stop the navigation 

  			} 

		); 

</script>



<!-- JQUERY SETTINGS FOR THE VARIOUS CLICKS AND MOUSE OVERS -->

<script type="text/javascript">

	$(document).ready(function() {

		var option = 'toronto';

		var url = window.location.href;

		option = url.match(/option=(.*)/)[1];

		showDiv(option);

	});

	function showDiv(option) {

		$('.hide_cities').hide();

		$('.show_cities').hide();

		$('#' + option).fadeIn("slow");

	};

</script>


