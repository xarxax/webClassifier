 <script>
  function get_temp_zip_by_city(){
			 // var temp_city = $("#temp_city").val();
			  var temp_city = $("#searchCity").find(':selected').attr('data-id')
			  if(temp_city){
				   $.ajax({
						url: 'https://eventsinamerica.com/contact/gettempzipbycity',
						type: 'POST',
						dataType: "html",
						data: 'temp_city=' + temp_city,
						success: function(response){
						   $("#zipcode").html(response);
						}
					});  
			  }
			}


function get_temp_city_by_state(){
  var temp_state = $("#searchState").val();
  if(temp_state){
	   $.ajax({
            url: 'https://eventsinamerica.com/contact/gettempcitybystate',
            type: 'POST',
            dataType: "html",
            data: 'temp_state=' + temp_state,
            success: function(response) {
               $("#searchCity").html(response);
			    $("#searchCity").select2();
            }
        });  
  }
}

function get_temp_city_by_state_code(){
  var temp_state = $("#searchState").val();
  if(temp_state){
	   $.ajax({
            url: 'https://eventsinamerica.com/contact/gettempcitybystatecode',
            type: 'POST',
            dataType: "html",
            data: 'temp_state=' + temp_state,
            success: function(response) {
               $("#searchCity").html(response);
			    $("#searchCity").select2();
            }
        });  
  }
}

function get_state_by_country(){
 var temp_country = $("#searchcountry").val();
  if(temp_country){
	  $("#searchCity").html('');
	  $("#zipcode").html('');
	   $.ajax({
            url: 'https://eventsinamerica.com/contact/getstatebycountryid',
            type: 'POST',
            dataType: "html",
            data: 'temp_country=' + temp_country,
            success: function(response) {
               $("#searchState").html(response);
			   $("#searchState").select2();
            }
        });  
  }
	
}

