<script type="text/javascript">
  var DomainUrl="https://www.gbta.org/";
    function setHeaderState(profile) {
		

         try {
		
     if(profile==0) {

       

				 $('#lijoinMe').removeClass('hide');
             if (location.href.replace(/.*\/\/[^\/]*/, '') != "/") {
                 ReturnURL = "?ReturnUrl=" + encodeURIComponent(location.href.replace(/.*\/\/[^\/]*/, ''));
             }
			
			if(ReturnURL.toLowerCase().indexOf("%2fstore%2f")>-1)
			{	
			ReturnURL='productstore';

			
			}
			

            switch (ReturnURL.toLowerCase()) {
               

					case 'productstore':
                     ReturnURL = "?ReturnUrl="+encodeURIComponent("/myaccount?actionurl=Product&redirect="+location.href.replace(/.*\/\/[^\/]*/, ''));
                    break;
                default:
                     ReturnURL = "?ReturnUrl=" + encodeURIComponent(location.href.replace(/.*\/\/[^\/]*/, ''));
             }
			
             if (window.location.href.toLowerCase().indexOf("sitelogout")!=-1)
                 ReturnURL = "";
            
             $('#dnn_HEADERSETTING1_liLogin').find('a').attr('href', DomainUrl + "ssologin"+ReturnURL); 
            }
			
        else
		{
            
             getHeaderProfilePhoto(profile);
			$('#lijoinMe').addClass('hide');
            }

     
 } catch (err) {
     $('#dnn_HEADERSETTING1_liLogin').find('a').attr('href', DomainUrl + "ssologin");
 }



 }

 function getHeaderProfilePhoto(CustomerCD) {
  
 
 $.ajax({
    url: DomainUrl + 'cvweb/cgi-bin/cvimage.dll/view?ENTITY=MEM',
	 data: { ENTITYCD: CustomerCD},
	 type: 'GET',
    success: function (res) {
        // Handle success here
			if (res.length > 30) {
		
                 $("#ImageHeaderProfile").attr("src", DomainUrl + "cvweb/cgi-bin/cvimage.dll/view?ENTITY=MEM&ENTITYCD=" + CustomerCD);
             	
			 } else {

                 $("#ImageHeaderProfile").attr("src", DomainUrl + "images/userplaceholder.jpg");

             }
    },
    cache: false
}).fail(function (jqXHR, textStatus, error) {
    // Handle error here
  
});
 

 }
	
	
</script>

