<meta name="google-site-verification" content="IlBoqniJKlXZwQyiJSLg0vXpwp63MO6tbYD1h-Sejrs" /><script type="text/javascript">
//<![CDATA[
    LightboxOptions = Object.extend({
        fileLoadingImage:        urlSkinsite+'images/loading.gif',     
        fileBottomNavCloseImage: urlSkinsite+'images/closelabel.gif',
    
        overlayOpacity: 0.8,   // controls transparency of shadow overlay
    
        animate: true,         // toggles resizing animations
        resizeSpeed: 7,        // controls the speed of the image resizing animations (1=slowest and 10=fastest)
    
        borderSize: 10,         //if you adjust the padding in the CSS, you will need to update this variable
    
    	// When grouping images this is used to write: Image # of #.
    	// Change it for non-english localization
    	labelImage: "Image",
    	labelOf: "of"
    }, window.LightboxOptions || {});
    (function($) {
        var halochange_i;
        var halochange_width;
        /***** Start Function *****/
        function halo_doHaloChange(i, width) {
            // Add class="halochange_NUMBER"
            $('body').removeClass('halochange_0 halochange_1 halochange_2 halochange_3 halochange_4')
    		.addClass('halochange_'+i);
        };
        
        function halo_fixZoomMobile() {
            if(isMobile){
            	var viewport = $('head meta[name=viewport]');
            	if (viewport.length == 0) {
            		$('head').append('<meta name="viewport" content="width=device-width, initial-scale=1.0"/>');
            		viewport = $('head meta[name=viewport]');
            	}	
            	var old_content = viewport.attr('content');	
            	function disabledZoom(){
            		viewport.attr('content', old_content + ', user-scalable=0');
            	}
            	function enabledZoom(){
            		viewport.attr('content', old_content);
            	}	
            	$("input[type=text], textarea, select").mouseover(disabledZoom).mousedown(enabledZoom);
            }
        };
        
        ADAPT_CONFIG.callback = function(i, width) {
        	halochange_i = i;
        	halochange_width = width;	
        	halo_doHaloChange(halochange_i, halochange_width);
        };
        
        $(document).ready(function(){ 
            // alert('ready');
            /* zoom mobile gitbub */             
        	halo_fixZoomMobile();
        });
        
        var tmChange;
        $(window).bind('load', function() {
            //alert('load');
            /* halo change */
            clearTimeout(tmChange);
            setTimeout(function(){halo_doHaloChange(halochange_i, halochange_width)},300);
        });
    })(jQuery);
    
//]]>
</script>
