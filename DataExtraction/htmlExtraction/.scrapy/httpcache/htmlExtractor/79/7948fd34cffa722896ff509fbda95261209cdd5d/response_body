        <script>
function viewFeedbackFormAjax() {
	$.ajax({
		type:"GET",
		url: '/users/feedbackFormAjax',
		success: function(html){
	               $('#popup').html(html);
	               $("#popup").dialog({
				        title: 'Feedback' 
				    });
			       $('#popup').dialog('open');
	    },
		error: function(){
			        showMessageError("Sorry, there was a problem loading this form, please try again. ");
	    }
	});
}

function postFeedbackFormData(idForm) {
    $('#loading_feedback').html("<img src='/img/loading.gif'/>");
	$.ajax({
		type:"POST",
		url: '/users/feedbackPostFormAjax',
		data: $("#feedbackForm").serialize(),
		success: function(html){
					if ( (html.length != 0) && (html.indexOf("Word not allowed: " > -1) )) {
						showMessageError(html);
					} else {
	               	showMessageSucess("Your feedback was sent successfully");
	               }
				   $('#popup').dialog('close');
	    },
		error: function(){
	               showMessageError("There was an error in the feedback process. ");
				   $('#popup').dialog('close');
	    }
	});

}
</script>
