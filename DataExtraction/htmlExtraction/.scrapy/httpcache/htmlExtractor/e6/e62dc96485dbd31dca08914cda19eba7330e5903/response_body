	var b = 100/a;	
	//alert(b);
	var c = b-0.1;
	$(".badges ul.nav li").css("width", c+"%");
	
	$(".nav-container > .nav-content:first-child").show();
	
	$("ul.nav > li:first-child").addClass("active");
	
    $(".badges ul.nav > li").click(function(){
        var x = $(this).attr("id");
        //alert(x);
        var y = x.split("-").pop(-1);
        //alert(y);
        var z = "nav-content-" + y;
        //alert(z);
		$(".nav-content").hide();
		$("#"+z).toggle();
		
		$("ul.nav li").removeClass("active");
		
		$(this).addClass("active");
    });
