    <script type="text/javascript">
		$(document).ready(function(){
			$("#nav").accordion({ active: false });

			var currentpage1 = "../" + location.pathname.substr(1, location.pathname.lastIndexOf(".") -1 );
			var currentpage = "";
			var parent = $('#nav a[@href^="'+currentpage1+'"]').parent().parent().attr('id');
			var currpage = location.href.substr(location.href.lastIndexOf("/")+1, location.href.lastIndexOf(".") - location.href.lastIndexOf("/")-1 );
			var currpage1 = "../" + location.pathname.substr(1, location.pathname.lastIndexOf(".") -1 );
			if( parent == "nav" )
			{
				parent = $('#nav a[@href^="'+currentpage1+'"]').parent().attr('id');
			}
			parent = parent-1;


			$(".menu_act").click( function() {
				var goto = $(this).attr("href");
				location.href = goto;
			});


			if(currentpage1 == "../")
			{
				parent = 1;
			}
			else if( !isNaN(parent) )
			{
				$("#nav").accordion("activate",parent);
			}

		});
	</script>
    <script type="text/javascript">
	function displaymsg(){
	   	var agree = confirm('By clicking on this link, you agree to be re-directed to a third-party partner web site, Icims, that processes the collected information on behalf of Amalgamated Life but is not maintained or owned by Amalgamated Life. Amalgamated Life assumes no responsibility in connection with your use of their web site. Any information that you provide directly to them is subject to the privacy policy posted on their site.')
		if(agree){
			window.location.href= "http://jobs-amalgamatedlife.icims.com/jobs/intro";
			return true;
		}
		else return false;
	}
    </script>
