	var closeNav = function(){
		if ($(window).width() <= 768){
				var navMain = $('.navbar-collapse');
				navMain.collapse('hide');
		}
	}

	//baking club nav
	$('.nav-item-133').click(function(event) {
	  event.preventDefault();
	  			$('.login-modal').modal('toggle');

		//if mobile close responsive nav on window pop up
		closeNav();	
	    
		event.stopPropagation();
			});
	//baking nation nav
	$('.nav-item-1668').click(function(event) {
	  event.preventDefault();
	  			$('.login-modal').modal('toggle');

		//if mobile close responsive nav on window pop up
		closeNav();	

		event.stopPropagation();
			});
	//browse recipes & recipe library nav
	$('.nav-item-125').click(function(event) {
	  event.preventDefault();
	  $('#recipe-category-nav').hide();
		$('.browse-recipes-submenu').modal('toggle');

		//if mobile close responsive nav on window pop up
		closeNav();	

		$('.browse-recipes-submenu').on('shown.bs.modal', function (e) {
		  $('#recipe-category-nav').hide();
		})
		$('.browse-recipes-submenu').on('hidden.bs.modal', function (e) {
		  $('#recipe-category-nav').show();
		})
		event.stopPropagation();
	});
	//recipe library nav
	$('.nav-item-1664').click(function(event) {
	  event.preventDefault();
	  $('#recipe-category-nav').hide();
		$('.recipe-library-submenu').modal('toggle');
		//if mobile close responsive nav on window pop up
		closeNav();
			
		$('.recipe-library-submenu').on('shown.bs.modal', function (e) {
		  $('#recipe-category-nav').hide();
		})
		$('.recipe-library-submenu').on('hidden.bs.modal', function (e) {
		  $('#recipe-category-nav').show();
		})
		event.stopPropagation();
	});

</script>
