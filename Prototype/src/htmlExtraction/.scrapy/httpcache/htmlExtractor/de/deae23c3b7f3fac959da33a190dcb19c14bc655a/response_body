<script language="JavaScript" type="text/JavaScript">
	
	function getID(theForm){
		var url = window.location.href;
		var index = url.indexOf('?id=');
		if(index!=-1) {
			document.getElementById(theForm).id.value = url.substring(index +4);
		}
	}
	
	function gup( name )
	{
	  var regexS = "[\\?&]"+name+"=([^&#]*)";
	  var regex = new RegExp( regexS );
	  var tmpURL = window.location.href;
	  var results = regex.exec( tmpURL );
	  if( results == null )
		return "";
	  else
		return results[1];
	}
	
		function myHref(){
		var id = gup("id");
		if(id != "") {
			document.getElementById('index').href=document.getElementById('index').href+"?id="+id;
			document.getElementById('indexb').href=document.getElementById('indexb').href+"?id="+id;

					document.getElementById('link1').href=document.getElementById('link1').href+"?id="+id;

					document.getElementById('link2').href=document.getElementById('link2').href+"?id="+id;

					document.getElementById('link3').href=document.getElementById('link3').href+"?id="+id;

					document.getElementById('link4').href=document.getElementById('link4').href+"?id="+id;

					document.getElementById('link5').href=document.getElementById('link5').href+"?id="+id;

					document.getElementById('link7').href=document.getElementById('link7').href+"?id="+id;
	

					document.getElementById('link1b').href=document.getElementById('link1b').href+"?id="+id;

					document.getElementById('link2b').href=document.getElementById('link2b').href+"?id="+id;

					document.getElementById('link3b').href=document.getElementById('link3b').href+"?id="+id;

					document.getElementById('link4b').href=document.getElementById('link4b').href+"?id="+id;

					document.getElementById('link5b').href=document.getElementById('link5b').href+"?id="+id;

					document.getElementById('link7b').href=document.getElementById('link7b').href+"?id="+id;
			
		}
			document.getElementById('link1').href=document.getElementById('link1').href+"?id="+id;
			document.getElementById('link1b').href=document.getElementById('link1b').href+"?id="+id;

		}
	


</script>


