	<script type="text/javascript">
	var title = 'TypoScript object path "lib.elantas.popup.title" does not exist';
	var subtitle = 'TypoScript object path "lib.elantas.popup.subtitle" does not exist';
	var text = 'TypoScript object path "lib.elantas.popup.text" does not exist';
	var buttonLeft = 'TypoScript object path "lib.elantas.popup.buttonLeft" does not exist';
	var buttonRight = 'TypoScript object path "lib.elantas.popup.buttonRight" does not exist';

	/* Documentation
	 * http://beeker.io/exit-intent-popup-script-tutorial
	 * We have to put this into a Template because otherwise we don't get our text translated
	 */

	$(document).ready(function () {
		/* We only show the popup when we are on the elantas page */
		
				var activated = 0;
			

		if (activated) {
			bioEp.init({
				html: '<div id="popup-improve">' +
				'<div class="headline">' +
				'<h1><strong>' + title + '</strong></h1>' +
				'</div>' +
				'<div class="subheadline">' + subtitle + '</div><br>' +
				'<p>' + text + '</p>' +
				'<div class="buttons">' +
				'<div class="button-left"><button type="button" class="btn btn-default">' + buttonLeft + '</button></div>' +
				'<div class="button-right"><button type="button" class="btn btn-secondary">' + buttonRight + '</button></div>' +
				'<div class="clearer"></div>' +
				'</div>' +
				'</div>'
			});

			$('#bio_ep .buttons .button-left button').on('click', function () {
				bioEp.hidePopup();
				window.location.href = 'https://www.elantas.com/index.php?id=33472&L=1';
			});
			$('#bio_ep .buttons .button-right button').on('click', function () {
				bioEp.hidePopup();
			});
			$('a').on('click', function (e) {
				var targetElementUrl = $(this).attr('href');
				var allowedUrls = [
					'elantas'
				];

				/* Check whether we have an external URL or an internal */
				var externalUrl = allowedUrls.some(function (arrVal) {
					return targetElementUrl.toLowerCase().indexOf(arrVal) == -1;
				});

				if (document.cookie.indexOf("bioep_shown") === -1 && externalUrl) {
					e.preventDefault();

					bioEp.showPopup();

					$('#bio_ep .buttons .button-right button, #bio_ep #bio_ep_close').on('click', function () {
						bioEp.hidePopup();
						if ($(this).attr('target') === '_blank') {
							var win = window.open(targetElementUrl, '_blank');
							win.focus();
						} else {
							window.location.href = targetElementUrl;
						}
					});
				}
			});
		}
	});
</script>



