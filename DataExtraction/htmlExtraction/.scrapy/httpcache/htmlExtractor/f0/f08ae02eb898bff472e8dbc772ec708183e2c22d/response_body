                contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("id")] = $("option[value='" + $(this).val() + "']", $(this)).text();
            });
    
            $("#contour_form_TopContactForm .contourPage input").each(function() {

	    
	    
                if($(this).attr('type') == "text"){
                    contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("id")] = $(this).val();
                }

                if($(this).attr('type') == "radio"){
                    if($(this).is(':checked'))
                    {
                        contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("name")] = $(this).val();
                    }
                }

                if($(this).attr('type') == "checkbox"){

                    if($(this).attr('id') != $(this).attr('name'))
                    {
                        if($(this).is(':checked'))
                        {
                            if(contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("name")] == null)
                            {
                                contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("name")] = $(this).val();
                            }
                            else
                            {
                                contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("name")] += "," + $(this).val();
                            }
                        }
                    } else {

                        contourFieldValuesafd2a51824394ed3a9f0aac23dc1966f[$(this).attr("name")] = $(this).is(':checked').toString();
                    }
                }

            });    
        }        

