    function closeWindow() {
        var oWnd = $find("ctl00_radWin");
        oWnd.close();
    }

    function forgotPassword() {
        var oWnd = $find("ctl00_radWin");
        oWnd.setUrl("/tools/forgot.aspx");
        oWnd.setSize(525, 300)
        oWnd.show();
    }

