<script type="text/javascript">

    $(document).ready(function () {

        $('#ctl00_ctrlQuickSearch_inp').keydown(function (event) {
            //if we get here enter has been struck without selecting any results in the autocomplete dropdown
            if (event.keyCode == '13') {
                event.stopPropagation();
                event.preventDefault();
                window.location.href = '/en/Corporate/Search/?query=' + $(this).val();
                return false;
            }
        });
    });

</script>
