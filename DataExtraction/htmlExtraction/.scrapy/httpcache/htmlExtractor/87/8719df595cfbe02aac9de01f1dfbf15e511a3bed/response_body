			var mediaQuery = 'desk';

			if (Modernizr.mq('only screen and (max-width: 600px)') || Modernizr.mq('only screen and (max-height: 520px)')) {

				mediaQuery = 'mobile';
				jQuery("a[rel^='prettyPhoto']").unbind('click');
				
				if (jQuery('.flexslider a').hasClass('popup-slideshow')) {
					jQuery("a.popup-slideshow[rel^='prettyPhoto']").prettyPhoto(ppArgs);
