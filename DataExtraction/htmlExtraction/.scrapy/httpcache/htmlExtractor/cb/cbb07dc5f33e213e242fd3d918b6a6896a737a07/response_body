    "use strict";

    function requestCookies() {
        var xmlhttp;

        if (window.XMLHttpRequest) {
            xmlhttp = new XMLHttpRequest();
        } else {
            xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
        }

        var url = "https://www.harveynichols.com/cookiepopulator/";
        if ('https:' == document.location.protocol) {
            if ('http://' == url.substr(0, 7)) {
                url = 'https' + url.substr(4);
            }
        }

        xmlhttp.open("GET", url, true);
        xmlhttp.send();
    }

    if (document.cookie.indexOf("cookies_populated") < 0 ||
        document.cookie.indexOf("CACHED_FRONT_FORM_KEY") < 0
    ) {
        requestCookies();
    }

    //]]
</script>
