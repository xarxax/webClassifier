     // helper func to trace _satellite.track() calls
     (function () {
         if (typeof console.trace !== 'function') return;
         var debugKey = 'debugAnalytics';
         if (localStorage && localStorage.getItem(debugKey)) {
             var trackCopy = window._satellite.track;

             window._satellite.track = function () {
                 console.trace('_satellite.track() trace');
                 trackCopy.apply(window._satellite, arguments);
             };
         }

         window.debugAnalytics = function (bool) {
             if (!localStorage) return;
             if (!bool) {
                 localStorage.removeItem(debugKey);
                 return;
             }

             localStorage.setItem(debugKey, bool);
         };
     })();
 </script>


