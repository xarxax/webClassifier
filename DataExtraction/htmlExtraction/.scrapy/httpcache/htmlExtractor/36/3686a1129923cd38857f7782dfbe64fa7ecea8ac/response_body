<script src="js/jquery.min.js"></script>

<script src="js/jquery.validate.min.js"></script>
<script src="js/validation-additional-methods.js"></script>
<script>
    $(document).ready(function($) {
    	
    	// Wait for the DOM to be ready
$(function() {
	
	
	$conv = $;
$conv.validator.addMethod("alphabetsnspace", function(value, element) {
        return this.optional(element) || /^[a-zA-Z ]*$/.test(value);
}); 

   $("form").each(function(){
        $(this).validate({
	
	  //$("form").validate({
   
    rules: {
      // The key name on the left side is the name attribute
      // of an input field. Validation rules are defined
      // on the right side
      //name: "required",
      name: {
      	  required: true,
           alphabetsnspace: true
      	  //lettersonly: true
      	},

      city: {
      	  required: true,
           alphabetsnspace: true
      	  //lettersonly: true
      	},
      
       email: {
        required: true,
        // Specify that email should be validated
        // by the built-in "email" rule
        email: true,
        },
        
      mobile: {
                required: true,
                number: true,
                maxlength: 10,
                minlength: 10
            },
            
      

      
      
    },
    // Specify validation error messages
    messages: {
      name: {
      	 required: "Please enter your name",
      	 alphabetsnspace: "Please enter only characters"
      	 //lettersonly: "Please enter only characters"
      	},

      city: {
      	 required: "Please enter your city",
      	 alphabetsnspace: "Please enter only characters"
      	 //lettersonly: "Please enter only characters"
      	},
     mobile: {
                required: "Please enter your mobile number",
                number: "Please enter only numeric value",
                minlength: "Please enter at least 10 digit",
                maxlength: "Please enter 10 digit only"
            },
     
     email: {
                required: "Please enter your email address",
                email: "Please enter valid email address"
            },
     
  
    },
    
  });   
  });/* end validate function */
  
});/* end function */

    	
    	
    }); /* end document ready */
</script>   



