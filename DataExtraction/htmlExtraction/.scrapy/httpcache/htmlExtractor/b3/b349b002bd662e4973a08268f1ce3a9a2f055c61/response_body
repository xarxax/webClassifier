        $("#headerbar_burger").click(function() {
			if ($("#MenuBar").is(":visible")) {
				$(this).removeClass("headerbar-burger-down");
				$("#MenuBar").hide();
			} else {
				$(this).addClass("headerbar-burger-down");
				$("#MenuBar").show();
			}
