$=jQuery;

function sfmcPopulateForm(){
	var formkeys = {
		"#input_1_6":"Name",
		"#input_1_1":"Company",
		"#input_1_5":"Title",
        "#input_1_4":"Email",
        "#input_1_7":"PhoneNumber",
        "#input_1_8":"Industry",
        "#input_1_9":"Other"				
	}

	$.each(formkeys,function(key,val){
		$(key).on('change', function(){
			$("input[name="+val+"]").val($(this).val());
		})
	})
}

$(function(){
	sfmcPopulateForm();
});

$(document).ready(function(){
	$('body').addClass('loaded');
})

$(document).bind('gform_confirmation_loaded', function(event, formId){
	if ($('form#sfmc').length){
        setTimeout(
            function(){ 
                $('form#sfmc').submit();				
            }, 4000);

	} else {
		$('.gated-popup').fadeIn('fast');
		
	}		
});

$(document).bind('gform_post_render', function(event, formId){
	sfmcPopulateForm();
});

</script>

