<script>
	function replaceSpecialChar(input,type){
        input = input.replace(/[^A-Za-z0-9\s-]/gi, '');
        if(type == "desc" || type == "desc2"){
          input = input.replace(/\s/g, '_');
        }else{
          input = input.replace(/\s/g, '');
        }
		input = input.toLowerCase();

        return input;
	}

    $(document).ready(function () {
                var url_action = 'https://www.americanholt.com/search/';

        $(document).on('input change blur keyup', '.industry , .oem , .model , .desc  , .part_number', function () {
            var industry = $('.industry :selected').data("slug");
            var oem = $('.oem :selected').data("slug");
            var model = $('.model :selected').data("slug");
            var desc = replaceSpecialChar($.trim($(".desc").val()),"desc");
            var part_number = replaceSpecialChar($.trim($(".part_number").val()),"part");


            var created_url = ''
            if (industry != '' && typeof industry != 'undefined') {
                created_url = industry + "/";
            }
            if (oem != '' && typeof oem != 'undefined') {
                created_url = created_url + oem + "/";
            }
            if (model != '' && typeof model != 'undefined') {
                created_url = created_url + model + "/";
            }
            if (desc != '' && typeof desc != 'undefined') {
                created_url = created_url + desc.replace(/\//g, "_") + "/";
            }
            if (part_number != '' && typeof part_number != 'undefined') {
                created_url = created_url + part_number.replace(/\//g, "") + "/";
            }

            $('#searching').attr('href', url_action + created_url);
        });

        $(document).on('change', '.industry', function () {
            var industry_id = $(this).val();

            $.ajax({
                url: 'https://www.americanholt.com/main/searchFilterOem/' + industry_id,
                type: "post",
                dataType: 'html',
                success: function (response) {

                    $('.oem').html(response);
                    $('.oem').removeClass('halfWidth');
                    $('.model').removeClass('halfWidth');
                    $('.model').hide();
                    $('.model').html("");


                    var industry = $('.industry :selected').data("slug");
                    var oem = $('.oem :selected').data("slug");
                    var model = $('.model :selected').data("slug");
                    var desc = replaceSpecialChar($.trim($(".desc").val()));
                    var part_number = replaceSpecialChar($.trim($(".part_number").val()));

                    var created_url = ''
                    if (industry != '' && typeof industry != 'undefined') {
                        created_url = industry + "/";
                    }
                    if (oem != '' && typeof oem != 'undefined') {
                        created_url = created_url + oem + "/";
                    }
                    if (model != '' && typeof model != 'undefined') {
                        created_url = created_url + model + "/";
                    }
                    if (desc != '' && typeof desc != 'undefined') {
                        created_url = created_url + desc + "/";
                    }
                    if (part_number != '' && typeof part_number != 'undefined') {
                        created_url = created_url + part_number + "/";
                    }

                    $('#searching').attr('href', url_action + created_url);

                }
            });
        });

        $(document).on('change', '.oem', function () {
            var model_id = $(this).val();
            var searching = $('.oem :selected').text();

            $.ajax({
                url: 'https://www.americanholt.com/main/searchFilterCategory/' + model_id,
                type: "post",
                dataType: 'html',
                success: function (response) {
                    if (response != 'false') {
                        $('.oem').addClass('halfWidth');
                        $('.pageformWraper').addClass('searchOme');
                        if ($("select").hasClass("model")) {

                            $('.model').html(response);


                            var industry = $('.industry :selected').data("slug");
                            var oem = $('.oem :selected').data("slug");
                            var model = $('.model :selected').data("slug");
                            var desc = replaceSpecialChar($.trim($(".desc").val()));
                            var part_number = replaceSpecialChar($.trim($(".part_number").val()));

                            var created_url = ''
                            if (industry != '' && typeof industry != 'undefined') {
                                created_url = industry + "/";
                            }
                            if (oem != '' && typeof oem != 'undefined') {
                                created_url = created_url + oem + "/";
                            }
                            if (model != '' && typeof model != 'undefined') {
                                created_url = created_url + model + "/";
                            }
                            if (desc != '' && typeof desc != 'undefined') {
                                created_url = created_url + desc + "/";
                            }
                            if (part_number != '' && typeof part_number != 'undefined') {
                                created_url = created_url + part_number + "/";
                            }

                            $('#searching').attr('href', url_action + created_url);
                        } else {
                            $('.oem').after(response);
                        }
                    } else {
                        $('.pageformWraper').removeClass('searchOme');
                        $('.oem').removeClass('halfWidth');
                        $('.model').remove();
                    }
                }
            });
        });
        $('input.desc2').keyup(function (e) { 
            console.log("input.desc2",e.which);
            if (e.which == 13) {
                $("#searchDesc").trigger('click');
            }
        }); 
        $("#searchDesc").click(function(event) {
            var action = $(this).attr("href");
            location.href = action;
        }); 
        
        var url_action2 = 'https://www.americanholt.com/generalsearch/';
        $(document).on('input change blur keyup', '.desc2', function () {

            var desc = replaceSpecialChar($.trim($(".desc2").val()),"desc2");
            var part_number = replaceSpecialChar($.trim($(".part_number").val()),"part");


            var created_url = ''

            if (desc != '' && typeof desc != 'undefined') {
                created_url = created_url + desc.replace(/\//g, "_") + "/";
            }
            if(created_url != "" ){
                $('#searchDesc').attr('href', url_action2 + created_url);
            }else{
                $('#searchDesc').attr('href', "javascript:void(0);");
            }
        });

        $("#searching").click(function (event) {
            var industry = $(".industry").val();
            var oem = $(".oem").val();
            var desc = replaceSpecialChar($.trim($('[name="desc"]').val()));
            var part_number = replaceSpecialChar($.trim($('[name="part_number"]').val()));

            if (industry == '' && oem == '' && desc == '' && part_number == '') {
                $.notify({
                    icon: 'glyphicon glyphicon-warning-sign',
                    title: "Search Request:",
                    message: " Please Select or Provide Value",
                }, {
                    type: 'danger',
                    animate: {
                        enter: 'animated bounceIn',
                        exit: 'animated bounceOutRight'
                    }
                });
                event.preventDefault();
            }
        });
    });

    $(function () {
        $(".part_number").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "https://www.americanholt.com/main/autocompleteSearch",
                    dataType: "json",
                    data: {
                        q: request.term,
                        industry: $("#ind_select").val(),
                        oem: $("#oem_select").val(),
                        model: $("#model_select").val()
                    },
                    success: function (data) {
                        response(data);
                    }
                });
            },
            minLength: 4,
            open: function () {
                $(this).removeClass("ui-corner-all").addClass("ui-corner-top");
            },
            close: function () {
                $(this).removeClass("ui-corner-top").addClass("ui-corner-all");
            }
        });
    });


</script>
    <style>
