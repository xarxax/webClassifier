    function setFocus(){
        if (document.getElementById("procura-text").value=="Pesquisa"){
            document.getElementById("procura-text").value="";
        }
        if (document.getElementById("procura-text").value=="Search"){
            document.getElementById("procura-text").value="";
        }
    }
    function lostFocus(){
        if (document.getElementById("procura-text").value==""){
            document.getElementById("procura-text").value="Search";
        }
    }
    function setFocusIE8(){
        if (document.getElementById("procura-text_ie8").value=="Pesquisa"){
            document.getElementById("procura-text_ie8").value="";
        }
        if (document.getElementById("procura-text_ie8").value=="Search"){
            document.getElementById("procura-text_ie8").value="";
        }
    }
    function lostFocusIE8(){
        if (document.getElementById("procura-text_ie8").value==""){
            document.getElementById("procura-text_ie8").value="Search";
        }
    }
</script><div class="clear"></div>
    <style type="text/css">
        /* rotator in-page placement */
        div.rotatornoticia {
            position:relative;
            height:75px;
            margin-left: -45px;
            margin-top: -5px;
            display: none;
        }
        /* rotator css */
        div.rotatornoticia ul li {
            float:left;
            position:absolute;
            list-style: none;
            width: 150px;
        }
        /* rotator image style */
        div.rotatornoticia ul li img {
            padding: 4px;
        }
        div.rotatornoticia ul li.show {
            z-index:500
        }
    </style>

    <script type="text/javascript">

        function theRotatorNoticia() {
            //Set the opacity of all images to 0
            $('div.rotatornoticia ul li').css({opacity: 0.0});
            //Get the first image and display it (gets set to full opacity)
            $('div.rotatornoticia ul li:first').css({opacity: 1.0});
            //Call the rotator function to run the slideshow, 6000 = change to next image after 6 seconds
            setInterval('rotateNoticia()',6000);
        }

        function rotateNoticia() {
            //Get the first image
            var current = ($('div.rotatornoticia ul li.show')?  $('div.rotatornoticia ul li.show') : $('div.rotatornoticia ul li:first'));
            if ( current.length == 0 ) current = $('div.rotatornoticia ul li:first');
            //Get next image, when it reaches the end, rotate it back to the first image
            var next = ((current.next().length) ? ((current.next().hasClass('show')) ? $('div.rotatornoticia ul li:first') :current.next()) : $('div.rotatornoticia ul li:first'));
            //Un-comment the 3 lines below to get the images in random order
            //var sibs = current.siblings();
            //var rndNum = Math.floor(Math.random() * sibs.length );
            //var next = $( sibs[ rndNum ] );
            //Set the fade in effect for the next image, the show class has higher z-index
            next.css({opacity: 0.0})
            .addClass('show')
            .animate({opacity: 1.0}, 1000);
            //Hide the current image
            current.animate({opacity: 0.0}, 1000)
            .removeClass('show');

        };

        $(document).ready(function() {
            //Load the slideshow
            theRotatorNoticia();
            $('div.rotatornoticia').fadeIn(1000);
            $('div.rotatornoticia ul li').fadeIn(1000); // tweek for IE
        });
    </script>

