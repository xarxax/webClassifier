<script>
  $(document).ready(function () {
	 
	 var owl = $("#owl-demo");
	 
	 owl.owlCarousel();
	 
	 // Custom Navigation Events
	 $(".next").click(function () {
		owl.trigger('owl.next');
	 })
	 $(".prev").click(function () {
		owl.trigger('owl.prev');
	 })
	 $(".play").click(function () {
		owl.trigger('owl.play', 1000);
	 })
	 $(".stop").click(function () {
		owl.trigger('owl.stop');
	 })
	 
	 
  });
</script>
