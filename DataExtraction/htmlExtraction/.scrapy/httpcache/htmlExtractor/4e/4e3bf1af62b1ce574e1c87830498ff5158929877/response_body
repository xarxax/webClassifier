<script type='text/javascript'>
function fetch_country_locale(country_id){
        var destination = 'http://soleildirect.com/estore/index.php?route=module/custom_locale/locale';
    $("#loading").ajaxStart(function () {
        $(this).show();
    });

    $("#loading").ajaxStop(function () {
        $(this).hide();
    });
    $.post(destination, {country_id : country_id}, function(data){
       var locale = $.parseJSON(data); 
       $('#admin_country_lang_sel_if_one').html('');
       $('#admin_country_curr_sel_if_one').html('');
       $('#admin_country_curr_sel_span').css('display', 'none');
       $('#admin_country_lang_sel_span').css('display', 'none');
       $('#admin_country_lang_sel').html('');
       $('#admin_country_curr_sel').html('');
       if(locale['languages'].length == 1){
           $('#customlocale_chosen_lang_code').val(locale['languages'][0]['code']);
           $('#admin_country_lang_sel_if_one').html('Language: ' + locale['languages'][0]['name']);
           $('#admin_country_lang_sel_if_one').css('display', 'block');
       }else{
           for(var i = 0; i < locale['languages'].length; i++){
               var langi = locale['languages'][i];
               var option_html_lang = '';
               if(langi['code'] == 'en'){
                    option_html_lang = "<option value='" + langi['code'] + "' selected>" + langi['name'] + "</option>";
               }else{
                    option_html_lang = "<option value='" + langi['code'] + "'>" + langi['name'] + "</option>";
               }
               $('#admin_country_lang_sel').append(option_html_lang);
           }
           $('#admin_country_lang_sel').change(function(){
              $('#customlocale_chosen_lang_code').val($(this).val()); 
           });
           $('#customlocale_chosen_lang_code').val($('#admin_country_lang_sel').val());
           $('#admin_country_lang_sel_span').css('display', 'block');
       }
       if(locale['currencies'].length == 1){
           $('#customlocale_chosen_curr_code').val(locale['currencies'][0]['code']);
           $('#admin_country_curr_sel_if_one').html('Currency: ' + locale['currencies'][0]['name']);
           $('#admin_country_curr_sel_if_one').css('display', 'block');
       }else{
           for(var i = 0; i < locale['currencies'].length; i++){
               var curri = locale['currencies'][i];
               var option_html_curr = '';
               if(curri['code'] == 'EUR'){
                   option_html_curr = "<option value='" + curri['code'] + "' selected>" + curri['name'] + "</option>";
               }else{
                   option_html_curr = "<option value='" + curri['code'] + "'>" + curri['name'] + "</option>";
               }
               $('#admin_country_curr_sel').append(option_html_curr);
           }
           $('#admin_country_curr_sel').change(function(){
              $('#customlocale_chosen_curr_code').val($(this).val()); 
           });
           $('#customlocale_chosen_curr_code').val($('#admin_country_curr_sel').val());
           $('#admin_country_curr_sel_span').css('display', 'block');
       }
    });
}
$(document).ready(function(){
    $('#top-container').
        append('<div id="custom_locale_header" style="width:auto;margin-right:6.2%">You are currently in: Netherlands <a href=\'javascript:void(0);\' onClick=\'showDialog();\'>(Change...)</a></div>');
    fetch_country_locale($('#admin_country_sel').val());
    $('#customlocale_chosen_country_name').val($('#admin_country_sel option:selected').text());
    $('#admin_country_sel').change(function(){
       fetch_country_locale($(this).val());
       $('#customlocale_chosen_country_name').val($(this).find('option:selected').text());
    });

                        $('#country_selector_modal').css('display', 'none');
            });

function showDialog(){
    $('#country_selector_modal').dialog({dialogClass : 'estore_dialog', modal : true,
        width : 'auto',
	zIndex: 10999,
        position : {my : "middle", at : "middle", of : window}, 
        buttons : {
            'OK' : function(){
                $(this).dialog('close');
                $('#customlocale_form').submit();
            }
        }
        }
    );
}

</script>
