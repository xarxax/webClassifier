            jQuery(".close_order_btn").click(function(){
               var a = confirm('Are you sure that you want to close the order?');
               var order_id = jQuery(this).attr('id');//alert(order_id);
               var DIRURL = jQuery("#template_directory_uri").val();
                if(a){
                    jQuery.ajax({
                        method: "POST",
                        async: false,
                        url: DIRURL+"/check.php",
                        data: {order_id:order_id}
                    })
                    .done(function (response) {    ///alert("response : "+response);
                        var responseback = response;
                        alert('Order is Closed Successfully');
                        location.reload();
                    });
                }
                
            })
    </script>
    <script src="http://www.hsfoodservers.com/wp-content/themes/hsfoodserver/js/jquery.cookie.js"></script>
    <script>
  //  console.log(jQuery('.wppb-error').html());
    if(jQuery('.wppb-error').html()== undefined ){jQuery("#user_login").val('');
}else {jQuery("#user_login").val(jQuery.cookie('uname'));}
jQuery( "#user_login" ).blur(function() {
//alert( "Handler for .blur() called." );

var uname = jQuery("#user_login").val();
 jQuery.cookie('usernamecookie', 1);
   jQuery.cookie('uname',uname);
  // alert(jQuery.cookie('uname'));
});

</script>


