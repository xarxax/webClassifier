<script>
  $(function() {
      if(!isMobile){ 
    $('.marqueeTxt').bxSlider({
      mode:'fade', //'horizontal', 'vertical', 'fade',
      captions: false, 
      //slideWidth:2500,  
      
      auto: true,  
      autoHover: true, 
      pager: true, 
      controls:true, 
      pause: 2000

    });
        
      }else{
        
      document.getElementById("bannerArea").style.height = "100%"          
       //banner
      $('.marqueeTxt').bxSlider({
        mode:'fade', //'horizontal', 'vertical', 'fade',
        captions: false, 
        slideWidth:2500,  
        adaptiveHeight: true,
        auto: true,  
        autoHover: true, 
        pager: false, 
        controls:false, 
        pause: 2000
      }); 

      }
    });
</script>
