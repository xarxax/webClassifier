<script>
$(document).ready(function(){
$('.item p:nth-of-type(1)').each(function(i, obj) {
	if($(this).text().length > 200 ){
		var str = $(this).text()
		var para = str.split(" ");

		var res = '';
		$.each( para, function( key, value ) {
			if(key <= 50){
			  res += " "+value;
			}
			if(key ==  50){
				res+" ..."
			}
		});
		$(this).text(res)  ;
		$('.item').find("p").slice(2, 12).remove();
	 }
});
