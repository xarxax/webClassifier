<!-- BEGIN Calendar Popup -->
<script>

window.addEvent('domready', function() {
	$$('div.cal-wdPopup-1').each(function(el) {
		el.addEvent('mouseout', function(e) {
			if (!e) var e = window.event;
			var reltg = (e.relatedTarget) ? e.relatedTarget : e.toElement;
			while (reltg.tagName != 'BODY') {
				if (reltg.id == this.id) { return; }
				reltg = reltg.parentNode;
			}
			var day = this.id.substring(14);
			ModCalendarClosePopOut1 (day);
		});
	});
});

function ModCalendarClosePopOut1(day) {

	cell = document.getElementById('cal-wdPopup-1-' + day);

	if (!cell) return ( false );

	var fadeOut = new Fx.Style(cell, 'opacity', {onComplete:function(){cell.setStyle('display', 'none');}, duration:700});

	if (cell.style.display == 'block') {
		fadeOut.start(1,0);
	}

	return ( false );

}

function ModCalendarOpenPopOut1(day) {

	cell = document.getElementById('cal-wdPopup-1-' + day);

	if (!cell) return ( false );

	var fadeIn  = new Fx.Style(cell, 'opacity', {onStart:function(){cell.setStyle('display', 'block');}, duration:700});

	if (cell.style.display != 'block') {
		fadeIn.start(0,1);
	}

	return ( false );

}

</script>
<!-- END Calendar Popup -->

