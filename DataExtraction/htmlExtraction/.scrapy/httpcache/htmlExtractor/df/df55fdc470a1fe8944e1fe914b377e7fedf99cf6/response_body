function SetBasketCurrency(thecurrency) {
	$("#international-selection-box").addClass("loadingminibasket");
	Set_Cookie("SSCURRENCY", thecurrency, "180", "/");
	$.ajax({
	  url: '/SiteSphereHandler.ashx?action=basketsetcurrency&basketuid=4c2551b6-cff0-4345-b5cd-e60a9b665693&currency=' + escape(thecurrency),
	  success: function( data ) {
	  	RefreshPage();
	  }
	});
}

function SetBasketCurrencyCountries(thecurrency,vDelCountryID, vInvCountryID) {
	$("#international-selection-box").addClass("loadingminibasket");
	Set_Cookie("SSCURRENCY", thecurrency, "180", "/");

	$.ajax({
	  //url: '/SiteSphereHandler.ashx?action=basketsetcurrencycountryregion&norecalc=1&currency=' + escape(thecurrency) + '&setcurrencyusingbilltocountry=true&basketuid=4c2551b6-cff0-4345-b5cd-e60a9b665693&visitoruid=684afe7c-3f67-4c7a-898b-4e2b3e625471&billtocountryid=' + vInvCountryID + '&shiptocountryid=' + vDelCountryID,
	  url: '/SiteSphereHandler.ashx?action=basketsetcurrencycountryregion&norecalc=1&currency=' + escape(thecurrency) + '&basketuid=4c2551b6-cff0-4345-b5cd-e60a9b665693&visitoruid=684afe7c-3f67-4c7a-898b-4e2b3e625471&billtocountryid=' + vInvCountryID + '&shiptocountryid=' + vDelCountryID,
	  success: function( data ) {
	  	RefreshPage();
	  }
	});
}

