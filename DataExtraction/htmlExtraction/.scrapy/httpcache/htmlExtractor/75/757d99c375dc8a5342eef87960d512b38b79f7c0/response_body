		jQuery(function($){
			$(document).ready(function() {

				$(".homefeature .step2 .menu ul li").click(function() {
					var subMenuID = $(this).attr('id'); //alert(subMenuID);
					if($(this).parent().parent().hasClass("submenu3")) {
						$(".homefeature .step2 .submenu.submenu3 ul li").removeClass('active');
					} else {
						$(".homefeature .step2 .menu ul li, .homefeature .step2 .submenu.submenu3").removeClass('active');
					}
					$(this).addClass('active');
					$(".homefeature .step2 .submenu.submenu3."+subMenuID).addClass('active');
				});

				$(".homefeature #step1 a").click(function() {
					var stepID = $(this).attr('id');
					if($('div.homefeature').hasClass("step2")) {
						$('div.homefeature').removeClass('step2');
					} else {
						$('div.homefeature').addClass('step2 active');
						$('div.homefeature div.step2#step2'+stepID).addClass('active');
					}
					$("#tm-top-a").addClass("step2"+stepID);
				});

				$(".homefeature .step2 .menu a.close").click(function() {
					$('div.homefeature').removeClass('step2');
					$(".homefeature .step2, .homefeature .step2 > div.uk-grid  .submenu").hide();
					$(".homefeature .step2 .menu table tr, .homefeature .step2 > div.uk-grid > .submenu table tr").removeClass('active');
					$(".homefeature .step2 .info").show();
					return false;
				});

				
			});
			// END jQuery document READY check 
		});

