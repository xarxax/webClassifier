<script type="text/javascript">
	function showLogo(divContainer){
		var myDiv = document.getElementById(divContainer);
		var merced = document.getElementById('merced_quote');
		var yahoo = document.getElementById('yahoo_quote');		
		var shopzilla = document.getElementById('shopzilla_quote');		
		if( merced.style.display == 'block' ) {
			merced.style.display = 'none';
		}
		if( yahoo.style.display == 'block' ) {
			yahoo.style.display = 'none';
		}
		if( shopzilla.style.display == 'block' ) {
			shopzilla.style.display = 'none';		
		}
		myDiv.style.display = 'block';

	}
</script>
