<script type="text/javascript" charset="utf-8">
        /*
	    playPauseBtn = document.getElementById('playPauseButton');*/
        var togl = "true";
        function toggle()
        {
            if(togl == "false")
	        {
	            //playPauseBtn.src = null;
	            //playPauseBtn.src = "pause.gif";
			    //playPauseBtn.alt = "pause";
	            togl = "true";
	        }
	        else
	        {
	            //playPauseBtn.src = null;
	            //playPauseBtn.src = "play.gif";
			    //playPauseBtn.alt = "play";
	            togl = "false";
	        }
        }
	    function pauseAlways()
	    {
		    //playPauseBtn.src = null;
    	    //playPauseBtn.src = "play.gif";
		    //playPauseBtn.alt = "play";
		    togl = "false";
	    }
    	
	    var slideshow = new MDSK.Slideshow("slideMainDiv", ".slideDiv", {seconds_per_slide: 5.0});
