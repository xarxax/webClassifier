   <a href="/admin/Default.aspx" id="gobackend" class="undis noprint">zzzzzz</a>
   <a href="/mobile/Default.aspx" id="gobackendmobile" class="undis noprint">zzzzzz</a>

<script type="text/javascript">

    var loginCallback = null;

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
        }
        return "";
    }


    $(function () {
            var site = getCookie('site');
            if (site == 1) {
                $('meta[name=viewport]').remove();
                $('head').append('<meta name="viewport" content="width=1280">');  
                $("#mobile").attr("class", "");
                setCookie("site", 1, 30);

            } else if (site==2){
                $('meta[name=viewport]').remove();
                $('head').append('<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">');
                $("#mobile").attr("class", "undis");
                setCookie("site", 2, 30);

            } else  {
                if (screen.width < 768) {
                    setCookie("site", 2,30);
                    $('meta[name=viewport]').remove();
                    $('head').append('<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">');
                    $("#mobile").attr("class", "undis");
                }
            }

            getLoginfo();


            ShowResetPassword();
        });

        function getLoginfo( callback) {

            $.ajax({
                url: "admin/webservices/UserWebService.aspx?op=loginfo",
                dataType: "json",
                success: function (rs) {
                    if (rs.Code == 0) {
                        $("#login_btn").css("display", "none");
                        $("#account_btn").css("display", "block");
                        loginfo = rs.Result;
                        if (callback)
                        {
                            callback();
                        }
                    }
                }
            });
        }

        var isApplyJobsLogin = false;
        var loginfo = null;

        function isMobileClient()
        {
            if (getCookie('site') == 2) {
                return true;
            } else {
                if (screen.width < 768) {
                    setCookie("site", 2, 30);
                    return true;
                }
            }

            setCookie("site", 1, 30);
            return false;
        }

        function goBackendFullOrMobile() {

            if ($('#remember_me').prop('checked')) {
                setCookie("userName", $('#userName').val(), 30);
            } else {
                setCookie("userName", "", 30);
            } 

            if (isMobileClient())
            {
                document.getElementById('gobackendmobile').click();
            }else
            {
                document.getElementById('gobackend').click();
            }
        }

        function loginSubmit() {
            $.ajax({
                url: "admin/webservices/UserWebService.aspx",
                async: false,
                data: "op=Login&userName=" + $("#userName").val() + "&userPass=" + $("#password").val(),
                type: "post",
                dataType: "json",
                success: function (rs) {
                    if (rs.Code == 0) {
                        if (rs.Result != "") {
                            NoShowLogin();
                            if (isApplyJobsLogin == false) {
                                //window.open('/admin/Default.aspx', '_blank');
                                goBackendFullOrMobile();
                            } else {
                                getLoginfo(function () {
                                    if ((null != loginCallback)) {
                                        //alert("has callback");
                                        loginCallback();
                                    } else {
                                        //alert("no callback");
                                    }
                                });
                            }
                            
                        } else {
                            ShowAlert(rs.Msg);
                        }
                    } else if (rs.Code == 5) {
                        if (rs.Result != "" && rs.Result.UserType == 4) {
                            NoShowLogin();
                            goBackendFullOrMobile();
                           
                        } else {
                            ShowAlert();
                        }
                    } else {
                        ShowAlert(rs.Msg);
                    }
                }
            });
        }

        //check
        function CheckLoginFormat() {
            var Uname = $("#userName").val()
            var Passwd = $("#password").val();
        
            if (Uname == "" || 1 > Uname.length) {
                DG.alert('Please Enter Your Email Address/Username');
            } else if (Passwd == "") {
                DG.alert("Please Enter Your Password");
            } else if (Passwd.length < 3 || Passwd.length > 30) {//Changed by Tan, Old RPI password length more than 15
                DG.alert("Password must be 6 to 20 characters in length");
            } else {
                loginSubmit();
            }
        }
        //reset
        function CheckEmailFormat() {
            var Email = document.getElementById('Email').value;
            var ep = document.getElementById('eproblem');
            if (Email == "Enter your Email Address/Username") {
                DG.alert("Please Enter Your Email Address/Username");
            } else if (!ValidateEmail(Email)) {
                DG.alert("Username must be in email format");
            } else {
                DG.alert("Your Password has been Reset!");
            }
        }

        function ValidateEmail(mail) {
            if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail)) {
                return (true)
            }
            return (false)
        }

        $(function () {
            $.ajaxSetup({ cache: false });
        });

        function FullsiteOrMobile(site) {
            var pageName = window.location.href.substr(window.location.href.lastIndexOf("/") + 1);
            if (site == 1) {
                setCookie("site", 1, 30);
                location.href = pageName;
            } else {
                setCookie("site", 2, 30);
                location.href = pageName;
            }

        }

        function NoShowLogin() {
            $("#dialog").attr("class", "dialog_login undis");
            document.onkeydown = null ;
        }

        function ShowLogin(successCallback) {
            loginCallback = null;
            if (null != successCallback)
            {
                loginCallback = successCallback;
            }
            
            $("#dialog").attr("class", "dialog_login");
            document.getElementById("loginform").reset();
            var user = getCookie("userName");

            if (user == "" || user == null) {
                $('#userName').val('');
                $("#remember_me").prop("checked", false);
            } else {
                $("#remember_me").prop("checked", true);
                $('#userName').val(user);
            }

            document.onkeydown = function (e) {
            var theEvent = window.event || e;
            var code = theEvent.keyCode || theEvent.which;
                if (code == 13) {
                    CheckLoginFormat();
                }
            }
        }

        function ShowAlert(msg, msg2, msg3, callback) {

            $("#massage_dialog").attr("class", "massage_dialog");
            $("#massage_dialog_text").html(msg);
            if (msg2 != "") {
                $("#massage_dialog_text2").html(msg2);
            } else if (msg3 != "") {
                $("#massage_dialog_text3").html(msg3);
            }

            if (callback) {
                $("#btn_ok").click(function () {
                    callback();
                });
            }
        }

        function NoShowAlert() {
            $("#massage_dialog").attr("class", "massage_dialog undis");
        }

        function GoUrl(link) {

            if (link == 'facebook') {
                location.href = 'https://www.facebook.com/RPIGroupToronto';
            } else if (link == 'twitter') {
                location.href = 'https://twitter.com/RPIConsultingGr';
            } else if (link == 'linkedin') {
                location.href = 'https://www.linkedin.com/company/960036';
            } else if (link == 'instagram') {
                location.href = 'http://instagram.com/rpiconsultinggroup';
            }

        }

        var currentPage = window.location.href.substr(window.location.href.lastIndexOf("/") + 1);
 
        if (currentPage == "healthcare.aspx") {
            $('#curopens').attr("onclick", "Javascript:location.href='PhysicianJobs.aspx");
            $('#classads').attr("onclick", "Javascript:location.href='PhysicianJobs.aspx");
           // $('#signups').attr("class", "undis");
            //$('#login').attr("class", "undis");


        }

        function ShowForgotPassword() {
            NoShowAlert();
            NoShowLogin();
            $("#dialog_forgotpassword").show();
        }

        function HideForgotPassword() {
            $("#ForgotPasswordEmail").val("");
            $("#dialog_forgotpassword").hide();
        }

        function ContinueForgotPassword() {
            var email = $("#ForgotPasswordEmail").val();
            if (ValidateEmail(email) == false) {
     
                DG.alert("Please enter a valid email address");
            } else {
                $.ajax({
                    url: "admin/webservices/UserWebService.aspx",
                    async: false,
                    data: "op=ForgotPassword&email=" + $("#ForgotPasswordEmail").val(),
                    type: "post",
                    dataType: "json",
                    success: function (rs) {
                     
                        if (rs.Code == 0) {
                            NoShowAlert();
                            NoShowLogin();
                            HideForgotPassword();
                            ShowAlert(rs.Msg);
                           
                        }else {
                            DG.alert(rs.Msg);
                        }
                    }
                });
            }
        }

        function ShowResetPassword() {
            
        }

        function HideResetPassword() {
            $("#ForgotPasswordEmail").val("");
            $("#dialog_resetpassword").hide();
        }
    
        function ConfirmResetPassword() {
   
            $.ajax({
                url: "admin/webservices/UserWebService.aspx",
                async: false,
                data: "op=ResetPassword&Code=" + $("#ResetPasswordCode").val() + "&Password=" + $("#ResetNewPassword").val() + "&ConfirmPassword=" + $("#ResetConfirmPassword").val(),
                type: "post",
                dataType: "json",
                success: function (rs) {
                    if (rs.Code == 0) {
                        NoShowAlert();
                        NoShowLogin();
                        HideForgotPassword();
                        HideResetPassword();

                        ShowAlert(rs.Msg);

                    } else {
                        DG.alert(rs.Msg);
                    }
                }
            });
        }

</script>


