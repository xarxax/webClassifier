	jQuery(document).ready(function(){
		if (localStorage.getItem("gdpr") != 1) {
			jQuery('#gdpr_banner').removeClass('close');
			jQuery('#gdpr_banner .close_banner').click(function(){
				jQuery('#gdpr_banner').addClass('close');
				localStorage.setItem("gdpr", 1);
			});
		}
	});
</script>
