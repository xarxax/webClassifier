    <script type="text/javascript">
        require(["jquery", "knockout", "knockout-mapping", "crossroads", "hasher", "breakpoints", "common", "productApi", "productRepository", "termRepository", "purl", "helperMethods", "bootstrap", "typeahead", "jquery.fancybox.pack", "jquery.fancybox-buttons", "jquery.fancybox-media", "jquery.fancybox-thumbs"],
        function ($, ko, koMap, crossroads, hasher, breakpoint, common, productApi, productRepository, termRepository, purl, helperMethods) {
            function MainModel() {
                // Common Stuff
                common.addCommonPropertiesToModel(this);
                common.addCommonMethodsToModel(this);

                this.getSubcategoryUrl = this.getSubcategoryUrl.bind(this);
                this.subcategoryOnClick = this.subcategoryOnClick.bind(this);

                this.productApi = new productApi(this);
            }

            MainModel.prototype.getSubcategoryUrl = function (subcategory) {
                if (this.productApi.currentCategory.CategoryType == 'Support' && subcategory.OS_Safe_Name == 'ServiceClub') {
                    var langParam = this.URL_language_name();
                    if (langParam != '') {
                        langParam = langParam.replace('?', '&');
                    }

                    return '/Support/ServiceClub?Subcategory_OS_Safe_Name=' + this.header.supportCategories()[1].Subcategories[0].OS_Safe_Name + langParam;
                }

                return helperMethods.getSubcategoryUrl(subcategory.OS_Safe_Name);
            }

            MainModel.prototype.subcategoryOnClick = function (subcategory) {
                if (!helperMethods.testHijax) {
                    if (this.productApi.currentCategory.CategoryType == 'Support' && subcategory.OS_Safe_Name == 'ServiceClub') {
                        var baseUrl = purl().attr('protocol') + '://' + purl().attr('host');

                        var port = purl().attr('port');
                        if (port != '') {
                            baseUrl = baseUrl + ':' + purl().attr('port')
                        }

                        var fullUrl = baseUrl + '/Support/ServiceClub' + this.URL_language_name() + '#!' + this.header.supportCategories()[1].Subcategories[0].OS_Safe_Name;;
                        window.location.href = fullUrl;
                    } else {
                        helperMethods.subcategoryOnClick(subcategory.OS_Safe_Name);
                    }
                }
            }

            // Refresh and reset
            MainModel.prototype.refresh = function (reset) {
                var self = this;

                if (!self.appliedBindingsToModel()) {
                    self.productApi.setCurrentCategory(self.header.errorCategories, 'PageNotFound');
                }

                return productRepository.get_Subcategories_With_Features(self.productApi.subcategories,
                    {
                        category_Id: self.productApi.currentCategory.Category_Id,
                        language_Id: self.footer.chosenLanguage().Language_Id }
                    ).then(function () {
                        if (reset) {
                            hasher.setHash('');
                            crossroads.resetState();
                        }

                        if (!self.appliedBindingsToModel() || reset) {
                            if (!!self.productApi.subcategories() && self.productApi.subcategories().length > 0)
                                self.productApi.loadSubcategory(self.productApi.subcategories()[0].OS_Safe_Name);
                        }

                        if (!self.appliedBindingsToModel()) {
                        self.productApi.setBannerImageSrc('/Images/');
                        }

                        return termRepository.get(self.terms, { language_Id: self.footer.chosenLanguage().Language_Id });
                    });
            };

            $(document).ready(function () {
                var m = new MainModel();

                common.init(m).then(function () {
                    m.refresh(false).then(function () {
                        m.productApi.showSubcategoryTitle_Init();

                        ko.applyBindings(m);
                        m.appliedBindingsToModel(true);

                        m.productApi.createRoute(m);

                        // Add default canonical link
                        if ($("head link[rel='canonical']").length == 0) {
                            $("head link[rel='stylesheet']").first().before('<link rel="canonical" href="' + m.baseURL + '" />');
                        }

                        m.helperMethods.activatePopupWindow();

                        m.helperMethods.scrollToAnchorPosition();

                        $('.fancybox-link').fancybox({
                            fitToView: true,
                            width: '100%',
                            height: '100%',
                            autoSize: true,
                            closeClick: false,
                            openEffect: 'elastic',
                            closeEffect: 'elastic',
                            helpers: {
                                overlay: {
                                    css: {
                                        'background': 'rgba(0,0,0,0.50)'
                                    }
                                }
                            }
                        });
                    });
                });
            });
        });
    </script>

