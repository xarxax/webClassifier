<script type="text/javascript">
$(function() {
    $("#newProlist").jCarouselLite({
        btnNext: 	'.newProBtnRight',
        btnPrev: 	'.newProBtnLeft'
    });
});
$(function() {
    $("#hotProin").jCarouselLite({
        btnNext: 	'.hotProBtnRight',
        btnPrev: 	'.hotProBtnLeft'
    });
});
$(function(){
	$('#tabctr a').each(function(index) {
		$(this).click(function(){
			if(!$(this).hasClass('hover')) {
				$('#tabctr a').removeClass('hover');
				$(this).addClass('hover');
				$('.homeNewContent').hide();
				$('.homeNewContent').eq(index).show();
			}
			return false;
		});               //鼠標點擊
		
		/*$(this).hover(function(){
			if(!$(this).hasClass('hover')) {
				$('#tabctr a').removeClass('hover');
				$(this).addClass('hover');
				$('.homeNewContent').hide();
				$('.homeNewContent').eq(index).show();
			}
			return false;
		});*/              //鼠標滑動
	});                  
});
</script>
