function getHostName() {
    var hostName = window.location.host.toString();
	var findex = hostName.indexOf('.');
	var lindex = hostName.lastIndexOf('.');
	if(findex == lindex) {
		hostName = hostName.substring(0, lindex);
	} else {
		hostName = hostName.substring(findex + 1, lindex);
	}
    return hostName;
}
var siteName = getHostName();
var accounts = {};
accounts['miltonroy'] = 'KOI-1WSDY9E';
accounts['haskel'] = 'KOI-1WTUQNA';
if(siteName in accounts){
	var _ss = _ss || []; 
	_ss.push(['_setDomain', 'https://koi-1HXWJMW.sharpspring.com/net']); 
	_ss.push(['_setAccount', accounts[siteName]]); 
	_ss.push(['_trackPageView']); 
	(function() { 
		var ss = document.createElement('script'); 
		ss.type = 'text/javascript'; ss.async = true; 
		ss.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'koi-1HXWJMW.sharpspring.com/client/ss.js?ver=1.1.1'; 
		var scr = document.getElementsByTagName('script')[0]; 
		scr.parentNode.insertBefore(ss, scr); 
	})(); 
}
if(siteName == 'lmipumps'){
	var _ss = _ss || []; 
	_ss.push(['_setDomain', 'https://koi-1HXWJMW.marketingautomation.services/net']); 
	_ss.push(['_setAccount', 'KOI-1WT4CGC']); 
	_ss.push(['_trackPageView']); 
	(function() { 
		var ss = document.createElement('script'); 
		ss.type = 'text/javascript'; ss.async = true; 

		ss.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'koi-1HXWJMW.marketingautomation.services/client/ss.js?ver=1.1.1'; 
