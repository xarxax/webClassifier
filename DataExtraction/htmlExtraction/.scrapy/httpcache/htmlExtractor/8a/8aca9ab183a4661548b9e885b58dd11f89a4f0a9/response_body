    function magsubemail(ths) {
        var getEmail = $("input#subemail").val();
        if (getEmail === '' || getEmail === undefined) {
            alert("Please enter the email");
            return false;
        }
        getmagsubemail(getEmail);
    }
    function getmagsubemail(id)
    {
        $.post('/sites/magsubemail', {
            ajax_request: 1,
            id: id,
        });
    }
