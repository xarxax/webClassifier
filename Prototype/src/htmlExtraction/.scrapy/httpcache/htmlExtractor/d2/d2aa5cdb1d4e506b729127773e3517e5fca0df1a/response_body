<script language="javascript" type="text/javascript">
    cmcolib.HeaderSearch.init()
    var running = false;
    $(".header-search-textbox").keyup( function(event) {
        if ( $(this).val().length>1 && (event.keyCode != '38' && event.keyCode != '40' && event.keyCode != '13' ) ) {
            if( !running ) {
                running = true;
                $.ajax({
                        type: "POST",
                        url: "/responses/SearchSuggest.ashx" + 
                                "?keyword=" + $(this).val(),
                        success: function(result){
                                    if (result.length>0) {
                                        $(".suggest-pop-div").html(result);
                                        $(".suggest-pop-div").removeClass("hide");
                                        $(".search-suggest-keyword").bind("mouseover", keywordmouseover);
                                        $(".search-suggest-keyword").bind("click", keywordclick);
                                    }
                                    else {
                                        $(".suggest-pop-div").html("");
                                        $(".suggest-pop-div").addClass("hide");
                                       
                                    }
                                    running = false;
                                 }
                });
            }
        }
        else {
        
        }
        
    });
    
    $(".header-search-textbox").keydown( function(event) {
        
        if ( event.keyCode == '13' ) {
           $(".suggest-pop-div").addClass("hide");
        }
        
        if ( !$(".suggest-pop-div").hasClass("hide") ) {
        
            var keywords = $(".search-suggest-keyword");
            var currentIndex = -1;
            
            for (var i=0; $(keywords).length>i; i++) {
                if ( $(keywords[i]).hasClass("current-keyword") ) {
                    currentIndex = i;
                }
            }
        
        
            if ( event.keyCode == '38' ) {
                //Up Key pressed
                if ( currentIndex > 0 ) {
                    $(keywords[currentIndex]).removeClass("current-keyword");
                    $(keywords[currentIndex-1]).addClass("current-keyword");
                }
            }
            if ( event.keyCode == '40' ) {
                //Down Key Pressed
                if ( currentIndex < $(keywords).length-1 ) {
                    $(keywords[currentIndex]).removeClass("current-keyword");
                    $(keywords[currentIndex+1]).addClass("current-keyword");
                }
            }
            
            ApplyCurrentKeywordStyle();
            
        }
        
    });
    
    
    var keywordclick = function() {
        $(".suggest-pop-div").addClass("hide");
        $(".header-search-textbox").val( $(this).html() );
        window.location = "/SearchResults.aspx?search=" + $(this).html();
    }

    var keywordmouseover = function() {
        if ( !$(this).hasClass("current-keyword") ) {
            $(".current-keyword").removeClass("current-keyword");
            $(this).addClass("current-keyword");
            ApplyCurrentKeywordStyle();
        }
    }    

    
    
    var ApplyCurrentKeywordStyle = function() {
        $(".search-suggest-keyword").css("background-color","white");
        $(".current-keyword").css("background-color","yellow");
        $(".current-keyword").css("cursor","pointer");
        if ( $(".current-keyword").html() != null ) {
            $(".header-search-textbox").val( $(".current-keyword").html() );
        }
        
    }
    
    $(".header-search-textbox").blur( function() {
        setTimeout("$(\".suggest-pop-div\").html(\"\"); $(\".suggest-pop-div\").addClass(\"hide\");",100);
    });
    
    $(".header-search-textbox").click( function() {
        if ($(this).val() == "Keyword(s) or Product #") {
            $(this).val("");
        }
    });
    
</script>
