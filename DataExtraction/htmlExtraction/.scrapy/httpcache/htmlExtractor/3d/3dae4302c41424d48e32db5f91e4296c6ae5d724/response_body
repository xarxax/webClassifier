function check(myform) {
    if (myform.search.value == "Danly Part #"  ||
        myform.search.value == "") {
        alert ('Please enter search criteria.');
        return false;
    }
    myform.submit();
    return false;
}

function clear_field(myform) {
    myform.search.value = "";
}

</script>

