<script type="text/javascript">
    
function slideshow(){

    if (siGo == 1){
    				//numINVITACIONES
            if (vez == 10){
                vez = 1;
            }
            $("#invitacion").backstretch("./pics/portada/a" + vez + ".jpg", {speed: 800});
            $("#portadaEnfrente").hide().attr("src", "./pics/portada/a" + vez + ".png").fadeIn(800);

            $(".dotsitos").attr("src", "./pics/dot0.png");

            $("#nav" + vez).attr("src", "./pics/dot1.png");


            elTimeout=setTimeout(function(){
                vez++;
                slideshow();
            },4500);

            console.log(vez);
            console.log("si camina");
    }
    else {
        console.log("no camina ya funcion");
    }


}

function fader(){
    //$("#portadaEnfrente").fadeOut(500);
    vez++;
    var t=setTimeout(function(){
		slideshow();
	},500);
}


var vez = 1;
var siGo = 1;
var elTimeout;



window.onload = function()
{
	slideshow();
    
    
    
    var jctest = $("#invitacion").height();
    jctest = jctest + 80;
    $("#navInvitacion").css('top', jctest + 'px').fadeIn();
};

</script>

 



<script type="text/javascript">
$("document").ready(function() {
     
     
        $(window).scroll(function(){
            if ($(window).scrollTop() > 0) {
            		$("#espaciadorFixed").show();
            		$("#tituloFixed").show();
                $('#logoMenuBar').addClass('makeFixed');
                
                $('#insideTitulo').addClass('makeFixedb');
               
								$("#insideTitulo_container" ).animate({
									fontSize: "18px",
								}, 500 );                
               
             }
             else {
                 $('#logoMenuBar').removeClass('makeFixed');
                 $('#insideTitulo').removeClass('makeFixedb');
                 $("#espaciadorFixed").hide();
                 $("#tituloFixed").hide();

             }
        });
        
        
    
    
    
    
    $(".dotsitos").click(function() {
            clearTimeout(elTimeout);
            $(".dotsitos").attr("src", "./pics/dot0.png");
            $(this).attr("src", "./pics/dot1.png");
            var cualNow = $(this).attr("rel");

            vez = cualNow;
            $("#invitacion").backstretch("./pics/portada/a" + vez + ".jpg", {speed: 800});
            $("#portadaEnfrente").hide().attr("src", "./pics/portada/a" + vez + ".png").fadeIn(800);
        
            siGo = 0;
            console.log("no camina click");
    
            elTimeout=setTimeout(function(){
            		console.log("gox");
            		siGo = 1;
                vez++;
                slideshow();
            },10000);
            
    });
    
    $("#portadaEnfrente").hide();
    
    screenAlto = $( window ).height();
    areaAlto = screenAlto - 420;

    if (areaAlto > 499){
        prodShotAlto = areaAlto - 500;
    }
    else {
        prodShotAlto = 0;
    }


    



    $("#invitacion").css('height', areaAlto + 'px');


    $("#invitacion-product").css('margin-top', prodShotAlto + 'px');





    


    

    /*
	$('#invitacion-container').cycle({ 
	    fx:      'fade', 
	    speed:    600, 
	    timeout:  2000 
	});	
    */
	         
        
        
});  
  
</script>


</head>
