    $('document').ready(function() {
        Cufon.replace('#lnd_nav_bottom .right a span, .replace', { fontFamily: 'DroidSerif' });

        $(".datepicker").datepicker({
            minDate: new Date(),
            maxDate: '+2m'
        });

        $('#hrefMakeReservation').click(function() {
            $('#about-top').slideUp('fast');
            $('#lnd_nav_top').slideDown('slow');
        });

        $('#btnClose').click(function() {
            $('#lnd_nav_top').slideToggle('slow');
        });

        //about LR
        $('#about-lawrys').click(function() {
            $('#lnd_nav_top').slideUp('fast');
            $('#about-top').slideDown('slow');
        });
        $('#btnAboutClose').click(function() {
            $('#about-top').slideToggle('slow');
        });

    });

