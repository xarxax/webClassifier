// function to reset images to default status

	function sectionchange()
        {
                employerCurrent.src = unselectedImage.src
                candidateCurrent.src = unselectedImage2.src
                aboutusCurrent.src = unselectedImage3.src
                positionsCurrent.src = unselectedImage4.src
                linksCurrent.src = unselectedImage5.src
                witwisdomCurrent.src = unselectedImage6.src
                normalCurrent.src = unselectedImage7.src
        }
        
function reset()
        {
                        employerSet = 0
                        candidateSet = 0
                        aboutusSet = 0
                        positionsSet = 0
                        linksSet = 0
                        witwisdomSet = 0
                        clearTimeout(employerid)
                        document.employer.src= employerCurrent.src
                        clearTimeout(candidateid)
                        document.candidate.src= candidateCurrent.src
                        clearTimeout(aboutusid)
                        document.aboutus.src= aboutusCurrent.src
                        clearTimeout(positionsid)
                        document.positions.src= positionsCurrent.src
                        clearTimeout(linksid)
                        document.links.src= linksCurrent.src
                        clearTimeout(witwisdomid)
                        document.witwisdom.src= witwisdomCurrent.src
                        document.normal.src = normalImage.src
        }
        
// set current sections

function sethome()
        {
                        sectionchange()
                        normalImage.src=normalImagebak.src
                        reset()
        }
        
// activate employer selection

function rollemployer(delay)
        {
                reset()
                document.normal.src = employerImage.src
                document.employer.src = selectedImage.src
                employerid=setTimeout("reset()", delay)
        }
        
// set current section to employer

function setemployer()
        {
                if (employerSet == 0) {     
                        sectionchange()
                        normalImage.src = employerImage.src
                        employerCurrent.src = currentImage.src
                        reset()
                        employerSet = 1     
                       
                }
        }
        
// activate candidate selection
        
function rollcandidate(delay)
        {
                reset()
                document.normal.src = candidateImage.src
                document.candidate.src = selectedImage2.src
                candidateid=setTimeout("reset()", delay)
        }
        
// set current section to links
        
function setcandidate()
        {
                if (candidateSet == 0) {    
                        sectionchange()
                        normalImage.src = candidateImage.src
                        candidateCurrent.src = currentImage.src
                        reset()
                        candidateSet = 1    
                        
                }
        }

// set rollover for aboutus

function rollaboutus(delay)
        {
                reset()
                document.normal.src = aboutusImage.src
