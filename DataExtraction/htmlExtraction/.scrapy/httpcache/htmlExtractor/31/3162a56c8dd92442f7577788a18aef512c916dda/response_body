	var $j = jQuery.noConflict();
	$j(document).ready(function(){

		$j(".mobile_menu").click(function(){
			$j("body").toggleClass("mobile_toggle");
		});

		$j(".open_close").click(function(e){
			e.preventDefault();
			$j(".nav_wrap").toggleClass("toggle");
		});

		$j('.slider').flexslider({
			controlNav: true,
			directionNav: false, 
		});

		$j('#tabs').responsiveTabs({
		    startCollapsed: 'accordion',
		});

		$j('#tabs').responsiveTabs('activate', 0);

		$j('.main_image').zoom({
			duration: 50,
		});

		$j(".additional_images li a").click(function(e)
	    {
	    	e.preventDefault();
	        $j(".main_image img").attr("src", $j(this).attr("href"));
	        $j(".main_image .zoom_popup").attr("href", $j(this).attr("href"));
	    });

		$j('.main_image a, #gallery li a').magnificPopup({
		  type: 'image',
		  gallery:{
		    enabled:true
		  }
		});

		$j(document).on('click', 'a.list_view', function(e) {
			e.preventDefault();
			$j('body').removeClass('body_grid');
			$j('a.grid_view').removeClass('toggle');
			$j('body').toggleClass("body_list");
			$j(this).toggleClass("toggle");
	    });

	    $j(document).on('click', 'a.grid_view', function(e) {
			e.preventDefault();
			$j('body').removeClass('body_list');
			$j('a.list_view').removeClass('toggle');
			$j('body').toggleClass("body_grid");
			$j(this).toggleClass("toggle");
	    });

		//$j(".head_wrap").sticky({topSpacing:0});
		$j(".nav_wrap").sticky({topSpacing:0});

		$j('.content_list li:nth-child(2n), ul.machines li:nth-child(3n)').after('<li class="clear"></li>');

		$j('.body_wrap table').wrap("<div class='table_wrapper'></div>");
		$j('.body_wrap .table_wrapper').prepend('<p class="mobile mobile_table_heading">Please scroll right to view more of the table below</p>');
		
		// validate email form
		$j("#newsletter-signup").click(function()
		{
			//console.log('Clicked');
		   var alert_msg = 'Please enter all details on the newsletter signup form';
		   
		   if ($j('#newsletter-fname').val().length == 0)
		   {
			   alert(alert_msg);
			   return false;
		   }
		   
		   if ($j('#newsletter-lname').val().length == 0)
		   {
			   alert(alert_msg);
			   return false;
		   }
		   
		   if ($j('#newsletter-company').val().length == 0)
		   {
			   alert(alert_msg);
			   return false;
		   }
		   
		   if ($j('#newsletter-email').val().length == 0 || !validateEmail($j('#newsletter-email').val()))
		   {
			   alert(alert_msg);
			   return false;
		   }
		   
		   return true;
		});
	}); 
	
	function validateEmail($email) 
	{
		var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		return emailReg.test( $email );
	}
</script>
</body>
</html>