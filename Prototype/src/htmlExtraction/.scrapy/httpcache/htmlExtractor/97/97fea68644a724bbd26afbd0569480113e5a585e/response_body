<script type='text/javascript'>
jQuery(document).ready(function($){
 
    $.backstretch("https://www.asterawards.com/wp-content/uploads/2016/09/bigstock-Aluminium-Silver-Background-2705735.gif");
})
jQuery(document).ready(function() {
	setTimeout(function() {
	jQuery('#index-slider ul.slides a').attr('href', '#').css('cursor', 'default').removeAttr('data-rel').removeAttr('rel').unbind();
	}, 500);

	jQuery('input:radio[name=ninja_forms_field_39]').change(function() {
		if( !!jQuery(this).attr('checked') && jQuery(this).attr('value') == 'PayPal' ) {
			via_paypal();
		} else {
			via_form();
		}
		return true;
	});

	jQuery('#ninja_forms_field_39_2_label').append('<img src="/paypal.png" border="0" alt="PayPal Acceptance Mark" style="height: 35px; width: auto; vertical-align: middle; margin-left: 10px;" />');
});

function via_paypal() {
	jQuery('#ninja_forms_field_31_div_wrap,#ninja_forms_field_33_div_wrap,#ninja_forms_field_34_div_wrap,#ninja_forms_field_35_div_wrap,#ninja_forms_field_36_div_wrap,#ninja_forms_field_37_div_wrap,#ninja_forms_field_38_div_wrap').fadeOut();
	jQuery('#ninja_forms_form_3').attr('action', 'https://www.paypal.com/cgi-bin/webscr').unbind('submit').submit(handle_paypal);
}

function via_form() {
	jQuery('#ninja_forms_field_31_div_wrap,#ninja_forms_field_33_div_wrap,#ninja_forms_field_34_div_wrap,#ninja_forms_field_35_div_wrap,#ninja_forms_field_36_div_wrap,#ninja_forms_field_37_div_wrap,#ninja_forms_field_38_div_wrap').fadeIn();
	jQuery('#ninja_forms_form_3').attr('action', '').unbind();
}

function handle_paypal() {
	var item_name = '';
	var amount = 0.00;
	var $form = jQuery(this);

	// Calc item name
	jQuery('#ninja_forms_form_3').find('input:checkbox:checked').each(function() {
		var $this = jQuery(this);
		if( $this.hasClass('aa-yearbook-combo') ) item_name += '2015 Yearbook Combo +125.00';
		if( $this.hasClass('aa-print-annual') ) item_name += '2015 Print Annual +60.00';
		if( $this.hasClass('aa-radio-annual') ) item_name += '2015 Radio Annual +60.00';
		if( $this.hasClass('aa-tv-annual') ) item_name += '2015 TV Annual +60.00';
		if( $this.hasClass('aa-copy-mht') ) item_name += 'MHT Copy +10.00';
		item_name += ', ';
	});
	if( item_name == '' ) {
		alert('Please select at least one item.');
		return false;
	} else {
		item_name = item_name.substr(0, item_name.length - 2);
	}

	// Calc total amount
	amount = parseFloat(jQuery('#ninja_forms_field_42').val());

	// Add PayPal values
	$form.prepend('<input type="hidden" name="cmd" value="_xclick" />' + 
		'<input type="hidden" name="add" value="1" />' +
		'<input type="hidden" name="business" value="rlucas@ciofnc.com" />' +
		'<input type="hidden" name="shipping" value="0.00" />' +
		'<input type="hidden" name="no_shipping" value="0" />' +
		'<input type="hidden" name="return" value="https://www.asterawards.com/thank-you/" />' +
		'<input type="hidden" name="item_name" value="' + item_name + '" />' +
		'<input type="hidden" name="amount" value="' + amount + '" />' +
		'<input type="hidden" name="currency_code" value="USD" />' +
		'<input type="hidden" name="lc" value="US" />' +
		'<input type="hidden" name="bn" value="PP-ShopCartBF" />');

	// Remove non-PayPal values
	jQuery('input:radio[name=ninja_forms_field_39]').remove();
} 
</script>
 
