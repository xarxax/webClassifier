$( document ).ready(function() {

    $('.searchForm form').on('submit', function (e) {
        $(".error").hide();
        var hasError = false;

        var searchVal = $("#search-text").val();
        if(searchVal == '') {
            $("#search-text").after('<span style="font-size:12px; color:red;" class="error">Please enter the valid search text.</span>');
            hasError = true;
        } 
        if(hasError == true) {return false;} 

    });
});   
</script>
