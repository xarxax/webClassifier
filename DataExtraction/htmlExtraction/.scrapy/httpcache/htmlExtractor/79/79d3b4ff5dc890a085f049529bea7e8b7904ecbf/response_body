    <div id="cocoon"><script type="text/javascript">

    if (typeof (IMConfig) != "undefined" && IMConfig.ff('SEO')) {
        var prefix = "/";
        require(["browser"], function(browser) {
            if (browser.isQBBrowser() && typeof $("#accountantLink") != "undefined") {
                    $("#accountantLink").css("display", "none");
                }
        });
        $('#cocoon').find("a[href^='#']").each(function () {
            //console.log($(this).attr('id') + " ------ " + $(this).attr('href'));
            var hrefVal = $(this).attr('href');

            var divId = $(this).attr('id');
            if (hrefVal=="#" || divId == "arrowSignOut" || divId == "chatSection") {
                return;
            }

            switch (hrefVal) {
                case "#voucherchecks": hrefVal = hrefVal.replace('#voucherchecks', "/checks/voucher-checks"); break;
                case "#standardchecks": hrefVal = hrefVal.replace('#standardchecks', "/checks/standard-checks"); break;
                case "#walletchecks": hrefVal = hrefVal.replace('#walletchecks', "/checks/wallet-checks"); break;
                case "#depforms": hrefVal = hrefVal.replace('#depforms', "/checks/direct-deposit-forms"); break;
                case "#w2kits": hrefVal = hrefVal.replace('#w2kits', "/tax-forms/w2-forms"); break;
                case "#1099kits": hrefVal = hrefVal.replace('#1099kits', "/tax-forms/1099-forms"); break;
                case "#w3forms": hrefVal = hrefVal.replace('#w3forms', "/tax-forms/w3-forms"); break;
                case "#1096forms": hrefVal = hrefVal.replace('#1096forms', "/tax-forms/1096-forms"); break;
                case "#checkwindows": hrefVal = hrefVal.replace('#checkwindows', "/envelopes/check-window"); break;
                case "#forms": hrefVal = hrefVal.replace('#forms', "/envelopes/form-envelopes/detail"); break;
                case "#customprinted": hrefVal = hrefVal.replace('#customprinted', "/envelopes/custom-printed-envelopes"); break;
                case "#pds": hrefVal = hrefVal.replace('#pds', "/deposit-slips/printable"); break;
                case "#mds": hrefVal = hrefVal.replace('#mds', "/deposit-slips/manual"); break;
                case "#depbags": hrefVal = hrefVal.replace('#depbags', "/deposit-slips/disposable-deposit-bags"); break;
                case "#labels": hrefVal = hrefVal.replace('#labels', "/supplies/stamps-labels"); break;
                case "#formsmm": hrefVal = hrefVal.replace('#formsmm', "/supplies/computer-forms"); break;
                case "#pens": hrefVal = hrefVal.replace('#pens', "/supplies/security-pens"); break;
                case "#checkvalue": hrefVal = hrefVal.replace('#checkvalue', "/value-packs"); break;
                default: hrefVal = hrefVal.replace('#', prefix);
            }
            $(this).attr('href', hrefVal);
        });

        //Special Header Links
        $('a[href="/ui/pages/home.html"]').attr('href', prefix);
        $('a[href="/ui/pages/checks.html"]').attr('href', prefix + "checks");
        $('a[href="/ui/pages/tax.html"]').attr('href', prefix + "tax-forms");
        $('a[href="/ui/pages/envelope.html#checkwindows"]').attr('href', "/envelopes/check-window");
        $('a[href="/ui/pages/envelope.html#forms"]').attr('href', "/envelopes/form-envelopes/detail");
        $('a[href="/ui/pages/envelope.html#customprinted"]').attr('href', "/envelopes/custom-printed-envelopes");

    }


    var loginwidget = null;
    var loginconfigurations = null;
    var logineventHandlerSettings = null;

    function LoginSuccess() {
        $('#reorderSignedOut').hide();
        $('#reorderSignedIn').show();
        closeWidgetAndHideOverlay();
        window.location.href = "#orderhistory";
    }
    function HideOverlayOnly() {
        $("#Overlay").hide();
        loginwidget = null;
    }

    function closeWidgetAndHideOverlay() {
        $("#Overlay").hide();
        $("#drawerParent").removeClass("open");
        if (loginwidget != null) {
            loginwidget.CloseWidget();
        } loginwidget = null;
    }

</script>

</style>
