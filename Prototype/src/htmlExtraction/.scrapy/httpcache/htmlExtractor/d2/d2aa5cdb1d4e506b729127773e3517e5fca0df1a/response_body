<script language="javascript" type="text/javascript">
    $(document).ready(function() {
        var lblExticker = $("#ctl00_UcSide1_ucMiniTicker1_lblExticker");
        var lblDate = $("#ctl00_UcSide1_ucMiniTicker1_lblDate");
        var lblTime = $("#ctl00_UcSide1_ucMiniTicker1_lblTime");
        var lblQuote = $("#ctl00_UcSide1_ucMiniTicker1_lblQuote");
        var lblVolShare = $("#ctl00_UcSide1_ucMiniTicker1_lblVolShare");
        var lblChange = $("#ctl00_UcSide1_ucMiniTicker1_lblChange");
        var lblPctChange = $("#ctl00_UcSide1_ucMiniTicker1_lblPctChange");
        var d = FormatDateTime(irxmlstockquote[0].datetime);
        $(lblDate).html(d.DateString);
        $(lblTime).html(d.TimeString);
        $(lblExticker).html(irxmlstockquote[0].exchange + ": " + irxmlstockquote[0].ticker);
        $(lblExticker).css("color", "#2f66d0").css("font-weight", "bold");
        $(lblQuote).html("$" + irxmlstockquote[0].lastprice);
        $(lblVolShare).html(irxmlstockquote[0].volume);
        AssignValueFormatColor(lblChange, irxmlstockquote[0].change);
        AssignValueFormatColor(lblPctChange, irxmlstockquote[0].pchange);
        $(lblChange).html("$" + $(lblChange).html());
        $(lblPctChange).html($(lblPctChange).html() + "%");
    });
    var AssignValueFormatColor = function(control, value) {
        //Change Processing
        var change = value;
        if (change == 0) {
            $(control).css("color", "#666666");
        }
        else {
            if (change > 0) {
                $(control).css("color", "#16b900");
            }
            else {
                $(control).css("color", "#b30000");
            }
        }
        $(control).html(change);
    }
    var FormatDateTime = function(d) {
        //format the Date
        var month = d.getMonth() + 1;
        var day = d.getDate();
        var year = d.getFullYear();
        //build date string
        var DateString = month + '/' + day + '/' + year;
        //define time variables
        var minute;
        var hour;
        var ampm = '';
        //format hours
        if ((d.getHours()) > 12) {
            hour = (d.getHours()) - 12;
            ampm = 'PM';
        }
        else {
            hour = d.getHours();
            ampm = 'AM';
        }
        //format minutes
        if (d.getMinutes() < 10) {
            minute = '0' + d.getMinutes();
        }
        else {
            minute = d.getMinutes();
        }
        //build time string
        var TimeString = hour + ':' + minute + ' ' + ampm + ' EST';

        //return dateTime object
        return { DateString: DateString, TimeString: TimeString };
    }
</script>
