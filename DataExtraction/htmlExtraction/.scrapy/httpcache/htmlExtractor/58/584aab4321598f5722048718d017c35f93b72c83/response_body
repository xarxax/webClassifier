window.onerror = null; 
var netscape = 0;
var goodIE = 0;
browserName = navigator.appName.substring(0,8);
browserVer = parseFloat(navigator.appVersion);
if (browserName == "Netscape" && browserVer >= 3)
{ netscape = 1; }
if (browserName == "Microsof" && browserVer >= 4)
{ goodIE = 1; }

if (document.images) {
names = new Array( "dist", "rc", "home", "prod" );
buttons = new Array(8);
for (m=0; m<=7; m++) {
    buttons[m] = new Image();
}
        buttons[0].src = "images/b_dist-on.gif";
        buttons[1].src = "images/b_dist-off.gif";
        buttons[2].src = "images/b_rc-on.gif";
        buttons[3].src = "images/b_rc-off.gif";
        buttons[4].src = "images/b_home-on.gif";
        buttons[5].src = "images/b_home-off.gif";
        buttons[6].src = "images/b_prod-on.gif";
        buttons[7].src = "images/b_prod-off.gif";
}

function swap(des,num) {
if (document.images) {
document.images[names[des]].src = buttons[num].src;  }}

//  close the comment tag, this hides the script from really old browsers! -->
