<script>
  function showSeo(a,b){
    for(i=1; i<=b; i++){
      if(i==a){
        document.getElementById('link'+i).className = "sel";
        document.getElementById('content'+i).style.display = "";
      }else{
        document.getElementById('link'+i).className = "";
        document.getElementById('content'+i).style.display = "none";
      }
    }
  }

  showSeo(1,4);	
	
</script><div class="wrapper">
