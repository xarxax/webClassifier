$(document).ready(function(){
    // Build search form
    $(".search-form").each(function(index) {
        var searchForm = $("<form></form>", {
            name: "SS_searchForm",
            id: "SS_searchForm" + (index + 1),
            action: "https://sitesearch.dmgmori.co.jp/search",
            method: "get",
            enctype: "application/x-www-form-urlencoded"
        });
        searchForm.append($("<input />", {
            type: "hidden",
            name: "site",
            value: "Q1T44A05"
        }));
        searchForm.append($("<input />", {
            type: "hidden",
            name: "charset",
            value: "UTF-8"
        }));
        searchForm.append($("<input />", {
            type: "hidden",
            name: "group",
            value: "2"
        }));
        searchForm.append($("<input />", {
            type: "hidden",
            name: "design",
            value: "2"
        }));
        
        if ($(this).attr("subcategory")) {
            var subcategories = $(this).attr("subcategory").split(",");
            for (var i in subcategories) {
                var subcategoryInput = $("<input />", {
                    type: "hidden",
                    name: "subcategory",
                    value: subcategories[i]
                });
                searchForm.append(subcategoryInput);
            }
        }
        searchForm.append($("<span></span>", {
            class: "input"
        }).append($("<input />", {
            type: "text",
            id: "SS_searchQuery" + (index + 1),
            name: "query",
            value: "",
            maxlength: "100",
            placeholder: "Search by words"
        })));
        searchForm.append($("<button />", {
            class: "btn-search",
        }).text("Search"));
        $(this).html(searchForm);
    });
    // End of Build search form
});
</script>




