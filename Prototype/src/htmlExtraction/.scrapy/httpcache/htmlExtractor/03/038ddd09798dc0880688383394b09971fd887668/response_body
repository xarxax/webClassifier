<script type="text/javascript">
jQuery(document).on('ready post-load', easy_fancybox_handler );
</script>
<script type="text/javascript">

	jQuery(document).ready(function() {
		
		var img = jQuery('#arrow-image').html();

		jQuery('#menu-main-menu > li').hover(function () {
			var currentEl = jQuery(this);
			if(!currentEl.hasClass("current-menu-item")) {
				jQuery('#menu-main-menu > li.current-menu-item').find('a').css('background','none');
			}
		},function () {
		   	jQuery('#menu-main-menu > li.current-menu-item').find('a').css('background',"url("+img+") no-repeat scroll center bottom rgba(0, 0, 0, 0)" );
          });

		var scrollToElement = function(el, ms){
	        var speed = (ms) ? ms : 600;
	        jQuery('html,body').animate({
	            scrollTop: jQuery(el).offset().top - 90
	        }, speed);
    	}

		jQuery('#scroll-brands').click(function(e) {

			jQuery(this).toggleClass('upper-arrow');

			jQuery( "#footer-brand-data" ).slideToggle( "slow" );
			e.preventDefault();
        	scrollToElement('#footer-brand-data', 1000);

		});

		function ValidateEmail(email) {

        	var expr = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        	return expr.test(email);
    	};

    	jQuery("#submit-connect").click(function() {

			var name = jQuery("#connect-name").val();    		
			var email = jQuery("#connect-email").val();
			var message = jQuery("#connect-message").val();

			if( name == '' && email == '' && message == '' ) {
				
				jQuery("#connect-name").addClass('red-border');
				jQuery("#connect-email").addClass('red-border');
				jQuery("#connect-message").addClass('red-border');

				return false;
			}
			return true;
    	});

    	jQuery("#connect-name").focus(function() {
  			jQuery( this ).css('background-color','#FFFFFF');
  			jQuery( this ).css('border','1px solid #e6e7e8');
		});

		jQuery("#connect-email").focus(function() {
  			jQuery( this ).css('background-color','#FFFFFF');
  			jQuery( this ).css('border','1px solid #e6e7e8');
		});

		jQuery("#connect-message").focus(function() {
  			jQuery( this ).css('background-color','#FFFFFF');
  			jQuery( this ).css('border','1px solid #e6e7e8');
		});

		jQuery("#connect-name").blur(function() {
			
			var name = jQuery("#connect-name").val();
			if( name == '' ) {
				
				jQuery("#connect-name").removeClass('green-border').addClass('red-border');
				jQuery("input[name='connect-name']").focus();
				return false;
			} else {
				jQuery("#connect-name").removeClass('red-border').addClass('green-border');
				return true;
			}
		});

		jQuery("#connect-email").blur(function() {

			if (!ValidateEmail(jQuery("#connect-email").val())) {
            	
            	jQuery("#connect-email").removeClass('green-border').addClass('red-border');
            	return false;
        	} else {
        		jQuery("#connect-email").removeClass('red-border').addClass('green-border');
        	}
        	return true;
		});

		jQuery("#connect-message").blur(function() {

			var message = jQuery("#connect-message").val();
			if( message == '' ) {
				
				jQuery("#connect-message").removeClass('green-border').addClass('red-border');
				return false;
			} else {
				jQuery("#connect-message").removeClass('red-border').addClass('green-border');	
			}
			return true;
		});
		
		jQuery( document ).ready(function($) {
			$(".js-modal-btn").modalVideo({channel:'vimeo'});

			$('.js-modal-btn').click(function(){
				var title = $(this).attr('title');
				var content = $(this).data('content');
				$('.modal-video-movie-wrap').append('<div class="meta-box"><h1>' + title + '</h1><p>' + content + '</p></div>');

			});
		});
	});

</script>
