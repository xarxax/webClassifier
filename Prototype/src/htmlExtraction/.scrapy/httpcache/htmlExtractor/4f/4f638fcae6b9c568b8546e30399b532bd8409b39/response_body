<script type="text/javascript">
var unique_psi = ""
        					 var psi = "0";
        


				$(window).load(function() {
					hasFlash = flash_plugin_check();
    				if(!hasFlash) {
    					window.location="/no_flash.php";
    				} else {
    					window.location = "/live";
    				}
   				});

function getModulesWithMusic() {
					return "FALSE";
				}

	  
				function adjustSocialLinkWidth() {
    			
    				total_links = $("#social-links ul li").length;
    				if(total_links <= 6)
    					return false;
    			
    				if(total_links > 12 && total_links % 4 == 0) {
    					$("#social-links ul").width(176);
    				} else if(total_links > 15 || total_links % 3 == 0) {
    					$("#social-links ul").width(132);
    				} else {
    					$("#social-links ul").width(88);
    				}
    			
    			
    			}
    			
    			function adjustPageHeight() {

    				if($(".front-pg-img").width() >= 500) {
    					if($("#site-footer").width() == $(".article").width()) {
							$(".article").width($(".front-pg-img").width());
    						$("#site-footer").width($(".front-pg-img").width());
    					}
    				}
    			
					fixPageFooterBar();
    			
				}
    			
				function fixPageFooterBar() {
    				
					if($(document).height()  > $(window).height()) {
						$("#site-footer").removeClass("bottom-align");
    					$("#site-footer").addClass("auto-bottom-align");	
					} else {
    			    	footerHeight = $("#site-footer").height();
						$("#site-footer").addClass("bottom-align");	
    					$("#site-footer").removeClass("auto-bottom-align");
					}
					
					$("#site-footer").show().css("z-index", "20");    				
				}

    			function centerArticleContents() {
    			
    			    paddingTop = parseInt($(".article").css("padding-top").replace("px","")); 
    				paddingBtm = parseInt($(".article").css("padding-bottom").replace("px",""));
    				footerHeight = $("#site-footer").height();
    			
					if($(".article").height() + paddingTop + paddingBtm < $(window).height()) {

    					articleHt = $(".article").height();
    					newHt = ($(window).height() - footerHeight - articleHt) / 2; //- paddingTop - paddingBtm    					
    					
    					if(newHt > paddingTop)
    						$(".article").css("padding-top", newHt + "px");
    			    	if(newHt > paddingBtm)		
    						$(".article").css("padding-bottom", newHt + "px");
    			
    				} else if($(document).height() <= $(window).height()) {
    					
    					$(".article").height($(window).height() - footerHeight - paddingTop - paddingBtm);

    				}
    			}
    			
    			$(document).ready(function() {
    				adjustSocialLinkWidth();    			
    			});
    			
    			$(window).load(function() {
    				centerArticleContents();	
    			});
    			
    			$(window).resize(function() {
    				centerArticleContents();
    				adjustPageHeight();
    			});
    			
    			    			
    			

window.cssFiles = new Array();

window.cssFiles.push("/css/frontpagetemplatev4/main.css");

window.cssFiles.push("/css/frontpagetemplatev4/center-main.css");

window.jsFiles = new Array();

window.jsFiles.push("/js/central/jquery.js");

window.jsFiles.push("/js/central/jquery.center.js");

window.jsFiles.push("/js/central/swfobject.js");
</script>

