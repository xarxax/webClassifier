<script type="text/javascript">
    function checkQuoteCaptchaPopup() {
        var exists = document.getElementById("quotepopupgrc-response");

            if(exists == null){
                    
            }
        
             else{

            var check = document.getElementById("quotepopupgrc-response").value;
                if(check=='' || check == null){
                   document.getElementById("recaptchaquotepopup").className += " recaptchaerror";
                    return false;
                }
                else{
                  document.getElementById("recaptchaquotepopup").classList.remove("recaptchaerror");
                    return true;
                }
            }
   
}

function requestQuotePopup() {
  require(["jquery"],function($) {
    var dataForm = $('#quickrfq_popupform');
    var ignore = null;
    dataForm.mage('validation', {
        ignore: ignore ? ':hidden:not(' + ignore + ')' : ':hidden'
    }).find('input:text').attr('autocomplete', 'off');

    
      var success = dataForm.validation('isValid'); 
      var successcaptcha =  checkQuoteCaptchaPopup() ;
      if((success) && (successcaptcha) ) {
          saveQuoteRequestPopup();
      }
      
  });

                  
}

function saveQuoteRequestPopup() {
        require(["jquery"],function($) {
            
            var customurl = "https://www.bushrefrigeration.com/quickrfq/index/post/";
             $.ajax({
                  showLoader: true,
                  url: customurl,
                  type: 'POST',
                   data: new FormData($('#quickrfq_popupform')[0]),
                   cache: false,
                   contentType: false,
                   processData: false,
                  complete: function(response) {  
                        document.getElementById('quote_popresponse_msg').innerHTML = response.responseJSON.message;
                        document.getElementById('quote_popresponse_msg').style.display = 'block';
                        if(response.responseJSON.status == 'success'){
                             document.getElementById("quote_popresponse_msg").className = "message-success success message";
                        }else{
                            
                             document.getElementById("quote_popresponse_msg").className = " message-error error message";
                        }
                        document.getElementById("quickrfq_popupform").reset();
                        grecaptcha.reset(gcaptchapopup);
                        $('#quote_popresponse_msg').focus();
                         setTimeout(function() {$('#popup-quote').modal('closeModal');}, 3000);
                  },
                  error: function (xhr, status, errorThrown) {
                     document.getElementById('quote_popresponse_msg').innerHTML = 'Error happens. Try again.';
                     document.getElementById("quote_popresponse_msg").className = " message-error error message";
                     document.getElementById('quote_popresponse_msg').style.display = 'block';
                     document.getElementById("quickrfq_popupform").reset();
                     $('#quote_popresponse_msg').focus();
                  }
                 
             });
                 
        });
            

}
</script>
</div>

    
    
    <script>
    require(
        [
            'jquery',
            'Magento_Ui/js/modal/modal'
        ],
        function(
            $,
            modal
        ) {
            var options = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: 'Request A Quote',
                buttons: [{
                    text: $.mage.__('Close'),
                    class: '',
                    click: function () {
                        this.closeModal();
                    }
                },
                {
                text: 'Request A Quote',
                class: '',
                click: function() {
                    
                    requestQuotePopup();
                 
                  
                                   
                }
            }
                
                
                ]
            };

            var popup = modal(options, $('#popup-quote'));
            $(".quote").on('click',function(){ 
                $("#popup-quote").modal("openModal");
            });

        }
    );
</script>
    
    
