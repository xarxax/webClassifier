				(function($){
					$('#topwide .sbxModTable-hprotatecols .sbxMod-content').each(function(){
						var container = $(this);
						var cols = container.find('.rotatingcol').detach();
						var slides = $('<div class="slides"></div>').append(cols);
						var slideswrap = $('<div class="slideswrap"></div>').append(slides);
						var prev = $('<a class="prev" href="#"></a>');
						var next = $('<a class="next" href="#"></a>');
						var nav = $('<div class="nav"></div>').append(prev, next);
						container.wrapInner($('<div class="copywrap"></div>'));
						container.prepend(slideswrap);
						container.append(nav, $('<div class="clear">&nbsp;</div>'));

						slides.carouFredSel({
					    scroll      : {
					      fx          : "scroll",
						    items		 : 1
					    },
							items		: {
								visible		: 3,
							},
							auto : {
						    items		 : 1,				
						    easing      : "linear",
						    duration    : 1000,
						    timeoutDuration: 3000,
						    pauseOnHover: true
							},
							// auto: false,
							prev: prev,
							next: next
						});

					});
				})(jQuery);
