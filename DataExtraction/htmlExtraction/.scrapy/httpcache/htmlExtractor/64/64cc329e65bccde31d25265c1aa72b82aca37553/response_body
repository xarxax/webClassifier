<script>
    var emailValidator = function(value, constraints) {
        const regexp = /^[_A-Za-z0-9-'`]+(\.[_A-Za-z0-9-'`]+)*@[_A-Za-z0-9-]+(\.[_A-Za-z0-9-]+)*(\.[A-Za-z]{2,})$/g;
        return value.length > 254 ? false : regexp.test(value);
    }
</script>
<script>			
		require(["dijit/form/SimpleTextarea", "dijit/form/ValidationTextBox", "dojo/_base/declare", "dojox/validate/web","dojox/validate/us", "dijit/Dialog", "dijit/form/Button", "dijit/form/Select", "dijit/form/TextBox"],
		function(SimpleTextarea, ValidationTextBox, declare, validate){
		
			declare("ValidationTextArea",[SimpleTextarea, ValidationTextBox], {
	
				constructor: function(params){
					this.baseClass += 'dijitValidationTextArea';
				},    
				templateString: "<textarea true data-dojo-attach-point='focusNode,containerNode,textbox' autocomplete='off'></textarea>"
		
			});
		});

		
		function loadAndShowDialog(dialog, action, dialogTitle, dimensions){		
			
			dialog.domNode.style.visibility='hidden';
			dialog.hide();
			//dialog.domNode.id="dialogId";
			//dialog.set("sizeToViewport", false);
			//dialog.set("dispute",dispute);
			dialog.set("content", "");
			dialog.set("title", dialogTitle);
			dialog.set("dimensions", dimensions);
			console.log("dimensions = " + dimensions);
			
			//dialog.layout();		
			
			dojo.style(dialog.closeButtonNode,"display","none");
			
			loadDialogContent(dialog, action, dialogTitle, dimensions);
			showDialog(dialog);
			
		}
		
		function loadDialogContent(dialog, action, dialogTitle,dim){
			require(["dojo/request/xhr"],
			function(xhr) {
				xhr.get(action,{
				}).then(function(response){
					//dialog.set("sizeToViewport", false);
					dialog.set("content", response);

				//	dialog.set("dispute",dispute);
					dialog.set("title",dialogTitle);
					console.log("dialogTitle = " + dialogTitle);
					dialog.set("dimensions",dim);
					console.log("dim = " + dim);
					//dialog.layout();
				});
			});
		}
		
		function showDialog(dialog){
			dialog.domNode.style.visibility='visible';
			dialog.show();
		}
			
		function addAnother(dialog, hiddenId, formId, action, errDim){
			document.getElementById(hiddenId).value="true";
			submitDialogFormWithErrorDimensions(dialog, formId, action, errDim);
		}

		function submitDialogFormWithErrorDimensions(dialog, formId, action, errDim){
			require(["dojo/request/xhr", "dojo/dom-form", "dojo/dom","dijit/registry", "dojo/query"],
			function(xhr, domForm, dom, registry, query) {
				
				hasPreviousErrors = query("#page .errors").length>0;
				oldDim=dialog.get("dimensions")
				adjustedDim=[oldDim[0]+errDim[0],oldDim[1]+errDim[1]];
				
				var form = registry.byId(formId);
				if(!form.validate()){		
					alert('Please fill out all required fields');
					return
				}
				
				xhr.post(action,{
					data: domForm.toObject(formId)
				}).then(function(data){
					dialog.hide();
					dialog.set("content", data);
					if(query("#page .errors").length==0 || hasPreviousErrors){
						dialog.set("dimensions",oldDim);
						}
						else{
						dialog.set("dimensions",adjustedDim);
					}
					dialog.show();
					
				},function(err){
					alert(err);
					//dialog.set("content", err);
					dialog.show();
				});
				
			});	
			return false;			
		}
		
		function submitDialogForm(dialog, formId, action){
			require(["dojo/request/xhr", "dojo/dom-form", "dojo/dom","dijit/registry", "dojo/query"],
			function(xhr, domForm, dom, registry, query) {
				var form = registry.byId(formId);
				if(!form.validate()){		
					alert('Please fill out all required fields');
					return
				}
				
				xhr.post(action,{
					data: domForm.toObject(formId)
				}).then(function(data){
                    if (formId == "contactForm") {
                        dialog.hide();
                        loadAndShowDialog(dialog, "/login/contactInfoConfirm.htm", "Confirmation",[800, 300]);
                        //loadDialogContent(dialog, "/login/contactInfoConfirm.htm", "Confirmation", [800, 300]);
                    }
                    if (formId == "applicantDisputeForm") {
                       dialog.hide();
                        loadAndShowDialog(dialog, "/login/applicantdisputeconfirm.htm", "Confirmation",[800, 300]);
                       // loadDialogContent(dialog, "applicantdisputeconfirm.htm", "Confirmation", [800, 300]);
					}

					if (formId == "applicantRequestForm") {
                        dialog.hide();
                        loadAndShowDialog(dialog, "/login/applicantrequestconfirm.htm", "Confirmation",[800, 300]);
						//loadDialogContent(dialog, "/login/applicantrequestconfirm.htm", "Confirmation", [800, 300]);
					}
					else {
						dialog.hide();
						dialog.set("content", data);
						dialog.show();
					}									
				},function(err){
					alert(err);
					//dialog.set("content", err);
					dialog.show();
				});
				
			});	
			return false;			
		}


		
		function getChallengeQuestion(){
	var url = '/login/CQServlet';	
	
	var xhr = require("dojo/request/xhr");
	var registry = require("dijit/registry");
	var dom = require("dojo/dom");
	var domstyle = require("dojo/dom-style");
	var question = "";
	var isValid = false;
	var validationError = "";
	var splitArray = [];
	var questionId = "";
	
	var userId = registry.byId("newUserId").value;
	
	if(userId != null && userId.length > 0) {
	
		xhr.post(url, {sync:true, preventCache:true, data: {userId: userId}}).then(function(returnValue) {		
			if(returnValue.toUpperCase() === 'FAIL') {
				validationError = "System Error.  Please contact Customer Service.";
				isValid = false;
			}
			else if(returnValue.toUpperCase() === "INVALID USER"){
				validationError = "Invalid User ID."
					isValid = false;
			}
			else if(returnValue.toUpperCase() === "INVALID QUESTION"){
				validationError = "No question found for this User.  PLease contact Customer Service.";
				isValid = false;
			}
			else {
				splitArray = returnValue.split("|");
				console.log(splitArray);
				if(splitArray.length === 2){
					questionId = splitArray[0];
					question = splitArray[1];
					isValid = true;
				}
				else{
					validationError = "System Error.  Please contact Customer Service.";
					isValid = false;
				}
			}
		});
	}
	else {
		validationError = "Please enter a User ID.";
	}
	
	//var v = document.getElementById(hiddenId);
	//v.value =
	
	if(isValid){
	    console.log("question = " + question);
	    
	    domstyle.set(dom.byId("hiddenFormDiv"), "visibility" ,"visible");
		dom.byId("validationErrorDiv").innerHTML = "";
		dom.byId("challengeQuestion").innerHTML = question;
		dom.byId("challengePhrase").value = questionId;
		domstyle.set(dom.byId("submitButton"), "visibility" ,"visible");
	}
	else{
		 domstyle.set(dom.byId("hiddenFormDiv"), "visibility" ,"hidden");
		dom.byId("challengeQuestion").innerHTML = "";
		dom.byId("validationErrorDiv").innerHTML = validationError;
		dom.byId("challengePhrase").value = "";
		domstyle.set(dom.byId("submitButton"), "visibility" ,"hidden");
	}
}

</script>
