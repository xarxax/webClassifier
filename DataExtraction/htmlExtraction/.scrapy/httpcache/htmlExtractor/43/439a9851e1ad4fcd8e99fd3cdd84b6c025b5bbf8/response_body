<script type="text/javascript">
	document.documentElement.className = 'js';

$(function(){
    $('#searchinput').val("search this site...").one("focus",function(){
        $(this).val("");
    }).blur(function(){
        if($(this).val()==""){
            $(this).val("search this site...").one("focus",function(){
                $(this).val("");
            });
        }
    });

    $('.section_name input').val("Name").one("focus",function(){
        $(this).val("");
    }).blur(function(){
        if($(this).val()==""){
            $(this).val("Name").one("focus",function(){
                $(this).val("");
            });
        }
    });

    $('.section_email input').val("Email Address").one("focus",function(){
        $(this).val("");
    }).blur(function(){
        if($(this).val()==""){
            $(this).val("Email Address").one("focus",function(){
                $(this).val("");
            });
        }
    });

    $('.section_subject input').val("Subject").one("focus",function(){
        $(this).val("");
    }).blur(function(){
        if($(this).val()==""){
            $(this).val("Subject").one("focus",function(){
                $(this).val("");
            });
        }
    });

    $('.section_message textarea').val("Message").one("focus",function(){
        $(this).val("");
    }).blur(function(){
        if($(this).val()==""){
            $(this).val("Message").one("focus",function(){
                $(this).val("");
            });
        }
    });
});

$(function() {
	var nav = $('.overimg');
	nav.hover(
		function(){
			$(this).fadeTo(800,0.5);
		},
		function () {
			$(this).fadeTo(800,1);
		}
	);
});

</script>

