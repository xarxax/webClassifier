	function findPageNumByInterpeters(url)
	{
		if(url.indexOf("news_") > -1)
		{
			pageNum = findPageNumByText(["Actueel", "News"]);
			if(pageNum > 0) return pageNum;
	            	return 1;
	       	}
	        else if(url.indexOf("job_") > -1)
        	{
			pageNum = findPageNumByText(["Vacatures", "Jobs", "Vacancies"]);
			if(pageNum > 0) return pageNum;
			return 1;
		}
		else if(url.indexOf("work_") > -1)
		{
			pageNum = findPageNumByText(["Werk", "Work", "References"]);
			if(pageNum > 0) return pageNum;
			return 1;
		}
		else if(url.indexOf("client_") > -1)
        	{
			pageNum = findPageNumByText(["Klanten", "Clients"]);
			if(pageNum > 0) return pageNum;
			return 1;
        	}
		return null;
	}</script> </body></html>
