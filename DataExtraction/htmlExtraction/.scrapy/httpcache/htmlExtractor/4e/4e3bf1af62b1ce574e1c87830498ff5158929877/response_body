    <p>Forgotten Password</p>
    Enter the e-mail address associated with your account. Click submit to have your password e-mailed to you.    <br /><br />
    <b>E-Mail Address:</b>
    <br />
    <input style="width:95%;" placeholder="Enter Your E-Mail Address" type="text" name="email" value="">
    <br /><br />
    <div style="text-align: center;"><input id="forgot" type="submit" value="Submit" class="button"></div>
    <br />
    <a class="login_price">Login</a>
</div>
</div>			
<script type="text/javascript">
$(document).ready(function() {
$("#closer1").on("click", function() {
 	$("#forgotten").bPopup().close(); 
});
$(".forgotten").on("click", function() {
$("#logmein").bPopup().close(); 
$('#forgotten').bPopup({
	speed: 600,
	easing: 'easeInOutBack',
	transition: 'fadeIn',
	transitionClose: 'slideUp',
	opacity: 0.6
});
});
$('#forgot').on('click', function() {
$('.warning').hide();
$('.warning').html('');
	$.ajax({
		url: 'index.php?route=account/forgotten/send_forgotten',
		type: 'POST',		
		data: $('#forgotten-form :input'),
		dataType: 'json',
		beforeSend: function() {
			$('#forgot').attr('disabled', true);
			$('#forgot').after('<span class="wait">&nbsp;<img src="catalog/view/theme/default/image/loading.gif" alt="" /></span>');
		},	
		complete: function() {
			$('#forgot').attr('disabled', false);
			$('.wait').remove();
		},				
		success: function(json) {
			if (json['error']) {
				$('#forgotten-form').after('<div class="warning">' + json['error'] + '</div>');
				setTimeout(function(){$('.warning').fadeOut();}, 5000);
			} 
			if (json['success']) {
				$('#forgotten input[name=\'email\']').val("");
        $('#forgotten-form').after('<div class="success">' + json['success'] + '</div>');	
			}
			setTimeout(function(){$('.success').fadeOut();}, 5000); 
		  }
	  });	
  });
});
</script>       			
   
