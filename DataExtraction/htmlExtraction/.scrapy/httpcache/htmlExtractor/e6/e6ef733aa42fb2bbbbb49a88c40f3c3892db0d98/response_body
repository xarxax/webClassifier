<script type="text/javascript">
	jQuery(document).ready(function() {
	    jQuery("#contactSubmit").click(function() {
	    
	        //collect input field values
	        //note: .val() gets the current value of the first element in a set of matched elements
	        //var user_name = jQuery('input[name=name]').val(); 
	        //var user_email = jQuery('input[name=email]').val();
	        //var user_message = jQuery('textarea[name=message]').val();
	        
	        var user_name=document.forms["contactForm"]["name"].value;
	        var user_company=document.forms["contactForm"]["company"].value;
			var user_email=document.forms["contactForm"]["email"].value;
			var user_phone=document.forms["contactForm"]["phone"].value;
			var user_message = document.forms["contactForm"]["comments"].value;
			
			
	        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	        
	        //simple validation at client's end
	        //we simply change border color to red if empty field using .css()
	        var proceed = true;
	        

	        if(user_name=="" || user_name.length < 2){ 
	        	jQuery('#result').slideDown();
	        	jQuery('#nameError').remove();
	        	jQuery("#result ul").append('<li id="nameError"><i class="fa fa-caret-right list-caret"></i>Name is too short!</li>');
	            jQuery('input[name="name"]').css('background','#efbaba'); 
	            jQuery('input[name="name"]').css('background','#efbaba'); 
	            proceed = false;
	        }
	        if(user_company=="" || user_name.length < 2){ 
	        	jQuery('#result').slideDown();
	        	jQuery('#companyError').remove();
	        	jQuery("#result ul").append('<li id="companyError"><i class="fa fa-caret-right list-caret"></i>Please fill in your company name!</li>');
	            jQuery('input[name="company"]').css('background','#efbaba'); 
	            proceed = false;
	        }
	        if(user_email=="" || !emailReg.test(user_email)){ 
	        	jQuery('#result').slideDown();
	        	jQuery('#emailError').remove();
	        	jQuery("#result ul").append('<li id="emailError"><i class="fa fa-caret-right list-caret"></i>Invalid email!</li>');
	            jQuery('input[name="email"]').css('background','#efbaba');
	            proceed = false;
	        }
	        if(user_phone=="" || user_phone.length < 7){ 
	        	jQuery('#result').slideDown();
	        	jQuery('#phoneError').remove();
	        	jQuery("#result ul").append('<li id="phoneError"><i class="fa fa-caret-right list-caret"></i>Phone number is too short!</li>'); 
	            jQuery('input[name="phone"]').css('background','#efbaba'); 
	            proceed = false;
	        }
	        if(user_message=="" || user_message.length < 10){  
	        	jQuery('#result').slideDown();
	        	jQuery('#messageError').remove();
	        	jQuery("#result ul").append('<li id="messageError"><i class="fa fa-caret-right list-caret"></i>Message is too short! (Minimum 10 characters)</li>');
	            jQuery('textarea[name="comments"]').css('background','#efbaba');
	            proceed = false;
	        }
	        
	        
	        
	        //everything looks good! proceed...
	        if(proceed) 
	        {
	        
	            loadXMLDoc(user_name, user_company, user_email, user_phone, user_message);
	            
	            
	            //data to be sent to server
	            /*post_data = {'userName':user_name, 'userEmail':user_email, 'userMessage':user_message};
	            
	            //Ajax post data to server
	            jQuery.post('/siteInclude/contact.php', post_data, function(data){  
	                
	                //load success massage in #result div element, with slide effect.       
	                jQuery("#result").hide().html('<div class="whiteline"></div><div class="success">'+data+'</div>').slideDown();
	                
	                //reset values in all input fields
	                jQuery('#contactus input').val(''); 
	                jQuery('#contactus textarea').val(''); 
	                
	            }).fail(function(err) {  //load any error data
	                jQuery("#result").hide().html('<div class="whiteline"></div><div class="error">'+err.statusText+'</div>').slideDown();
	            });*/
	            
	        }
	    });  
	     function loadXMLDoc(user_name, user_company, user_email, user_phone, user_message){
	     
			var xmlhttp = new XMLHttpRequest();
			var parameters="userName="+user_name+"&userCompany="+user_company+"&userEmail="+user_email+"&userPhone="+user_phone+"&userMessage="+user_message;
			var link = "https://esdandassociates.com/wp-content/themes/strappress/contact-form.php";
			
			//alert(parameters);
			xmlhttp.open("POST",link,true);
			xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xmlhttp.setRequestHeader("Content-length", parameters.length);
			xmlhttp.setRequestHeader("Connection", "close");
			
	
			xmlhttp.onreadystatechange = function() {//Call a function when the state changes.
    			if(xmlhttp.readyState == 4 && xmlhttp.status == 200) {   
					//reset value	
					document.forms["contactForm"]["name"].value = '';
					document.forms["contactForm"]["company"].value = '';
					document.forms["contactForm"]["email"].value = '';
					document.forms["contactForm"]["phone"].value = '';
					document.forms["contactForm"]["comments"].value = '';
    				
					// Upon success: do these actions
					jQuery("#postSuccess").css("display","block");
					jQuery(".okayBtn").click(function(){
						jQuery("#postSuccess").css("display","none");
					});
					
					
					/*jQuery("div#contact_form").slideUp("fast", function(){
		                jQuery("div#contact").removeClass("showed");
		                jQuery("div#contact").css("height","auto");
		                jQuery(".whiteline").css("display","none");    
		            });  	*/
					
									
				}
			}
			
			xmlhttp.send(parameters);
		}
	   	
	   	jQuery('#contactForm input, #contactForm textarea').keyup(function(){
	    	jQuery("#result").slideUp();
	    	jQuery("#result ul").html('');
	    });
	        
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="name"]').keyup(function() {
	    	jQuery('input[name="name"]').css('background',''); 
	    });
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="company"]').keyup(function() {
	    	jQuery('input[name="company"]').css('background',''); 
	    });
	    jQuery('input[name="email"]').keyup(function() {
	    	jQuery('input[name="email"]').css('background',''); 
	    });
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="phone"]').keyup(function() {
	    	jQuery('input[name="phone"]').css('background',''); 
	    });
	    jQuery('textarea[name="comments"]').keyup(function() {
	    	jQuery('textarea[name="comments"]').css('background',''); 
	    });
	    /*jQuery("#contactus input, #contactus textarea").keyup(function() { 
	        jQuery("#contactus input, #contactus textarea").css('border-color',''); 
	        jQuery("#contactus input, #contactus textarea").css('background',''); 
	        jQuery("#result").slideUp();
	            
	    });*/
	});

</script>                            				            
			           </div>    
			       </div><!-- End 3 columns container -->
					       
					        
					       </li>
					     </ul><!-- end Contact menu dropdown -->
					</nav>
		
