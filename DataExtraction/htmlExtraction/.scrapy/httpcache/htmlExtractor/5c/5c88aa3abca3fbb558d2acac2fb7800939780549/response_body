    <script>

    $(function(){
        
    $.ajax({
      method: "POST",
      url: "/global/en/preview_pages/forms/contact_form.jsp",
      success: function(data){
        $(".cnt_form").find(".container").find(".row").children().last().append(data);
        $(".cnt_form select").selectmenu();
        $(".form .ui-selectmenu-button").removeAttr("style");
        setListener();
      }
    });


    });

    </script> 
    <script>
    $(function() {
    $("#drupa_c_form").validate({
        ignore: "",
        rules: {
            "drupa_contact_salutation": {
                required: true
            },
            "drupa_contact_privacy": {
              required: true
            },
            "drupa_contact_email": {
                required: true,
                email: true
            }
        },
        errorPlacement: function(error, element){
            if(element.attr("type") == "checkbox") {
                element.parent().siblings(".errorwrapper").append(error);
            } 
        }
    });
    });

    </script>
