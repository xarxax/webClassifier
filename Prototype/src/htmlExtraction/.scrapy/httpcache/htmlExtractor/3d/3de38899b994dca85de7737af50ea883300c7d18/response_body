<script type="text/javascript">
    $(document).ready(function (e) {
        clrSwapReqQuote('ffffff');
    });

    function clrSwapReqQuote(currentColor) {
        switch (currentColor) {
            case "ffffff":
                $('#reqQuote').animate({ color: "#ffff00" }, 2500, function () { clrSwapReqQuote('ffff00') });
                break;
            case "ffff00":
                $('#reqQuote').animate({ color: "#00ff00" }, 2500, function () { clrSwapReqQuote('00ff00') });
                break;
            case "00ff00":
                $('#reqQuote').animate({ color: "#00ffff" }, 2500, function () { clrSwapReqQuote('00ffff') });
                break;
            case "00ffff":
                $('#reqQuote').animate({ color: "#ffffff" }, 2500, function () { clrSwapReqQuote('ffffff') });
                break;
        }        
    }
</script>
