jQuery(document).ready(function () {
    jQuery('.carousel.auto').carousel({
        interval: 3000
    });
    jQuery('.carousel.auto2').carousel({
        interval: 5000
    });
    jQuery('.carousel.auto3').carousel({
        interval: 2000
    });
    jQuery('.carousel.auto').carousel('cycle');

	jQuery(".carousel-inner").swiperight(function() {  
		jQuery(this).parent().carousel('prev');  
		});  
    jQuery(".carousel-inner").swipeleft(function() {  
    	jQuery(this).parent().carousel('next');  
		});

 
});
