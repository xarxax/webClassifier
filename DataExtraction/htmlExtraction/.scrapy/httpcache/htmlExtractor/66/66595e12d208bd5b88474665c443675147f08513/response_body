		//create Chart class object
		var chart = new Chart(ctx, {
			type: "bar",
			data: data,
			options: options
		});
		
	});
	
	/* 메인 메뉴 섹션으로 스크롤 */
	function scrollSection(section) {
		/* 헤더와 타겟 섹션 사이의 값을 구함 */
		var target = $(section);
		var headerHeight = ($('.navbar.navbar-inverse.navbar-fixed-top').height() - $('.navbar-collapse.collapse.in').height());
		var offsetTop = target.offset().top - headerHeight;
		
		/* 계산된 값만큼 애니메이션 걸어줌 */
		$('html, body').animate({
			scrollTop: offsetTop
		}, 500);
	}
	
	function scrollToTop() {
		$('html, body').animate({
			scrollTop: 200 
		}, 500);
	}
</script>

