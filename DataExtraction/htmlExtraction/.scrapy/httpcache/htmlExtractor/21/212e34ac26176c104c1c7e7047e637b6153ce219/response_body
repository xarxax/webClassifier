	    function toggleChecks(sClass, b) {
	        var oChecks = $("." + sClass + " input[type=checkbox]");

	        if (oChecks != null) {
	            oChecks.each(function() {
	                if (typeof this.checked != "undefined") {
	                    if (b == null)
	                        this.checked = (!this.checked);
	                    else
	                        this.checked = b;
	                }
	            });
	        }
	    }
	</script>
	
	
	
	
<script>
    function HomeText(id)
    { 
        var div = document.getElementById('aboutasc');  
        var paras = div.getElementsByTagName('p'); 
        
        for (var i = 0; i < paras.length; i++)  
            paras[i].style.display = paras[i].id == id ? 'block' : 'none';   
    }
</script>

    
