<script language="JavaScript">
<!--

var loaded = true

defaultStatus = "Seq loaded!"
Seq.item("RunPath").At(0, "Ani", -1, 0.1, 1, 0.1)
Seq.item("RunPath").Play()

if (ie) {
  SB_DIV.style.pixelWidth += 4
  SB_DIV.style.pixelHeight += 4
}

var Haruhi_loaded
var Haruhi_loaded_count = 0

function Haruhi_Start() {
  if (!document.getElementById)
    return
  if (++Haruhi_loaded_count != 3)
    return

  Haruhi_loaded = true
  document.getElementById("Lcoming").innerHTML = "Enter Anime Theme now~!"
  document.getElementById("Lharuhi_bg").style.backgroundColor = "rgb(" + Haruhi_color.join("%,") + "%)"
  document.getElementById("Lharuhi").style.visibility = "inherit"

  Haruhi_over()
}

var Haruhi_count = 0
var Haruhi_mod = -1
var Haruhi_style

function Haruhi_over() {
  Haruhi_mod = 1
  document.getElementById("Lspeech").style.visibility = "inherit"
}

function Haruhi_out() {
  Haruhi_mod = -1
  document.getElementById("Lspeech").style.visibility = "hidden"
}

var Haruhi_color = [0,0,0]
var Haruhi_color_mod = [-1,-1,-1]

function Haruhi_come() {
  if (((Haruhi_count == 10) && (Haruhi_mod == 1)) || ((Haruhi_count == 0) && (Haruhi_mod == -1))) {
    if (Haruhi_count == 0) {
      if (document.getElementById("Iharuhi_hand_L").style.visibility == "hidden")
        return

      Haruhi_color = [0,0,0]
      Haruhi_color_mod = [-1,-1,-1]
    }
    else {
      for (var i = 0; i < 3; i++) {
        var c = Haruhi_color[i]
        if ((c == 0) || (c == 100)) {
          var mod = (Haruhi_color_mod[i] > 0) ? -1 : 1
          mod *= Math.floor(Math.random() * 9) + 2
          Haruhi_color_mod[i] = mod
        }
        c += Haruhi_color_mod[i]
        if (c < 0)
          c = 0
        else if (c > 100)
          c = 100
        Haruhi_color[i] = c
      }
    }

    document.getElementById("Iharuhi_hand_L").style.visibility = "hidden"
    document.getElementById("Iharuhi_hand_R").style.visibility = "hidden"

    document.getElementById("Lharuhi_bg").style.backgroundColor = "rgb(" + Haruhi_color.join("%,") + "%)"
    return
  }

  if ((Haruhi_count == 10) || (Haruhi_count == 0))
    Haruhi_style = Math.round(Math.random())

  Haruhi_count += Haruhi_mod

  var c_top = 484/2
  var c_left = 312/2

  var top_mod = parseInt(c_top/10 * Haruhi_count)
  var left_mod = parseInt(c_left/10 * Haruhi_count)

  var clip
  var L_top, L_left, R_top, R_left
  if (Haruhi_style == 0) {
    clip = "rect(" + (c_top - top_mod) + "px " + (c_left + left_mod) + "px " + (c_top + top_mod) + "px " + (c_left - left_mod) + "px)"
    L_top = (c_top - top_mod) - 8
    if (L_top < 36)
      L_top = 36
    R_top = (c_top + top_mod)
    if (R_top > 279)
      R_top = 279
  }
  else {
    clip = "rect(" + ((Haruhi_count == 10) ? 0 : 43+1) + "px " + (c_left + left_mod) + "px " + ((Haruhi_count == 10) ? 484 : 463-1) + "px " + (c_left - left_mod) + "px)"
    L_top = 36
    R_top = 279
  }

  L_left = (c_left - left_mod) - 17
  R_left = (c_left + left_mod) - 26
  if (L_left < 2)
    L_left = 2
  if (R_left > 262)
    R_left = 262

  document.getElementById("Lharuhi").style.clip = clip

  var ds = document.getElementById("Iharuhi_hand_L").style
  ds.top = L_top + "px"
  ds.left = L_left + "px"
  ds.visibility = "inherit"

  ds = document.getElementById("Iharuhi_hand_R").style
  ds.top = R_top + "px"
  ds.left = R_left + "px"
  ds.visibility = "inherit"
}

function Ani() {
  if (SB_loaded)
    SB_scroll()

  if (Haruhi_loaded)
    Haruhi_come()
}

//-->
</script>

