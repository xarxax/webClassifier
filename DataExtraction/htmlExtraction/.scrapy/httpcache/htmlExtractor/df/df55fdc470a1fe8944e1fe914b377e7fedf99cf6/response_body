function EmptyBasketItems() 
{
	var agree=confirm("Are you sure you want to empty the basket?");
	if (agree)
	{
		$.ajax( {
			    cache:true,
			    url:'/SiteSphereHandler.ashx?action=basketemptyitems&norecalc=1&vuid=684afe7c-3f67-4c7a-898b-4e2b3e625471&basketuid=4c2551b6-cff0-4345-b5cd-e60a9b665693',
			    error: function(request, error) { if(typeof console == 'object') { console.log('populateInventoryDivHandler ajax error: ' + error); } },
			    success: function(rdata) { document.location.href='/cart/';

			    }
		  })	    
	}
}

