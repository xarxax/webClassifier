	<script type="text/javascript">
	/*@cc_on
	  @if (@_jscript_version == 10)
	    document.write(' <link type= "text/css" rel="stylesheet" href="https://www.permanentmarking.com/wp-content/themes/Avada/css/ie10.css" />');
	  @end
