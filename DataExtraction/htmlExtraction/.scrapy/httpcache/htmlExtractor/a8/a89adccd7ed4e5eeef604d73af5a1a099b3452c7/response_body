    <script type="text/javascript">
        $(document).ready(function(){


            var links = [
                "http://www.ntma.org/benefits/",
                "http://www.ntma.org/benefits/initiatives/",
                "http://www.ntma.org/benefits/resources/",
                "http://www.ntma.org/events/",
                "http://www.ntma.org/events/registration/",
                "http://www.ntma.org/members/",
                "http://www.ntma.org/resources/job-board/",
                "http://www.ntma.org/resources/marketplace/",
                "http://www.ntma.org/store/",
                "http://www.ntma.org/store/publications/",
                "http://www.ntma.org/store/merchandise/",
                "http://www.ntma.org/store/give/",
                "http://www.ntma.org/resources/industry-news/",
                "http://www.ntma.org/about/foundation/"
            ];

            var hrefs = [
                "http://www.ntma.org/benefits/join-us/",
                "http://www.ntma.org/initiatives/",
                "http://www.ntma.org/resources/",
                "http://www.ntma.org/events/upcoming-events/",
                "https://members.ntma.org/eweb/dynamicpage.aspx?webcode=eventregistration",
                "http://www.ntma.org/members/directory/",
                "https://members.ntma.org/eweb/dynamicpage.aspx?webcode=JobBank",
                "https://members.ntma.org/eweb/dynamicpage.aspx?webcode=marketplace",
                "https://members.ntma.org/eweb/dynamicpage.aspx?webcode=store",
                "https://members.ntma.org/eweb/dynamicpage.aspx?webcode=publications",
                "https://members.ntma.org/eweb/dynamicPage.aspx?webcode=merchandise",
                "https://members.ntma.org/eweb/dynamicPage.aspx?webcode=merchandise",
                "http://www.ntma.org/events/news/",
                "http://www.ntma.org/initiatives/foundation/"
            ];


            $('#nav-full a, #nav-sub a').each(function(){
                var arrayI = jQuery.inArray($(this).attr('href'),links);
                if(arrayI > -1) $(this).attr('href',hrefs[arrayI]);
            });

            $('.logoutbtn').click(function(){
                $(this).after('<div class="lgload hidden"></div>');
                lgloc = $(this).attr('href');
                $('.lgload').load('/members/logoutthankyou/', function(){ window.location = lgloc; });
                return false;
            });

            $('.topSearchButton').click(function(){
                $('#search_form').submit();
                return false;
            });

            $('#search_form .submit').hide();

            $('#search_form .keywords').focus(function(){$(this).val('');});

            $('#search_form .keywords').blur(function(){
                if($(this).val()=='') $(this).val('Search');
            });
        });



    </script>

