<script>
    function revealPage() {
        var il = document.getElementById("init-load-imageBackground");
        il.classList.add("animate");
        il.addEventListener("animationend", function () {
            this.parentNode.removeChild(this);
        })
    }

    function setViewMidpoint() {
        styleValue = Math.round(window.innerHeight / 2) + "px";
        document.documentElement.style.setProperty('--viewable-midpoint', styleValue);
    }

    window.addEventListener('resize', setViewMidpoint);
    setViewMidpoint();
</script>
