/* ---------- TOP AD BANNERS INTERVAL SCRIPT ---------- */



var switchAd = 10000; // how long an ad will be displayed

var delayAd = 1000; // time delay to show next ad



function showfirstbanner(){

	jQuery('.top-ad-banners').hide();

	jQuery('.conquest-ad-banners').show();

	setTimeout(function(){

		if (jQuery('#conquest-extended-banner').css('height') == '0px'){

			jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 300px;');

			secondtimeout();

		}

	}, switchAd)

	function secondtimeout(){

		setTimeout(showsecondbanner, delayAd);

	}

}



function showsecondbanner(){

	jQuery('.top-ad-banners').hide();

	jQuery('.aue-ad-banners').show();

	setTimeout(function(){

		if (jQuery('#aue-extended-banner').css('height') == '0px'){

			jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 300px;');

			firsttimeout();

		}

	}, switchAd)

	function firsttimeout(){

		setTimeout(showfirstbanner, delayAd);

	}

}



var adArray = ['showfirstbanner', 'showsecondbanner']; // list ad functions in array

var randAd = adArray[Math.floor(Math.random() * adArray.length)] + '()';

setTimeout(randAd, 1000); // show first ad randomly with 1 second delay



/*-------------------------------------*/





/* TOP AD BANNERS SCRIPTS */



function showPush() { // show top hettich expanded

	jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 613px;');

	jQuery('.menu-shadow').animate({top:638},300);

	jQuery('#ac204ff2').animate({height:'0px'},0);

	jQuery('#a5aa37f0').animate({height:'415px'},600);

}



function hidePush() { // hide top hettich expanded

	jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 302px;');

	jQuery('.menu-shadow').animate({top:327},300);

	jQuery('#a5aa37f0').animate({height:'0px'},600);

	setTimeout(function(){jQuery('#ac204ff2').animate({height:'95px'},300)},600);

	setTimeout(function(){jQuery('.hettich-ad-banners').hide()},1900)

	setTimeout(showsecondbanner,2900);

}



function showAUEbanner(){ // show top aue expanded

	jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 613px;');

	jQuery('#aue-top-banner').animate({height:'0px'},0);

	jQuery('#aue-extended-banner').animate({height:'415px'},600);

}



function hideAUEbanner(){ // hide top aue expanded

	jQuery('body').attr('style','background: url(/images/custom/bg.png) repeat-x 0px 302px;');

	jQuery('#aue-extended-banner').animate({height:'0px'},600);

	setTimeout(function(){jQuery('#aue-top-banner').animate({height:'95px'},300)},600);

	setTimeout(function(){jQuery('.aue-ad-banners').hide()},1900)

	setTimeout(showsecondbanner,2900);

}





function InputName(value){ jQuery('#AIVname').val(value);}

function InputEmail(value){ jQuery('#AIVemail').val(value);}

function InputComp(value){ jQuery('#AIVcomp').val(value); setTimeout(sendFlashAIV,3000);}



function sendFlashAIV() //sends AIV Flash form

{

var str1 = jQuery('#AIVname').val();

var str2 = jQuery('#AIVemail').val();

var str3 = jQuery('#AIVcomp').val();

if (window.XMLHttpRequest)

  {// code for IE7+, Firefox, Chrome, Opera, Safari

  xmlhttp=new XMLHttpRequest();

  }

else

  {// code for IE6, IE5

  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");

  }

xmlhttp.onreadystatechange=function()

  {

  if (xmlhttp.readyState==4 && xmlhttp.status==200)

    {

    //document.getElementById("").innerHTML=xmlhttp.responseText;

    }

  }

xmlhttp.open("GET","/codes/send-sidekick-ad.php?senderName="+str1+"&senderEmail="+str2+"&senderCompany="+str3,true);

xmlhttp.send();

}



