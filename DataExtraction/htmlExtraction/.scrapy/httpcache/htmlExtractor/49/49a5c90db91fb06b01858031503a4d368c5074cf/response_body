	      /* Variables */
	      var form = document.getElementById("Apply_Now");
	      var eventCategory = "Leads";
	      var eventAction = "Lead_Form_Submitted";
	      var leadSource = "";
	      var match = /leadSource=([^&]+)/.exec(document.location.search); 
	      if (match != null && match.length == 2) 
	      {
	      leadSource = match[1];
	      }
	      function createFunctionWithTimeout(callback, opt_timeout) {
	      var called = false;
	      function fn() {
	      if (!called) {
	      called = true;
	      callback();
	      }
	      }
	      setTimeout(fn, opt_timeout || 2000);
	      return fn;
	      }
	      function myFunction(event)
	      {    
	      event.preventDefault();
	      //do ga
	      ga('set', 'dimension1', leadSource);
	      ga('send', 'event', eventCategory, eventAction, {
	      hitCallback: createFunctionWithTimeout(function() {
	      form.submit();
	      })
	      });
	      //end ga    
	      } 
	      form.addEventListener('submit', function(event){myFunction(event);});
	</script>
