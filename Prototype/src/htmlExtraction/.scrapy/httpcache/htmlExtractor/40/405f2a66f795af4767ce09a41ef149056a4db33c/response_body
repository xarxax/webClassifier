<script type="text/javascript">
    $(document).ready(function () {

        var input = $("#keyword");
        var HotItemPanel = $("#idHotItems");
        var UpdatePanel1 = 'search_panel_TAF5F392F002_cartUpdatePanel';


        input.parent().click(function () {
            var url = '/SearchByKeyword?word=' + escape(input.val());

            window.location.href = url;
        });

        input.click(function (event) {
            event.stopPropagation();
        });

        input.keypress(function (event) {
            if (event.which == 13) {
                event.preventDefault();
                input.parent().click();
            }



        });

        input.focus(function () {
            clearText(this);
        });

        input.blur(function () {
            clearText(this);
        });


        input.on('input', function (e) {
            var valx = input.val();

            if (valx.trim() == "")
                HotItemPanel.css("display", "none");
            else {
                
                                     
                var HotPushHid = $('#search_panel_TAF5F392F002_HotPush');

                HotPushHid.val(valx);
                ShowItems();
                HotItemPanel.css("display", "inline-block");
            }
        });



        function clearText(field) {
            if (field.defaultValue == field.value) field.value = '';
            else if (field.value == '') field.value = field.defaultValue;
        }

  function ShowItems() {
        if (UpdatePanel1 != null) {
            __doPostBack(UpdatePanel1, '');
        }
    }


    });


   

  



</script>





            
