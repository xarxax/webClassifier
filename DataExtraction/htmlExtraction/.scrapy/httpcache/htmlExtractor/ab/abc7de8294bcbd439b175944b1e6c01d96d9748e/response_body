  <script>
      // Create the dropdown base
      $("<select />").appendTo("nav#products");

      // Create default option "Go to..."
      $("<option />", {
         "selected": "selected",
         "value"   : "",
         "text"    : "Products"
      }).appendTo("nav select");

      // Populate dropdown with menu items
      $("nav#products a").each(function() {
       var el = $(this);
       $("<option />", {
           "value"   : el.attr("href"),
           "text"    : el.attr("title")
       }).appendTo("nav#products select");
      });
      $("nav#products select").change(function() {
        window.location = $(this).find("option:selected").val();
      });
  </script>

