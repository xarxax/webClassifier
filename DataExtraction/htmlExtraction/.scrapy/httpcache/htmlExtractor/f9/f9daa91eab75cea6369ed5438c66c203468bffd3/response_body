function populate(indexNum)	{
    var c_cat;
    var lastone = 0;
    if (regions[indexNum] == null){
        c_cat	=	regions['xx'];
    }else{
        c_cat = regions[indexNum];
    }
    document.PropertySearch.select_area.options.length = c_cat.length;
    for (i=0;i<c_cat.length;i++){
        var tempVals = c_cat[i].split("=");
        document.PropertySearch.select_area.options[i].text = tempVals[0];
        document.PropertySearch.select_area.options[i].value = tempVals[1];
        if (document.PropertySearch.select_area.options[i].value == 0){
            document.PropertySearch.select_area.options[i].selected=true;
        }
        }
}
function changes()	{
    var indexNumber=0;
    indexNumber = document.PropertySearch.country_code.options[document.PropertySearch.country_code.options.selectedIndex].value;
    populate(indexNumber);
    }
