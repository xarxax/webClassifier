        <script>
            $(function () {
                // run the currently selected effect
                function runEffect() {
                    // get effect type from
                    var selectedEffect = $("#effectTypes").val();

                    // most effect types need no options passed by default
                    var options = {};
                    // some effects have required parameters
                    if (selectedEffect === "scale") {
                        options = {percent: 0};
                    } else if (selectedEffect === "size") {
                        options = {to: {width: 200, height: 60}};
                    }

                    // run the effect
                    $("#effect").toggle(selectedEffect, options, 500);
                }
                ;

                // set effect from select menu value
                $("#button").click(function () {
                    runEffect();
                    $(this).toggleClass('active');
                });
            });
        </script>
        <script language="javascript">
            function newwindow() {

                $("#modal33").show();
            }

            function newwindow2() {
                $("#modal33").hide();
            }

            function forgotpswd() {
                var showme = document.getElementById("forgotpswd");
                showme.style.visibility = "visible";
            }

            function forgotpswd2() {
                var showme = document.getElementById("forgotpswd");
                showme.style.visibility = "hidden";
            }

            function openEmail() {

                $("#mailing2").show();
            }

            function closeMail() {
                $("#mailing2").hide();
            }

            function openRequestAccess() {
                $("#requestAccess").show();
                $("#loginW").hide();
            }
            function closeRequestAccess() {
                $("#requestAccess").hide();
            }
            function openReset() {
                $("#resetPassword").show();
            }
            function closeReset() {
                $("#resetPassword").hide();
            }
            function openLogin() {
                $("#loginW").show();
            }
            function closeLogin() {
                $("#loginW").hide();
            }

        </script>
