			map = new google.maps.Map(document.getElementById('map-canvas'), {
				center: new google.maps.LatLng(latitudec,longitude),
				zoom: zoom,
				mapTypeId: 'roadmap',
				scrollwheel: false
			});
			var myLatlng = new google.maps.LatLng(latitudec,longitude);
