<script type="text/javascript">

$(window).scroll(function(){relocateButton();});
$(window).resize(function(){relocateButton();}); 
var sectionHeight = 0;

function relocateButton()
{	
	if(sectionHeight != document.getElementById('contactRow').getBoundingClientRect().height)
	{		
		document.getElementById('contactBtn').style.marginTop = "0px";
		
		sectionHeight = document.getElementById('contactRow').getBoundingClientRect().height;
		
		var w  = window,
			d  = w.document,
			de = d.documentElement,
			db = d.body || d.getElementsByTagName('body')[0],
			x  = w.innerWidth || de.clientWidth || db.clientWidth,
			y  = w.innerHeight|| de.clientHeight|| db.clientHeight;
		
		if(x >= 992)
		{
			var section_rect = document.getElementById('contactRow').getBoundingClientRect();			
			var button_rect = document.getElementById('contactBtn').getBoundingClientRect();				
			document.getElementById('contactBtn').style.marginTop = ((section_rect.height -  button_rect.height) / 2) -15 + "px";
		}
	}
}
