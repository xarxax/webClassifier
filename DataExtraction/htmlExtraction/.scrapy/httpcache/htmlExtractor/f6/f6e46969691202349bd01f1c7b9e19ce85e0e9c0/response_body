<script type="text/javascript">
if (document.baseURI.indexOf("entry/preview") !== -1){
  href_list=document.querySelectorAll('[href*="resource?domainId"]');
  for (var i=0; i < href_list.length; i++){
    href=href_list[i].href;
    if (href.indexOf("__resources__/") == -1){
        if (href.indexOf("&path=/") !== -1){
          href_list[i].href=href.replace("&path=", "&path=/published");
        } else {
          href_list[i].href=href.replace("&path=", "&path=/published/");
        }
        console.log(href_list[i].href);
    }
  }
  img_list= document.querySelectorAll('[src*="resource?domainId"]');
  for (var i=0; i < img_list.length; i++){
    src=img_list[i].src;
    if (src.indexOf("__resources__/") == -1){
        if (src.indexOf("&path=/") !== -1){
          img_list[i].src=src.replace("&path=", "&path=/published");
        } else {
          img_list[i].src=src.replace("&path=", "&path=/published/");
        }
        console.log(img_list[i].src);
    }
  }
};
</script>

