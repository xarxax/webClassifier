		$(".more-btn").click(
			function(){
				var btn_id = $(this).attr("id");
				if(btn_id == "moreBtn"){
		 	    	$(".latest-news-posts").removeClass("d-none");
					$(this).prop('value', 'Less');
					$(this).attr("id","lessBtn");
		 		}else if(btn_id == "lessBtn"){
		 			$(".to-hide").addClass("d-none");
		 			$(this).prop("value","More");
		 			$(this).attr("id","moreBtn");
				}
			}
		);
	</script>
