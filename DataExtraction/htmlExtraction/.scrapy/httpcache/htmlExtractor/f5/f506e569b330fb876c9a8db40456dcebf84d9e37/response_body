	<script> // For carousel
		jQuery(document).ready(function(){
			jQuery('.multiple-items').slick({
			  infinite: true,
			  slidesToShow: 2,
			  slidesToScroll: 1,
			  accessibility: true,
			  responsive: [
			  	{
				  	breakpoint: 1150,
				  	settings: {
					  	slidesToShow:2,
					  	slidesToScroll: 1,
					  	infinite: true
				  	}
				 },
				 {
				 	breakpoint: 680,
				 	settings: {
					 	slidesToShow:1,
					 	slidesToScroll: 1,
					 	infinite: true
				 	}
			  	}]
			});
		});
	</script>
	
