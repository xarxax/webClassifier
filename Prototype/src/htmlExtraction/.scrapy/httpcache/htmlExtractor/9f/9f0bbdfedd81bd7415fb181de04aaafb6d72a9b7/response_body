        #responsive-menu-container #responsive-menu ul.responsive-menu-submenu li.responsive-menu-item a .responsive-menu-subarrow {
            right: 0;
            height: 40px;
            line-height: 40px;
            width: 40px;
            color: #ffffff;
            border-left: 1px solid #3f3f3f;
