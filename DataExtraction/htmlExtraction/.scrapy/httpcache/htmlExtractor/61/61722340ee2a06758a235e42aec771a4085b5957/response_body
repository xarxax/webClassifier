<script asyncd>
	$( document ).ready(function() {
	var currentSlide = 1;
	var slideTotal = $('.slide').length;
	var transition = 0;
	function goToTarget(targ){
		var target = targ;
		currentSlide = target;
		// console.log (currentSlide);
		$('.slide' + target).prependTo( $( ".cust-carousel" ) );
		$('.slide' + target).fadeIn( 500, function() {
			$('.slide').not('.slide' + target).fadeOut(1000);
			$('.pip' + target).addClass("active");
			$('.pip').not('.pip' + target).removeClass("active");
			transition = 0;
		});
	}
	function goNext(){
		if (transition == 0){
			transition = 1;
			clearInterval(t);
			t = setInterval(function(){ goNext(); }, 3000);
			if (currentSlide != slideTotal){
				goToTarget(parseInt(currentSlide, 10) + 1);
			} else {
				goToTarget(currentSlide = 1);
			}
		}
	}
	function goPrev(){
		if (transition == 0){
			transition = 1;
			clearInterval(t);
			t = setInterval(function(){ goNext(); }, 3000);
			if (currentSlide != 1){
				goToTarget(parseInt(currentSlide, 10) - 1);
			} else {
				goToTarget(currentSlide = slideTotal);
			}
		}
	}
	goToTarget(1);
	var t = setInterval(function(){ goNext(); }, 3000);
	$( ".pip" ).click(function() {
		if (transition == 0){
			transition = 1;
			clearInterval(t);
			t = setInterval(function(){ goNext(); }, 3000);
			goToTarget($(this).attr('class').substring(7,8));
		}
	});
	$( ".prev" ).click(function() {
		goPrev();
	});
	$( ".next" ).click(function() {
		goNext();
	});
	var mrg = (0.5 * $( ".cust-pips" ).width()) + 2;
	$( ".cust-pips" ).css('margin-left', '-' + mrg +'px');
});

</script>