        <script type="text/javascript">
            $(document).ready(function(){
                $('#mail-signup').submit(function(e){
                    e.preventDefault();
                    $.ajax({
                        type: 'POST',
                        url: $(this).attr('action'),
                        data: $(this).serialize(),
                        success: function(r){
                            $('#mail-signup')[0].reset();
                            $('#signup-thankyou').bPopup();
                        }
                    });
                });
            });
        </script>
				
				
