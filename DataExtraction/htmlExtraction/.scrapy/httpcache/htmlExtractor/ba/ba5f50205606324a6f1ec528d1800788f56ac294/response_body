<script language="JavaScript" type="text/javascript">
// News + Awards feeds
google.load("feeds", "1");
//var newsFeedURL="http://middlebynews.blogspot.com/feeds/posts/default/-/Top News";
var awardsFeedURL="http://middlebynews.blogspot.com/feeds/posts/default/-/Awards";
var feedLimit=4;
var months = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");

function feedInitialize() {
	//var newsFeed = new google.feeds.Feed(newsFeedURL);
	var awardsFeed = new google.feeds.Feed(awardsFeedURL);
	//newsFeed.load(function(result) {
		//if (!result.error) {
			//parseFeed(result.feed, "newsRSS");
		//}
	//});
	awardsFeed.load(function(result) {
		if (!result.error) {
			parseFeed(result.feed, "awardsRSS");
		}
	});
}
function parseFeed(rss, container) {
	for (var i = 0; i < feedLimit; i++) {
	
		var entry = rss.entries[i];
		var entryDt = new Date(entry.publishedDate);
		var entryURL = entry.link;
		var entryTitle = entry.title;
		
		var entryHTML = '<h4>' + months[entryDt.getMonth()] + ' ' + entryDt.getDate() + ', ' + entryDt.getFullYear() + '</h4><p><a href="' + entryURL + '" target="_blank">' + entryTitle + '</a></p><hr />';
		$("#" + container).append(entryHTML);
					
	}
}
google.setOnLoadCallback(feedInitialize);

// Flash stock ticker
//var params = {wmode: "transparent"};
//var attributes = {id: "stockTicker", name: "stockTicker"};
//swfobject.embedSWF("http://www.middleby.com/stock.swf", "flashcontent", "100", "25", "8", "#080808", params, attributes);
</script>
