<script type="text/javascript">
    $(function () {
        $('.propertyShortlists').click(function () {
            //e.preventDefault();
            console.log("property shortlists");
            $.ajax({
                type: "POST",
                url: "/Search/GetUserShortlists?userCode=",
                success: function (result) {
                    console.log(result);
                    $("#Ul-shortlists").html(result);
                    $(".dropdown-content").toggle();
                },
                error: function (response) {
                    console.log("Error occurred");
                }
            });
        });

        $('#id-arrivaldate').datepicker({
            format: 'dd-M-yyyy'
        });

        $('#id-departuredate').datepicker({
            format: 'dd-M-yyyy'
        }).datepicker('setDate', addDays(new Date(), 7));

    });

    $(document).on("click",
        function (event) {
            var $trigger = $(".dropdown");
            if ($trigger !== event.target && !$trigger.has(event.target).length) {
                $(".dropdown-content").hide();
            }
        });

    function EnterArrivalAndDepartureDates(shortlistId) {
        console.log("shortlistId: " + shortlistId);
       // $("#id-country").val("");
       //$("#id-city").val("");
        $("#arrivalDepartureDates").modal('show');
        $("#id-shortlistId").val(shortlistId);

        var monthNames = [
            "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct",
            "Nov", "Dec"
        ];

        var arrival = new Date();
        var arrivalDate = arrival.getDate() + "-" + monthNames[arrival.getMonth()] + "-" + arrival.getFullYear();
        console.log(arrivalDate);
        $('#id-arrivaldate').val(arrivalDate);

        var departure = addDays(new Date(), 7);
        console.log(departure);
        var departureDate = departure.getDate() +
            "-" +
            monthNames[departure.getMonth()] +
            "-" +
            departure.getFullYear();
        console.log(departureDate);
        $('#id-departuredate').val(departureDate);

        var url = window.document.location.pathname + window.document.location.hash;
        aDate = url.match('arrival-([0-9]{6})') != null ? url.match('arrival-([0-9]{6})')[1] : null;
        dDate = url.match('departure-([0-9]{6})') != null ? url.match('departure-([0-9]{6})')[1] : null;
        $.ajax({
            type: "POST",
            contentType: "application/json;charset=utf-8",
            url: "/Reservation/GetCountryAndCity?url=/&aDate=" + aDate + "&dDate=" + dDate,
            success: function (result) {
                var obj = jQuery.parseJSON(result);
                if (obj != null) {
                    obj.ADate = new Date(obj.ADate);
                    obj.DDate = new Date(obj.DDate);
                    var aD = obj.ADate.getDate() + "-" + monthNames[obj.ADate.getMonth()] + "-" + obj.ADate.getFullYear();
                    var dD = obj.DDate.getDate() + "-" + monthNames[obj.DDate.getMonth()] + "-" + obj.DDate.getFullYear();
                    if (obj.Country != undefined && obj.Country != null)
                    $("#id-country").val(obj.Country);
                    //if (obj.City != undefined && obj.City != null)
                        $("#id-city").val(obj.City);
                   $("#id-arrivaldate").val(aD);
                    $("#id-departuredate").val(dD);
                }
            },
            error: function (response) {
                console.log("Error occurred");
            }
        });
    }

    function addDays(theDate, days) {
        return new Date(theDate.getTime() + days * 24 * 60 * 60 * 1000);
    }

    function SendEnquiriesToEtrak() {
        console.log($("#id-shortlistId").val());
        console.log($("#id-arrivaldate").val());
        console.log($("#id-departuredate").val());
        var propertyDetails = {
            shortlistId: $("#id-shortlistId").val(),
            arrival: $("#id-arrivaldate").val(),
            departure: $("#id-departuredate").val(),
            shortlistedcountry: $("#id-country").val(),
            shortlistedcity: $("#id-city").val(),
            userCode: ''
        };
        if (propertyDetails.shortlistedcountry == "" || propertyDetails.shortlistedcity == "") {
            $("#statusDisplay").modal('show');
            $("#statusMessage").text("Please fill Country and city before sending an enquiry.");
        }
        else if ((new Date(propertyDetails.arrival) > new Date(propertyDetails.departure)))
        {
            $("#statusDisplay").modal('show');
            $("#statusMessage").text("Departure date must be greater than Arrival date");
        }
        else {
            $("#arrivalDepartureDates").modal('hide');
            $("#sendStatusDisplay").modal('show');
            
            $.ajax({
                type: "POST",
                url: "/Search/SendEnquiriesToEtrak",
                data: propertyDetails,
                success: function (status) {
                    console.log(status);
                    $("#sendStatusDisplay").modal('hide');
                    $("#statusDisplay").modal('show');
                    if (status == 'Failure' || status == '') {
                        $("#statusMessage").text("Enquiry sending failed");
                    } else {
                        $("#statusMessage").text("Your enquiry was sent successfully");
                    }
                },
                error: function (response) {
                    console.log("Error occurred");
                }
            });
        }
    }

    function ConfirmDeleteShortlist(shortlistId) {
        console.log("shortlistId: " + shortlistId);
        $("#deleteShortlist").modal('show');
        $("#deleteMessage").text("Are you sure you want to delete this shortlist?");
        $("#shortlistId").val(shortlistId);
    }

    function DeleteShortlist() {
        console.log($("#shortlistId").val());
        var shortlistId = $("#shortlistId").val();
        $.ajax({
            type: "POST",
            url: "/Search/DeleteShortlist?shortlistId=" + shortlistId,
            success: function (status) {
                console.log(status);
                $("#arrivalDepartureDates").modal('hide');
                $("#deleteShortlist").modal('hide');
                $("#statusDisplay").modal('show');
                if (status == 'Failure' || status == '') {
                    $("#statusMessage").text("Shortlist deletion failed");
                } else {
                    $("#statusMessage").text("Shortlist deletion was successful");
                }
            },
            error: function (response) {
                console.log("Error occurred");
            }
        });
    }

</script>

