<SCRIPT LANGUAGE="JavaScript">


startday = new Date(2011, 3, 1);
//alert(startday);
clockStart = startday.getTime();
function initStopwatch() { 
var myTime = new Date(); 
var timeNow = myTime.getTime();  
var timeDiff = timeNow - clockStart; 
this.diffSecs = timeDiff/1000; 
return(this.diffSecs); 
} 
function getSecs() { 
//alert('Helloooo');
var mySecs = initStopwatch(); 
var mySecs1 = ""+mySecs; 
mySecs1= mySecs1.substring(0,mySecs1.indexOf(".")) * 174; 

strTotalCans = "" + mySecs1;
	strNewTotalCans = "";
	len = strTotalCans.length;
	posCount = 0;
	for(i=len-1; i>=0;i--)
	{
		posCount++;
		strNewTotalCans=strTotalCans.substring(i,i+1) + strNewTotalCans;
		if (posCount%3==0 && i!=0)
		{
			strNewTotalCans=","+strNewTotalCans;
		}
	}
	
document.getElementById("canCounter").childNodes[0].nodeValue = strNewTotalCans 
window.setTimeout('getSecs()',1000); 
}

</SCRIPT>
