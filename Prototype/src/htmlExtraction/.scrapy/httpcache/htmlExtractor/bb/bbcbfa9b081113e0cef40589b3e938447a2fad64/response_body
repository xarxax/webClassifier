<script type="text/javascript">
function sw_tab(x) {
	$("#rpp > DIV").css("display", "none");
	$("#rpp_t"+x).css("display", "block");
	$("#rpp LI.active").removeClass("active");
	$("#l"+x).addClass("active");
	$("#l"+x+" > A").blur();
}

function reload_chart(x) {
	if (x == 'belex15') {
		//set_pbutton(document.getElementById('pd'), true);
		//set_tbutton(document.getElementById('bline'), true);
		init_buttons('pd', 'bline', new Array());
		mchart.set_symbol(x, 'd', 'line');
	} else if (x == 'belexline') {
		//set_pbutton(document.getElementById('pm'), true);
		//set_tbutton(document.getElementById('bline'), true);
		init_buttons('pm', 'bline', new Array('pd', 'bcandle'));
		mchart.set_symbol(x, 'm', 'line');
		//button_disable(document.getElementById('pd'));
		//button_disable(document.getElementById('bcandle'));

	} else if (x == 'srx') {
		/*
		set_pbutton(document.getElementById('pd'), true);
		set_tbutton(document.getElementById('bline'), true);

		button_disable(document.getElementById('pm'));
		button_disable(document.getElementById('p3m'));
		button_disable(document.getElementById('p6m'));
		button_disable(document.getElementById('p12m'));
		*/
		init_buttons('pd', 'bline', new Array('pm', 'p3m', 'p6m', 'p12m', 'p3y'));
		mchart.set_symbol(x, 'd', 'line');
	}
	//document.my_chart.Update_URL("/xml/indeksi.php?s=" + x, true, 30, 2, "reset");
	
	$("#ig LI.active").removeClass("active");
	$("#ig_"+x).addClass("active");
	$("#ig_"+x+" > A").blur();
}

</script>
