  jQuery(document).ready(function() {

	jQuery.noConflict();

    jQuery( "#machines" ).autocomplete({

      source: 'http://www.skbraly.com/index.php?option=com_machinery&view=machine&format=raw',

      search  : function(){ jQuery(this).addClass('loading');

	  },

      open    : function(){ jQuery(this).removeClass('loading');},

	  response: function(even, ui){setTimeout(function() {

        		jQuery( "#machines" ).removeClass('loading');

      }, 2000 );}

    });

  });

