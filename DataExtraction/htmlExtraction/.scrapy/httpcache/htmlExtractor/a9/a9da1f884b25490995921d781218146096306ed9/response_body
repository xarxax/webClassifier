			$(function() {
				$("#one").click(function(){
					$.ajax({
						type:"POST",
						url:"mediaAjax.html",
						dataType:"html",
						success:function(data) {
							
							 $('.ajax_data').html(data);
						},
						error:function() {
							alert("Error");
						}
					})
					
					$("#one").css({"background-color":"#3462b7","color":"#ffffff"});
					$("#two,#three,#four").css({"background-color":"transparent","color":"#3462b7"});
					});
				});
			
			$(function() {
