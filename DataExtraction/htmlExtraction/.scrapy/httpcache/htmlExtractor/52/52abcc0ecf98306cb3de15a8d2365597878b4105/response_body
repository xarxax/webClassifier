$(document).ready(function() {
	var menu = $("#langswitch ul.items");
	$("#langswitch").hover(
		function(){ 
			if (menu.is(':animated')) {
				return true;
			} else {
				menu.slideDown("fast"); 
			}
		},
		function(){ 
			if (menu.is(':animated')) {
				return true;
			} else {
				menu.slideUp("normal"); 
			}
		}
	);
});
</script>

