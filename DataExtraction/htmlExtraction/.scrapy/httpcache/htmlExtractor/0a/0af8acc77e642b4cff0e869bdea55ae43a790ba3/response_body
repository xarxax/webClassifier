	else{
		var height = 250;
	}
	
	$("#product_reveal").click(function () {
		$("#product_section").animate({
			height: ($("#product_section").height() == height) ? 0 : height
		}, 200);
		$( ".open" ).each(function() {
		  $( this ).removeClass( "open" );
		});
		return false;
	});
