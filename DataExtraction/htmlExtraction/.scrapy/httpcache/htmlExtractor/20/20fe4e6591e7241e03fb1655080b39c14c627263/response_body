		<script>
			$(document).ready(function() {
				
				// carousel
				var carousel = $(".carousel");
				carousel.carouFredSel({
					items: 1,
					responsive:true,
					scroll: {
						 fx: "crossfade",
						items: 1,
						duration: 1000
						// pauseOnHover: true		
					},
					auto: {
						timeoutDuration: 5000,
						delay: 2000
					},
					prev: {
						button: ".previous",
						key: "left"
					},
					next: {
						button: ".next",
						key: "right"
					},
					pagination: {
						container: ".pagination",
						keys: true
					},
					swipe: true,
					onCreate : function () {
						$(window).on('resize', function(){
							carousel.parent().add(carousel).css('height', carousel.children().first().height() + 'px');
						}).trigger('resize');
					}
				});	
				
				 $(window).resize(function() {
					if ($(window).width() > 768) {
						$('.carousel, .caroufredsel_wrapper').css({'height':($('.featured').width()*0.31125)+'px'});
					} else {
						$('.carousel, .caroufredsel_wrapper').css({'height':($('.featured').width()*0.74662669+$('.caption').outerHeight())+'px'});
					}
				}).resize(); 
				
				/*
				$(function() {
					$('.slide img').load(function() {  
					    $(this).fadeIn('slow');  
					});
				});
				*/
				
			});
		</script> 
		
		
	
		
