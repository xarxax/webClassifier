<SCRIPT language="JavaScript" >

function msg121Client()

{

	var company="wtopharma";

	var msg121RootUrl="http://165.21.82.40:8080/msg121";

	var url="client.jsp?company="+company+"&referer=http://165.21.82.40:8080";

	var MSIEIndex = navigator.userAgent.indexOf("MSIE");

	var MacIndex = navigator.userAgent.indexOf("Mac");

	var swidth=window.screen.width;

	var sheight=window.screen.height;

	var currentDate = new Date();

	var winTitle = "MSG121Client_"+currentDate.getTime();

	if(MSIEIndex!=-1){

		var width=240;

		var height=412;

		var sx=swidth-width-10;

		var sy=0;

		var s="";

		if(MacIndex!=-1){

			s="toolbar=no,directories=no,resizable=no,scrollbars=no,width="+width+",height="+height+",top=0,left="+sx;

		}

		else{

			s="toolbar=no,directories=no,resizable=yes,scrollbars=no,width="+width+",height="+height+",top=0,left="+sx;

		}

		msg121window = window.open("",winTitle,s);

		msg121window.location.href=msg121RootUrl+"/client/ie/"+url;

	

	}

	else{

		var width=240;

		var height=412;

		var sx=swidth-width-10;

		var sy=0;

		var s="toolbar=no,directories=no,resizable=no,scrollbars=no,width="+width+",height="+height+",screenY=0,screenX="+sx;

		var msg121window = window.open(msg121RootUrl+"/client/netscape/"+url,winTitle,s);

		msg121window.focus();

	}

}

</script>

