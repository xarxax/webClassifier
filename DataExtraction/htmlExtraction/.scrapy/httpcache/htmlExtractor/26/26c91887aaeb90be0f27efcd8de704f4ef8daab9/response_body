//iOS Temp Fix   
   var iOS = ( navigator.userAgent.match(/(iPad|iPhone|iPod)/g) ? true : false );

	window.onload=function(){
		if(iOS){
			iOS_links = document.getElementsByClassName("rtsLI");
			while(iOS_links.length > 0){
				for (var i=0; i<iOS_links.length; i++){
			 		iOS_links[i].className = iOS_links[i].className.replace(/\brtsLI\b/g,'iOS_rtsLI');
				}
				iOS_links = document.getElementsByClassName("rtsLI");
			}
		}
	};
