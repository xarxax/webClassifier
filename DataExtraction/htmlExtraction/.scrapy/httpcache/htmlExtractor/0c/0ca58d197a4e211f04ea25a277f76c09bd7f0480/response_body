		var alert_status = true;
		var alert_delay = 1600;

		function hide_alert() {
			$('div#site-alert ul').fadeOut(800);
			setTimeout(function() {
				$('#site-alert').slideUp();
				if ($(window).width() < 940) {
					$('nav.header-nav-bar').animate({ top: ($('.header-nav-bar').height() - $('#compact-nav').height()) * -1 +1 });
				}
				else {
					$('.main-content').animate({ marginTop: 77 });
				}
				alert_status = false;
				alert_delay = 0;
			}, 800);

		}
		
		$(function() {

				


			$('#hide-alert').click(function() {
				hide_alert();
			});

			
				// display random background on page load
				var randomnumber = Math.floor(Math.random()*3+1)
				var background_img = $(document).data('base_url') + 'css/images/bg-homepage-' + randomnumber + '.jpg';
				$('img.bg-homepage').attr('src', background_img);


			
		});
		

		
