<script type="text/javascript">
$(function() {  
    $(".JMyCarousel").jMyCarousel({  
        visible: '816px',  
        eltByElt: true,
		speed: 500,
		circular: false
    });
	 
	$('.img_click').click(function () {
		$("#maincontent-html").fadeOut("slow");
		var cid = $(this).attr("id")
		//alert(cid);
		
		$.post("index-image-load.php", { thumbindex: cid },
 			function(data){
			$("#maincontent-html").html(data);
			$("#maincontent-html").fadeIn("slow");
    		//alert("Data Loaded: " + data + "\nData Sent: " + cid);
  		});
	});
	
	$('.next').css('background-image','url(images/carousel/arrow_right.gif)');
	$('.next').css('background-repeat','no-repeat');
	$('.prev').css('background-image','url(images/carousel/arrow_left.gif)');
	$('.prev').css('background-repeat','no-repeat');

	$('.prev').css('width','38px');
	$('.next').css('width','38px');
	$('.prev').css('height','48px');
	$('.next').css('height','48px');

	
});

//if user has flash plugin installed redirect to flash version
if (pluginlist.indexOf("Flash")!=-1&&0!=1) {
	var version = getFlashVersion().split(',').shift();
	//if flash version is 8 or higher send to flash version
	if (version > 7) {
		var reURL = 'http://www.plasmacam.com/indexfla.php';
//		var reURL = 'http://www.plasmacam.com/indexfla.php';
		//this is the redirect, uncomment for use to redirect to flash
		if(javascriptVersion1_1) {
  			window.location.replace(reURL);
		} else {
  			window.location = reURL;
		}
	}
}
</script>
