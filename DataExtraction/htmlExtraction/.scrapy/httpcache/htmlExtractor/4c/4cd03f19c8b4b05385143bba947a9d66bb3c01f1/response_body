//<![CDATA[
RateProgramChange();var emailLinksPopupMessage = 'Email is not a secure method of communication and should NOT be used to send personal or financial information.';
var externalSitesLinkPopupMessage = 'You are leaving our website and going to a 3rd party site that we cannot guarantee security.';
var enableExternalLinkSecurityPopUp = false;
var enableEmailSecurityPopUp = true;
attachPageOnLoad(function (){
	var logo = getFirstChild(document.getElementById('LogoContainer'));
	var footerImgLs = document.getElementsBySelector('.SmLogoContainer');
	var footerImgRs = document.getElementsBySelector('.EqualContainer');
	var aList = document.getElementsByTagName('a');
	var a = new Array();
	for (var i = 0; i < aList.length; i++)
	{
	    a.push(aList[i]);
	}
	if (logo) a.push(logo);
	for (var i = 0; i < footerImgLs.length; i++)
	{
	    a.push(getFirstChild(footerImgLs[i]));
	}
	for (var i = 0; i < footerImgRs.length; i++)
	{
	    a.push(getFirstChild(footerImgRs[i]));
	}
	var s = location.href.toLowerCase();
	var n = s.indexOf('//');
	if (n >= 0)
	{
		s = s.substr(0, s.indexOf('/', n + 2));
	}
	for (var i = 0; i < a.length; i++)
	{
		if (a[i] == null) continue;
		var href = a[i].href;
		if (!href) href = a[i].getAttribute('href');
		var url = href == null ? '' : href.toLowerCase();
		if (0 == url.length ||
		    0 == url.indexOf('javascript:') ||
			0 < url.indexOf('www.superiormg.com') ||
			0 < url.indexOf('5641910295.secure-onlineorigination.com') ||
			0 < url.indexOf('5641910295.mortgage-application.net') ||
			0 == url.indexOf(s)
			) continue;
			setExternalLink(a[i], '');
	}
});//]]>
