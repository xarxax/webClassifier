<script type="text/javascript">
    $(document).ready(function() {
        $(".giftcards,.contact-us").fancybox({
            'width': 960,
            'height': '90%',
            'autoScale': true,
            'transitionIn': 'elastic',
            'transitionOut': 'elastic',
            'speedIn': 100,
            'speedOut': 100,
            'type': 'iframe',
            'scrolling': 'auto',
            'overlayShow': true,
            'overlayOpacity': 0.8,
            'overlayColor': '#000',
            'hideOnContentClick': false,
            'hideOnOverlayClick': false,
            'enableEscapeButton': true
        });

        var here = location.href;
        if(here.indexOf('/five-crowns',0) > 0){ $('#social-icons').show(); }else{ $('#social-icons').hide(); }
        if(here.indexOf('/lawrys-carvery',0) > 0){ $('#social-icons-carv').show(); }else{ $('#social-icons-carv').hide(); }
