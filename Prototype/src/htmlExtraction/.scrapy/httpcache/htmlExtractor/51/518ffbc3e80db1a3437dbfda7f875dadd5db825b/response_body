		var disablepopupbelow = "";
		var popupcookieconfigpath = "/";
		
		var popupgaeventcategory = "";
		var popupgavieweventaction = "";
		var popupgaconversioneventaction = "";
		var popupSendToAnalytics = "None";
		
		initExitPopupTimer(5000)
		
