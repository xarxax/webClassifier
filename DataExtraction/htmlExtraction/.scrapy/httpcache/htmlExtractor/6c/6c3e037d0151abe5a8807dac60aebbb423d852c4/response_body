	<script type="text/javascript">
	//<![CDATA[
	config.ShopPath = 'http://www.baldwinmedical.com.au';
	config.AppPath = '';
	var ThumbImageWidth = 65;
	var ThumbImageHeight = 65;
	
    $(document).ready( function()
    {         
    	/**************** SEARCH BAR WIDTH **********************/ 
        var navWidth = 0;
        var maxWidth = 750;

        // Calculate the total nav width, excluding the overlap for each item.
        $("#nav > li").each(function() 
        { 
            navWidth += $(this).width() - 20;
        });
        
      	
      	// Make sure the nav is not too wide.  If it is, high the end elements.
        if(navWidth > maxWidth)
        {
   			// The nav is too wide
   			var lis = $("#nav > li");
   			var end = lis.length;
   			
   			// Keep hiding nav items until the menu fits.
   			while(navWidth > maxWidth)
   			{
   				// Get the width of the end item
				var thisItem = $(lis).find(":eq(" + end + ")");
				var width = ($(thisItem).width());
				
				// Subtract the width of this item from the total navWidth
				navWidth = navWidth - (width - 20);
				
				// Hide this item
				$(thisItem).css("display", "none");
				
				// Move the end point backwards 
				end = end - 1;
   			}
        }

        var searchWidth = $('#Container').width() - navWidth; 
                        
        $('#quickSearch').css("width", searchWidth); 
        
        /**************** NAV COLOURS **********************/
        // Note the nav colours array is created and defined in PagesMenu.php     
        var idx = 0;
        
        for(idx = 0; idx < colours.length; idx++)
        {
			$("#nav > li:eq(" + idx + ") a").css("background-color", colours[idx]);	
        }
		
		//************** INSERT SCROLL PANE AFTER THE FIRST H1 IN THE DOCUMENT *********************/
		// Find the h1 tag
		var h1 = $("#PageContent h1:first");
		if((h1 != undefined) && (h1 != null) && (h1.html() != null))
		{
			// We've found a h1 tag - store its contents
			var h1html = h1.html();

			// Remove the h1 tag from the document
			$("#PageContent h1:first").remove();
			
			// Get the whole page block html
			var blockHTML = $("#PageContent .BlockContent").html();
			
			// Reconstructor the page block WITH the heading and the scroll pane after the heading
			blockHTML = '<h1>' + h1html + '</h1>' +
				 '<div id="innerContentWrapper" class="scroll-pane">' +
				 blockHTML +
				 '</div>';
			
			// Set the reconstructed page html back into the page.
			$("#PageContent .BlockContent").html(blockHTML);
				
			// Invoke the scroll pane 
			$('.scroll-pane').jScrollPane({showArrows: true});
		}
		else
		{
			$('.scroll-pane').jScrollPane({showArrows: true}); 
		}
		
		// Header innerfade
		/*
		$('#heroFader').innerfade(
		{
			speed: 'slow', 
			timeout: 6000
		});
		*/
		
		var flashvars = {};
		var params = {wmode: "transparent"};
		var attributes = {};	
		
swfobject.embedSWF("http://www.baldwinmedical.com.au/templates/default/test_banner.swf", "flashheader", "1000", "225", "9.0.0", "http://www.baldwinmedical.com.au/templates/default/expressInstall.swf", flashvars, params, attributes);


		
		/************** CONTACT FORM SUBMISSION HANDLING ****************/
		$("#ContactForm").submit(function()
		{
			if(!$("#ContactForm").validate().form())
			{
				alert("Please fill in all required fields");
				return false;
			}
			
			return true;
		});		
		
		$('#ContentWrapper table tr:odd').addClass('odd');
		
		if(navigator.userAgent.indexOf("MSIE 7") > 0)
		{
			setTimeout(function()
			{
				$(".scroll-pane").css("z-index", 1);
				$("ul.dropdown").css("z-index", 2);
			}, 250);		
		}
    });	
	//]]> 
	</script>
