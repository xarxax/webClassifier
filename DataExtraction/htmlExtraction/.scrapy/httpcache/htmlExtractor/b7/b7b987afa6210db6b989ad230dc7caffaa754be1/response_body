<script type="text/javascript">
    $(function (){
        // banner
        $('#banner > ul').bxSlider({
            mode:'fade',
            pager:false,
            nextSelector:'.bannerControl > .bannerRight',
            prevSelector:'.bannerControl > .bannerLeft',
            nextText:'',
            prevText:'',
        });


        $('.bisStage > li > a').hover(function() {
            $(this).find("i:last").fadeToggle();
        });

        $(".accordion > .item > .title").click(function(){
            var accordion = $(this).parent('div').parent('div');
            var curItem = $(this).parent('div');
            $(accordion).find('.text').animate({height:0},300);
            $(curItem).find('.text').animate({height:'140px'},400);
            $(accordion).children('div').removeClass('active');
            $(accordion).find('i.fa').removeClass('fa-minus');
            $(accordion).find('i.fa').addClass('fa-plus');
            $(curItem).find('i.fa').addClass('fa-minus');
            $(curItem).addClass('active');
        })
    })
</script>

