	<script type="text/javascript">
		function getCookie(cname) {
		    var name = cname + "=";
		    var decodedCookie = decodeURIComponent(document.cookie);
		    var ca = decodedCookie.split(';');
		    for(var i = 0; i <ca.length; i++) {
		        var c = ca[i];
		        while (c.charAt(0) == ' ') {
		            c = c.substring(1);
		        }
		        if (c.indexOf(name) == 0) {
		            return c.substring(name.length, c.length);
		        }
		    }
		    return "";
		}
		$(document).ready(function() { // USED FOR MOTULITPLE THINGS
			window.onProontoChatLoad = function(isAgentOnline, isControlGroup){
				proonto.on(proonto.Events.onClientDetailsEntered, function(name, email, phone, question) {
					ga('send', 'event', 'ProntoChat', 'Client Details Entered', 'ChatLead', 1);
				});
				proonto.on(proonto.Events.onChatMessage, function() {
					ga('send', 'event', 'ProntoChat', 'Chat Opened', 'Chat Started', 0);
				});
			}
		    $('input[name="wp_job_manager_send_application"]').click(function(){
		      var _job_id = $('[name="job_id"]').val(),
				_email = $('#email-address').val(),
				$utms = getCookie('sonder_utm'),
				$params = {job_id: _job_id, email:_email, utms : $utms};
				$.ajax({
					type: "POST",
					url:      atc_ajax.ajaxurl,
					dataType: 'json',
					data:     {
						action:'send_hubspot_job',
						params: $params  
					},
					success: function( response ){
						console.log('success');
						console.log(response);
					},
					error: function( xhr ){
						console.log(xhr.responseText);
					}
				});
			})
	    });			 
	</script>
