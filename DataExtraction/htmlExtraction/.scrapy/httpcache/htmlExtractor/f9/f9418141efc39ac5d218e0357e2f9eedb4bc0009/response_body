<script language="Javascript" type="text/javascript">
   var http_request = false;
   var submitForm;

   function submitForm(formObject,url) {

       url += '?';
       submitForm = formObject;
       for(var i=0;i < formObject.elements.length;i++)
       {
            if ( i != 0 )
            {
                url += '&';
            }
            url += formObject.elements[i].name + "=" + escape(formObject.elements[i].value);
       }

       if (window.XMLHttpRequest) { // Mozilla, Safari,...
           http_request = new XMLHttpRequest();
       } else if (window.ActiveXObject) { // IE
           http_request = new ActiveXObject("Microsoft.XMLHTTP");
       }
       //alert(url);
       http_request.onreadystatechange = alertContents;
       http_request.open('GET', url, true);
       http_request.send(null);

   }

   function alertContents() {

       if (http_request.readyState == 4) {
           if (http_request.status == 200) {
               alert(http_request.responseText);
               for(var i=0;i < submitForm.elements.length;i++)
               {
                   submitForm.elements[i].disabled = 1;
               }
           } else {
               alert('There was a problem with the request.');
           }
       }

   }

function hoverUpdate(imageName,goToClass,image)
{
    var obj = image;
    var x = obj.offsetLeft;
    var y = obj.offsetTop + obj.height;
    while(obj.offsetParent)
    {
        if ( obj == document.getElementsByTagName('body')[0])
        {
            break;
        }
        else
        {
            x += obj.offsetParent.offsetLeft;
            y += obj.offsetParent.offsetTop;
            obj = obj.offsetParent;
        }
    }
     var element = (document.getElementById) ? document.getElementById(imageName + "_caption") : eval("document.all['" + imageName + "_caption']");
     element.className = goToClass;
     if (( x != -1 ) && ( y != -1 ) ) 
     {
         element.style.top = y - 5 + "px";
         element.style.left = x + "px";
     }
}

function changeMainimage(imageName,src,width,height,space)
{

    if ( ! document.images[imageName] )
    {
        //alert("Sorry - which? " + imageName );
    }
    else if ( document.images[imageName].src != src ) 
    {
        document.images[imageName].src = src;
        document.images[imageName].width = width;
        document.images[imageName].height = height;
        document.images[imageName].hspace = space;
        document.images[imageName].vspace = space;
    }
}

function changeMainvideo(imageName,src,width,height,space)
{
	var element = (document.getElementById) ? document.getElementById(imageName + '_video') : eval("document.all[imageName + '_video']");
	var element_mp4 = (document.getElementById) ? document.getElementById(imageName + '_mp4') : eval("document.all[imageName + '_mp4']");
	element_mp4.src = src + '.mp4';
	var element_webm = (document.getElementById) ? document.getElementById(imageName + '_webm') : eval("document.all[imageName + '_webm']");
	element_webm.src = src + '.webm';
	var element_ogv = (document.getElementById) ? document.getElementById(imageName + '_ogv') : eval("document.all[imageName + '_ogv']");
	element_ogv.src = src + '.ogv';
	element.width = width;
	element.height = height;
	element.src = src + '.mp4';
}

function resizeMainimageForBrowserSize()
{
    var WindowWidth = 0, WindowHeight = 0;
    if( typeof( window.innerWidth ) == 'number' )
    {
        WindowWidth = window.innerWidth;
        WindowHeight = window.innerHeight;
    } else if( document.documentElement && ( document.documentElement.clientWidth || document.documentElement.clientHeight ) )
    {
        WindowWidth = document.documentElement.clientWidth;
        WindowHeight = document.documentElement.clientHeight;
    } else if( document.body && ( document.body.clientWidth || document.body.clientHeight ) )
    {
        WindowWidth = document.body.clientWidth;
        WindowHeight = document.body.clientHeight;
    }

    var StyleSheetCount = document.styleSheets.length;

    if (!document.styleSheets) return;
    var theRules = new Array();
    if (document.styleSheets[0].cssRules)
        theRules = document.styleSheets[0].cssRules
    else if (document.styleSheets[0].rules)
        theRules = document.styleSheets[0].rules
    else return;

    var fullMargin = WindowWidth - 650;
    if ( fullMargin < 0 ) fullMargin = 0;

    var onePortraitMargin = WindowWidth - 850;
    if ( onePortraitMargin < 0 ) onePortraitMargin = 0;

    var oneLandscapeMargin = WindowWidth - 950;
    if ( oneLandscapeMargin < 0 ) oneLandscapeMargin = 0;

    var twoPortraitMargin = WindowWidth - 1250;
    if ( twoPortraitMargin < 0 ) twoPortraitMargin = 0;

    var tableCellMargin = (WindowWidth - 1050) / 2;
    if ( tableCellMargin < 0 ) tableCellMargin = 0;

    var topMargin = 5;
    var fontSize = 100;
    SourceWidth = WindowWidth;
    var mediaQuery3 = '(-webkit-min-device-pixel-ratio: 3.0), (min--moz-device-pixel-ratio: 3.0), (-o-min-device-pixel-ratio: 3/1), (min-resolution: 3.0dppx)';
    var mediaQuery2 = '(-webkit-min-device-pixel-ratio: 2.0), (min--moz-device-pixel-ratio: 2.0), (-o-min-device-pixel-ratio: 2/1), (min-resolution: 2.0dppx)';
    var mediaQuery15 = '(-webkit-min-device-pixel-ratio: 1.5), (min--moz-device-pixel-ratio: 1.5), (-o-min-device-pixel-ratio: 3/2), (min-resolution: 1.5dppx)';
    var mediaQuery13 = '(-webkit-min-device-pixel-ratio: 1.3), (min--moz-device-pixel-ratio: 1.3), (min-resolution: 1.3dppx)';
    var mediaQuery125 = '(-webkit-min-device-pixel-ratio: 1.25), (min--moz-device-pixel-ratio: 1.25), (min-resolution: 1.25dppx)';

    var root = (typeof exports === 'undefined' ? window : exports);

    if (root.matchMedia && root.matchMedia(mediaQuery3).matches) {
	SourceWidth = WindowWidth * 3;
    }
    else if (root.matchMedia && root.matchMedia(mediaQuery2).matches) {
	SourceWidth = WindowWidth * 2;
    }
    else if (root.matchMedia && root.matchMedia(mediaQuery15).matches) {
    	SourceWidth = WindowWidth * 1.5;
    }
    else if (root.matchMedia && root.matchMedia(mediaQuery13).matches) {
	SourceWidth = WindowWidth * 1.3;
    }      
    else if (root.matchMedia && root.matchMedia(mediaQuery125).matches) {
	SourceWidth = WindowWidth * 1.25;
    } 

    var a0;
    var a1;
    var a2;

    if ( SourceWidth > 2300 )
    {
        sa0 =  "_550H.jpg"
        sa1 =  "_550W.jpg"
    }
    else if ( SourceWidth > 1900 )
    {
        sa0 =  "_500H.jpg"
        sa1 =  "_500W.jpg"
    }
    else if ( SourceWidth > 1600 )
    {
        sa0 =  "_450H.jpg"
        sa1 =  "_450W.jpg"
    }
    else if ( SourceWidth > 1400 )
    {
        sa0 =  "_400H.jpg"
        sa1 =  "_400W.jpg"
    }
    else if ( SourceWidth > 1200 )
    {
        sa0 =  "_350H.jpg"
        sa1 =  "_350W.jpg"
    }
    else if ( SourceWidth > 1050 )
    {
        sa0 =  "_300H.jpg"
        sa1 =  "_300W.jpg"
    }
    else if ( SourceWidth > 870 )
    {
        sa0 =  "_250H.jpg"
        sa1 =  "_250W.jpg"
    }
    else if ( SourceWidth > 770 )
    {
        sa0 =  "Sm.jpg"
        sa1 =  "Sm.jpg"
    }
    else if ( SourceWidth > 670 )
    {
        sa0 =  "Sm.jpg"
        sa1 =  "Sm.jpg"
    }
    else if ( SourceWidth > 570 )
    {
        sa0 =  "_150H.jpg"
        sa1 =  "_150W.jpg"
    }
    else
    {
        sa0 =  "_150H.jpg"
        sa1 =  "_150W.jpg"
    }

    if ( WindowWidth > 2300 )
    {
        topMargin = 20;
        fontSize = 130;
        a0 =  367;
        a1 =  550;
        a2 =  368;
    }
    else if ( WindowWidth > 1900 )
    {
        topMargin = 20;
        fontSize = 130;
        a0 =  333;
        a1 =  500;
        a2 =  334;
    }
    else if ( WindowWidth > 1600 )
    {
        topMargin = 20;
        fontSize = 130;
        a0 =  300;
        a1 =  450;
        a2 =  301;
    }
    else if ( WindowWidth > 1400 )
    {
        topMargin = 20;
        fontSize = 120;
        a0 =  267;
        a1 =  400;
        a2 =  267;
    }
    else if ( WindowWidth > 1200 )
    {
        topMargin = 20;
        fontSize = 115;
        a0 =  233;
        a1 =  350;
        a2 =  234;
    }
    else if ( WindowWidth > 1050 )
    {
        topMargin = 15;
        fontSize = 110;
        a0 =  200;
        a1 =  300;
        a2 =  201;
    }
    else if ( WindowWidth > 870 )
    {
        topMargin = 10;
        fontSize = 105;
        a0 =  167;
        a1 =  250;
        a2 =  167;
    }
    else if ( WindowWidth > 770 )
    {
        topMargin = 10;
        fontSize = 105;
        a0 =  133;
        a1 =  200;
        a2 =  134;
    }
    else if ( WindowWidth > 670 )
    {
        topMargin = 10;
        fontSize = 105;
        a0 =  133;
        a1 =  200;
        a2 =  134;
    }
    else if ( WindowWidth > 570 )
    {
        topMargin = 10;
        fontSize = 105;
        a0 =  100;
        a1 =  150;
        a2 =  100;
    }
    else
    {
        a0 =  100;
        a1 =  150;
        a2 =  100;
    }

    changeMainimage("linkimage345", "KyleOnSnow" + sa0, a0, a1, topMargin);
    changeMainimage("linkimage343", "KaylieOnSnow" + sa1, a1, a0, topMargin);
    changeMainimage("linkimage353", "TappedTree" + sa0, a2, a1, topMargin);
    changeMainimage("linkimage344", "KyleHoldingLines" + sa1, a1, a2, topMargin);
    changeMainimage("linkimage346", "LinesFromSapWell" + sa1, a1, a0, topMargin);
