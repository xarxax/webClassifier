<SCRIPT LANGUAGE='JavaScript'>
function validateFormEclub(){

	with(document.eclub_form){
		if(step.value == ''){
			step.focus();
			return false;

		}else {
			return true;
		}
	}
}
</SCRIPT>


<SCRIPT LANGUAGE='JavaScript'>
function validateFormGiftCard(){
	with(document.giftcard_form){
		if(giftcard_step.value == ''){
			giftcard_step.focus();
			return false;

		}else {
			return true;
		}
	}
}
</SCRIPT>

<SCRIPT LANGUAGE='JavaScript'>
function modify_div(div_id1, div_content1, div_id2, div_content2){

	var div_id1 = div_id1;
	var div_content1 = div_content1;
	
	var div_id2 = div_id2;
	var div_content2 = div_content2;
	
	if (div_id1 != '' && div_content1 != ''){
		document.all[div_id1].innerHTML = div_content1;
	}

	if (div_id2 != '' && div_content2 != ''){
		document.all[div_id2].innerHTML = div_content2;
	}

}

function allowForm(){
		document.getElementById('send_form').value = 'y';		
}

function add_comment(){
		document.getElementById('add_comment').value = 'y';		
}

function add_post(id){
		
		var id = id;
		document.getElementById(id).value = 'y';		
		
}
</SCRIPT>


