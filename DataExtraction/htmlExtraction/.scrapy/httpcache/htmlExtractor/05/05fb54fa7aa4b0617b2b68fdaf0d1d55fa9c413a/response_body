<script>
    function psLaunchUrlOption() {
        if ($("#payrollLoginSelect")[0].selectedIndex > 0) {
            _gaq('send', 'event', 'Payroll Login', 'Click', $("#payrollLoginSelect").val() );
            window.open($("#payrollLoginSelect").val(), '_blank');
        }
    }
</script>
