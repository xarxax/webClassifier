		jQuery(window).load(function() {
			jQuery('input[type=submit]').on('click',function(){if(typeof dataLayer != "undefined"){dataLayer.push({ "event":"ga_event", "category":"Form Submission", "action":"Click", "label": "Form: " + jQuery(this).parents("form").attr("action").replace(/\//g,"") + " ", });}});
		});
		</script>	<script>
		function link_is_external(link_element) {
			return (link_element.host !== window.location.host);
		}

		var links = document.getElementsByTagName('a');
		for (var i = 0; i < links.length; i++) {
			if (link_is_external(links[i])) {
				links[i].onclick = function() {
                    if(typeof dataLayer != "undefined"){dataLayer.push({ "event":"ga_event", "category":"External Links", "action":"Click", "label": ""+this.href+"", });}				}
			}
		}
	</script>
