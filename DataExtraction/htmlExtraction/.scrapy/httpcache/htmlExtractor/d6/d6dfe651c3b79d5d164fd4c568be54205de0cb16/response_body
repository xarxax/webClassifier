<script>
  $(document).ready(function () {
    window.USER_SELECTED_LOCALE = '';
    window.USER_SELECTED_REGION = '';
    window.USER_SELECTED_LOCALE = window.Settings.isMultiLingual ? window.USER_SELECTED_LOCALE : window.USER_SELECTED_REGION;
    if (!window.USER_SELECTED_LOCALE) {
      var region_modal = $("#js__modal-region_selection");
      var modalBinder = App.Modal.binder;
      modalBinder.toggleAllBg();
      modalBinder.modalTransaction(region_modal);
    }
  });
</script>
