<script type="text/javascript">
    function custom_notifier(message) {
      var $temp = $('<div>' + message + '</div>');
      var $title = $temp.find('a').last().prev();
      var timeout = '1700';
      $.pnotify({
        title: $title.html(),
        delay: parseInt(timeout),
        text: message,
        type: 'success',
      });
    }
    window.custom_notifier = custom_notifier;
</script>

