	<header class="entry-header">
		<h1 class="entry-title">Home</h1>
	</header><!-- .entry-header -->

	<div class="entry-content">
		<!-- MetaSlider -->
<div style="max-width: 650px; margin: 0 auto;" class="ml-slider-3-6-7 metaslider metaslider-flex metaslider-471 ml-slider">
    
