<script type="text/javascript">
  
  $(function() {
      var pgurl = window.location.href.substr(window.location.href.lastIndexOf("/")+1);
      $('.active_list').removeClass('new_active');
      $("#nav ul li a").each(function(){
        if($(this).attr("href").substr($(this).attr("href").lastIndexOf("/")+1) == pgurl || $(this).attr("href").substr($(this).attr("href").lastIndexOf("/")+1) == '') {
          $(this).parent().addClass("new_active");
        }
        if(pgurl == '') {
          $('#home').addClass('new_active');
        }
    })
  });
</script>

