function setCookie(name, value, expiredays, path, domain, secure) 
{
    var ExpireDate = new Date ();
    ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 3600 * 1000));

    document.cookie= name + "=" + escape(value) +
        ((expiredays) ? "; expires=" + ExpireDate.toGMTString() : "") +
        ((path) ? "; path=" + path : "") +
        ((domain) ? "; domain=" + domain : "") +
        ((secure) ? "; secure" : "");
}

function getCookie(name) {
    var dc = document.cookie;
    var prefix = name + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) {
        begin = dc.indexOf(prefix);
        if (begin != 0) return null;
    } else {
        begin += 2;
    }
    var end = document.cookie.indexOf(";", begin);
    if (end == -1) {
        end = dc.length;
    }
    return unescape(dc.substring(begin + prefix.length, end));
}

function deleteCookie(name, path, domain) {
    if (getCookie(name)) {
        document.cookie = name + "=" +
            ((path) ? "; path=" + path : "") +
            ((domain) ? "; domain=" + domain : "") +
            "; expires=Thu, 01-Jan-70 00:00:01 GMT";
    }
}

var cookieLanguageId = new String ( getCookie ( "cookieLanguageId" ) );
if ( cookieLanguageId == "1" ) window.location = "http://qmp-powders.com/en/home.ch2";
else if ( cookieLanguageId == "2" ) window.location = "http://qmp-powders.com/fr/home.ch2";
else window.location = "http://qmp-powders.com/en/home.ch2";

</script>
