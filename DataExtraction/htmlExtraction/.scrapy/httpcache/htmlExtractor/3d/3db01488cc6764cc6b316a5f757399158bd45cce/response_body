  <script>

function searchZip() {
  var zip = document.getElementById('zip').value;
  window.location.href="tex-mex-locator.php?action=viewall&address="+zip;
}
//checks if geolocation is enabled, then ajax call to getClosestLocation.php
//finds closest store, populates map, and swaps MyTW info.
//Shouldn't run unless geolocation is enabled.
function getLocation2() {
		if(navigator.geolocation != undefined) {
			navigator.geolocation.getCurrentPosition(function(position) {            
      $.ajax({
          type: "POST",
          url: "getClosestLocation.php",
          data: {latitude: lat, longitude: lng},
          dataType: 'json',
          success: function(data){
            closestStore = data;
            swapInfo(closestStore[0]);
            initialize();
          },
          error:function() {
            alert("Error Getting Closest Location");
          }
      });   
		    // DO STUFF HERE
		}, function(error) {
		    	switch(error.code) {  
		            case error.PERMISSION_DENIED:
		                break;  
		            case error.POSITION_UNAVAILABLE:  
			            break;  
		            case error.TIMEOUT:
		    	        break;  
		            default: 
		        	    break;  
					}  
			});
		} else {
		  alert('geolocation services are disabled');
		  }	  
}

//Takes a location object. Swaps info into MyTW Bar
function swapInfo(location){
  //console.log(location);
      $(".currentName").html(location.name);
      $(".currentAddress").html(location.address);
      $(".currentTelephone").html("<a href = 'tel:" + stripNonNumbers(location.phone) +"'>" +location.phone+"</a>");
      $(".currentHours").html(location.hours[day]);
      $(".locationDisplay").html(location.name);
      $(".addressDisplay").html(location.address);
      $(".phoneNumberDisplay").html("<a href = 'tel:" + stripNonNumbers(location.phone) +"'>" +location.phone+"</a>");
      $(".timeDisplay").html(location.hours[day]);
      $(".directionsLink").html("<a href ='"+ getAddress() +"' target='_blank'>Get Directions</a>");
}

// Helper function for google maps directions link
function getAddress() {
  address = closestStore[0].address;
  var replaced = address.split(' ').join('+');
  address = 'https://www.google.com/maps/place/' + replaced;
  return address;
}

