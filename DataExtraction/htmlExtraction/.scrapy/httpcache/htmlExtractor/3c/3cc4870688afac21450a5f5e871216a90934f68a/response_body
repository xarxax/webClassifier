var backgroundArray = ['url(/Portals/0/Images/banner-background2.png)', 'url(/Portals/0/Images/banner-background3.png)', 'url(/Portals/0/Images/banner-background4.png)', 'url(/Portals/0/Images/banner-background5.png)', 'url(/Portals/0/Images/banner-background.png)'];
var y = -1;

function changeBackground() {
    ++y;
    if (y < backgroundArray.length) {
        document.getElementById("banner-background").style.background= backgroundArray[y];
    } else {
    y = 0;
        document.getElementById("banner-background").style.background= backgroundArray[y];
    }
}
var intervalID = setInterval(changeBackground, 5000);
</script><!-- End_Module_1733 --></div>
