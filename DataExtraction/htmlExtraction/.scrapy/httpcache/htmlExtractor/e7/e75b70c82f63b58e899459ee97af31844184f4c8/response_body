	$(window).scroll(function () {
    	var top = $(window).scrollTop();
		if (top > 500) {
			$(".backToTop").addClass('wizard');
		}
		else {
			$(".backToTop").removeClass('wizard');
		}
	});
	$(function () {	
		$('.backToTop').click(function () {
			$('html, body').animate({scrollTop: 0}, 1000);
		});
	});  
</script>
