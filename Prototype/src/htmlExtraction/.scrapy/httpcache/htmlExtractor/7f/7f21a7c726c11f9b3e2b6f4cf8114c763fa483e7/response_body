    $(function () {

        $(".sdcms-dropdown-toggle").click(function (e) {
            e.stopPropagation();
            $(".sdcms-dropdown-menu").not($(this).parent().find(".sdcms-dropdown-menu")).hide();

            $(this).parent().find(".sdcms-dropdown-menu").slideToggle("fast");
            return false;
        });

        $("#spnHeaderMidMenuDropTrigger").click(function () {
            $(".divHeaderMidMenuDropSlide").slideToggle("fast");
        });

