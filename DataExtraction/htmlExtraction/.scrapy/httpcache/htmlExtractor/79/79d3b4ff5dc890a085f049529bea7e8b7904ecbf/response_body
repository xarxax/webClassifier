            <script type="text/javascript">
    var logged = null;
    function ReorderLoginSuccess() {
        closeWidgetAndHideOverlay();
        window.location.href = "#orderhistory";
    }

    function AddReorderWidget() {
        $(function () {
            try {
                closeWidgetAndHideOverlay();
                var displayValue = document.getElementById('reorderSignedOut').style.display;
                if (displayValue != "none") {
                    logged = "0";
                }
                else {
                    logged = "1";
                }

                if (logged === '0' || logged == null) {
                    requirejs(['../../ui/widgets/Login/LoginWidget.js'], function (widgetFactory) {
                        try {
                            loginconfigurations = { "divElement": "#reorderWidgetContainer", "source": "reorder" };
                            logineventHandlerSettings = { "onLoginSuccess": ReorderLoginSuccess, "onCloseWidget": HideOverlayOnly };
                            loginwidget = widgetFactory.CreateWidget(loginconfigurations);
                            loginwidget.setupEventHandler(logineventHandlerSettings);
                            loginwidget.render();
                            var overlay = $("#Overlay");
                            overlay.css({ left: 0, top: 0, width: '100%', height: '100%', position: 'fixed' }).show();
                            var reorderChecksLink = $('#reorderChecksWidget');
                            var leftOffsetForReorder = reorderChecksLink.offset().left - 35;
                            var topOffsetForReorder = (reorderChecksLink.offset().top + reorderChecksLink.height()) - loginwidget.getHeight() - 45;
                            $('#reorderWidgetContainer').offset({ top: topOffsetForReorder, left: leftOffsetForReorder });
                            $('#reorderWidgetContainer').show();
                        }
                        catch (exp1) {
                            window.top.location.href = "#signin";
                        }
                    });
                }
                else {
                    window.top.location.href = "#orderhistory";
                }
            }
            catch (exp2) {
                window.top.location.href = "#signin";
            }
        });
    }
</script>
