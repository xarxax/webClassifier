$(document).ready(function() {
  var compasslider = {
            items: 1,
			dots: false,
			autoplay: true,
            autoplayTimeout:12000,
            loop:true,

			// responsive: true
			responsive: {
				0: {
					items: 1,
					mouseDrag: true,
					//autoplay: true
				},
				480: {

					items: 1,
					mouseDrag: true,
					//autoplay: true

				},
				640: {

					items: 1,
					mouseDrag: true,
					//autoplay: true

				},

				768: {

					items: 1,
					mouseDrag: true,
					//autoplay: true

				}
			}


		};

		var owlcs = $('#compasSlider');
		owlcs.owlCarousel(compasslider);
		$(".compassslider .ins-next").on('click', function () {
			owlcs.trigger('next.owl.carousel');
		});
		$(".compassslider .ins-prev").on('click', function () {
			owlcs.trigger('prev.owl.carousel');
		});
        });
</script>
 

