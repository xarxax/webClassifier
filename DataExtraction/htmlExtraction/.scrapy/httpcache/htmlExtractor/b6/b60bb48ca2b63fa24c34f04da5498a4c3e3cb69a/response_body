<script>
	$(".resources > a").click(function(evt){
		$(".resources .sub-nav").slideToggle(500);
	});
	
	$(".content-list li a").click(function(evt){
		if($(evt.currentTarget).parent().hasClass("curr")) return false;
		
		$(".content-list li").removeClass("curr");
		$(evt.currentTarget).parent().addClass("curr")
		
		$(".content-text").hide();
		$("."+$(evt.currentTarget).prop("id")).show();
	});
</script>

