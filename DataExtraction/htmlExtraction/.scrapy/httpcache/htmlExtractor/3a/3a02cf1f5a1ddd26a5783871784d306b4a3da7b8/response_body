// the following image swap functions need to be passed three parameters
// 1)the image name
// 2)whether or not the respective secondary picture shall be displayed represented as '1' (true) or '0' (false)
// note: code was written with only one 'additional menu' requirement so multiple 'additional menus' are not
// accessible through the functions
// example use: turnImageOn("imageOne", "0", "1");

function turnImageOn(imgName, icon) {
  document[imgName].src = eval(imgName + "On.src");
  if (icon == "1") {
    document["icon"].src = eval(imgName + "Pic.src");
  } else {
    document["icon"].src = "./images/spacer.gif";
  }
 
}

function turnImageOff(imgName, icon) {
  document[imgName].src = eval(imgName + "Off.src");
  if (icon == "1") {
    document["icon"].src = "./images/spacer.gif";
  } else {
    // do nothing for now
  }

}



// -->
