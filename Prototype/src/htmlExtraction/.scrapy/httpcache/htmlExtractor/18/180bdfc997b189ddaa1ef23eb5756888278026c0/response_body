jQuery(function($){	
  
	var customer_form_filled = false;
	var cust_form_fields_selector = '#customer-info-form input[type="text"], #customer-info-form input[type="email"], #customer-info-form textarea';
  
  // Trigger this if to check if the user has started filling up the form
	$(document).on('keyup', cust_form_fields_selector, function(e){
    
		// Check if the current form field has value
		if( $(this).val() != '' ){

			// Check if others already have value
			$(cust_form_fields_selector).not($(this)).each(function( index, object ){

				// check if there already is one that has been filled
				if( $(object).val() != '' ){
					customer_form_filled = true;
					return false;
				}

			});

			if( !customer_form_filled ){
				customer_form_filled = true;
				//console.log( 'customer filled form' );
        ga('send', 'pageview', '/virtual/reservation/form-filled-up');
			}
		}

	});
  
  $('#reservation-form').submit(function(e){
    customer_form_filled = false;
    //console.log( 'customer checking availability' );
    ga('send', 'pageview', '/virtual/reservation/check-availability');
  });
  
  // Submit customer form
  /*$(document).on('submit','#customer-info-form', function(e){
		//console.log('customer submitted form');
    ga('send', 'pageview', '/virtual/reservation/submit-reservation');
	});*/
  
});

/* FEatured promotions alignment*/
function promotionAdjustment() {
  var max = 0;
 	jQuery(".featured-promotion-container .home-features div").css({ height: "" });
	jQuery(".featured-promotion-container .home-features div").each(function(index, el) {
		if( jQuery(this).height() > max ){
			max = jQuery(this).height();
		}
	});
	
	jQuery(".featured-promotion-container .home-features div").css('height', max + 80);
}

jQuery(document).ready(promotionAdjustment);
jQuery(window).resize(promotionAdjustment);
</script>
