<script type="text/javascript">
       
    function getProductList(value) {
        var countprd = document.getElementById('hdnPrdCounter').value;
        var cookie = document.getElementById('hdnShoppingInfo').value;			
        var count = document.getElementById('hdnField').value;     
        
        if (value.name == 'full') {
            if (count == "" && cookie == 0) {

                window.open("/product-list", "_self");
            }
        }
        else if (value.name == 'mobile') {
            if (count == "" && cookie == 0) {

                window.open("/product-list", "_self");
            }
            else if (countprd != 0) {
                window.open("/product-list?q=1", "_blank");
				
            }
        }

    }


</script>

