<script type="text/javascript">


function updateWhitebox(imageName) {
  if (!document.getElementById) return
  var whiteBox = document.getElementById('whiteboximg');
  if (!whiteBox) return;

  var agt = navigator.userAgent.toLowerCase();
  var isIe = ((agt.indexOf("msie") != -1) && (agt.indexOf("opera") == -1));
  var doFilters = isIe;

  if (doFilters) {
	if (whiteBox.filters.length == 0)  
	whiteBox.style.filter += " progId:DXImageTransform.Microsoft.Fade(duration=1)";
	//whiteBox.style.filter += " progId:DXImageTransform.Microsoft.Iris(irisstyle='CIRCLE', duration=1)";
	//whiteBox.style.filter += " progId:DXImageTransform.Microsoft.RandomDissolve(duration=1)";
	//whiteBox.style.filter += " progId:DXImageTransform.Microsoft.GradientWipe(duration=1)";

	whiteBox.filters.item(0).Apply();

	whiteBox.src = imageName;

	whiteBox.filters.item(0).Play(1);
}
else {
	whiteBox.src = imageName;
}

}

/****************************************************
*	        DOM Image rollover:
*		by Chris Poole
*		http://chrispoole.com
*               Script featured on http://www.dynamicdrive.com
*		Keep this notice intact to use it :-)
****************************************************/

function init() {
  if (!document.getElementById) return
  var imgOriginSrc;
  var imgTemp = new Array();
  var imgarr = document.getElementsByTagName('img');
  for (var i = 0; i < imgarr.length; i++) {
    if (imgarr[i].getAttribute('hsrc')) {
        imgTemp[i] = new Image();
        imgTemp[i].src = imgarr[i].getAttribute('hsrc');
        imgarr[i].onmouseover = function() {
            imgOriginSrc = this.getAttribute('src');
            this.setAttribute('src',this.getAttribute('hsrc'))
        }
        imgarr[i].onmouseout = function() {
            this.setAttribute('src',imgOriginSrc)
        }
    }
  }
}
onload=init;

</script>

</head>

