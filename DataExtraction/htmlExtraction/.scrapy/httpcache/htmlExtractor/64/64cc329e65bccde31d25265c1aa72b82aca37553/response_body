		<script type="text/javascript">
			function replaceInputText(obj) {
				var uidText = "User ID";
				var passwordText = "Password";
			
				if (obj.name == "faux_username") {
					if (obj.value == "User ID") {
						obj.className = "input1On";
						obj.value = "";
					}
					obj.onblur = function() {
						if (obj.value == "") {
							obj.className = "input1";
							obj.value = "User ID";
						}
					}
					return;
				}
			
				else if (obj.name == "password" && obj.getAttribute('type') == 'text') {
					var thisParent = obj.parentNode;
					var newO = document.createElement('input');
					newO.setAttribute('type', 'password');
					newO.setAttribute('onblur', 'checkLogin(this)');
					newO.setAttribute('name', obj.getAttribute('name'));
					newO.className = "input2On";
					newO.id = "password";
					obj.parentNode.removeChild(obj);
					thisParent.appendChild(newO);
				}
			
				var startFocus = function() {
					newO.focus();
				}
				setTimeout(startFocus, 10);
			}
			
			function checkLogin(obj) {
				if (obj.name == "password" && obj.value == "") {
					var thisParent = obj.parentNode;
			
					var newO = document.createElement('input');
					newO.setAttribute('type', 'text');
					newO.setAttribute('onfocus', 'replaceInputText(this)');
					newO.setAttribute('name', obj.getAttribute('name'));
					newO.className = "input2";
					newO.id = "password";
					newO.value = "Password";
					obj.parentNode.removeChild(obj);
					thisParent.appendChild(newO);
				}
			}
		</script>
	</body>

    

