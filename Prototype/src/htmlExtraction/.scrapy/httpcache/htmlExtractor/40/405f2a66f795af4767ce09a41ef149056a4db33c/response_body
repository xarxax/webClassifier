        function pageLoad() {
           
            var insedtBox = $(".QOIedt");

            if (null != insedtBox[0]) {

                $(insedtBox[0]).bind('blur', function () {
                    var inspstBtn = $(".QOIbtn");

                    eval($(inspstBtn[0]).attr('href'));

                });

            }

