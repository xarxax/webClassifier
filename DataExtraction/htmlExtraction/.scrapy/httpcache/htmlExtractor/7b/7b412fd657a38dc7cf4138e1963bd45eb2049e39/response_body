                jQuery(this).find(".sub_child_cat").css('width','195px');
            }
            if($catNum == 2){
                jQuery(this).find(".sub_child_cat").css('width','400px');
            }
            if($catNum == 3){
                jQuery(this).find(".sub_child_cat").css('width','585px');
            }
            if($catNum > 3){
                jQuery(this).find(".sub_child_cat").css('width','780px');
            }
        },function(){
            jQuery(this).find(".box-child-cat").hide();
            jQuery(this).removeClass("over");
        });
    });
</script>
