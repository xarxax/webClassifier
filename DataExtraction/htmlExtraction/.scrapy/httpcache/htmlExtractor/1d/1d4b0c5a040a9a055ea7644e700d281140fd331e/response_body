<script type="text/javascript">
	jQuery(document).ready(function($) {
		$(".acordioncontent2").hide(0);
		$(".acordioncontent2:first").removeClass("active");
		$(".accordion2 > li > a").on("click", function(e) {
			e.preventDefault();
			$(this).parent().siblings().children("a").removeClass("active");
			$(this).toggleClass("active");
			$(this).parent().siblings().children(".acordioncontent2").slideUp();
			$(this).siblings(".acordioncontent2").slideToggle();
		});
	});

</script>
