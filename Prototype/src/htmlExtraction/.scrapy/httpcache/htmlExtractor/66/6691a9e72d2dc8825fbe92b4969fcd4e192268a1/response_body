<script>
    $(document).ready(function () {
        $(document).on('click touchstart','body:not(.hide-ada-switch) .close-ada', function() {
            $('body').addClass('hide-ada-switch');
            var myStorage = window.localStorage;
            myStorage.adaswitch = 'hide-ada-switch';
            //document.cookie = 'ada-switch=hide-ada-switch;expires=0;path=/'
        });
        $(document).on('click touchstart', '.hide-ada-switch .close-ada', function () {
            $('body').removeClass('hide-ada-switch');
            var myStorage = window.localStorage;
            myStorage.adaswitch = 'show-ada-switch';
            //document.cookie = 'ada-switch=show-ada-switch;expires=0;path=/'
        });
        function getCookie(key) {
            var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
            var myStorage = window.localStorage;
            return myStorage.adaswitch;
            //return keyValue ? keyValue[2] : null;
        }
        var adaSwitchCookieValue = getCookie('ada-switch');
        if (adaSwitchCookieValue === 'hide-ada-switch') {
            $('body').addClass('hide-ada-switch');
        }
    });
</script></body>
