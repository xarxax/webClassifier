            <script language="javascript" type="text/javascript">
                var _sSearchText = 'Search Keyword(s)';
                var _sUsernameText = 'Email Address';
                var _sPasswordText = 'password';


                document.getElementById('tbxSearch').value = _sSearchText;


                function CheckFocus(tbxSearch) {
                    if (tbxSearch.value == _sSearchText) tbxSearch.value = '';
                    tbxSearch.select();
                }
                function CheckBlur(tbxSearch) {
                    if (tbxSearch.value == '') tbxSearch.value = _sSearchText;
                }

                function UsernameFocus(tbxUsername) {
                    if (tbxUsername.value == _sUsernameText) tbxUsername.value = '';
                    tbxUsername.select();
                }
                function UsernameBlur(tbxUsername) {
                    if (tbxUsername.value == '') tbxUsername.value = _sUsernameText;
                }

                function PasswordFocus(tbxPassword) {
                    if (tbxPassword.value == _sPasswordText) tbxPassword.value = '';
                    tbxPassword.select();
                }
                function PasswordBlur(tbxPassword) {
                    if (tbxPassword.value == '') tbxPassword.value = _sPasswordText;
                }

                function DoSearch() {
                    var sKeywords = document.getElementById('tbxSearch').value;
                    if (sKeywords == _sSearchText || sKeywords == '') {
                        alert('Please enter the Keyword(s) you wish to search for.');
                        document.getElementById('tbxSearch').focus();
                    } else {
                        location.href = "/search.aspx?keywords=" + sKeywords;
                    }
                }
            </script>  

