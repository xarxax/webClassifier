	.qmmc .qmtitle{display:block;cursor:default;white-space:nowrap;position:relative;z-index:1;}
	.qmclear {font-size:1px;height:0px;width:0px;clear:left;line-height:0px;display:block;float:none !important;}
	.qmmc {position:relative;zoom:1;z-index:10;}
	.qmmc a, .qmmc li {float:left;display:block;white-space:wrap;position:relative;z-index:1;}
	.qmmc div a, .qmmc ul a, .qmmc ul li {float:none;}
	.qmsh div a {float:left;}
	.qmmc div{visibility:hidden;position:absolute;}
	.qmmc .qmcbox{cursor:default;display:block;position:relative;z-index:1;}
	.qmmc .qmcbox a{display:inline;}
	.qmmc .qmcbox div{float:none;position:static;visibility:inherit;left:auto;}
	.qmmc li {z-index:auto;}
	.qmmc ul {left:-10000px;position:absolute;z-index:10;}
	.qmmc, .qmmc ul {list-style:none;padding:0px;margin:0px;}
	.qmmc li a {float:none}
	.qmmc li:hover>ul{left:auto;}
	#qm0 ul {top:100%;}
	#qm0 ul li:hover>ul{top:0px;left:100%;}
	/*!!!!!!!!!!! QuickMenu Styles [Please Modify!] !!!!!!!!!!!*/ /* QuickMenu 0 */ /*"""""""" (MAIN) Items""""""""*/ #qm0 a { padding:0px 0px 0px 0px; color:#FFFFFF; font-family:Arial; font-size:11px; text-decoration:none; }
	/*"""""""" (SUB) Container""""""""*/ #qm0 div, #qm0 ul { padding:0px; margin:0px 0px 0px; background-color:transparent;  border-width:0px; border-style:solid; border-color:#DADADA; width:184px;}
	/*"""""""" (SUB) Items""""""""*/ #qm0 div a, #qm0 ul a { padding:10px 10px 10px 20px; background-color:transparent; font-size:11px; color:#FFFFFF; border-width:0px; border-style:none; }
	/*"""""""" (SUB) Hover State""""""""*/ #qm0 div a:hover { background-color:#000094; color:#FFFFFF; }
	/*"""""""" (SUB) Hover State - (duplicated for pure CSS)""""""""*/ #qm0 ul li:hover>a { background-color:#000094; color:#FFFFFF; }
	/*"""""""" (SUB) Active State""""""""*/ body #qm0 div .qmactive, body #qm0 div .qmactive:hover { background-color:#000094; color:#FFFFFF; }
	/*"""""""" Individual Titles""""""""*/ #qm0 .qmtitle { cursor:default; padding:0px; color:#444444; font-family:arial; font-size:11px; font-weight:bold; }
