$(".mobile_nav_menu .mobile-navigation ul.nav-menu li.menu-item-has-children" ).prepend( "<span class='p_arrow'><i class='fa fa-angle-right'></i></span>" );
$(".mobile_nav_menu .mobile-navigation ul.nav-menu li.menu-item-has-children ul.sub-menu li.menu-item-has-children span" ).addClass('sub_arrow');
$(".mobile_nav_menu .mobile-navigation ul.nav-menu  li ul.sub-menu li.menu-item-has-children span" ).removeClass('p_arrow');

        $('.mobile_menu').click(function(){
            if(jQuery(this).hasClass("active")){
                jQuery(this).removeClass("active");
                jQuery(".mobile_nav_menu").slideUp();
            }else{
                jQuery(this).addClass("active");
                jQuery(".mobile_nav_menu").slideDown();
            }
        })

        $('.mobile_nav_menu .mobile-navigation ul.nav-menu li span.p_arrow').click(function (e) {
            if($(this).hasClass('active')==true){
                $(this).next().next().slideUp(300);
                $(this).removeClass("active");

            }else{
                $('.mobile_nav_menu .mobile-navigation ul.nav-menu li.menu-item-has-children').find('ul.sub-menu').slideUp(300);
                $('.mobile_nav_menu .mobile-navigation ul.nav-menu li.menu-item-has-children span').removeClass("active");

                $(this).next().next().slideDown(300);
                $(this).addClass("active");
            }
            return false;
        });

        $('.mobile_nav_menu .mobile-navigation ul.nav-menu span.sub_arrow').click(function (e) {
            $(this).next().next().slideToggle(300);
            $(this).parent('li').toggleClass('changebg');
            $(this).toggleClass('active');
            return false;
        });

         $('.mobile_nav_menu .other_menu ul li.have_sub span.p_arrow').click(function (e) {
            $(this).next().next().slideToggle(300);
            $(this).parent('li').toggleClass('changebg');
            $(this).toggleClass('active');
            return false;
        });
    })
    jQuery(window).resize(function(){

        if(jQuery(window).width()>767){
             jQuery(".mobile_menu").removeClass("active");
                jQuery(".mobile_nav_menu").slideUp();

        }

    })
    </script>

