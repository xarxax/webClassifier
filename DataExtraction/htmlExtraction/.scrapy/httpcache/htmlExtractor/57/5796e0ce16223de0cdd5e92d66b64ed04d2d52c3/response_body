<script type="text/javascript">
	$(document).ready(function(){
	
		$("div.collapse").hide();
		$("div.show").show();
		$("h3.collapse-button").click(function(){
	
			if ($(this).next("div.collapse").is(":hidden"))
				{
              		$(this).next("div.collapse").slideToggle(300);
              		$(this).addClass("active");
				} else {
              		$(this).next("div.collapse").slideToggle(100);
              		$(this).removeClass("active");
			}  
	
		});
		
		$( "a.togglemenu" ).click(function() {
			$( ".nav ul" ).slideToggle( "fast" );
		});
		
		$('#zipform').submit(function(e) {
			
			var zip = $("#zip").val();
			location.href = "http://nyrei.foreclosure.com/search.html?st=NY&cno=047&z=" + zip
			e.preventDefault();
    		return false;
		});

		$("#submitme").click(function(){
			$('#form').submit(); // this triggers the submit event
		});
			
	});
</script>
