		alert('you clicked!');
	
	});
	
*/
	
	$( ".dgx-white" ).click(function() {
	
		$(".main").moveTo(1);
	
	});
	
	$( ".call-us" ).click(function() {
	
		$(".main").moveTo(9);
	
	});
	
	
	</script>
	
	
	
	<script>
		$('.owl-carousel').owlCarousel({
		    loop:true,
		    margin:0,
		    nav:true,
		    dots:false,
		    lazyLoad: true,
		    autoHeight:true,
		    autoplay:false,
    		autoplayHoverPause:true,
		    responsive:{
		        0:{
