 var m = ref.match(urls[1]);
 if ((m!==null) || (n!==null)) {
 stop;
 }
 else if (ref=='') {
 var r = confirm("Small Display Detected.\nClick \"OK\" for MOBILE NAVIGATION");
 if (r==true) {
  window.location = "http://Big3D.com/sitemap.html";
  }
