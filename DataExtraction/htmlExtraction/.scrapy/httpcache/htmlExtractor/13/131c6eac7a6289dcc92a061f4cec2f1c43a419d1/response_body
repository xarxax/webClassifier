<script>
	jQuery(document).on("scroll",function(){
	    if(jQuery(document).scrollTop()>100){
	        jQuery("#rt-header-surround").addClass("menu-small");
	    } else{
	        jQuery("#rt-header-surround").removeClass("menu-small");
	    }
	});
