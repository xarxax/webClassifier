<script language="javascript">

var quote = new Array()
quote[0] = 'images/hm_btm_img3a.jpg'
quote[1] = 'images/hm_btm_img3b.jpg'
quote[2] = 'images/hm_btm_img3c.jpg'
quote[3] = 'images/hm_btm_img3d.jpg'

var j = 0
var p = quote.length;
var preBuffer = new Array()
for (i = 0; i < p; i++){
   preBuffer[i] = new Image()
   preBuffer[i].src = quote[i]
}
var whichImage = Math.round(Math.random()*(p-1));
function showImage(){document.write('<img src="'+quote[whichImage]+'"class="hm_team">');}
</script>





		
		
