	<script>
		var fiWidth = 350;
		$('FlyIn').show();
		$$('#FlyIn .fiButton')[0].observe('click', function ()
		{
			$$('#FlyIn .fiContent')[0].toggleClassName('open');
		});
		Event.observe(window, 'load', function()
		{
			fiSize();
		});
		Event.observe(window, 'resize', function()
		{
			fiSize();
		});
		function fiSize()
		{
			if (document.viewport.getWidth() - $('main').getWidth() >= fiWidth * 2)
			{
				$$('#FlyIn .fiContent')[0].addClassName('open');
			}
			else
			{
				$$('#FlyIn .fiContent')[0].removeClassName('open');
			}
		}
	</script>
