    ,onValidationComplete: function(form, status){ 
    
        
    var stat = 0;  
    var SITEURL = jQuery("#siteurl").val();
    var DIRURL = jQuery("#template_directory_uri").val();
    
    if(status==true){
    jQuery.ajax({
    method: "POST",
    async: false,
    url: SITEURL+"/captchacheck.php",
    data: jQuery('#formRegistration').serialize()
    })
    .done(function (response) {  //  alert("response : " + response);
            var responseback = response;
            if (responseback == 0) {            
             stat =0 ;
            } else {
                stat=1;  
            }
    });  
    if (stat == 0) {
    jQuery('.cptValMsg').show();
    
    return false;
    } else {
    return true;
    }
    }}
    });    
