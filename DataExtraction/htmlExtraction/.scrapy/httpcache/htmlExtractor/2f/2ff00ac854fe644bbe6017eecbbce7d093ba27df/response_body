<script type="text/javascript">
  var revapi104;
  $(document).ready(function() {
    if($("#rev_slider_homepage_1").revolution == undefined) {
      revslider_showDoubleJqueryError("#rev_slider_homepage_1");
    }
    else {
      var win_h = $(window).height();
      var win_w = $(window).width();

      revapi104 = $("#rev_slider_homepage_1").show().revolution({
        sliderType:"standard",
        sliderLayout:((win_h > win_w) ? "fullwidth": "auto"),
        gridheight: (win_h-($("#rev_slider_homepage_1_wrapper").offset().top || 0)),
        jsFileLocation:"/revolution/js/",
        delay:9000,
        dottedOverlay:"none",
        lazyType:"none",
        navigation: {
          keyboardNavigation:"off",
          keyboard_direction: "horizontal",
          mouseScrollNavigation:"off",
          onHoverStop:"off",
          touch:{
            touchenabled:"on",
            swipe_threshold: 75,
            swipe_min_touches: 1,
            swipe_direction: "horizontal",
            drag_block_vertical: false
          },
          bullets: {
            enable:true,
            hide_onmobile:true,
            hide_under:770,
            style:"zeus",
            hide_onleave:true,
            direction:"horizontal",
            h_align:"right",
            v_align:"top",
            h_offset:60,
            v_offset:80,
            space:5,
            tmp:'<span class="tp-bullet-image"></span><span class="tp-bullet-imageoverlay"></span><span class="tp-bullet-title">{{title}}</span>'
          }
        },
        parallax: {
          type:"mouse",
          origo:"slidercenter",
          speed:2000,
          levels:[2,3,4,5,6,7,12,16,10,50],
          disable_onmobile:"on"
        },
        shadow:0,
        spinner:"off",
        stopLoop:"off",
        stopAfterLoops:-1,
        stopAtSlide:-1,
        shuffle:"off",
        autoHeight:"off",
        fullScreenAlignForce:"off",
        fullScreenOffsetContainer: "",
        fullScreenOffset: "60px",
        disableProgressBar:"on",
        hideThumbsOnMobile:"on",
        hideSliderAtLimit:0,
        hideCaptionAtLimit:0,
        hideAllCaptionAtLimit:0,
        debugMode:false,
        fallbacks: {
          simplifyAll:"off",
          nextSlideOnWindowFocus:"off",
          disableFocusListener:false,
        }
      });
    var newCall = new Object(),
      cslide;

    newCall.callback = function() {
      var proc = revapi104.revgetparallaxproc(),
        fade = 1+proc,
        scale = 1+(Math.abs(proc)/10);

        punchgs.TweenLite.set(revapi104.find('.slotholder, .rs-background-video-layer'),{opacity:fade,scale:scale});
    }
    newCall.inmodule = "parallax";
    newCall.atposition = "start";

    revapi104.bind("revolution.slide.onloaded",function (e) {
      revapi104.revaddcallback(newCall);
    });

    $(window).on('scroll', function(e) {
      var show = !!(($(this).width() >= 770) && ($(this).scrollTop() < 5));
      $("#slide-learnmore-layer").toggle(show);
    });

    $(window).resize(function(e) {
      var div = $("#rev_slider_homepage_1_wrapper");
      if (div.length > 0) {
        var h = $(window).height() - (div.offset().top || 0);
        var w = div.width();
        if(h > w) { h = w/1920*1080; }
        if (h > 100) {
          div.find(".rev_slider").css({"height":h+"px", "max-height":h+"px", "min-height":h+"px"});
          $(".slider").css({"height":h+"px", "max-height":h+"px", "min-height":h+"px"});
        }
        $(".ScrollDownContainer").toggle(!!(($(this).width() >= 770) && ($(this).scrollTop() < 5)));
      }

      var $mask = $("#mask");
      if ($mask.length > 0) {
        w = $(window).width();
        h = $(window).height();
        $mask.css({"width":w+"px", "max-width":w+"px", "min-width":w+"px","height":h+"px", "max-height":h+"px", "min-height":h+"px"});
      }

    }).trigger('resize');
  }

  var $mask = $("#mask").click(function () {
    $(this).hide();
    $('div.dialog').hide();
  });

  function showDialog(id) {
    var w = $(window).width();
    var h = $(window).height();

    var $dlg = $(id);
    if ($dlg.length > 0) {
      if ($mask.length > 0) {
        $mask.css({"width":w+"px", "max-width":w+"px", "min-width":w+"px","height":h+"px", "max-height":h+"px", "min-height":h+"px"});
        $mask.fadeIn(500);
        $mask.fadeTo("slow",0.9);      
      }

      var dw=560;
      var dh=315;
      if (w < (dw+30)) { dw = w-30; dh = Math.floor(315/560*dw); }
      if (h < (dh+30)) { dh = h-30; dw = Math.floor(560/315*dh); }

      $dlg.find('iframe').css({"width":dw+"px", "max-width":dw+"px", "min-width":dw+"px","height":dh+"px", "max-height":dh+"px", "min-height":dh+"px"});
      dw+=30; dh+=30;
      var mt = -Math.floor(dh / 2);
      var ml = -Math.floor(dw / 2);
      $dlg.css({"width":dw+"px", "max-width":dw+"px", "min-width":dw+"px","height":dh+"px", "max-height":dh+"px", "min-height":dh+"px", "margin-top":mt+"px", "margin-left":ml+"px"});
      $dlg.show();
    }
  }

  $("a.TechnicalVideo").click(function() {
    if ($("#TechnicalVideoPopup").is(':empty')) { $("#TechnicalVideoPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/H_lJri-EQ1Q?rel=0" frameborder="0" allowfullscreen></iframe>'); }
    showDialog("#TechnicalVideoPopup");
  });

  $("a.GraphicsVideo").click(function() {
    if ($("#GraphicsVideoPopup").is(':empty')) { $("#GraphicsVideoPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/BNSox5LHWws?rel=0" frameborder="0" allowfullscreen></iframe>'); }
    showDialog("#GraphicsVideoPopup");
  });
  $("a.ImageProVideo").click(function() {
    if ($("#ImageProGraphicsVideoPopup").is(':empty')) { $("#ImageProGraphicsVideoPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/X8ZJBoYeImA?rel=0" frameborder="0" allowfullscreen></iframe>'); }
    showDialog("#ImageProGraphicsVideoPopup");
  });

  $("a.Video700").click(function() {
    if ($("#Video700Popup").is(':empty')) { $("#Video700Popup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/pRr01-m2I-c?rel=0" frameborder="0" allowfullscreen></iframe>'); }
    showDialog("#Video700Popup");
  });
  $("a.Video900").click(function() {
    if ($("#Video900Popup").is(':empty')) { $("#Video900Popup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/-KjzjLL3xl8?rel=0" frameborder="0" allowfullscreen></iframe>'); }
    showDialog("#Video900Popup");
  });
  $("a.worldKip").click(function() {
    if ($("#worldKipPopup").is(':empty')) { $("#worldKipPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/UtU4C0PsZZc?rel=0" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>'); }
    showDialog("#worldKipPopup");
  });  
/*
  window.setTimeout(function() {
    $("#TechnicalVideoPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/H_lJri-EQ1Q?rel=0" frameborder="0" allowfullscreen></iframe>');
    $("#GraphicsVideoPopup").html('<iframe width="560" height="315" src="https://www.youtube.com/embed/BNSox5LHWws?rel=0" frameborder="0" allowfullscreen></iframe>');
