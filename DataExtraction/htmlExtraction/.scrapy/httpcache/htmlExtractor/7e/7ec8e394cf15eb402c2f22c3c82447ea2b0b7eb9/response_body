<script type="text/javascript">

$("#select-region").on("change",function(){
	var val=$(this).val();
	
	if(val=="all"){
		$(".gbta-events-container .gbta-grouping").fadeIn();
	}else{
	
	$(".gbta-events-container .gbta-grouping").each(function() {
        if($(this).data("grouping") == val) {
			$(this).stop().fadeIn().siblings(".gbta-grouping").hide();
		}
		
    });
}
})
</script>
 
<div style="display:none;" >
    
