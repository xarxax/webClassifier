  });
});
$(window).load(function() {
    function setVideo() {
        var mv =  $('.video #mv').height();
  $('.video').css({'height':mv-2})
    }
    setVideo();
    $(window).resize(function() {
        setVideo();
    });
});

$(window).load(function() {
  $('.skip').click(function() {
    $('.slidetop .video').css({'display':'none'})  ;
    $('.slidetop .image').css({'display':'block'})  ;
    $('.slidetop #skip').css({'display':'none'})  ;
  })
});
</script>
