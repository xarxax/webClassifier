    <script type="text/javascript">
        var $k = jQuery.noConflict();
        $k(document).ready(function(){

            $k("ul.subnav").parent().append("<span></span>"); //Only shows drop down trigger when js is enabled - Adds empty span tag after ul.subnav

            $k("ul.topnav > li").hover(function() { //When trigger is hovered...

                //Stop previous animations
                $k(this).find("ul.subnav").stop(true, true);

                //Following events are applied to the subnav itself (moving subnav up and down)
                $k(this).find("ul.subnav").slideDown('fast').show(); //Drop down the subnav on click

                $k(this).hover(function() {
                }, function(){
                    $k(this).find("ul.subnav").slideUp('slow'); //When the mouse hovers out of the subnav, move it back up
                });

                //Following events are applied to the trigger (Hover events for the trigger)
            }).hover(function() {
                $k(this).addClass("subhover"); //On hover over, add class "subhover"
            }, function(){	//On Hover Out
                $k(this).removeClass("subhover"); //On hover out, remove class "subhover"
            });

        });
    </script>
