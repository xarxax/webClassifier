function Display_Cases_and_Walkin__Coolers_rotate(){



 if (Display_Cases_and_Walkin__Coolers_halt!=1){



  if (Display_Cases_and_Walkin__Coolers_rand==0) {Display_Cases_and_Walkin__Coolers_sctr++;}



   else {Display_Cases_and_Walkin__Coolers_sctr=Math.floor(Math.random()*Display_Cases_and_Walkin__Coolers_imganz);}



  if (Display_Cases_and_Walkin__Coolers_sctr>Display_Cases_and_Walkin__Coolers_imganz-1) Display_Cases_and_Walkin__Coolers_sctr=0;



  if (document.images) {



   document.Display_Cases_and_Walkin__Coolers.src=Display_Cases_and_Walkin__Coolers_isn[Display_Cases_and_Walkin__Coolers_sctr].src;



  } else {



   document.Display_Cases_and_Walkin__Coolers.src=Display_Cases_and_Walkin__Coolers_isn[Display_Cases_and_Walkin__Coolers_sctr];



  }



  setTimeout("Display_Cases_and_Walkin__Coolers_rotate()",Display_Cases_and_Walkin__Coolers_timeout);



 }



}



function Display_Cases_and_Walkin__Coolers_link(){



 Display_Cases_and_Walkin__Coolers_halt=1;



