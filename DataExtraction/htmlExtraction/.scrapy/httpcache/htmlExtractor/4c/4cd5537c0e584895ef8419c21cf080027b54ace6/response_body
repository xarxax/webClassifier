$(document).ready(function(){

	$('#tp_header_menu').each(function(){

		$('#tp_header_menu_item_news,#tp_fixed_header_news').unbind('click').bind( 'click', function(){

			console.log($(this).attr('href'));

			$('body, html').stop().animate( { scrollTop : ( $('#tp_top_recentnews').offset().top - ( $('#tp_fixed_header').height() + 40 ) ) + 'px' }, 1000, 'easeOutExpo' );

