                <script type="text/javascript" src="https://www.acmemarkets.com/wp-content/themes/acme/js/jquery.cycle.all.min.js"></script>
                <script type="text/javascript">
    jQuery(document).ready(function ($) {

        // =======================================
        // SLIDESHOW
        // =======================================

    
        var index = 0, hash = window.location.hash, homeslide = '';

        //if no hash, check homeslide queryvar
        if (hash == null || hash == '' || hash == ' ' && homeslide != null && homeslide != '' && homeslide != ' ') {
            hash = homeslide;
        }

        var slideTimeOut = 8000;
        if (hash) {
            index = /\d+/.exec(hash)[0];
            index = (parseInt(index) || 1) - 1; // slides are zero-based
            slideTimeOut = 0;
        }

        $('#slide').after('<div id="slide-nav">').cycle({
            fx: 'fade',
            timeout: slideTimeOut,
            pager: '#slide-nav',
            pause: true,
            startingSlide: index,
            after: function (curr, next, opts) {
                window.location.hash = opts.currSlide + 1;
            }
        });

    });
                </script>
