        $("#ctl00_Login1_errmsgouter").hide();
        $(document).ready(function () {
            $("#ctl00_Login1_errmsgouter").hide();
        });
        Sys.Application.add_init(appl_init);

        function appl_init() {
            var pgRegMgr = Sys.WebForms.PageRequestManager.getInstance();
            //   pgRegMgr.add_beginRequest(BeginHandler);
            pgRegMgr.add_endRequest(EndHandler);
        }

        //  function BeginHandler() {
        //     jScript1();
        //  }

        function EndHandler() {
            jScript();
        }

        function jScript() {

            $("#ctl00_Login1_UserName").focus(function () {
                $("#ctl00_Login1_errmsgouter").hide(500);
            });

            $("#ctl00_Login1_Password").focus(function () {
                $("#ctl00_Login1_errmsgouter").hide(500);
            });

            $("#ctl00_Login1_ibtnlogin").click(function () {
                $("#ctl00_Login1_errmsgouter").hide(500);
                var username = $("#ctl00_Login1_UserName").val().trim().toLowerCase();
                var password = $("#ctl00_Login1_Password").val()
                if (username.length == 0 || username == "") {
                    CommonOnPage('Please enter your username');
                    $("#ctl00_Login1_forgotPassword").css('display', 'none');
                    return false;
                }
                else if (password.length == 0 || password == "") {
                    CommonOnPage('Please enter your password');
                    return false;
                }
                else {
                    $("#ctl00_Login1_errmsgouter").hide(500);
                }
            });
            function CommonOnPage(msg) {
                $("#ctl00_Login1_errmsgouter").show();
                $("#ctl00_Login1_errmsg").text(msg);
                $("#ctl00_Login1_errmsg").hide().slideDown('medium');
            }

        }

        //  function jScript1() {




        $("#ctl00_Login1_UserName").focus(function () {
            $("#ctl00_Login1_errmsgouter").hide(500);
        });

        $("#ctl00_Login1_Password").focus(function () {
            $("#ctl00_Login1_errmsgouter").hide(500);
        });

