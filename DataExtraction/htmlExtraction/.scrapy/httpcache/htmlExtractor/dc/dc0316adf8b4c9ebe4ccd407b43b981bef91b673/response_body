        <script type="text/javascript">

$('#search_btn').click( function (){
	$('#search_form').submit();
	return false;
});

$('#search_fld').focus(function (){
	
	if ($(this).val()=='Search')
	{
		$(this).val('');
	}
}).blur(
	function(){
		if ($(this).val()=='')
		{
			$(this).val('Search');
		}
	
});

</script>

