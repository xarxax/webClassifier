	$(document).ready(function(){
		$("#submit_quote").click(function(event){
			var failed;
			event.preventDefault();
			if($("input:checked").length != 6){failed = true;}
		    if (($("input[name='q1']:checked").val() == 'No' || $("input[name='q2']:checked").val() == 'No' || $("input[name='q3']:checked").val() == 'No')){ failed = true; }
			if(failed){window.location='failed.php';} else { $("#quoteform").submit();}	
			});
		});
</script>
