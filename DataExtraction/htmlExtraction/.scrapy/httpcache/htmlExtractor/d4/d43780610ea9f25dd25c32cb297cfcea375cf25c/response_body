                                 function stopLayer()
                                 {
                                 	chkOver = false;
                                 }
                                 
                                 function startLayer()
                                 {
                                 	chkOver=true;
                                 	moveLayer()
                                 	
                                 }
                                 
                                 function initMsg()
                                 {
                                 	for (i=0;i < msgs.length;i++)
                                 	{
                                 		document.all['outerLayer'].innerHTML += msgs[i];
                                 		Obj = eval("document.all['msg_"+i+"']");
                                 		//Obj.style.top = i * layerHeight;
                                 		Obj.style.left = i * layerWidth;
                                 	}
                                 	moveLayer();
                                 }
                                 function moveLayer()
                                 {
                                 	for (i=0;i < msgs.length;i++)
                                 	{
                                 		Obj = eval("document.all['msg_"+i+"']");
                                 		Obj.style.left = Obj.style.left.substr(0,Obj.style.left.length - 2) - step;
                                 		if(Obj.style.left.substr(0,Obj.style.left.length - 2) == -layerWidth) Obj.style.left = (msgs.length - 1) * layerWidth;
                                 		if(Obj.style.left == '0px')
                              			{
                              				stopLayer();
                              				setTimeout("startLayer()", 1) ;
                              				//alert(Obj.style.left);
                              			}
                              			
                              		   //alert(Obj.style.left.substr(0,Obj.style.left.length - 2));
                                 	}
                                 	if (chkOver) setTimeout("moveLayer()",interval);
                                 	
                                 	
                                 	
                                 }
                                 function moveLayer1()
                                 {
                                 	for (i=0;i < msgs.length;i++)
                                 	{
                                 		Obj = eval("document.all['msg_"+i+"']");
                                 		Obj.style.right = Obj.style.right.substr(0,Obj.style.right.length - 2) - step;
                                 		if(Obj.style.right.substr(0,Obj.style.right.length - 2) == layerWidth) Obj.style.right = -layerWidth;
                                 		if(Obj.style.right == '0px')
                              			{
                              				stopLayer();
                              				setTimeout("startLayer()", 1) ;
                              				//alert(Obj.style.left);
                              			}
                              			
                              		   //alert(Obj.style.left.substr(0,Obj.style.left.length - 2));
                                 	}
