var type=navigator.appName
if (type=="Netscape")
var lang = navigator.language
else
var lang = navigator.userLanguage

// 국가코드에서 앞 2글자만 자름
var lang = lang.substr(0,2)

//한국인 경우
if (lang == "ko")
window.location.replace('kr/index.php')

// 영어인 경우
if (lang == "en")
window.location.replace('en/index.php')

// 독일어인 경우
