		function postTypekit() {
			$('.navbar_item').tagology('justify', { 
				trimEnds	:	true,
				afterFunction	:	'orderFix'
			});
			$('.footer_navbar_item').tagology('justify', { 
				trimEnds	:	true,
				afterFunction	:	'orderFix'
			});
		}
		
		$(window).load(function() {
			$(window).resize();
			if($(window).width() > 767) {
				$('#nav_order').find('a').attr('href', $('#nav_order').find('a').attr('href')+"?order=true");
			}
			$('input.formsfdatepicker').datepicker({dateFormat:'mm/dd/yy'});
		})
		
		
		$(window).resize(function() {
			$('#footer_top_div').tagology('gutterfill', {
				direction	:	'right',
				withPadding	:	false
			});
		})
		
		function orderFix() {
			$('#nav_order').css({
				'padding-left'	:	parseInt($('#nav_order').css('padding-right'))/2,
				'padding-right'	:	parseInt($('#nav_order').css('padding-right'))/2
			})
			$('#footer_nav_order').css({
				'padding-left'	:	parseInt($('#footer_nav_order').css('padding-right'))/2,
				'padding-right'	:	parseInt($('#footer_nav_order').css('padding-right'))/2
			})
		}
		
		$('#mobile_menu_btn').click(function() {
			if(parseInt($('#header_bottom').css('top')) < 105) {
				$('#mobile_menu_btn').addClass('open');
				$('#header_bottom').animate({
					'top'	:	105
				}, 500);
			} else {
				$('#mobile_menu_btn').removeClass('open');
				$('#header_bottom').animate({
					'top'	:	-275
				}, 500);
			}
		})
	</script>

	<script>
		$(window).resize(function() {
			$('#hp_slider_wrapper').css('height', $(window).width()*(0.295));
			$('.hp_slider_info_item_inner').each(function() {
				if($(this).parent().width() < 295) {
					$(this).css('width', $(this).parent().width());	
				} else {
					$(this).css('width', 'auto');	
				}
				$(this).tagology('absoluteCenter');
			})
			var maxHeight = 0;
			$('.hp_callout_item').each(function() {
				maxHeight = $(this).children('a').height() > maxHeight ? $(this).children('a').height() : maxHeight;
				$(this).children('a').tagology('absoluteCenter', {
					'horizontal50'	:	true	
				});
			})
			$('.hp_callout_item').css('height', maxHeight);
			var winWidth = $(window).width();
			if($(window).width() > 767) {
				$('.hp_menu_item').each(function() {
					$(this).css('width', ((winWidth+(winWidth/4))-winWidth));
				})
				var menuHeight = ((((winWidth+(winWidth/4))-winWidth) > 390) ? 390 : ((winWidth+(winWidth/4))-winWidth));
				$('#hp_menu_slider_wrapper').css('height', menuHeight);
				$('#hp_menu_slider').css({
					'width'			: ((winWidth+(winWidth/4))-winWidth)*6,
					'margin-left'	:	(((winWidth+(winWidth/4))-winWidth)/2)*(-1),
					'height'		:	menuHeight
				})
				$('#hp_menu_slider').parent().css({
					'margin'	: 	'auto',
					'height'	:	menuHeight
				});
				$('.hp_menu_item').each(function() {
					$(this).children('img').css({
						'padding-left'	:	$(this).width() > 360 ? 0 : ($(this).width()-$(this).children('img').width())/2,
						'margin-left'	:	$(this).width() > 360 ? ($(this).width()-360)/2 : 0
					});
					$(this).find('.hp_menu_item_hover').tagology('absoluteCenter');
					$(this).find('.hp_menu_item_clicked').tagology('absoluteCenter');
				})
				$('#hp_menu').css('height', '');
				menuHeightMax = $('#hp_menu').outerHeight(true);
				menuHeightMin = parseInt($('#hp_menu').css('padding-top'))+parseInt($('#hp_menu').css('padding-bottom'))+$('#hp_menu_header').outerHeight(true)+$('#hp_menu_slider').height();
				$('#hp_menu').css('height', menuHeightMin);
				$('#hp_map').css('height', $('#hp_map').width()*(.78539));
			} else {
				var maxMenuItemHeight = 0;
				$('.hp_menu_item').each(function() {
					maxMenuItemHeight = $(this).children('img').height() > maxMenuItemHeight ? $(this).children('img').height() : maxMenuItemHeight;
				})
				$('.hp_menu_item').each(function() {
					$(this).css({
						'width'	: (winWidth/2)-20,
						'height'	:	(winWidth/2)-20
					});
				})
				$('#hp_map').css('height', $(window).width());
			}
			
		})
		
		$(window).load(function() {
			$('#hp_slider_wrapper').css('opacity', '1');
			$("#hp_slider_info_inner").carouFredSel({
				auto	: 7000,
				items	: 1,
				width	:	'100%',
				prev    : {
					button  : "#prev_btn",
					key     : "left"
				},
				next    : {
					button  : "#next_btn",
					key     : "right"
				},
				pagination  : {
					container	:	"#hp_slider_pag",
					anchorBuilder	:	false
				},
				scroll		: {
					fx			: "crossfade",
					duration	: 1000,
					pauseOnHover: true
				},
				responsive	: true,
				synchronise	:	["#hp_slider_inner", false, true, 0]
			});
			$("#hp_slider_inner").carouFredSel({
				auto	: false,
				items	: 1,
				width	:	'239px',
				scroll		: {
					fx			: "crossfade",
					duration	: 1000,
					pauseOnHover: true
				},
				responsive	: true
			});
			if($(window).width() > 767) {
				$("#hp_menu_slider").carouFredSel({
					auto	: false,
					items	: 5,
					prev    : {
						button  : "#hp_menu_prev",
						key     : "left"
					},
					next    : {
						button  : "#hp_menu_next",
						key     : "right"
					},
					scroll		: {
						items		:	1,
						duration	: 1000,
						pauseOnHover: true
					}
				});
			}
			
			$('.script').each(function() {
				if($.isNumeric($(this).html())) {$(this).html('#'+$(this).html());}
			})
			
			$(window).resize();
			
			loopTracks();
			
			$('#hp_slider_pag').tagology('absoluteCenter', {
				'horizontal50'	:	true	
			});
			
			if($(window).width() < 768) {
				$('#map_all').click();
				$('.mobile_input_wrapper').each(function() {
					if(!$(this).children('div').hasClass('dform_radio') && !$(this).children('div').hasClass('submit_button')) {
						$(this).find('input').wrap('<div class="input_wrap"></div>');
						$(this).find('textarea').wrap('<div class="input_wrap"></div>');
					}
				})
			}
			
			$('#menu_hobo').css('opacity', '1');
		})
		
		var menuHeightMax = 0;
		var menuHeightMin = 0;
				
		$('.hp_menu_item').click(function() {
			if($(window).width() > 767) {
				if(!$(this).hasClass('menu_clicked') && !$(this).hasClass('move_item')) {
					hoboMenuClick($(this));
					$('.hp_menu_ctrl').each(function() {
						$(this).animate({
							'opacity'	:	0
						}, 500, function() {
							$(this).css('display', 'none');	
						})
					})
				}
			} else {
				window.location.href = "https://www.hungryhobo.com/menu/?category="+$(this).attr('id');
			}
		})
		
		$('#map_all').click(function() {
			$('.map_selected').removeClass('map_selected');
			$('#before_map').remove();
			$('#loc_map').addClass('loaded');
			initMap('','','', true);
		})
		
		$('.state_title').click(function() {
			$('.map_selected').removeClass('map_selected');
			$('#before_map').remove();
			$('#loc_map').addClass('loaded');
			initMap('','','', $(this).attr('id'));
		})
				
		$('.hp_menu_loc').click(function() {
			$('.map_selected').removeClass('map_selected');
			$(this).addClass('map_selected');
			$('#before_map').remove();
			if(!$('#loc_map').hasClass('loaded')) {
				$('#loc_map').addClass('loaded');
				initMap($(this).children('.location_address').children('.the_lat').html(),$(this).children('.location_address').children('.the_lon').html(),$(this).children('.location_address').children('.the_address').html(), false,$(this).children('.location_address').children('.loc_photo').html(),$(this).children('.location_address').children('.loc_phone').html());
			} else {
				map.setCenter(new google.maps.LatLng(parseFloat($(this).children('.location_address').children('.the_lat').html()), parseFloat($(this).children('.location_address').children('.the_lon').html())));
				map.setZoom(15);	
				clearOverlays();
				var marker = new google.maps.Marker({
					map: map,
					position: new google.maps.LatLng(parseFloat($(this).children('.location_address').children('.the_lat').html()), parseFloat($(this).children('.location_address').children('.the_lon').html())),
					icon: '/assets/images/hobo_pin.png'
				});
				var infowindow = new google.maps.InfoWindow({
						content: '<div class="box_content">'+(($(this).children('.location_address').children('.loc_photo').html().trim() != '') ? '<img src="'+$(this).children('.location_address').children('.loc_photo').html()+'" style="margin-right: 10px;" />' : '')+$(this).children('.location_address').children('.the_address').html()+'<br /><a href="tel:'+$(this).children('.location_address').children('.loc_phone').html()+'">Phone: '+$(this).children('.location_address').children('.loc_phone').html()+'</a></div>'
				});
				markersArray.push(marker);
				marker.addListener('click', function() {
					infowindow.open(map, marker);
				});
			}
		})
		
		$('.hp_menu_item').mouseover(function() {
			$('.hobo_hover').removeClass('hobo_hover');
			$(this).addClass('hobo_hover');
			moveMenuHobo();
		})
		
		$('#hp_menu_slider').mouseleave(function() {
			$('.hobo_hover').removeClass('hobo_hover');
			defaultHobo();
		})
		
		$('.hp_menu_ctrl').click(function() {
			$('.hobo_hover').removeClass('hobo_hover');
			defaultHobo();
		})
		
		function hoboMenuClick(whichClick) {
			whichClick.addClass('menu_clicked');
			whichClick.children('.hp_menu_item_hover').css('opacity', 1);
			var clickedIndex = $('.hp_menu_item').index(whichClick);
			var countUp = 1;
			var countDown = 0;
			$('#hp_menu').animate({
				'height'	:	menuHeightMax
			}, 500);
			$('.hp_menu_item').each(function() {
				var moveOffset = $(this).offset();
				if(!$(this).hasClass('menu_clicked')) {
					$(this).addClass('move_item');
					var thisIndex = $('.hp_menu_item').index($(this));
					if(thisIndex < clickedIndex) {
						$(this).animate({
							'left'	:	(moveOffset.left*(-1))-$(this).outerWidth(true)
						}, (500*countUp));
					} else {
						countDown--;
						$(this).animate({
							'right'	:	($(window).width()-moveOffset.left)*(-1)
						}, (500*countDown));
					}
				} else {
					countDown = countUp;
					whichClick.children('.hp_menu_expanded').css({
						'left'		:	whichClick.outerWidth(true)+30,
						'opacity'	:	0,
						'display'	:	'block',
						'width'		:	$(window).width()-(($(window).width()*(.1))+whichClick.outerWidth(true)+30)
					});
					whichClick.animate({
						'left'	:	(moveOffset.left*(-1))+($(window).width()*(.05))
					}, (500*countUp), function() {
						whichClick.children('.hp_menu_expanded').animate({
							'opacity'	:	1
						}, 500)
					})
					whichClick.children('.hp_menu_item_hover').animate({'opacity':0}, 500);
					whichClick.children('.hp_menu_item_clicked').animate({'opacity':1}, 500);
				}
				countUp++;
			})
		}
		
		function loopTracks() {
			$('#hp_map_tracks_rail').css('left', ($('#hp_map_tracks').height()*(6.842105))*(-1));
			$('#hp_map_tracks_rail').animate({
				'left'	:	0
			}, 4300, 'linear', function() {loopTracks();})
		}
		
		$('.collapse_btn').click(function() {
			var clickedIndex = $('.hp_menu_item').index($('.menu_clicked').first());
			var countUp = 1;
			var countDown = 0;
			$('#hp_menu').animate({
				'height'	:	menuHeightMin
			}, 500);
			$(this).parent().animate({
				'opacity'	:	0
			}, 500, function() {
				$(this).css('display', 'none');
			})
			$('.hp_menu_item').each(function() {				
				if($(this).hasClass('move_item')) {
					console.log((2000*(1-((countUp/5).toFixed(2)))));
					if(parseInt($(this).css('left')) < 0) {
						$(this).animate({
							'left'	:	0
						}, (2000*(1-((countUp/6).toFixed(2)))), function() {
							$(this).removeClass('move_item');
							$(this).css({'left':'','right':''});	
						})
					} else {
						countDown--;
						$(this).animate({
							'right'	:	0
						}, (2000*(1-((countDown/6)))), function() {
							$(this).removeClass('move_item');
							$(this).css({'left':'','right':''});	
						})
					}
				} else if($(this).hasClass('menu_clicked')) {
					countDown = countUp;
					$(this).animate({
						'left'	:	0
					}, 400, function() {
						$(this).removeClass('menu_clicked');
						$(this).css({'left':'','right':''});
						$(this).children('.hp_menu_item_hover').css('opacity', '');	
					})
					$(this).children('.hp_menu_item_clicked').animate({
						'opacity'	:	0
					}, 400, function() {
						$('.hp_menu_ctrl').each(function() {
							$(this).css('display', 'block');
							$(this).animate({
								'opacity'	:	1
							}, 400, function() {$(this).css({'display':'','opacity':''});
							})
						})	
					})
				}
				countUp++;
			})
		})
				
		function moveMenuHobo() {
			itemPosition = $('.hobo_hover').offset();
			armPosition = $('#menu_hobo').offset();
			var theAngle = angle((itemPosition.left+($('.hobo_hover').width()/2)),(itemPosition.top+($('.hobo_hover').height()/2)),(armPosition.left+373),(armPosition.top+97));
			$('#hobo_arm').tagology('imgRotate', {
				startAngle	:	getRotationDegrees($('#hobo_arm')),
				angle		:	theAngle+20,
				duration	:	500,
				easing		:	'swing',
				complete	:	function () {},
				step		:	function () {}});
		}
		
		function defaultHobo() {
			$('#hobo_arm').tagology('imgRotate', {
				startAngle	:	getRotationDegrees($('#hobo_arm')),
				duration	:	500,
				easing		:	'swing',
				complete	:	function () {},
				step		:	function () {}});
		}
		
		function getRotationDegrees(obj) {
			var matrix = obj.css("-webkit-transform") ||
			obj.css("-moz-transform")    ||
			obj.css("-ms-transform")     ||
			obj.css("-o-transform")      ||
			obj.css("transform");
			if(matrix !== 'none') {
				var values = matrix.split('(')[1].split(')')[0].split(',');
				var a = values[0];
				var b = values[1];
				var angle = Math.round(Math.atan2(b, a) * (180/Math.PI));
			} else { var angle = 0; }
			return angle;
		}
		
		function angle(cx, cy, ex, ey) {
			var dy = ey - cy;
			var dx = ex - cx;
			var theta = Math.atan2(dy, dx); // range (-PI, PI]
			theta *= 180 / Math.PI; // rads to degs, range (-180, 180]
			  //if (theta < 0) theta = 360 + theta; // range [0, 360)
			return theta;
		}
		
	var map;
	var markersArray = [];
		
	function initMap(theLat, theLon, theAddress, showAll, thePhoto, thePhone) {
		if(showAll == true) {
			map = new google.maps.Map(document.getElementById('loc_map'), {
				zoom: 11,
				center: new google.maps.LatLng(41.560850, -90.483440)
			});
			$('.hp_menu_loc').each(function() {
				placeTheMarkers(parseFloat($(this).children('.location_address').children('.the_lat').html()), parseFloat($(this).children('.location_address').children('.the_lon').html()), $(this).children('.location_address').children('.the_address').html(), $(this).children('.location_address').children('.loc_photo').html(), $(this).children('.location_address').children('.loc_phone').html());
			})
		} else if(showAll == 'il_locations') {
			map = new google.maps.Map(document.getElementById('loc_map'), {
				zoom: 12,
				center: new google.maps.LatLng(41.496117,  -90.498945)
			});
			$('.hp_menu_loc_il').each(function() {
				placeTheMarkers(parseFloat($(this).children('.location_address').children('.the_lat').html()), parseFloat($(this).children('.location_address').children('.the_lon').html()), $(this).children('.location_address').children('.the_address').html(), $(this).children('.location_address').children('.loc_photo').html(), $(this).children('.location_address').children('.loc_phone').html());
			})
		} else if(showAll == 'ia_locations') {
			map = new google.maps.Map(document.getElementById('loc_map'), {
				zoom: 12,
				center: new google.maps.LatLng(41.598847, -90.481628)
			});
			$('.hp_menu_loc_ia').each(function() {
				placeTheMarkers(parseFloat($(this).children('.location_address').children('.the_lat').html()), parseFloat($(this).children('.location_address').children('.the_lon').html()), $(this).children('.location_address').children('.the_address').html(), $(this).children('.location_address').children('.loc_photo').html(), $(this).children('.location_address').children('.loc_phone').html());
			})
		} else {
			map = new google.maps.Map(document.getElementById('loc_map'), {
				zoom: 15,
				center: new google.maps.LatLng(theLat, theLon)
			});
			var marker = new google.maps.Marker({
				map: map,
				position: new google.maps.LatLng(theLat, theLon),
				icon: '/assets/images/hobo_pin.png'
			});
			var infowindow = new google.maps.InfoWindow({
					content: theAddress
			});
			var theInfo = '<div class="box_content">'+((thePhoto.trim() != '') ? '<img src="'+thePhoto+'" style="margin-right: 10px;" />' : '')+theAddress+'<br /><a href="tel:'+thePhone+'">Phone: '+thePhone+'</a></div>';
			markersArray.push(marker);
			marker.addListener('click', function() {
				infowindow.setContent(theInfo);
				infowindow.open(map, marker);
			});
		}
	}
	
	function clearOverlays() {
		for (var i = 0; i < markersArray.length; i++ ) {markersArray[i].setMap(null);}
	  	markersArray.length = 0;
	}
	
	function placeTheMarkers(lat, lon, address, thePhoto, thePhone) {
		var infowindow = new google.maps.InfoWindow();
		var marker = new google.maps.Marker({
			map: map,
			position: new google.maps.LatLng(lat, lon),
			icon: '/assets/images/hobo_pin.png'
		});
		markersArray.push(marker);
		var theInfo = '<div class="box_content">'+((thePhoto.trim() != '') ? '<img src="'+thePhoto+'" style="margin-right: 10px;" />' : '')+address+'<br /><a href="tel:'+thePhone+'">Phone: '+thePhone+'</a></div>';
		google.maps.event.addListener(marker, 'click', (function(marker) {
			return function() {
				infowindow.setContent(theInfo);
				infowindow.open(map, marker);
			}
		})(marker));
	}
	
	$('.hp_menu_items_number').click(function() {
		var menuItem = $(this).html();
		menuItem = menuItem.split("</span> ");
		window.location.href = "https://www.hungryhobo.com/menu/?item="+menuItem[1];
	})
	
	</script>
