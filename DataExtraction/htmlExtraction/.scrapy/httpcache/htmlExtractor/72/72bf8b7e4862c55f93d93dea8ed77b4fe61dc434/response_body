    <script type="text/javascript">
        $(document).ready(function(){
            var parameter = {
                "year":             0,
                "month":            0,
                "templateedit_id":  165,
                "lang":  'en'
            };
            
            $.ajax({
                data:       parameter,
                url:        '/direct/calendar/calendar/',
                type:       'post',
                success:    function (response) {
                    if ( response.indexOf('class="calendar"') != -1) {
                        $("#select-from-date").html(response);
                    }
                }
            });
        });
    </script>

