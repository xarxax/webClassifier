jQuery( document ).ready(function( $ ) {
	
	var winWidth = $(window).width();
	var winTop = $(window).scrollTop();
	
	$(window).resize(function(){
		winWidth = $(window).width();
		
	});

	$('#bestNav').bestnav({
		breakpoint : 300,
		dropDownWidth : 200,
		customUlClass : "mainMenu",
		displayArrow : true,
		linkArrow : true,
		mobileBtnCustom:false,
		mobileBtnClass : "navBtn",
		mobiSelfClose : true
	});	
	
	$('#bestNavMobi').bestnav({
		breakpoint : 0,
		dropDownWidth : 200,
		customUlClass : "mainMenuMobi",
		displayArrow : true,
		linkArrow : true,
		mobileBtnCustom:false,
		mobileBtnClass : "navBtnMobi",
		mobiSelfClose : true
	});	
	
	$('#topBarNav li').on('mouseenter', function(){
		if($(this).has('ul')){
			$(this).find('> ul').stop().slideDown();	
		}
	}).on('mouseleave', function(){
		if($(this).has('ul')){
			$(this).find('> ul').stop().slideUp();	
		}
	});
	
	if($('.cusEqElems').length > 0){
		$('.cusEqElems').matchHeight({ byRow: true, property: 'height', target: null, remove: false });
	}
	
	if($('.gform_wrapper .qntBox .gfield_label').length > 0){
		$('.gform_wrapper .qntBox .gfield_label').matchHeight({ byRow: true, property: 'height', target: null, remove: false });
	}
	if($('.gform_wrapper .qntBox .gfield_description').length > 0){
		$('.gform_wrapper .qntBox .gfield_description').matchHeight({ byRow: true, property: 'height', target: null, remove: false });
	}

				var $mainSlider = $('.bxslider').bxSlider({
		  mode: 'fade',
		  auto: true,

          pause:7000,
		  captions: false,
		  onSlideBefore : function($slideElement, oldIndex, newIndex){ 
		  	$('.mainSlider .sliCapCell').animate({'left':'-30px', 'opacity':'0'});
		  	$('.sliThumbnails li.active').removeClass('active');
			$('.sliThumbnails li').eq(newIndex).addClass('active');
		  },
		  onSlideAfter : function($slideElement, oldIndex, newIndex){ 
		  	$('.mainSlider .sliCapCell').css('left','30px').animate({'left':'0', 'opacity':'1'});
		  }
		});
		
		$('.sliThumbnails li').on('click', function(){
			var pos = $(this).index();
			$(this).addClass('active').siblings().removeClass('active');
			$mainSlider.goToSlide(pos);
		});
		
		$('.hsBox').matchHeight({ byRow: true, property: 'height', target: null, remove: false });

		var bnrTop = $('#slider').css('top').slice(0,-2);
		var winTopParx = $(window).scrollTop();
		$(window).scroll(function(){
			winTopParx = $(this).scrollTop();
			bannerParallax();
		});
		function bannerParallax(){
			$('.sliderCaption').css({'opacity':(1 - winTopParx/500)});
			$('.sliThumbnails, .bx-pager').css({'opacity':(1 - winTopParx/300)});
			$('.bxslider').css('top','+' + ((winTopParx*0.5)+parseInt(bnrTop)) + 'px');
		}
		bannerParallax();
		
		
		
	if( $('a.vidPopLink').length > 0 ){
  		$("a.vidPopLink").fancybox({ type: "iframe" });
	}
	
	
		$("#testiSlider").lightSlider({
		item: 1,
		auto: true,
		loop: true,
		controls : false,
		pager: true,
		pause: 7000,

	});
		
	if($('.cus-simpleImg-box').length > 0){
		$('.cus-simpleImg-box h4').matchHeight({ byRow: true, property: 'height', target: null, remove: false });
	}
	if($('.resListWrap').length > 0){
		$('.resListWrap').matchHeight({ byRow: true, property: 'height', target: null, remove: false });
	}
	
	
});
	
</script>
