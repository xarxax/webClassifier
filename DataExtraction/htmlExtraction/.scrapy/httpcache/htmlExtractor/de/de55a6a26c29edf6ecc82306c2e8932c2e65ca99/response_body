	 <script>
    (function(settings) {
        $(document).ready(function() {
            var $wrapper = $('#' + settings.wrapperId);
            var $imagecycle = $wrapper.find('.image-cycle-slideshow-container');
            var $slideshow = $imagecycle.find('.image-cycle-slideshow');
            
           //Append our slideshow template
           $imagecycle.find('script.SlideshowSlideTemplate').tmpl(settings).appendTo($slideshow);
           
           //Grab some slide height and colour settings
            var $slides = ".image-cycle-slideshow,.image-cycle-slideshow-container,.image-cycle-slideshow-slide,.image-cycle-slideshow-slide-inner";
            var $thisSlide = $imagecycle.find('.image-cycle-slideshow-slide');
            $imagecycle.find($slides).css({height: settings.height});
            $imagecycle.find('.image-cycle-slideshow-slide:first').show();
            
            $imagecycle.find('.ImageCycleSlideshowTitle').html(settings.slideshowTitle);
            
            //Grab some dimensions
            var $slideheight = $slideshow.height();
            var $slidewidth = $slideshow.width();
            
            //Slide colours
            $thisSlide.find('h1 a, p').css({color: '#' + settings.captionTitleColor});
            
            //Slideshow
            $slideshow.cycle({ 
                fx:      settings.slidefx, 
                speed:    settings.slideSpeed, 
                timeout:  settings.slideTimeout,
                before: function(){
                    $slideshow.find('.image-cycle-slideshow-caption').animate({bottom: -$slideheight},{duration: 750});
                },
                after: function(){
                    $slideshow.find('.image-cycle-slideshow-caption').animate({bottom: '0px'},{duration: 750});
                },
                pause: 1
            });
            
            //Show captions
            if (settings.showCaptions == "false"){
                $slideshow.find('.image-cycle-slideshow-caption').remove();
            }
            
            $slideshow.find('.image-cycle-slideshow-caption').each(function(k, capt) {
                var text = $(capt).text().replace(/^\s+|\s+$/g, '');
               if ( text.length == 0 ) {
                   $(capt).remove();
               }
            });
            
            //Slide dimensions flower
            if(settings.showDimensions == "true"){
                var dimensionsFlower =    '<div class="slideDimensions">';
                dimensionsFlower +=           '<div class="slideDimensionsValues">';
                dimensionsFlower +=               '<span class="slideDimensions-width">Width: '+ $slidewidth +'px</span>';
                dimensionsFlower +=               '<span class="slideDimensions-width">Height: '+ $slideheight +'px</span>';
                dimensionsFlower +=           '</div>';
                dimensionsFlower +=       '</div>';
                $imagecycle.find('.image-cycle-slideshow').append(dimensionsFlower);
            }       
            
        });
    })(MrSite.Widget.getSettings());
</script>

