function setShippingService()
{
	var vServiceID=document.getElementById("delivery-options-dropdown").value;
	var w = document.getElementById("delivery-options-dropdown").selectedIndex;
	var vServiceName=document.getElementById("delivery-options-dropdown").options[w].text;
	SetBasketShippingService(vServiceID, vServiceName);
}

function increaseQty(inutid)
{
	var qt = $("#"+inutid).val();
	var nqt= 1;
	try { nqt=parseInt(qt)+1;
	} catch (ex) {}
	if (isNaN(nqt)) { nqt=1; }
	if (nqt > 9999)
	{
		nqt=9999;
	}
	$("#"+inutid).val(nqt);
}

function decreaseQty(inutid)
{
	var qt = $("#"+inutid).val();
	var nqt= 1;
	try { nqt=parseInt(qt)-1;
	} catch (ex) {}
	if (isNaN(nqt)) { nqt=0; }
	if (nqt < 0)
	{
		nqt=0;
	}
	$("#"+inutid).val(nqt);
}

function CheckCalculateBasket()
{
	$('#readytocheckout').html(vMainLoadingImage);
	$('#notreadytocheckout').html(vMainLoadingImage);
	setTimeout('document.location.href="/cart/default.aspx?recalc=1"',500);
	//$("#readytocheckout").show();
	//$("#notreadytocheckout").hide();
}

function TidyPageURL(pageurl)
{
	if (pageurl.length > 0)
	{
		if (pageurl.substring(0,4) != "http")
		{
			if (pageurl.substring(0,1) != "/")
			{
				pageurl="/"+pageurl;
			}
		}
	}

	return pageurl;
}

function AddItemToBasketJQWithPage(id, code, name, qty, price, listprice, discpc, pageurl) {

	//pageurl=TidyPageURL(pageurl);

	var buid='4c2551b6-cff0-4345-b5cd-e60a9b665693';
	$.ajax({
	 url:    '/SiteSphereHandler.ashx?action=basketadditem&norecalc=1&vuid=684afe7c-3f67-4c7a-898b-4e2b3e625471&basketuid=' + buid + '&itemcode=' + escape(code) + '&itemprice=' + price + '&listprice=' + listprice + '&discpc=' + discpc + '&itemname=' + escape(name) + '&itemid=' + id + '&itemquantity=' + qty + '&pageurl=' + escape(pageurl) + "&accountuid=" + escape('') + "&accountdscode=" + escape('') + "&accountcode=" + escape('') + "&currency=GBP",
	 success: function(result) {
		      if(result.isOk == false)
			  alert(result.message);
		  },
	 async:   false
	}); 
}

