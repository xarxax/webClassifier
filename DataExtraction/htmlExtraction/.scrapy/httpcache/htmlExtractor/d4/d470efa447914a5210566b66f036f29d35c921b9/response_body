								<div id="menu-search">
									<ul class="list-clear">
										<li class="header-icon header-search">
											<button type="button" class=" btn-clear" data-toggle="modal" data-target="#myModal">
												<i class="fa fa-search" aria-hidden="true"></i>
											</button>
											<!-- Modal -->
											<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display: none;">
