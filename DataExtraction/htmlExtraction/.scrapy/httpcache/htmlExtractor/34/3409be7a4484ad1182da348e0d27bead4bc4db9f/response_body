            parent.popover({
                title: 'Where in the world is Inductotherm Group?' + ' <span id="inducto_close">&times;</span>',
                content: "Where aren’t we? Did you know we span the globe with over 32 manufacturing companies, hundreds of products and a wide array of services?  Use our Company Index to find a product, service or manufacturing facility near you.",
                placement: 'bottom',
                html: true,
                //viewport: '.sidebar'
            });
            parent.popover('show');
            //Set cookie, so it doesn't show on refresh.
            document.cookie = "inductopop_closed=true";

            // Close the popover when clicked.
            $(document).on('click', '#inducto_close', function(){
                parent.popover('destroy');
            });
        });

		

	})(jQuery);
	</script>
									</nav>

