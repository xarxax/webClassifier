	$(document).ready(function(){ 
    	$('#m_name').focus(function() {
            strIDVal = $(this).val();
    
            if (strIDVal == "Name:") {
                strIDVal="";
                $(this).val(strIDVal);	
            }
        });
        $('#m_name').blur(function() {
            strIDVal = $(this).val();	
            if (strIDVal == "") {
                strIDVal="Name:";
                $(this).val(strIDVal);	
            }
        });
        $('#m_email').focus(function() {
            strIDVal = $(this).val();	
            if (strIDVal == "Email:") {
                strIDVal="";
                $(this).val(strIDVal);	
            }
        });
        
        $('#m_email').blur(function() {
            strIDVal = $(this).val();	
            if (strIDVal == "") {
                strIDVal="Email:";
                $(this).val(strIDVal);	
            }
        });   
	});
</script>


