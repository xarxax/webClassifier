 <script type="text/javascript">
  var DomainUrl="https://www.gbta.org/";
    function setMobileHeaderState(profile) {
		
	
         try {
		
     if(profile==0) {

        


             if (location.href.replace(/.*\/\/[^\/]*/, '') != "/") {
                 ReturnURL = "?ReturnUrl=" + encodeURIComponent(location.href.replace(/.*\/\/[^\/]*/, ''));
             }
           
			if(ReturnURL.toLowerCase().indexOf("%2fstore%2f")>-1)
			{	
			ReturnURL='productstore';

			
			}
			

            switch (ReturnURL.toLowerCase()) {
               

					case 'productstore':
                     ReturnURL = "?ReturnUrl="+encodeURIComponent("/myaccount?actionurl=Product&redirect="+location.href.replace(/.*\/\/[^\/]*/, ''));
                    break;
                default:
                     ReturnURL = "?ReturnUrl=" + encodeURIComponent(location.href.replace(/.*\/\/[^\/]*/, ''));
             }
			  
             if (window.location.href.toLowerCase().indexOf("sitelogout")!=-1)
                 ReturnURL = "";
				 
				
				$('#dnn_MobileHeaderSetting1_ahrefLogin').attr("href", DomainUrl + "ssologin" + ReturnURL);
           //  $('#dnn_ctr3088_MobileHeaderSetting_ahrefLogin').attr('href', DomainUrl + "ssologin" + ReturnURL);
			// alert( $('#dnn_ctr3088_MobileHeaderSetting_ahrefLogin').attr('href', DomainUrl + "ssologin" + ReturnURL));
             //$(".gbta-utility-item").find('a.gbta-login-desktop').text("Login").attr("href", DomainUrl+"ssologin"+ReturnURL);
             //$(window.parent.document).find(".gbta-login-desktop").find('a.gbta-btn--primary').attr("href", DomainUrl+"SiteLogout"+ReturnURL);
             // $(".gbta-header--mobile").find('a.gbta-login-mobile').text("Login").attr("href", DomainUrl+"ssologin"+ReturnURL);
             //alert($('#liLogin').find('a').attr('href'));
            }
        else
		{
             //$(".gbta-utility-item").find('a.gbta-login-desktop').text("Logout").attr("href", DomainUrl+"SiteLogout");
             // $(window.parent.document).find(".gbta-login-desktop").find('a.gbta-btn--primary').attr("href", DomainUrl+"SiteLogout");
             //$(".gbta-header--mobile").find('a.gbta-login-mobile').text("Logout").attr("href", DomainUrl+"SiteLogout");

             getMobileHeaderProfilePhoto(profile);

            }

     
 } catch (err) {
 
    $('#dnn_MobileHeaderSetting1_ahrefLogin').attr('href', DomainUrl + "ssologin");
 }



 };

 function getMobileHeaderProfilePhoto(CustomerCD) {
 
     var hiddenCustomercdVal = CustomerCD;
     $.ajax({
         url: DomainUrl + 'cvweb/cgi-bin/cvimage.dll/view?ENTITY=MEM',
         data: {
             ENTITYCD: hiddenCustomercdVal
         },
         type: 'GET',
         success: function(res) {


             if (res.length > 30) {

                 $("#ImageMobileHeaderProfile").attr("src", DomainUrl + "cvweb/cgi-bin/cvimage.dll/view?ENTITY=MEM&ENTITYCD=" + hiddenCustomercdVal);
             } else {

                 $("#ImageMobileHeaderProfile").attr("src", DomainUrl + "images/userplaceholder.jpg");

             }
         }

     });

 }
	
	
</script>


