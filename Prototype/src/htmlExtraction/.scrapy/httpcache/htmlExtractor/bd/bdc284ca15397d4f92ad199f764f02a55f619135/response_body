<script language="javascript">
	var searchBarShown719 = 'hidden';
	function showSearchBar( serviceID ) {
		//alert(serviceID);
		if (searchBarShown719 == 'hidden') {
			document.getElementById('menu' + serviceID).style.display = 'none';
			document.getElementById('searchBarDiv' + serviceID).style.display = '';
			document.getElementById('shoppingCartIcon' + serviceID).style.display = 'none';
			document.getElementById('closeSearchBarIcon' + serviceID).style.display = '';
			searchBarShown719 = 'shown';
		} else {
			var searchBarValue719 = document.getElementById('searchBar' + serviceID).value;
			if (searchBarValue719 != '') {  /* if value found in search bar, do search */
				window.location = 'https://www.greenpasture.org/public/servicepages/searchresults?norobot=&init=1&noBack=1&SearchCriteria=' + searchBarValue719 + '&x=0&y=0';
			} 
			//document.getElementById('menu' + serviceID).style.display = '';
			//document.getElementById('searchBarDiv' + serviceID).style.display = 'none';
			//document.getElementById('shoppingCartIcon' + serviceID).style.display = '';
			//document.getElementById('closeSearchBarIcon' + serviceID).style.display = 'none';
			//searchBarShown719 = 'hidden';
		}
	}
	function hideSearchBar( serviceID ) {
		document.getElementById('menu' + serviceID).style.display = '';
		document.getElementById('searchBar' + serviceID).value = '';
		document.getElementById('searchBarDiv' + serviceID).style.display = 'none';
		document.getElementById('shoppingCartIcon' + serviceID).style.display = '';
		document.getElementById('closeSearchBarIcon' + serviceID).style.display = 'none';
		searchBarShown719 = 'hidden';
	}
	
	function doSearchOnEnter(event, serviceID) {
		
		if (!event) { var event = window.event; }
		var keyCode = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;
		if (keyCode == 13 || keyCode ==10) {
			showSearchBar( serviceID );
		}
	}

</script>

