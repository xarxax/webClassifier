<script>
    jQuery(function($) { // DOM is now ready and jQuery's $ alias sandboxed
        $(document).ready(function() {





            function createSlick(){

                $("#resourceSlider").not('.slick-initialized').slick({
                    slidesToShow:2,
                    slidesToScroll: 1,

                    responsive: [
                        {
                            breakpoint: 768,
                            settings: {
                                slidesToShow: 2,
                                slidesToScroll: 1,
                                infinite: true
                            }
                        },
                        {
                            breakpoint: 480,
                            settings: {
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                infinite: true
                            }
                        },
                        {
                            breakpoint: 375,
                            settings: {
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                infinite: true
                            }
                        },
                        {
                            breakpoint: 320,
                            settings: {
                                slidesToShow: 1,
                                slidesToScroll: 1,
                                infinite: true
                            }
                        },
                    ]
                });

            }
            createSlick();

            $(window).on( 'resize', createSlick );


        });
    });
</script></div></div>			</div> 		
