		<script>
			$( document ).ready(function() {
				var isFirstClick = 1;
				//var isProductMenu;
				//$('#the-left-navbar').on('show.smapi', function(e, menu) {
				//	$('#the-left-navbar').smartmenus('itemActivate', $('.megamenu-turkey'));
				//});
				$('#the-left-navbar').on('beforeshow.smapi', function(e, menu) {
					if (!isMobile || isMobile) {$('#the-right-navbar').smartmenus('menuHideAll');}
					//isProductMenu = $(menu).hasClass("mega-menu");
					//if (isProductMenu) {
						// console.log("ok");
						// $('#the-left-navbar').smartmenus('itemActivate', $('a#megamenu-turkey-link'));
					//}
				});
				$('#the-right-navbar').on('beforeshow.smapi', function(e, menu) {
					if (!isMobile || isMobile) {$('#the-left-navbar').smartmenus('menuHideAll');}
				});
				$(".navbar-nav > li.dropdown > a").click(function() {event.preventDefault();});
				$(".has-mega-menu").click(function() {
				  if (isMobile && isFirstClick) {
				  	$('.megamenu-categories').addClass("open");
					$('#megamenu-categories-link').attr('aria-expanded', $('#megamenu-categories-link').attr('aria-expanded')=="false");
					$('.megamenu-turkey').addClass("open");
					$('#megamenu-turkey-link').attr('aria-expanded', $('#megamenu-turkey-link').attr('aria-expanded')=="false");
					isFirstClick = false;
				  }
				});
				$(".mega-menu > li > a.dropdown-toggle").click(function() {
				  if (isMobile) {
				  	event.preventDefault();
				  	event.stopPropagation();
					$(this).parent().toggleClass("open");
					$(this).attr('aria-expanded', $(this).attr('aria-expanded')=="false");
				  }
				});
				$("a.menu-final-link").click(function() {
				  if (isMobile) {event.stopPropagation();}
				});
			});
		</script>

		

			

			

		

		

		

