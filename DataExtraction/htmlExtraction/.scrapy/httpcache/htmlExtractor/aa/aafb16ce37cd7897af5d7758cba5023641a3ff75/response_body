$("document").ready(function($){
    var nav = $('#outernav');
    $(window).scroll(function () {
		if(window.innerWidth > 768) {
        if ($(this).scrollTop() > 400) {
            nav.addClass("fix-nav").fadeIn(5000);
        } else {
            nav.removeClass("fix-nav");
        }
		}
    });
});
</script>
