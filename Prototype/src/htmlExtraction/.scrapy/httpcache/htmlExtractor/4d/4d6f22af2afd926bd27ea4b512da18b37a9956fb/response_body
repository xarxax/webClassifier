  <script>
    $(document).ready(function() {
      $('#s-share-buttons a').click(function(e) {
        e.preventDefault();
        window.open(this.href, '_blank', 'width=500,height=400');
      });

    // Click the body and close the side section
    $(document).click(function(e) {
      
      if ( $(e.target).closest(".side-section").length === 0 ) {
        
        var toggleTarget = $('.side-section2');
        
        toggleTarget.animate({
          right: '-274'
        });
        
        toggleTarget.removeClass('opened');
      }
    });
    
    // Click the close button
    $('.side-section2 .close').click(function() {
      
      var toggleTarget = $('.side-section2');
      
      toggleTarget.animate({
          right: '-274'
        });
      toggleTarget.removeClass('opened');
    });
    
    // Toggle the side section
    $('.side-section').click(function(e) {
      
      var toggleTarget = $('.side-section2');
      
      if ( !toggleTarget.hasClass('opened') ) {
        
        toggleTarget.animate({
          right: '3'
        });
        toggleTarget.addClass('opened');
        
      } else {
        
        toggleTarget.animate({
          right: '-274'
        });
        toggleTarget.removeClass('opened');
        
      }
      
      e.preventDefault();
    });
    
    });
  
  </script>

