<script src="assets/js/libs/match-heights/jquery.matchHeight-min.js"></script>
<script>
	$(function() {
		$('.one-fourth .sub').matchHeight();		
		
		var winH  = $(window).height();		
		
		// Put GIF In Center Have to calculate Wrapper Height		
		var openerH = $('#opener').height(), headerH = $('header').height(), bannerH = winH - openerH - headerH;
		if(bannerH > 150) {
			$('.image-wrapper').css({'height' : bannerH+'px', 'margin-top': headerH});
		} else {
			$('.image-wrapper').hide();
		}
		$(window).resize(function() {			
			var winH  = $(window).height(), openerH = $('#opener').height(), headerH = $('header').height(), bannerH = winH - openerH - headerH;	
			// Put GiF In Center Have to calculate Wrapper Height			
			if(bannerH > 150) {
				$('.image-wrapper').show();
				$('.image-wrapper').css({'height' : bannerH+'px', 'margin-top': headerH});
			} else {
				$('.image-wrapper').hide();
			}
		});
	});	
</script>

