<script type="text/javascript">
    $(function () {
        var storeAddressCookie = 'StoreCity';
        var storeStateCookie = 'StoreState';
        var storeIdCookie = 'StoreID';
        var storeAddress = getCookie(storeAddressCookie);
        var storeState = getCookie(storeStateCookie);
        var storeId = getCookie(storeIdCookie);

        if (storeAddress !== "") {
            $('.store-location').html("- " + storeAddress);
        }
        var baseLocationsUrl = "https://locations.ctownsupermarkets.com/";
        if (storeAddress !== "" && storeState !== "" && storeId	!== "") {
            //set url to selected store details
            var storeDetailsUrl = baseLocationsUrl + storeState.toLocaleLowerCase() + "-" + storeAddress.toLocaleLowerCase().replace(' ', '-') + "-" + storeId.toLocaleLowerCase();
            $('.btn.btn-alternate.mystore').attr('href', storeDetailsUrl);
        } else {
            //set url to locations
            $('.btn.btn-alternate.mystore').attr('href', baseLocationsUrl);
        }

    });
    

    function getCookie(name) {
        var cookieName = name + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(cookieName) === 0) {
                return c.substring(cookieName.length, c.length);
            }
        }
        return "";
    }
</script>
