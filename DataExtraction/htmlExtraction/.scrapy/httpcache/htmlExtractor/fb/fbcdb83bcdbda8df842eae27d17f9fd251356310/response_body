		<script type="text/javascript">
            function qs(key) {
                key = key.replace(/[*+?^$.\[\]{}()|\\\/]/g, "\\$&"); // escape RegEx meta chars
                var match = location.search.match(new RegExp("[?&]"+key+"=([^&]+)(&|$)"));
                return match && decodeURIComponent(match[1].replace(/\+/g, " "));
            }
            // Give the URL parameters variable names
            var source = qs('utm_source'); var medium = qs('utm_medium'); 
            var campaign = qs('utm_campaign'); var content = qs('utm_content');
            // avoid string value of 'null'
            if( source && source != 'null'){ Cookies.set('utm_source', source, { expires: 2 }); }
            if( medium && medium != 'null' ){ Cookies.set('utm_medium', medium, { expires: 2 }); }
            if( campaign && campaign != 'null' ){ Cookies.set('utm_campaign', campaign, { expires: 2 }); }
            if( content && content != 'null' ){ Cookies.set('utm_content', content, { expires: 2 }); }
        </script>
