<script type="text/javascript">
var d = new Date();
var weekday = new Array(7);
weekday[0] = "Sunday";
weekday[1] = "Monday";
weekday[2] = "Tuesday";
weekday[3] = "Wednesday";
weekday[4] = "Thursday";
weekday[5] = "Friday";
weekday[6] = "Saturday";
var day = weekday[d.getDay()];
  var display = false;
  var closestStore;  
  var markers = new Array();
  var map;
  var lat, lng;
  function initialize() {
    markers.length=0;
    geocoder = new google.maps.Geocoder();
    
    var mapOptions = {
    center: {lat: parseFloat(lat), lng: parseFloat(lng)},
    zoom: 14
    };
    map = new google.maps.Map(document.getElementById("map-canvas"),
        mapOptions);
    
    addMarkerToArray(lat, lng, 'you are here');
    addMarkerToArray(parseFloat(closestStore.latitude), parseFloat(closestStore.longitude), closestStore.name);
    for (var x = 0; x < markers.length; x++){
      markers[x].setMap(map);
    }
  }

  
  google.maps.event.addDomListener(window, 'resize', initialize, function() {
     var center = map.getCenter({lat: parseFloat(lat), lng: parseFloat(lng)});
     google.maps.event.trigger(map, "resize");
     map.setCenter(center); 
    });
  function addMarkerToArray(lat, lng, title) {
    var marker = new google.maps.Marker({
        position: {lat: parseFloat(lat), lng: parseFloat(lng)},
        title: title
    });
    markers.push(marker);
  }

</script>


