					  .setTargeting("a", "v1.0.0||22|")
					  .setTargeting("testad", testad)
					  .setTargeting("loggedin", userIsLoggedIn)
					googletag.enableServices();
					googletag.display('div-gpt-ad-homeDesktop-1');
				  });
				i = -1;
			}

			if (--i > -1) {
			  setTimeout(function () { delayedGoogletag(i); }, 2000);
			}
		}

		delayedGoogletag(5);
    </script>
