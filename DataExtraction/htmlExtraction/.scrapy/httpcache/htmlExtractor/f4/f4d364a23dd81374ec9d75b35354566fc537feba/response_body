    });
    $(".bcd-form-newsletter").submit(function (event) {
      $("p#captcha_conf").remove();
      /*console.log($("#g-recaptcha-response-2").length);
       console.log($("#g-recaptcha-response-2").val());*/
      if (($("#g-recaptcha-response-2").val()) == ('')) {
        $("<p id=\"captcha_conf\">Confirm that you are not a robot!</p>").insertAfter("#recaptcha_5");
        event.preventDefault();
      }
      if (($(".bcd-form-newsletter #list").val()) == ('')) {
        $(".bcd-form-newsletter #list").css('boxShadow', '0 0 3px 2px #f00');
        event.preventDefault();
      }
    });
  });
</script>

<script type="text/javascript">
  jQuery(document).ready(function () {
    $("[name='nazione']").on('change', function () {
      if($(this).val() == "United States") {
        $("[name='nazione_us']").removeClass('hidden');
      } else {
        $("[name='nazione_us']").addClass('hidden');
      }
    });
  });
</script>
