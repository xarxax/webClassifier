var actualnews = 1;

function change_news(news){
		document.getElementById('news_1').style.display='none';document.getElementById('newslink_1').className='linkChangeNews';document.getElementById('news_2').style.display='none';document.getElementById('newslink_2').className='linkChangeNews';document.getElementById('news_3').style.display='none';document.getElementById('newslink_3').className='linkChangeNews';document.getElementById('news_4').style.display='none';document.getElementById('newslink_4').className='linkChangeNews';document.getElementById('news_5').style.display='none';document.getElementById('newslink_5').className='linkChangeNews';document.getElementById('news_6').style.display='none';document.getElementById('newslink_6').className='linkChangeNews';document.getElementById('news_7').style.display='none';document.getElementById('newslink_7').className='linkChangeNews';document.getElementById('news_8').style.display='none';document.getElementById('newslink_8').className='linkChangeNews';document.getElementById('news_9').style.display='none';document.getElementById('newslink_9').className='linkChangeNews';document.getElementById('news_10').style.display='none';document.getElementById('newslink_10').className='linkChangeNews';document.getElementById('news_11').style.display='none';document.getElementById('newslink_11').className='linkChangeNews';		document.getElementById('news_'+news).style.display='block';
		document.getElementById('newslink_'+news).className='linkChangeNewsSelect';
}

var numberNews = 11;
var roll = '';
function timer_news(){
	change_news(actualnews);
	actualnews = actualnews+1;
	if(actualnews > numberNews){actualnews=1;}
	roll = setTimeout("timer_news()",5*1000);
}

function stoproll(news){
	change_news(news);
	clearTimeout(roll);
}

