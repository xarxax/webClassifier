// Comma separated list of images to rotate 
var imgs = new Array('images/rota2/image_01.jpg','images/rota2/image_04.jpg','images/rota2/image_03.jpg','images/rota2/image_02.jpg','images/rota2/image_0Xa.jpg','images/rota2/image_0Xb.jpg','images/rota2/image_0Xc.jpg','images/rota2/image_0Xd.jpg','images/rota2/image_0Xe.jpg');
// delay in milliseconds between image swaps 1000 = 1 second 
var delay = 3000;
var counter = 0;

function preloadImgs(){
  for(var i=0;i<imgs.length;i++){
    MM_preloadImages(imgs[i]);
  }
}

function randomImages(){
  if(counter == (imgs.length)){
    return;
  }
  MM_swapImage('rotator', '', imgs[counter++]);
  setTimeout('randomImages()', delay);
}
