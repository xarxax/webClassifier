function searchSendit() {
	var form=document.search_Form;
	if(form.search.value=="") {
		alert("Please enter a search term.");
		form.search.focus();
	} else {
		form.submit();
	}
}

function searchInputSendit() {
	if(event.keyCode==13) { 
		searchSendit();
	}
}
