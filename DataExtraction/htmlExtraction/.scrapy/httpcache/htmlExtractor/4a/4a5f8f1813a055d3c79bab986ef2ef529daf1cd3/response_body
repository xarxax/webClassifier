    <script type="text/javascript">
    (function ( $ ) {
        $(document).ready(function() {
            var active = 1;
            if (active == 1 ) {
                $(window).scroll(function() {
                    if ($(window).scrollTop() > 180) {
	                    $(".flat_responsive_header").css({
	                    	"position":"fixed", 
	                    	"right":"0px",
	                    	 "left":"0px", 
	                    	 "top": "0px",
	                    	 "z-index":"1080", 
	                    	 'box-shadow': '0.5px 0.5px 0.5px #EAEAEA'
	                    });
	                   

                    } else {
                        $(".flat_responsive_header").css({
                        	"position":"relative"
                        });
                    }

                });
            }
        });
    })(jQuery);;        

    </script> 
