 curentimage=1;
 previousimage=5;
 

  
 function autoslideprevious(){
 		
 		
		totalimgs=5;
		
		nextimage=curentimage;
        previousimage=curentimage-1;

 	 	
		document.getElementById(previousimage).style.display='inline';
		document.getElementById(curentimage).style.display='none';
		curentimage=curentimage-1;

	 	
  }
	
 
 
 
 function autoslidenext(){
 		
 		
		totalimgs=5;
		
		nextimage=curentimage+1;
        previousimage=curentimage-1;

 		 	
		document.getElementById(nextimage).style.display='inline';
		
		document.getElementById(curentimage).style.display='none';


		curentimage=curentimage+1;

	 	
		
		
        
   
       }
	

</script>

