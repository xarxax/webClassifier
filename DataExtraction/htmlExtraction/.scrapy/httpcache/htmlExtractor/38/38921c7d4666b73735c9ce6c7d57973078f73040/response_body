<script>$(function(){function checkCookie(cookieName){var cookies=document.cookie.split(';');for(var c in cookies){var cN=cookies[c].split('=')[0];if(cN.replace(/\s/,'')==cookieName){return !0}}
return !1}
$(function(){$('a.accept').click(function(){var elBox=$(this).parents('div.cookiebar');var cookieValue=new Date();var ms=cookieValue.getTime();var expir=ms+8000*60*20;expir=new Date(expir).toUTCString();document.cookie='CookieBar_'+elBox.attr('id')+'='+ms+'; expires='+expir+'; path=/';elBox.fadeOut();$('#cookiebar').remove();return !1});$(document).ready(function(){$('div.cookiebar').map(function(index,elem){if(checkCookie('CookieBar_'+elem.id)){$("#"+elem.id).hide();$('#cookiebar').remove()}else{$("#"+elem.id).fadeIn("slow")}})})})})</script>
