		   $('.flyers li a').hover(function() {
				$(this).stop();
			   var name = $(this).attr('title');
			   $(this).find('span').html(name).fadeIn();
		   }, function() {
				$(this).stop();
			   $(this).find('span').fadeOut();
		   });
		});
	</script>
<script>
var myCenter=new google.maps.LatLng(57.48249,-1.8149);

function initialize()
{
var mapProp = {
  center:myCenter,
  zoom:15,
  disableDefaultUI:true,
  mapTypeId:google.maps.MapTypeId.ROADMAP,
  panControl:false,
  zoomControl:true,
  mapTypeControl:false,
  scaleControl:false,
  streetViewControl:false,
  overviewMapControl:false,
  rotateControl:false,
  scrollwheel: false,
  };
  
var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);

var marker=new google.maps.Marker({
  position:myCenter,
  });

function checkMap(){
	var windowWidth = $(window).width();
	if (windowWidth <= 1024){
		map.setOptions({draggable: false, zoomControl: false, scrollwheel: false, disableDoubleClickZoom: true});
	} else {
		map.setOptions({draggable: true, zoomControl: true, scrollwheel: false, disableDoubleClickZoom: false});
	}
}
checkMap();
$(window).resize(function(){
	checkMap();
});
marker.setMap(map);

var infowindow = new google.maps.InfoWindow({
  content:"<center>Kite International Ltd<br />Ian M. Cheyne Building, Wellbank<br /> Peterhead, Aberdeenshire<br />AB42 3GL<br/>01779 482300</center>"
  });
  
google.maps.event.addListener(marker, 'click', function() {
  infowindow.open(map,marker);
  });

infowindow.open(map,marker);
}

google.maps.event.addDomListener(window, 'load', initialize);
</script>    
</body>
</html>