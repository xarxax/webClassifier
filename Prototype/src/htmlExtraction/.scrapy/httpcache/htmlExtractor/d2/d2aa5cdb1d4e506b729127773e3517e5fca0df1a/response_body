                                                                   resizable: false
                                                               });
                                                $dialog.dialog('open');
                                                $(".ui-dialog-titlebar").hide();

                                                $(".modalBackCover").css("width", window.screen.width);
                                                $(".modalBackCover").css("height", window.screen.height);
                                                $(".modalBackCover").css("z-index", 198);
                                                $(".ui-dialog").css("z-index", 199);
                                                $(".modalBackCover").fadeIn(20).removeClass("hide");

                                                $(".modalBackCover").click(function () {
                                                    $(".modalBackCover").fadeOut(20).addClass("hide");
                                                    $dialog.dialog('close');

                                                    return false;
                                                });
                                            });
