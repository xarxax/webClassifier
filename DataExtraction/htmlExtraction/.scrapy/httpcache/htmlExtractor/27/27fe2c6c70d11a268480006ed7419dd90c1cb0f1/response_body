<script type="text/javascript"  data-cookiescript="accepted">

var saveclass = null;

function saveLoc(cookieValue)
{
    var sel = document.getElementById('Location');
    
    saveclass = saveclass ? saveclass : document.body.className;
    document.body.className = saveclass + ' ' + sel.options[sel.value];

    setCookie('Location', cookieValue, 365);

//window.location.href = "http://www.vulcanseals.com";
  window.location.reload();
}

function setCookie(cookieName, cookieValue, nDays) {
    var today = new Date();
    var expire = new Date();

    if (nDays==null || nDays==0)
        nDays=1;

    expire.setTime(today.getTime() + 3600000*24*nDays);
    document.cookie = cookieName+"="+escape(cookieValue) + ";expires="+expire.toGMTString();
}


</script>

<!--Start Cookie Script--> <script type="text/javascript" charset="UTF-8" src="//eu.cookie-script.com/s/0000f2e8ab6f4db7f3a7b9d60e4d4a99.js"></script> <!--End Cookie Script-->


</body>

</html>