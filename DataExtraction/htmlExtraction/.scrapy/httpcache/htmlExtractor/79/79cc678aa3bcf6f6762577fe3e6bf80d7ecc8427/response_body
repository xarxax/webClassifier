        <script language="javascript">
            function isemail(s)
            {   if(!s)return(false);
                var n=s.indexOf("@");
                if(n < 1)return(false);
                n=s.substr(n).indexOf(".");
                if(n<2)return(false);
                return(true);
            }

function FP_popUpMsg(msg) {//v1.0
 alert(msg);
}
