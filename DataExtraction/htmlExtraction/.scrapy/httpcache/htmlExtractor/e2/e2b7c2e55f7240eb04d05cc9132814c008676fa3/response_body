				<td width="328"  class="topRow" valign="middle" align="left"><script type="text/javascript" language="JavaScript">

var clearStat
var city;
var state;
var zip;
/***********************/

function initSearch()
	{
	zip = storeSearch.frmSrchZip.value
	city = storeSearch.frmSrchCity.value
	state = storeSearch.frmSrchState.value
	if(zip == 'Zip')
		{
		zip = ''
		}
	if(city == 'City')
		{
		city = ''
		}
			
	if(!state && !zip)
		{
		alert('Please enter City & State, State or Zip')
		}
	else
		{
		runSearch(city,state,zip)
		}
	}

/***********************/

function runSearch(city,state,zip)
	{				
	params = 'frmSrchState='+state+'&frmSrchCity='+city+'&frmSrchZip='+zip
	//launchStoreDirectoryWin(params)
	var bName = navigator.appName;
	if(bName == 'Microsoft Internet Explorer')
		{
		url = 'https://www.goldenkrust.com/content/?do=contact&amp;sect=storeLocator&'+params;	
		}
	else
		{
		url = 'https://www.goldenkrust.com/content/?do=contact&sect=storeLocator&'+params;
		}
	top.location = url;
	}
	
/***********************/
		
function clearFields()
	{
	if(!clearStat)
		{	
		storeSearch.frmSrchCity.value=''
		storeSearch.frmSrchZip.value = ''
		}
	
	clearStat = 'cleared'
	}
	
</script>

