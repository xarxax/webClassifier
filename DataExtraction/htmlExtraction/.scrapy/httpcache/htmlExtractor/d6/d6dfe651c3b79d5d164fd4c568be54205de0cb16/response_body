  function social_padding() {
    var padding = ($('.site-wrap').width()) + 'px';
    if ($('.site-wrap').width() <= 992) {
      padding = ($('.site-wrap').width() - 28) + 'px';
    }
    $('.social-media-bar__icon').css('padding-left', padding);
  }
