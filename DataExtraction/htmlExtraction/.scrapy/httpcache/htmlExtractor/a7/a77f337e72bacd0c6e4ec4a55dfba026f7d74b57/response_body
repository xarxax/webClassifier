	<script>
		function setrestvalue()
		{
			var restid = $("option:selected", $("#RestaurantID")).val();
			var restname = $("option:selected", $("#RestaurantID")).text();
			var partysize = $("option:selected", $("#bookPartySize")).val();

			$('#RestaurantReferralID').val(restid);
			$('#RestaurantName').val(restname);
			$('#psize').val(partysize);
		}
	</script>
