<script src="js/jquery.mb.YTPlayer-master/jquery.mb.YTPlayer.min.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	$(".indexBanner .indexBannerArea").slick({
		slidesToShow: 1,
		slidesToScroll: 1,
		speed: 500,
  		fade: true,
		autoplay: true,
		autoplaySpeed: 3000,
		dots: true,
		dotsClass: 'slickCus-dots',
		appendDots: $('.indexBanner .customDots'),
		prevArrow: '<button type="button" data-role="none" class="slickCus-prev slick-cusArrow" aria-label="Previous" role="button"><span class="arrows customFonts-arrow-1_left"></span></button>',
		nextArrow: '<button type="button" data-role="none" class="slickCus-next slick-cusArrow" aria-label="Next" role="button"><span class="arrows customFonts-arrow-1_right"></span></button>',
	});
	$('.indexBanner__ItemsArea.video').each(function(){
		$(this).YTPlayer();
		$(this).on('YTPEnd' ,function(){
			$(".indexBannerArea").slick('slickNext');
			$(".indexBannerArea").slick('slickPlay');
		});
	})
	$('.indexBanner__Items').click(function(){
		console.log('index:'+$(this).index());
	});
	$('.indexBannerArea').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		$('.indexBanner__ItemsArea.video').each(function(){
			if($(this).YTPGetPlayer() != null && $(this).YTPGetPlayer() != undefined && $(this).YTPGetPlayer() != ''){
				$(this).YTPStop();
			}
			if( $(this).parent().data('slickIndex') == nextSlide){
				$(".indexBannerArea").slick('slickPause');
				playBannerVideo($(this));
			}
		})
	});
});
function playBannerVideo(ele){
	if(ele.YTPGetPlayer() != null && ele.YTPGetPlayer() != undefined && ele.YTPGetPlayer() != ''){
		ele.YTPPlay();
	}
}
</script>

</body>
