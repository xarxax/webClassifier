                }
        }

//set rollover for positions

function rollpositions(delay)
        {
                reset()
                document.normal.src = positionsImage.src
                document.positions.src = selectedImage4.src
                positionsid=setTimeout("reset()", delay)
        }
        
//set current section to positions

function setpositions()
        {
