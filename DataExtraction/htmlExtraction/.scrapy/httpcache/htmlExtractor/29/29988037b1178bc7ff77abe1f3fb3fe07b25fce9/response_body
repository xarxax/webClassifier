<script type="text/javascript">
$(document).ready(function(){  
    $("form#login").submit(function() {  
    // we want to store the values from the form input box, then send via ajax below  
    var nome = $('#nome-login-form').attr('value');  
    var email = $('#email-login-form').attr('value');
    var comment = $('#comentario-login-form').attr('value');
    
        $.ajax({  
            type: "POST",  
            url: "/en/co/view",
            data: "LoginForm[nome-login-form]="+ nome +"& LoginForm[email-login-form]="+ email +"& LoginForm[comentario-login-form]="+ comment,  
            success: function(){  
                $('form#login').hide();
                $('#success-login-form').show();
            }  
        });  
    return false;  
    });  
});</script>                </div>
