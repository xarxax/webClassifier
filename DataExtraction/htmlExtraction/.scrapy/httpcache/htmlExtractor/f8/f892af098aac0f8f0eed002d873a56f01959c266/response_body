	jQuery(document).ready(function($) {
		
		//cycle vars for top slider
		var featured = $('#top-slider'),
			prev = $('#prev'),
			next = $('#next'),
			p = $('.pager');
				
		//cycle for top slider
		featured.cycle({
			fx: 'scrollHorz',
			speed: 500,
			timeout: 10000,
			prev: prev,
			next: next,
			pager: p,
			pagerAnchorBuilder: function() {
				return '<a href="#"></a>';
			}
		});
		
		$('.fbox').fancybox({
			'titlePosition'		: 'none',
			'transitionIn'		: 'none',
			'transitionOut'		: 'none'
		});
		
	});
</script>
