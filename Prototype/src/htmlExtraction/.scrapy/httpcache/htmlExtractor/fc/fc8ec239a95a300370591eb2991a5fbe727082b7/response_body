				768: {
				
					items: 4,
					
				}
			}
		};
		var owl8 = $('#owl-carousel-expertise');
		owl8.owlCarousel(owl_carousel_expertise);
		$("#expertise .ex-next").on('click', function () {
			owl8.trigger('next.owl.carousel');
		});
		$("#expertise .ex-prev").on('click', function () {
			owl8.trigger('prev.owl.carousel');
		});
        });
