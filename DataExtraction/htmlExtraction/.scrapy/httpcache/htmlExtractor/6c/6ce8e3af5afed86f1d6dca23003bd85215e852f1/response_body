_findObj(n,d.layers[i].document);  if(!x && d.getElementById) x=d.getElementById(n); return x;}function MM_swapImage() { //v3.0  var i,j=0,x,a=MM_swapImage.arguments; document.MM_sr=new Array; for(i=0;i<(a.length-2);i+=3)   if ((x=MM_findObj(a[i]))!=null){document.MM_sr[j++]=x; if(!x.oSrc) x.oSrc=x.src; x.src=a[i+2];}}</script><link href="styles/danz.css" rel="stylesheet" type="text/css" /><style>#header{		position: relative;}#leftCol{	background-image: none;}#main {	padding-top: 0px;}body,td,th {	font-size: 12px;	color: #333;}#news p, #news h5 {	font-size: 11px;	line-height: 14px;	padding: 0px 9px;	margin: 0px;}#news h5 {	padding: 0px 9px;	margin: 0px;}#mcontent {	margin-left: 210px;	padding-top: 0px;}#main {	background-image: url(images/page_bg2.jpg);	background-repeat: no-repeat;	height: 500px;	background-position: 0px 0px;	width: 1037px;}#footer {		padding-left: 0px;}.videoLayer {	width: 620px;	height: 410px;	position:absolute;		z-index:1;	left: 160px;	top: 180px;	background-color: #333;	padding: 5px 10px 10px;	visibility: hidden;}</style><script>function showVideo(which) {	if (which == '1') { video = "cNKZGuB-BE8" } else { video = "WTHRlAIWjRw" }	document.getElementById('videoEmbed' + which).innerHTML = '<iframe width="615" height="346" src="http://www.youtube.com/embed/' + video + '" frameborder="0" allowfullscreen></iframe>';	document.getElementById('videoPlayer' + which).style.visibility='visible';}function hideVideo(which) {			document.getElementById('videoEmbed' + which).innerHTML = "";		document.getElementById('videoPlayer' + which).style.visibility='hidden';	}</script></head><body onload="MM_preloadImages('images/eye.gif','images/home_video_2_roll.jpg','images/closex_ro.gif')"><div id="container">	<div  id="header">    <div>    <div style="float: left; width: 815px;"><img src="images/header.gif" alt="Phillip A. Danz &amp; Associates, Inc.  Hand-sculpted and hand-painted 