  function setActive() {
    activeButton = document.getElementById('js-nav').getElementsByTagName('a');
    for(i=0; i < activeButton.length; i++) {
      if(document.location.href.indexOf(activeButton[i].href) >= 0) {
        activeButton[i].className = 'active';
      }
    }
  }
  
  $(document).ready(function () {
  setActive();
  });
</script>
