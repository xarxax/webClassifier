<script>
function checkform()
{
	var S;
	S = document.getElementById("username");
	if (S.value == "")
	{
		alert("用户名不可为空");
		S.focus();
		return false;
	}
	S = document.getElementById("pwshow");
	if (S.value == "")
	{
		alert("密码不可为空");
		S.focus();
		return false;
	}
}

function gook() {
		var S;
	S = document.getElementById("username");
	if (S.value == "")
	{
		alert("用户名不可为空");
		S.focus();
		return false;
	}
	S = document.getElementById("pwshow");
	if (S.value == "")
	{
		alert("密码不可为空");
		S.focus();
		return false;
	}
	document.getElementById("pwhidden").value = encode(document.getElementById("pwshow").value, parseInt(document.getElementById("picnum").value));
	var mailName=document.getElementById("username").value;
	var mailNameIndex=mailName.split("@")[0];
	document.getElementById("username").value=mailNameIndex+"@xinqili.com";
	//f1.action="default.asp";
	f1.submit();
}

function encode(datastr, bassnum) {
	var tempstr;
	var tchar;
	var newdata = "";

	for (var i = 0; i < datastr.length; i++)
	{
		tchar = 65535 + bassnum - datastr.charCodeAt(i);
		tchar = tchar.toString();

		while(tchar.length < 5)
		{
			tchar = "0" + tchar;
		}

		newdata = newdata + tchar;
	}

	return newdata;
}

function checkimg(obj){
var img=new Image();
img.src=obj.src;
var imgw = img.width;
var imgh = img.height;
if(imgw*165<imgh*207){
if(imgh>165){
obj.height=165;
}else{
obj.height=imgh
}
}else{
if(imgw>207){
obj.width=207;
}else{
obj.width=imgw
}
}
}
</script>
