<script type="text/javascript"><!--

function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}
function changeImagesArray(array) {
	if (preloadFlag == true) {
		var d = document; var img;
		for (var i=0; i<array.length; i+=2) {
			img = null; var n = array[i];
			if (d.images) {img = d.images[n];}
			if (!img && d.getElementById) {img = d.getElementById(n);}
			if (img) {img.src = array[i+1];}
		}
	}
}
function changeImages() {
	changeImagesArray(changeImages.arguments);
}

// --></script>
<script type="text/javascript"><!--
var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		pre_bandbrasilhi = newImage('/index/bandbrasilhi.gif');
		pre_bandusahi    = newImage('/index/bandusahi.gif');
		pre_bandchinahi  = newImage('/index/bandchinahi.gif');
		pre_bandjapanhi  = newImage('/index/bandjapanhi.gif');
		pre_bandkoreahi  = newImage('/index/bandkoreahi.gif');
		preloadFlag = true;
	}
}

// --></script>
<script type="text/javascript">
function trackOutboundLink(link) { 
 
	try { 
	_gaq.push(['_trackEvent', 'Links Externos' , link.href]); 
	} catch(err){}
 
	setTimeout(function() {
	document.location.href = link.href;
	}, 100);
}
</script>
