<script type='text/javascript'>//<![CDATA[ 
$(function(){
var shadow = $('<div id="shadowElem"></div>');
var speed = 1000;
$(document).ready(function () {
    $('body').prepend(shadow);
});
$(window).load(function () {
    screenHeight = $(window).height();
    screenWidth = $(window).width();
    elemWidth = $('#dropElem').outerWidth(true);
    elemHeight = $('#dropElem').outerHeight(true);

    leftPosition = (screenWidth / 2) - (elemWidth / 2);
    topPosition = (screenHeight / 2) - (elemHeight / 2);
    //console.log(screenHeight);

    $('#dropElem').css({
        'left': leftPosition + 'px',
            'top': -elemHeight + 'px'
    });
    $('#dropElem').show().animate({
        'top': topPosition
    }, speed);
	 document.body.style.overflow="hidden"


    shadow.animate({
        'opacity': 0.7
    }, speed);

    $('#dropClose').click(function () {
        shadow.animate({
            'opacity': 0
        }, speed);
		 document.body.style.overflow="scroll";
        $('#dropElem').animate({
            'top': -elemHeight + 'px'
        }, speed, function () {
            shadow.remove();
            $(this).remove();
        });
        shadow.remove();
			 document.body.style.overflow="auto"

    });
});
});//]]>  

</script>
