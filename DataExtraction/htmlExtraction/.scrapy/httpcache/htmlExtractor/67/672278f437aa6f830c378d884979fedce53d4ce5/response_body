<script src="https://use.typekit.net/olr1jvi.js"></script>
<script>try{Typekit.load({ async: true });}catch(e){}</script>
	
	<script src="/js/owl.carousel.js"></script>
<script>
$(document).ready(function() {
			$('#owl_carousel').owlCarousel({
			loop:true,
			margin:10,
			responsiveClass:true,
			responsive:{
				0:{
					items:2,
				},
				600:{
					items:3,
				},
				1400:{
					items:4,
				},
				1650: {
					items:5,
				}
			}
		});
		
		$('#owl_slider').owlCarousel({
			//animateOut: 'fadeOut',
			loop:true,
			autoplay: true,
			responsiveClass:true,
			responsive:{
				0:{
					items:1,
				},
				600:{
					items:1,
				},
				1000:{
					items:1,
				}
			}
		});
	
		$('#sub_menu').click(function(){ 
			$("nav ul ul").slideToggle();
			$(this).addClass('active').parent('active').removeClass('active');
		});
	
		$('.featured_section_in ul li').click(function(){
			id_array = $(this).attr('id').split('_'); // This splits the id of the trigger-overlay_x (where x is a counting integer) 
			// from line 93 in home.php
			$('.workfor_' + id_array[1] ).animate({left:0}, 400); // the number is then used to open up workfor_x (where x is
			// the same corresponding number)
			$('.page-wrap').addClass("transform");
		});
	
		$('.closework').click(function(){
			id_array = $(this).attr('id').split('_'); // This splits the unique class name of the X that closes the overlay
			// Which gives us the unique counting number that corresponds to the same counting number involved in opening the overlay 
			$('.workfor_' + id_array[1] ).animate({left:"-100%"}, 400);
			$('.page-wrap').removeClass("transform");
		});
		
	$(".toggle-nav").click(function(){
		  $("nav").slideToggle("slow");
		  $(this).toggleClass("change");
		  $(this).toggleClass('active');
    });
});
	</script>
<!-- END: SCRIPTS -->
