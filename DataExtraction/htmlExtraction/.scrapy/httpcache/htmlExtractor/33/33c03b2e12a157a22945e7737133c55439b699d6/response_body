<script>
jQuery(document).ready(function($) {
    $('img[title]').each(function() { $(this).removeAttr('title'); });
    $('a[title]').each(function() { $(this).removeAttr('title'); });

    $( ".taptap-menu-button-wrapper" ).click(function() {
        if ($(this).hasClass("taptap-menu-active")) {
            $("html").addClass("taptap-menu-active");
        } else {
            $("html").removeClass("taptap-menu-active");
        }
    });
    
    $('.flexslider').flexslider({
        slideshow: false
    });
    
    function resetSliderVideo() {
        $('.video iframe').each(function (){
            var src= $(this).attr('src');
            //$(this).fadeOut('fast', function() {
                $(this).attr('src', src);
            //});
            //$(this).fadeIn();
        });
    }
    
    $('a.flex-prev, a.flex-next').on('click', function() {
        //$('.flexslider').flexslider('pause');
        resetSliderVideo();
    });
});
