<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="js/jquery.min.js"><\/script>')</script>
<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script>
<script type="text/javascript" src="https://ajax.microsoft.com/ajax/jquery.validate/1.9/jquery.validate.min.js"></script>
<script type="text/javascript" src="https://www.emjmetals.com/bootstrap/js/bootstrap.min.js"></script>
<script type="text/javascript" src="https://www.emjmetals.com/js/functions.js"></script>
<script type="text/javascript" src="https://www.emjmetals.com/js/animations.js"></script>
		
<script> 
(function($){
	$(function(){

		/* active states */
		var windowURL = location.href;
		var $home = "";
		var $family = 'null';
		var removeProductActive = false;
		var subnavSelected = false;
		var $products = '';
		
		$('a[href]').each(function(){
			var url = $(this).attr('href');
				// remove trailing slash from href
				url = stripTrailingSlash(url);
				// remove trailing slash from windowURL
				windowURL = stripTrailingSlash(windowURL);
			var parent = $(this).parent();
				
			if(windowURL.indexOf(url) >-1)
			{
				if( 		url != 'https://www.emjmetals.com/'
					&& 	url.indexOf('https://www.emjmetals.com/about') != -1 
					&&	$(this).text() != "About EMJ"
				)
					$(this).addClass('active');
				else if( url == windowURL)
					$(this).addClass('active');
			}
			if($family != 'null' && url.indexOf($family) !== -1) 
			{
				$(this).addClass('active');
				removeProductActive = true;
				subnavSelected = true;
			}
			else if( windowURL.indexOf('1') !== -1 )
				subnavSelected = true;
			else if( windowURL.indexOf('2') !== -1 )
				subnavSelected = true;
			else if( windowURL.indexOf('3') !== -1 )
				subnavSelected = true;
			else if( windowURL.indexOf('4') !== -1 )
				subnavSelected = true;
				
			if( $(this).text() == "Home" )
			{
				$home = $(this);
				if( windowURL !== url )
				{	
					$home.removeClass('active');
				}
			}			
			if( $(this).text() == "Products" )
			{
				$products = $(this);
				if( windowURL !== url )
					$products.removeClass('active');
					
				if (windowURL.indexOf(url) != -1)
				{
					$products.addClass('active');
					removeProductActive = false;
				}
			}

		});
		//	console.log(removeProductActive);
	//		console.log(subnavSelected);
			if( $products != '')
			{
				if( $products.hasClass('not-active') && removeProductActive)
				{
					$products.removeClass('active');
				}
				else if(subnavSelected)
				{				
					$products.removeClass('active');
				}
			}
	});
})(jQuery);
/*
<!--[if (gte IE 6)&(lte IE 8)]>
  <script type="text/javascript" src="js/selectivizr/selectivizr-min.js"></script>
  <noscript><link rel="stylesheet" href="[fallback css]" /></noscript>
<![endif]-->
*/
</script>
