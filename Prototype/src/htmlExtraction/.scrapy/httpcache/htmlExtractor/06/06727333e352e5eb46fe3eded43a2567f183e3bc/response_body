<script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-2002990-11']);
    _gaq.push(['_trackPageview']);
    _gaq.push(['_trackPageLoadTime']);

    (function () {
        var ga = document.createElement('script');
        ga.type = 'text/javascript';
        ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);
    })();

    jQuery(document).ready(function ($) {
        $(document).on('click', '[data-track=\"click\"]', function () {
            doGoogleGaqPush($(this));
        });

        $(document).on('click', '[data-analytics-onclick]', function() {
            doGoogleGaqPush($(this));
        });

        if ($('[data-analytics-onload]').length > 1) {
            $.each('[data-analytics-onload]', function() {
                doGoogleGaqPush($(this));
            });
        } else if ($('[data-analytics-onload]').length == 1)
        {
            doGoogleGaqPush($('[data-analytics-onload]'));
        }
    });

    function doGoogleGaqPush(element) {
        var category = element.attr('data-track-category');
        var action = element.attr('data-track-action');
        var label = element.attr('data-track-label');
        doGoogleGaqPushWithParam(category, action, label);
    }

    function doGoogleGaqPushWithParam(category, action, label) {
        if (label == '')
        {
            if (action == '')
            {
                _gaq.push(['_trackEvent', category, null]);
            } else
            {
                _gaq.push(['_trackEvent', category, action]);
            }
        } else
        {
            _gaq.push(['_trackEvent', category, action, label]);
        }
        if (window.console) console.log('-GoogleAnalyticsCode- Category:' + category + ', Action:' + action + ', Label:' + label);
    }
</script>

    
