}

#monialsfader #bottom-link a{
	width:100%;
	color:#005500;
	font-size:10px;
	text-align:Right;
	font-weight:Normal;
	font-style:Normal;
	font-family:;
	float:left;
}

#monialsfader #monials-top, #monialsfader #monialsfader_next, #monialsfader #monialsfader_prev{
	float:left;
	position:absolute;
	z-index:15;
	text-align: left;
}

#monialsfader #monials-top{
	left:0px;
	top:0px;;
}

