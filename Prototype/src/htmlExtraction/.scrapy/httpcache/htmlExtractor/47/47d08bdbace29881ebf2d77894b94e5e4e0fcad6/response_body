<script>
    $(document).ready(function () {
        var urlPath = window.location.pathname;

        $(".toggle-nav").on('click', function () {
            $("html").toggleClass("nav_is-active");
        });

        if (urlPath != '/') {
            $('.hero-simple').addClass('js-inner-hero');
        };

        if (urlPath == '/') {
            $('body').addClass('home');
        }

        if (urlPath == '/faq') {
            $('body').addClass('faq');
        }

        if (urlPath == '/our-story') {
            $('body').addClass('our-story');
        }

        if (urlPath.indexOf('products') !== -1) {
            $('body').addClass('products');
        }

        if (urlPath.indexOf('login') !== -1 || urlPath.indexOf('create-account') !== -1) {
            $('body').addClass('login');
        }

        $(".site-search .ico").click(function () {
            $("body").toggleClass("search_is-active");
        });

        var impCarousel = $('.imp-carousel');

        if (impCarousel.length > 0) {

            var impCarouselSlides = $('.imp-carousel__slide');

            var debounce = function (func, wait, immediate) {
                var timeout;
                return function () {
                    var context = this, args = arguments;
                    var later = function () {
                        timeout = null;
                        if (!immediate) func.apply(context, args);
                    };
                    var callNow = immediate && !timeout;
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                    if (callNow) func.apply(context, args);
                };
            };

            $(window).on('resize', debounce(init, 200));

            function wrapSlides() {
                var clientWidth = document.documentElement.clientWidth;

                if (impCarouselSlides.parent().is(".imp-carousel__slide-wrapper")) {
                    impCarouselSlides.unwrap();
                }

                if (clientWidth < 767) {
                    impCarouselSlides.wrap("<div class='imp-carousel__slide-wrapper'></div>");
                } else if (clientWidth < 1023) {
                    for (var i = 0; i < impCarouselSlides.length; i += 2) {
                        impCarouselSlides.slice(i, i + 2).wrapAll("<div class='imp-carousel__slide-wrapper'></div>");
                    }
                } else {
                    for (var i = 0; i < impCarouselSlides.length; i += 3) {
                        impCarouselSlides.slice(i, i + 3).wrapAll("<div class='imp-carousel__slide-wrapper'></div>");
                    }
                }
            }

            function init() {
                wrapSlides();

                var impCarouselWrappedSlides = $('.imp-carousel__slide-wrapper');
                impCarouselWrappedSlides.addClass('-future');
                impCarouselWrappedSlides.first().removeClass('-future');
                impCarouselWrappedSlides.first().addClass('-present');
            }

            init();

            $(
                '<ul class="imp-carousel__controls">' +
                '<li class="_prev"></li>' +
                '<li class="_next"></li>' +
                '</ul>'
            ).appendTo(impCarousel);

            var impCarouselControlPrev = $('.imp-carousel__controls ._prev');
            var impCarouselControlNext = $('.imp-carousel__controls ._next');

            function isPresent(element) {
                return element.className.includes('-present');
            }

            function getCurrentSlideIndex() {
                var impCarouselWrappedSlides = $('.imp-carousel__slide-wrapper');
                var slidesArray = $.makeArray(impCarouselWrappedSlides);
                return slidesArray.findIndex(isPresent);
            }

            impCarouselControlNext.on('click', function () {
                setNextSlide(getCurrentSlideIndex());
            });

            impCarouselControlPrev.on('click', function () {
                setPrevSlide(getCurrentSlideIndex());
            });


            //var touchstartX = 0;
            //var touchendX = 0;

            //impCarousel.on('touchstart', function (event) {
            //    touchstartX = event.touches[0].pageX;
            //});

            //impCarousel.on('touchend', function (event) {
            //    touchendX = event.originalEvent.changedTouches[0].pageX;
            //    handleSwipe();
            //});

            //function handleSwipe() {
            //    if (touchendX < touchstartX) {
            //        setNextSlide(getCurrentSlideIndex());
            //    }
            //    if (touchendX > touchstartX) {
            //        setPrevSlide(getCurrentSlideIndex());
            //    }
            //}

            function setNextSlide(index) {
                var impCarouselWrappedSlides = $('.imp-carousel__slide-wrapper');
                var limit = impCarouselWrappedSlides.length - 1;
                var newIndex = index < limit ? index + 1 : 0;
                var currentSlide = $('.-present');
                var nextSlide = $(impCarouselWrappedSlides.get(newIndex));

                impCarouselWrappedSlides.removeClass('-present');

                if (newIndex === 0) {
                    impCarouselWrappedSlides.removeClass('-past');
                    impCarouselWrappedSlides.addClass('-future');
                } else {
                    currentSlide.addClass('-past');
                }

                nextSlide.removeClass('-future');
                nextSlide.removeClass('-past');
                nextSlide.addClass('-present');
            }

            function setPrevSlide(index) {
                var impCarouselWrappedSlides = $('.imp-carousel__slide-wrapper');
                var limit = impCarouselWrappedSlides.length - 1;
                var newIndex = index > 0 ? index - 1 : limit;
                var currentSlide = $('.-present');
                var prevSlide = $(impCarouselWrappedSlides.get(newIndex));

                impCarouselWrappedSlides.removeClass('-present');

                if (newIndex === limit) {
                    impCarouselWrappedSlides.removeClass('-future');
                    impCarouselWrappedSlides.addClass('-past');
                } else {
                    currentSlide.addClass('-future');
                }

                prevSlide.removeClass('-past');
                prevSlide.removeClass('-future');
                prevSlide.addClass('-present');
            }
        }
    });
</script>
    
