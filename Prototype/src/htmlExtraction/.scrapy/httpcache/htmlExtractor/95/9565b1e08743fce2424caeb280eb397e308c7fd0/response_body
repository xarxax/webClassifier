		 <div class="row fair-value">
			<div class=" medium-12 large-12 columns">
			  <h2>
				DJIA Fair Value Analysis &nbsp; <a href="https://members.stocktradersdaily.com/reports/focusStock/stock/dow30" class="button radius secondary">View More</a>
			  </h2>
			  <p>DJIA EPS, DJIA PE Ratio, DJIA Peg Ratio:</p>
			  <p><script type="text/javascript">
$(function () {
	var chart;
	$(document).ready(function() {
	  Highcharts.setOptions({
		  colors: ['#e0ad13'],
	  });
  
      var averageLabel =[1286251200000, 1294203600000, 1301976000000, 1309838400000, 1317787200000, 1325739600000, 1333598400000, 1341460800000, 1349409600000, 1357362000000, 1365134400000, 1372996800000, 1380945600000, 1388898000000, 1396670400000, 1404532800000, 1412481600000, 1420434000000, 1428206400000, 1436068800000, 1444017600000, 1451970000000, 1459828800000, 1467691200000, 1475640000000, 1483592400000, 1491364800000, 1499227200000, 1507176000000, 1515128400000, 1522900800000, 1530763200000, 1538712000000, 'THIS YR', 'NEXT YR'];
      var growthData   = [20.86, 21.30, 17.99, 18.45, 14.23, 12.23, 9.21, 6.15, 3.63, 0.61, -0.46, 0.76, 5.49, 8.88, 12.10, 10.12, 6.47, 2.70, -2.23, -5.68, -9.33, -12.75, -13.59, -8.12, 4.00, 17.02, 20.35, 15.38, 10.25, 12.06, 15.26, 19.47, 12.75, '-15.35##red', '10.78##red'];
      for(var i = 0; i < growthData.length; i++){
			if (typeof growthData[i] == 'string') {
            var valData = growthData[i].split('##');             
            var dtpoint = parseFloat(valData[0]);
            var dtcolor = valData[1];
          } else {
            var dtpoint = growthData[i];
            var dtcolor = "";
          }
					growthData[i] =  {//x: categories[i],
                         y:dtpoint,
                         color:dtcolor}
      }
	  var grwYearValue = [12.06,15.26,19.47,12.75,'-15.35##red','10.78##red'];
	  for(var i = 0; i < grwYearValue.length; i++){
		if (typeof grwYearValue[i] == 'string') {
            var valData = grwYearValue[i].split('##');             
            var dtpoint = parseFloat(valData[0]);
            var dtcolor = valData[1];
          } else {
            var dtpoint = grwYearValue[i];
            var dtcolor = "";
          }
		  grwYearValue[i] =  {y:dtpoint,color:dtcolor}
      }

	  var grwThrdYearValue = [-12.75,-13.59,-8.12,4.00,17.02,20.35,15.38,10.25,12.06,15.26,19.47,12.75,'-15.35##red','10.78##red'];
	  for(var i = 0; i < grwThrdYearValue.length; i++){
		if (typeof grwThrdYearValue[i] == 'string') {
            var valData = grwThrdYearValue[i].split('##');             
            var dtpoint = parseFloat(valData[0]);
            var dtcolor = valData[1];
          } else {
            var dtpoint = grwThrdYearValue[i];
            var dtcolor = "";
          }
		  grwThrdYearValue[i] =  {y:dtpoint,color:dtcolor}
      }


      var chartY = new Highcharts.Chart({
        
        chart: {
          height: 400,
          renderTo: 'highchart-growth-container2',
          type: 'line',
          marginBottom: 100,
          marginTop: 100,
          marginLeft: 20,
          marginRight: 40,
         events: {
                  load: function() {
                  this.renderer.image('https://members.stocktradersdaily.com/images/logo.png',40,60,187,40).add();
                }
            }
        },

        title: {
          text: " Growth Rate - Yearly",
          align: 'center',
          x: 10,
          y: 20,
          style: {
            fontWeight: 'bold'
          }
        },
	    navigation: {
            buttonOptions: {
                theme: {
				 'stroke-width': 1,
				  'class':'highcharts-button',
				   stroke: 'silver',
                }
            }
        },
		exporting: {
            buttons: {
                contextButton: {
                    enabled: true
                }, 
                allButton: {
                    text: 'ALL',
                    onclick: function() {
                        yearlyCallback('all');
                    }
                },
                oneyrButton: {
                    text: '3 Yr',
                    onclick: function () {
                        yearlyCallback('3')	 ;
                    }
                },
                threeButton: {
                    text: '1 Yr',
                    onclick: function () {
                        yearlyCallback('1') ;
                    }
                }
            }
        }, 
        xAxis: {
					categories: averageLabel,
          type: 'datetime',
          labels: {
			   formatter: function () {
				  var s = "";
				  if (typeof this.value == 'string') {
					s = this.value
				  };
				  if (Highcharts.dateFormat('%b', this.value) == 'Jan') {
					  s = Highcharts.dateFormat('%Y', this.value) + "-";
					  s = s + "Q1"
				  };
				  if (Highcharts.dateFormat('%b', this.value) == 'Apr') {
					  s = Highcharts.dateFormat('%Y', this.value) + "-";
					  s = s + "Q2"
				  };
				  if (Highcharts.dateFormat('%b', this.value) == 'Jul' ) {
					  s = Highcharts.dateFormat('%Y', this.value) + "-";
					  s = s + "Q3"
				  };  
				  if (Highcharts.dateFormat('%b', this.value) == 'Oct' ) {
					  s = Highcharts.dateFormat('%Y', this.value) + "-";
					  s = s + "Q4"
				  };
				  //s = Highcharts.dateFormat('%Y', this.value) + "-" + s;
				  return s;
			  },
				rotation: -45,
            },
            //tickInterval: 3 * 30 * 24 * 3600 * 900,
        },
       tooltip: {
           formatter: function () {
                var s = '';
                if (Highcharts.dateFormat('%b', this.x) == 'Jan') {
                    s = s + "Q1"
                };
                if (Highcharts.dateFormat('%b', this.x) == 'Apr') {
                    s = s + "Q2"
                };
                if (Highcharts.dateFormat('%b', this.x) == 'Jul') {
                    s = s + "Q3"
                };
                if (Highcharts.dateFormat('%b', this.x) == 'Oct') {
                    s = s + "Q4"
                };

							if (typeof this.x == 'string') {
								s = this.x;
											}  else {
								s = Highcharts.dateFormat('%Y', this.x) + '-'+ s;
							}

                $.each(this.points, function (i, point) {
                    s += '<br/>' + point.series.name + ': <b>';

                        s += Highcharts.numberFormat(point.y, 2, '.');
                        s += '%</b>';

                });
                return s;
            },
            shared: true,
            valueDecimals: 2,
            valueSuffix: ' %'
         },
        yAxis: {
            endOnTick: true,
            allowDecimals: false,
            min: -15.35,
            opposite: true,
            labels: {
				formatter: function() {
				  return this.value+'%';
				}
           },      
          title: {
            text: ''
          },
        },
        credits: {
			   enabled: false,
        },
        series: [{
            data: growthData,
            name: "Trailing 12 month EPS growth"
        }],

      });
		 function yearlyCallback(val)
		 {
				chartY.series[0].remove(false);
				if(val == '1') {
							chartY.xAxis[0].setCategories([1515128400000,1522900800000,1530763200000,1538712000000,'THIS YR','NEXT YR'], false);
							chartY.addSeries({
							data: grwYearValue,
							marker: {
								symbol: 'circle'
							},
							name: "Trailing 12 month EPS growth"
						}, true, {
							duration: 200   
						}
					);
				} else if(val == '3') {
						chartY.xAxis[0].setCategories([1451970000000,1459828800000,1467691200000,1475640000000,1483592400000,1491364800000,1499227200000,1507176000000,1515128400000,1522900800000,1530763200000,1538712000000,'THIS YR','NEXT YR'], false);
						chartY.addSeries({
						data: grwThrdYearValue,
						marker: {
							symbol: 'circle'
						},
						name: "Trailing 12 month EPS growth"
									}, true, {
											duration: 200   
					}
				);
				} else {
						chartY.xAxis[0].setCategories(averageLabel, false);
						chartY.addSeries({
						data: growthData,
						marker: {
							symbol: 'circle'
						},
						name: "Trailing 12 month EPS growth"
									}, true, {
											duration: 200   
					}
				);
			}	
	   }
		
	});
});
</script>
	<div id="highchart-growth-container2" style="min-width: 330px; max-width:1091px; height:400px; margin: 0 auto">Loading...</div>
	
</p>
			</div>
		  </div><!--end fair-value-->
