<script>
function jumpfund()
{
	// getelement id 
	var text1 = document.getElementById('FUND');
	
	var sel = text1.selectedIndex;


if (sel <=0)
	{
	}
else
	{
		window.location = "fund.php?id="+sel+'#performance';
	}	 	
	
}


function jumpfund2()
{
	// getelement id 
	var text2 = document.getElementById('FUND2');
	
	var sel2 = text2.selectedIndex;


if (sel2 <=0)
	{
	}
else
	{
		window.location = "fund.php?id="+sel2+'#performance';
	}	 	
	
}

</script>