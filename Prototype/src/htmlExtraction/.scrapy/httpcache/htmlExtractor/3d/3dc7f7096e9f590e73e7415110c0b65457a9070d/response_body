<script type="text/javascript">
    $(window).bind('load', function () {
        var maxHeight = -1;
        $('.homepage #content .featuredBlocks .contentBlock').each(function () {
            maxHeight = Math.max(maxHeight, $(this).height());
        });
        $('.homepage #content .featuredBlocks .contentBlock').each(function () {
            $(this).height(maxHeight);
        });
    });
    $(document).ready(function () {
        if ($('body').hasClass('mobile')) {
            $('#cb1 img').click(function () {
                window.open('https://youtu.be/nCyJLi1g1GE?rel=0');
            });
        } else {
            $('#cb1 img').click(function () {
                GB_show('', 'https://www.youtube.com/embed/nCyJLi1g1GE?rel=0', 800, 600);
            });
        }

        var vWidth = $(window).width();
        $('.slideRight').width(vWidth/2 - 200);

        $('.slideRight').mouseenter(function () {
            $(this).animate({ "margin-left": "-315px", "width": (vWidth / 2 + 315) }, "slow");
            $(this).addClass('actionLeft');
        });
        $('.slideRight').mouseleave(function (e) {

            var oslideRight = this;

            if ($(this).hasClass('actionLeft')) {
                setTimeout(function () {
                    $(oslideRight).animate({ "margin-left": "200px" , "width": (vWidth / 2 - 200) }, "slow");
                }, 1000);
            }
            e.stopPropagation(e);
        });

    });


    var divs = $('div[id^="slide-"]').hide(),
    i = 0;

    (function cycle() {
        divs.eq(i).fadeIn(400)
                  .delay(9000)
                  .fadeOut(400, cycle);

        i = ++i % divs.length; // increment i,
        //   and reset to 0 when it equals divs.length
    })();
</script>

