<script type="text/javascript">
    function toogleMenuPro_menumobile_3400(){
        var $=jQuery;
        var container = $("#toogle_menu_menumobile_3400");
        var textClick = $("#displayMenu_menumobile_3400");
        if(jQuery('body').hasClass('halochange_0') == true){
            textClick.show();
			container.hide();           
			textClick.unbind('click');
            textClick.bind('click',function(){
                container.slideToggle();
            });
        }else{
            textClick.hide();
            container.show();
        }
    };	
	jQuery(window).load(function(){
		setTimeout(function(){toogleMenuPro_menumobile_3400()},300);
	});			
    jQuery(window).bind('halochange orientationchange', function() {
        setTimeout(function(){toogleMenuPro_menumobile_3400()},300);
    });
</script>
