<script>
$(function(){  
  /*開啟訂單查詢*/
  $(document).on('click','.orderToggle',function(){
    $.blockUI({
      message:$('.orderCheckArea'),
      css:{cursor:null},
      onOverlayClick: $.unblockUI,
      onBlock:function(){
        $('input').blur();
      }
    });
  });
  
  /*關閉訂單查詢*/
  $('#closeOrder').click(function(){
    $.unblockUI(); 
    return false; 
  });
  
  /*開啟購物清單*/
  $('.cartToggle').click(function(){
    $.blockUI({
      message:$('.cartArea'),
      css:{cursor:null},
      onOverlayClick: $.unblockUI
    });
  });
  
  /*關閉購物清單*/
  $('#closeCart').click(function(){
    $.unblockUI(); 
    return false;
  });
  get_page_buycar();
});
</script>  
