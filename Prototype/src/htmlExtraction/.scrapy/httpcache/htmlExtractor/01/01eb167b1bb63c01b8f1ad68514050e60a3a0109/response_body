    <script type="text/javascript">
    $('#nav-home').addClass('active');

    function isBreakpoint( alias ) {
      return $('.device-' + alias).is(':visible');
    }

$(function(){
    var slidesId = '';
    if( isBreakpoint('xs') ) {
      slidesId = 'mobile_slides'; 
    }
    else {
      slidesId = 'desktop_slides';
    }
    $("#" + slidesId).slidesjs({
      navigation: {
        active:false,
        effect:'slide'
      },
      play: {
        active: true,
        effect: "slide",
        interval: 5000,
        auto: true,
        swap: true,
        pauseOnHover: false,
        restartDelay: 2500
      },
      effect: {
        slide: {
          speed: 1500
        }
      }
    });
});
</script>
