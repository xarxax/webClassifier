Goshen, Indiana. (Nov. 5, 2017) – Goshen Stamping, LLC announced the successful completion...</p><div class="date">Apr 11, 2018</div><a class="readMore" href="/blog/?p=76">Read More</a></div><a class="moreNews" href="/blog">More News</a></div><script>
var gallery = {
        gallery:null,
        index:null,
        interval: null,
        count:0,
        current:0,
        next:1,
        direction:'left',
        animation0:null,
        animation1:null,
        init: function() {
                gallery.gallery = document.getElementById('home-gallery');
                gallery.count = gallery.gallery.children.length;
                
                function setupClick(div, index) {
                        div.onmousedown = stop;
                        div.onclick = function(e) {
                                stop(e);
                                if (gallery.interval) {
                                        clearInterval(gallery.interval);
                                        gallery.interval = null;
                                }
                                if (gallery.current == index)
                                        return;
                                gallery.next = index;
                                gallery.direction = gallery.next > gallery.current ? 'left' : 'right';
                                gallery.slide();
                        };
                }
                
                gallery.index = document.getElementById('home-gallery-index');
                for (var j = 0; j < gallery.count; j++) {
                        var div = document.createElement('div');
                        div.className = 'home-gallery-index-item'+(j == 0 ? ' active' : '');
                        gallery.index.appendChild(div);
                        setupClick(div, j);
                }
                
                gallery.interval = setInterval(gallery.next, 4000);
        },
        next: function() {
                if (gallery.count < 2) return;
                
                gallery.next = gallery.current + 1;
                if (gallery.next >= gallery.count)
                        gallery.next = 0;
                
                gallery.direction = 'left';
                
                gallery.slide();
        },
        previous: function() {
                if (gallery.count < 2) return;
                
                gallery.next = gallery.current - 1;
                if (gallery.next < 0)
                        gallery.next = gallery.count - 1;
                        
                gallery.direction = 'right';
                
                gallery.slide();
        },
        slide: function() {
                if (gallery.animation0)
                        gallery.animation0.kill();
                if (gallery.animation1)
                        gallery.animation1.kill();
                                        
                for (var j = 0; j < gallery.index.children.length; j++)
                        gallery.index.children[j].className = 'home-gallery-index-item' + (j == gallery.next ? ' active' : '');
                
                gallery.animation0 = animate(gallery.gallery.children[gallery.current], 'left', '%', 0, gallery.direction == 'left' ? -100 : 100, 500);
                gallery.animation1 = animate(gallery.gallery.children[gallery.next], 'left', '%', gallery.direction == 'left' ? 100 : -100, 0, 500);
                
                gallery.current = gallery.next;
        }
};
gallery.init();
</script> </div>
