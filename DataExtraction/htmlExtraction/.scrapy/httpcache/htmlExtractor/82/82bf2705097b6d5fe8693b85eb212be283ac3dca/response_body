<SCRIPT language=JavaScript>
function ModifyFormAction(actionvalue)
{
      document.search.action = actionvalue;
}
//compact, stand-alone version of trim()
function trim(sVal)
{
 sTrimmed = "";
    
 for (i = 0; i < sVal.length; i++)
 {
  if (sVal.charAt(i) != " "
   && sVal.charAt(i) != "\f"
   && sVal.charAt(i) != "\n"
   && sVal.charAt(i) != "\r"
   && sVal.charAt(i) != "\t")
  {
   sTrimmed = sTrimmed + sVal.charAt(i);
  }
 }
 return sTrimmed;
}
function checkKeyword() 
{
   var keyword = document.search.keyword.value;
   if (keyword == "keyword(s) or Item #") {
      keyword = "";
   }
   keyword = trim(keyword);
   if (keyword == "") {
      // something is wrong
      alert('You must enter a search value.');
      return false;
   }
}
</SCRIPT>

