	<script type="text/javascript">
	function extractDomain(url) {
    var domain;
    if (url.indexOf("://") > -1) {
        domain = url.split('/')[2];
    }
    else {
        domain = url.split('/')[0];
    }
    //find & remove port number
    domain = domain.split(':')[0];
    return (domain.length > 0 ? domain : "OUR WEBSITE");
	}
	$("#site-domain").html(extractDomain(window.location.host));
	</script>
