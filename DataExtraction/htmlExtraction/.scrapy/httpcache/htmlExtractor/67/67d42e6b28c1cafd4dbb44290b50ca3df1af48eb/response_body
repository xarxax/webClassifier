    <script type="text/javascript">
        function ValidateInfotypeList(source, args) {
            var chkListModules = document.getElementById('ctl00_ContentPlaceHolder1_NavigatorSearch_cblInfotypes');
            var chkListinputs = chkListModules.getElementsByTagName("input");
            for (var i = 0; i < chkListinputs.length; i++) {
                if (chkListinputs[i].checked) {
                    args.IsValid = true;
                    return;
                }
            }
            args.IsValid = false;
        }
        function EnterEvent(e) {
            if (e.keyCode == 13) {
                __doPostBack('search', 'search');
        }
        }
        function setCBL(checked) {
            var cbl = document.getElementById('ctl00_ContentPlaceHolder1_NavigatorSearch_cblInfotypes').getElementsByTagName("input");
            for (i = 0; i < cbl.length; i++)
                cbl[i].checked = checked;
        }

    </script>

