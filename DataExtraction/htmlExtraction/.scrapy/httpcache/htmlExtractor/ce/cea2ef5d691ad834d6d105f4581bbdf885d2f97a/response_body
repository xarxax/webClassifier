<script  type="text/javascript">
$(document).ready(function() {
   //$(function() {alert("HI3");});
});
$(document).ready(function() {
  $('input#searchphrase.hdrsearchclear').each(function() {
    $(this)
      .data('default', $(this).val())
      .addClass('inactive')
	  
      .focus(function() {
        $(this).removeClass('inactive');
        if($(this).val() == $(this).data('default') || '') {
          $(this).val('');
        }
      })
	  
      .blur(function() {
        var default_val = $(this).data('default');
        if($(this).val() == '') {
          $(this).addClass('inactive');
          $(this).val($(this).data('default'));
        }
      });
  });
});


$(document).ready(function() {
  $('input#search_gobtn').hover(function() {
		$(this).attr("src","imgh2/searchGo_hover.gif");
			}, function() {
		$(this).attr("src","imgh2/searchGo.gif");
  });
})


// bp pop 
var newistWindow = null;
function newWindow(myurl,w,h,myinstructions) {
	if ((newistWindow != null) && (newistWindow.closed==false)) {newistWindow.close()}


	var isChrome = !!window.chrome && !!window.chrome.webstore;

	if (isChrome) {
	    h = h + 50;
	}


	if (w > 300) {
	    w = w - 60;
	    if (h > 496) {
	        h = h - 40
	    };
	}

	temp = "width=" + w + ",height=" + h + "," + myinstructions;  
	newistWindow = window.open(myurl,'name1',temp);


	var windowX = (screen.width/2) - (w/2);
	newistWindow.moveTo(windowX, 20);


	newistWindow.focus();
	}

function OpenWindow(winName, myurl, w, h, showStatus, isViewer, needsNav) {
	var newistWindow = null;
	l = (screen.availWidth - w)/2;
	t = (screen.availHeight - h)/2;
	feature  = "toolbar=" + (needsNav?"yes":"no");   // yes|no 
	feature += ",location=no";    // yes|no 
	feature += ",directories=no"; // yes|no 
	feature += ",status=" + (showStatus?"yes":"no");  // yes|no 
	feature += ",menubar=no";     // yes|no 
	feature += ",scrollbars=" + (isViewer?"yes":"no");   // auto|yes|no 
	feature += ",resizable=" + (isViewer?"yes":"no");   // yes|no 
	feature += ",dependent";      // close the parent, close the popup, omit if you want otherwise 
	feature += ",height=" + h;
	feature += ",width=" + w;
	feature += ",left=" + l;
	feature += ",top=" + t;
	winName = winName.replace(/[^a-z]/gi,"_");
	
	newistWindow = window.open(myurl,winName,feature);
	newistWindow.focus();
	return;
} 

</script>


