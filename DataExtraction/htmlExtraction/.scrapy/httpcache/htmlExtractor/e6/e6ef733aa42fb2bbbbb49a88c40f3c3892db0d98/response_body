<script type="text/javascript">
	jQuery(document).ready(function() {
	    jQuery("#contactSubmit2").click(function() {
	    
	        //collect input field values
	        //note: .val() gets the current value of the first element in a set of matched elements
	        //var user_name = jQuery('input[name=name]').val(); 
	        //var user_email = jQuery('input[name=email]').val();
	        //var user_message = jQuery('textarea[name=message]').val();
	        
	        var user_name2=document.forms["contactForm2"]["name2"].value;
	        var user_company2=document.forms["contactForm2"]["company2"].value;
			var user_email2=document.forms["contactForm2"]["email2"].value;
			var user_phone2=document.forms["contactForm2"]["phone2"].value;
			var user_message2= document.forms["contactForm2"]["comments2"].value;
			
			
	        var emailReg2 = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
	        
	        //simple validation at client's end
	        //we simply change border color to red if empty field using .css()
	        var proceed = true;

	        if(user_name2=="" || user_name2.length < 2){ 
	        	jQuery('#result2').show();
	        	jQuery('#nameError2').remove();
	        	jQuery("#result2 ul").append('<li id="nameError2"><i class="fa fa-caret-right list-caret"></i>Name is too short!</li>');
	            jQuery('input[name="name2"]').css('background','#efbaba'); 
	            jQuery('input[name="name2"]').css({'background':'#efbaba','box-shadow':'none','-moz-box-shadow':'none','-webkit-box-shadow':'none'}); 
	            proceed = false;
	        }
	        if(user_company2=="" || user_name2.length < 2){ 
	        	jQuery('#result2').show();
	        	jQuery('#companyError2').remove();
	        	jQuery("#result2 ul").append('<li id="companyError2"><i class="fa fa-caret-right list-caret"></i>Please fill in your company name!</li>');
	            jQuery('input[name="company2"]').css({'background':'#efbaba','box-shadow':'none','-moz-box-shadow':'none','-webkit-box-shadow':'none'}); 
	            proceed = false;
	        }
	        if(user_email2=="" || !emailReg2.test(user_email2)){ 
	        	jQuery('#result2').show();
	        	jQuery('#emailError2').remove();
	        	jQuery("#result2 ul").append('<li id="emailError2"><i class="fa fa-caret-right list-caret"></i>Invalid email!</li>');
	            jQuery('input[name="email2"]').css({'background':'#efbaba','box-shadow':'none','-moz-box-shadow':'none','-webkit-box-shadow':'none'});
	            proceed = false;
	        }
	        if(user_phone2=="" || user_phone2.length < 7){ 
	        	jQuery('#result2').show();
	        	jQuery('#phoneError2').remove();
	        	jQuery("#result2 ul").append('<li id="phoneError2"><i class="fa fa-caret-right list-caret"></i>Phone number is too short!</li>'); 
	            jQuery('input[name="phone2"]').css({'background':'#efbaba','box-shadow':'none','-moz-box-shadow':'none','-webkit-box-shadow':'none'}); 
	            proceed = false;
	        }
	        if(user_message2=="" || user_message2.length < 10){  
	        	jQuery('#result2').show();
	        	jQuery('#messageError2').remove();
	        	jQuery("#result2 ul").append('<li id="messageError2"><i class="fa fa-caret-right list-caret"></i>Message is too short! (Minimum 10 characters)</li>');
	            jQuery('textarea[name="comments2"]').css({'background':'#efbaba','box-shadow':'none','-moz-box-shadow':'none','-webkit-box-shadow':'none'});
	            proceed = false;
	        }
	        
	        jQuery(".okayBtn").click(function(){
				jQuery("#result2").css("display","none");
			});
	        
	        //everything looks good! proceed...
	        if(proceed) 
	        {
	        
	            loadXMLDoc(user_name2, user_company2, user_email2, user_phone2, user_message2);
	            
	            
	            //data to be sent to server
	            /*post_data = {'userName':user_name, 'userEmail':user_email, 'userMessage':user_message};
	            
	            //Ajax post data to server
	            jQuery.post('/siteInclude/contact.php', post_data, function(data){  
	                
	                //load success massage in #result div element, with slide effect.       
	                jQuery("#result").hide().html('<div class="whiteline"></div><div class="success">'+data+'</div>').slideDown();
	                
	                //reset values in all input fields
	                jQuery('#contactus input').val(''); 
	                jQuery('#contactus textarea').val(''); 
	                
	            }).fail(function(err) {  //load any error data
	                jQuery("#result").hide().html('<div class="whiteline"></div><div class="error">'+err.statusText+'</div>').slideDown();
	            });*/
	            
	        }
	    });  
	     function loadXMLDoc(user_name2, user_company2, user_email2, user_phone2, user_message2){
	     
			var xmlhttp = new XMLHttpRequest();
			var parameters="userName="+user_name2+"&userCompany="+user_company2+"&userEmail="+user_email2+"&userPhone="+user_phone2+"&userMessage="+user_message2;
			var link = "https://esdandassociates.com/wp-content/themes/strappress/contact-form.php";
			
			//alert(parameters);
			xmlhttp.open("POST",link,true);
			xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			xmlhttp.setRequestHeader("Content-length", parameters.length);
			xmlhttp.setRequestHeader("Connection", "close");
			
	
			xmlhttp.onreadystatechange = function() {//Call a function when the state changes.
    			if(xmlhttp.readyState == 4 && xmlhttp.status == 200) {   
					//reset value	
					document.forms["contactForm2"]["name2"].value = '';
					document.forms["contactForm2"]["company2"].value = '';
					document.forms["contactForm2"]["email2"].value = '';
					document.forms["contactForm2"]["phone2"].value = '';
					document.forms["contactForm2"]["comments2"].value = '';
    				
					// Upon success: do these actions
					jQuery("#postSuccess2").css("display","block");
					jQuery(".okayBtn").click(function(){
						jQuery("#postSuccess2").css("display","none");
					});
					
					
					/*jQuery("div#contact_form").slideUp("fast", function(){
		                jQuery("div#contact").removeClass("showed");
		                jQuery("div#contact").css("height","auto");
		                jQuery(".whiteline").css("display","none");    
		            });  	*/
					
									
				}
			}
			
			xmlhttp.send(parameters);
		}
	   	
	   	jQuery('#contactForm2 input, #contactForm2 textarea').keyup(function(){
	    	jQuery("#result2").slideUp();
	    	jQuery("#result2 ul").html('');
	    });
	        
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="name2"]').keyup(function() {
	    	jQuery('input[name="name2"]').css({'background':'','box-shadow':'','-moz-box-shadow':'','-webkit-box-shadow':''}); 
	    });
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="company2"]').keyup(function() {
	    	jQuery('input[name="company2"]').css({'background':'','box-shadow':'','-moz-box-shadow':'','-webkit-box-shadow':''}); 
	    });
	    jQuery('input[name="email2"]').keyup(function() {
	    	jQuery('input[name="email2"]').css({'background':'','box-shadow':'','-moz-box-shadow':'','-webkit-box-shadow':''}); 
	    });
	    //reset previously set border colors and hide all message on .keyup()
	    jQuery('input[name="phone2"]').keyup(function() {
	    	jQuery('input[name="phone2"]').css({'background':'','box-shadow':'','-moz-box-shadow':'','-webkit-box-shadow':''}); 
	    });
	    jQuery('textarea[name="comments2"]').keyup(function() {
	    	jQuery('textarea[name="comments2"]').css({'background':'','box-shadow':'','-moz-box-shadow':'','-webkit-box-shadow':''}); 
	    });
	    /*jQuery("#contactus input, #contactus textarea").keyup(function() { 
	        jQuery("#contactus input, #contactus textarea").css('border-color',''); 
	        jQuery("#contactus input, #contactus textarea").css('background',''); 
	        jQuery("#result").slideUp();
	            
	    });*/
	});

</script>                            	            	
