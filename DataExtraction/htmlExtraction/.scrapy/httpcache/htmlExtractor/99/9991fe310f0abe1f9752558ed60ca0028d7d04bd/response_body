    <div id="block-views-promoted-posts-block-1" class="block block-views clearfix">

    <h2 class="title">Highlights</h2>
  
  <div class="content">
    <div class="view view-promoted-posts view-id-promoted_posts view-display-id-block_1 row view-promoted-items view-dom-id-bcd5f2c03a66c60e9041468d9d7f3328">
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first col-sm-4 col-md-4">
      
  <div class="views-field views-field-field-link-override-1">        <h3 class="field-content"><a href="/19th-biennial-hemingway-society-conference-wyomingmontana">19th Biennial Hemingway Society Conference in Wyoming/Montana</a></h3>  </div>  
  <div class="views-field views-field-field-subheader-body">        <div class="field-content"></div>  </div>  
