<script type="text/javascript">
	
        $(document).ready(function(){
            $("#mch_subscribe").live("click", function(){
                var oEmail = $("#mch_email");
                var oName = $("#mch_name");
                var email = oEmail.val();
                var name = oName.val();
                var error = false;
                if (!email || !name){
                    error = true;
                }
                if (!error) {
                    var content = $("#mailchimp-nl-cont");
                    content.html("<img src=\"http://rehabworld.com/templates/_system/main/images/ajax_preloader_circular_32.gif\" />")
                    .css("text-align", "center");
                    $.ajax({
                        url: 'http://rehabworld.com/system/miscellaneous/mailchimp/',
                        type: "GET",
                        data: "mch_name="+name+"&mch_email="+email+"&subscribe=1",
                        success: function(data) {
                            content.html($(data).find("#mailchimp-nl-cont"));
                        }
                    });
                }
                return false;
            });
        });
	
</script>


