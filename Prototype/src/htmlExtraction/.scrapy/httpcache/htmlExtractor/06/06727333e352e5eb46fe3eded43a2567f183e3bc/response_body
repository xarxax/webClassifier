<script type="text/javascript">
    var amount = 0;

    jQuery(document).ready(function ($) {
        $.colorbox({
            onLoad: function () { rezsizeIsClose = false; },
            onClosed: function () { rezsizeIsClose = true; },
            width: "700px",
            opacity: '0.4',
            inline: true,
            href: "#content_0_odyssey_0_RepeaterRows_ctl00_5_LightBoxPanel_5",
            open: true,
            onComplete: function () {
                amount = getCookie('lightbox_Newsletter Form', 0);
                    amount++;
                    setCookie('lightbox_Newsletter Form', amount, 7);
                }
        });

        $('#remember_me').on('change', function () {
            if ($(this).is(':checked')) {
                setCookie('lightbox_Newsletter Form', -1, 7);
                } else {
                    setCookie('lightbox_Newsletter Form', amount, 7);
                }
            });
        $("#colorbox, #cboxOverlay").appendTo('form:first');
    });

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toGMTString();
        document.cookie = cname + "=" + cvalue + "; " + expires;
    }

    function getCookie(cname, defaultValue) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i].trim();
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return defaultValue;
    }
</script>

