var images = [ // list images
["img/nav_home_off.jpg","img/nav_home_on.jpg","img/nav_home_on.jpg"], // HOME
["img/nav_about_off.jpg","img/nav_about_on.jpg","img/nav_about_on.jpg"], // ABOUT
