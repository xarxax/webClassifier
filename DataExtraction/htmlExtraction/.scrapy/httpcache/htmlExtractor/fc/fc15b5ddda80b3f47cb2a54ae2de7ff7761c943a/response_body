	<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script>
		
	<script type="text/javascript" language="javascript">
	    $(document).ready(function() {
	        var fadeduration = 500;
	        $('#body-content-ps').hide();
	        $('#body-content-pos').hide();
	        $('#body-content-pd').hide();
	        $('#quick-find-directory').hide();
			
			$('body').click(function(e) {
				//alert(e.target.id);
	            if ((e.target.id != 'quick-find') && ($('#quick-find').height() == 600)) {
	                $('#quick-find').height(51);
	                $('#quick-find-directory').fadeOut(fadeduration);
	            }
			});
			
	        $('#quick-find').click(function() {
	            if ($('#quick-find').height() < 600) {
	                $('#quick-find').height(600);
	                $('#quick-find-directory').fadeIn(fadeduration);
	            }
	            else {
	                $('#quick-find').height(51);
	                $('#quick-find-directory').fadeOut(fadeduration);
	            }
	        });

	        $('#ppm-rollover').mouseover(function() {
	            $('#body-content-ppm').fadeIn(fadeduration);
	            $('#body-content-ps').fadeOut(fadeduration);
	            $('#body-content-pos').fadeOut(fadeduration);
	            $('#body-content-pd').fadeOut(fadeduration);
	        });

	        $('#ps-rollover').mouseover(function() {
	            $('#body-content-ppm').fadeOut(fadeduration);
	            $('#body-content-ps').fadeIn(fadeduration);
	            $('#body-content-pos').fadeOut(fadeduration);
	            $('#body-content-pd').fadeOut(fadeduration);
	        });

	        $('#pos-rollover').mouseover(function() {
	            $('#body-content-ppm').fadeOut(fadeduration);
	            $('#body-content-ps').fadeOut(fadeduration);
	            $('#body-content-pos').fadeIn(fadeduration);
	            $('#body-content-pd').fadeOut(fadeduration);
	        });

	        $('#pd-rollover').mouseover(function() {
	            $('#body-content-ppm').fadeOut(fadeduration);
	            $('#body-content-ps').fadeOut(fadeduration);
	            $('#body-content-pos').fadeOut(fadeduration);
	            $('#body-content-pd').fadeIn(fadeduration);
	        });
	    });  		
	</script>
</head>

