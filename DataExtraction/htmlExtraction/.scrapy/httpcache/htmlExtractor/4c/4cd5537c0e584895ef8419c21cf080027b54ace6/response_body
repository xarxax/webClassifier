<script>

$(document).ready(function(){

	function AddTwitterTLStylesheet(){

		var $ttl = $('iframe.twitter-timeline');

		if ( $ttl.length > 0 && $ttl[0].contentWindow.document.body.innerHTML !== ""){

			var $ttl_contents = $ttl.contents();

			$ttl_contents.find('head').append('<link href="http://www.tatsunoko.co.jp/app-def/S-102/wordpress/wp-content/themes/tatsunoko_theme/common/css/twitter-timeline.css" rel="stylesheet">');

		} else {

			setTimeout( function(){ AddTwitterTLStylesheet(); }, 250 );

		}
	}

	AddTwitterTLStylesheet();

});

</script>

