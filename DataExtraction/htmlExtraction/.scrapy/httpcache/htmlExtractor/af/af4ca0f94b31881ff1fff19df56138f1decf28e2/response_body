<script type="text/javascript">
function getNewCaptchaImage()
{
  document.newcaptcha.submit();
}

$(document).ready(function() {
	var movementStrength = 20;
	var height = movementStrength / $(window).height();
	var width = movementStrength / $(window).width();
	$("#main-container").mousemove(function(e){
		if ($(window).width() > 992)
		{
			var pageX = e.pageX - ($(window).width() / 2);
			var pageY = e.pageY - ($(window).height() / 2);
			var newvalueX = width * (pageX+300) * -1 - 25;
			var newvalueY = height * (pageY-2000) * -1 - 50;
			$('#main-container').css("background-position", newvalueX+"px "+newvalueY+"px");
		}
	});
	
	if(window.location.hash) {
		var scrollLoc = $( $(".contact").attr('href') ).offset().top - 350;
		$('html, body').animate({
			scrollTop: scrollLoc
		}, 1300);
	}
	
	
	
	$('.forgotpw').click(function (event) // This function is called when the visitor selected "Forgot Password." Opens up a new window where they can provide their email address to receive email with their password
	{
		event.preventDefault();
		window.open($(this).attr("href"), "popupWindow", "width=600, height=600, scrollbars=yes");
	});
	
	$("a.contact").click(function(){
		if ($(window).width() < 768)
		{
			$(".navbar-toggle").trigger("click");
		}
	});
	
	var browserIS = determineBrowser();
	if ( browserIS == "ie"){ $('#atr_text img').css("padding", ".25em .5em 2em") }
	$.cookie('currentVolume', '.5');
	
	$("#myname").on('change, blur', function(e){ 
		$.cookie("myname", $("#myname").val());
	});
	$("#myemail").on('change, blur', function(e){ 
		$.cookie("myemail", $("#myemail").val());
	});
	$("#mysubject").on('change, blur', function(e){ 
		$.cookie("mysubject", $("#mysubject").val());
	});
	$("#mymessage").on('change, blur', function(e){ 
		$.cookie("mymessage", $("#mymessage").val());
	});
	
	$("#myname").val($.cookie("myname"));
	$("#myemail").val($.cookie("myemail"));
	$("#mysubject").val($.cookie("mysubject"));
	$("#mymessage").val($.cookie("mymessage"));
	
		if($.cookie('inregardsto') != null) 
		{
			$('#inregardsto option[value="' + $.cookie('inregardsto') + '"]').attr('selected', 'selected');
		}
	
	$('#inregardsto').change(function() 
	{
    	$.cookie('inregardsto', $('#inregardsto option:selected').val());
	});
	
	if (window.location.hash == "#contactus") { $("#contact").addClass("current"); }
	
	var LMS_clearfix = $(window).on('resize', function(){
		if ($(window).width() < 767) { $('#LMS_clearfix').css("display", "block"); } else { $('#LMS_clearfix').css("display", "none"); }
    }).trigger('resize');
});

function showHighlight()
{
	$('#asteriskfootnote p').removeClass('showhighlight');
	$('#asteriskfootnote p').removeClass('fadeaway');
	setTimeout(fadeInFootNote, 2000);
	setTimeout(fadeOutFootNote, 6500);
	function fadeInFootNote() {	$('#asteriskfootnote p').addClass('showhighlight'); }
	function fadeOutFootNote() { $('#asteriskfootnote p').addClass('fadeaway'); }
}
$('a#preventionasterisk').click(function(){
    $('html, body').animate({
        scrollTop: $( $(".preventionasterisk").attr('href') ).offset().top
    }, 1300);
    return false;
});
$('a#contact').click(function(){
    $('html, body').animate({
        scrollTop: $( $(".contact").attr('href') ).offset().top
    }, 1300);
    return false;
});

$('#closeAtRiskQuiz').click(function(){
	$('#closeAtRiskQuiz').css("display", "none");
	$('#atriskquizcontainer').css("box-shadow", "none");
    $('#atriskquiz').animate({left: "+=260"}, 900,  'easeOutQuart', function() {
		$('#atriskquiz').css("display", "none");
		$('#atriskquizclosed').css("display", "block");
		$('#atriskquizcontainer').css("width", "28px");
		$('#atriskquizcontainer').css("box-shadow", "0 0 12px 1px #000");
		$('#atriskquizclosed').delay(200).animate({left: "-=28"}, 600, 'easeOutQuart');		
  	});
    return false;
});


$('#atriskquiz').click(function(){
	var win = window.open("https://www.interphasesystems.com/hrtrain/", '_blank');
  	win.focus();
    return false;
});
$('#atriskquizclosed').click(function(){
    var win = window.open("https://www.interphasesystems.com/hrtrain/", '_blank');
  	win.focus();
    return false;
});

$('#atriskquizclosed').mouseover(function(){
    $('#closeAtRiskQuiz').css("display", "block");
	$('#atriskquizclosed').css("display", "none");
	$('#atriskquizcontainer').css("box-shadow", "none").css("width", "240px");
	$('#atriskquiz').css("display", "block").animate({left: "-=260"}, 1000, 'easeOutQuart');
    $('#atriskquizclosed').animate({left: "+=28"}, 900,  'easeOutQuart', function() { $('#atriskquizcontainer').css("box-shadow", "0 0 12px 1px #000") });
    return false;
});

function determineBrowser()
{
	var isOpera = !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;
    // Opera 8.0+ (UA detection to detect Blink/v8-powered Opera)
	var isFirefox = typeof InstallTrigger !== 'undefined';   // Firefox 1.0+
	var isSafari = Object.prototype.toString.call(window.HTMLElement).indexOf('Constructor') > 0;
		// At least Safari 3+: "[object HTMLElementConstructor]"
	var isChrome = !!window.chrome && !isOpera;              // Chrome 1+
	var isIE = /*@cc_on!@*/false || !!document.documentMode;   // At least IE6
	
	var browserIS;
	if (isOpera){browserIS = "opera";} else if (isFirefox){browserIS = "firefox";}else if (isSafari){browserIS = "safari";}else if (isChrome){browserIS = "chrome";}else if (isIE){browserIS = "ie";}
	
	return browserIS;
}

