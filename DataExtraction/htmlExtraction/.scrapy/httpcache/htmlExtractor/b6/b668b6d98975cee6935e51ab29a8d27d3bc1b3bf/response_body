<script type="text/javascript">
    /* <![CDATA[ */

    var mappedProductsById = {};
    var hawkitemlistcontent = '';

    function InitAnalytics(cStep) {
        //console.log("InitAnalytics", cStep);
        if ($(typeof onCheckout == 'function')) {
            $.ajax({
                url: '/ajax/store/checkout.aspx?F=InitAnalytics&cStep=' + cStep,
                type: 'GET',
                dataType: "json",
                success: function (o) {
                    onCheckout(o);
                }
            });
        }
    }

    function GetProducts(list) {
        return $.map(list, function (val, key) {
            if (!mappedProductsById.hasOwnProperty(val.id)) {
                mappedProductsById[val.id] = true;
                return {
                    name: val.name,
                    id: val.id,
                    price: Number(val.price.replace(/[^0-9\.]+/g, "")).toString(),
                    list: val.list,
                    position: val.position,
                    category: val.category,
                    brand: val.brand,
                    variant: val.variant
                };
            }
        });
    }

    function ItemListImpression(list) {
        var products = GetProducts(list);

        //console.log("ItemListImpression called", list);
        //console.log(products);

        if (products.length !== 0) {
            dataLayer.push({
                'event': 'impressions',
                'ecommerce': {
                    'currencyCode': 'USD',
                    'impressions': products
                }
            });
        }
    }

    function OnProductClick(e) {
        var elem = $(e);
        EnsureAnalytics(elem.data("id"), elem.data("position"), ListingEnum.Detail);

        var product = $.map(_litm, function (n, i) {
            if (i == 'litm_' + elem.data("id")) {
                return {
                    name: n.name,
                    id: n.id,
                    brand: n.brand,
                    price: Number(n.price.replace(/[^0-9\.]+/g, "")).toString(),
                    position: n.position,
                    category: n.category,
                    variant: n.variant
                }
            }
        });

        if (product.length !== 0) {
            //console.log('Product Click called');
            //console.log(product);

            dataLayer.push({
                'event': 'productClick',
                'ecommerce': {
                    'click': {
                        'actionField': { 'list': _ga_category },
                        'products': product
                    }
                }
            });
            return true;
        }
        return false;
    }

    function DetailItemImpression() {
        var products = $.map(_ditm, function (val, key) {
            return {
                name: val.name,
                id: val.id,
                brand: val.brand,
                price: Number(val.price.replace(/[^0-9\.]+/g, "")).toString(),
                category: val.category,
                variant: val.variant
            };
        });

        //console.log('Detail Item Impression called', products);
        dataLayer.push({
            'event': 'productDetails',
            'ecommerce': {
                'detail': {
                    'actionField': { 'list': _ga_category },
                    'products': products
                }
            }
        });
    }

    function AddToCartClick(o) {
        var products = $.map(_ditm, function (val, key) {
            return {
                name: val.name,
                id: val.id,
                price: Number(o.Price.replace(/[^0-9\.]+/g, "")).toString(),
                category: val.category,
                brand: val.brand,
                variant: val.variant,
                quantity: o.Details.AddedQuantity
            };
        });

        //console.log("Add To Cart called", products);
        dataLayer.push({
            'event': 'addToCart',
            'ecommerce': {
                'add': {
                    'products': products
                }
            }
        });
    }

    function onCheckout(o) {
        //console.log("Checkout called", o.Data);
        //console.log(o.Products);

        dataLayer.push({
            'event': 'checkout',
            'ecommerce': {
                'checkout': {
                    'actionField': { 'step': o.Data.step },
                    'products': o.Products
                }
            }
        });
    }

    function onPurchase(o) {
        //console.log("Purchase called", o.GoogleAnalyticsData);

        dataLayer.push({
            'event': 'orderConfirmation',
            'ecommerce': {
                'purchase': {
                    'actionField': {
                        'id': o.GoogleAnalyticsData.Data.id,
                        'revenue': o.GoogleAnalyticsData.Data.revenue,
                        'tax': o.GoogleAnalyticsData.Data.tax,
                        'shipping': o.GoogleAnalyticsData.Data.shipping,
                        'coupon': o.GoogleAnalyticsData.Data.coupon
                    },
                    'products': $.map(o.GoogleAnalyticsData.Products, function(el) { return el; })
                }
            }
        });
    }

    // Ensures the ga data is loaded for item events, 
    // If something wasnt loaded, it never got an impression so send it here..
    function EnsureAnalytics(id, i, listing) {
        var product = $.map(_litm, function (n, i) { if (i == 'litm_' + id) { return n; } });
        var promise = null;

        if (product.length == 0) {
            promise = $.ajax({
                url: '/ajax/store/ajax.aspx?F=EnsureAnalytics&Id=' + id + '&Pos=' + i + '&lt=' + listing,
                type: 'GET',
                dataType: 'json'
            }).done(function (o) {
                if (!o.Success) return;
                if (!o.id) {
                    o.id = "";
                }
                _litm['litm_' + id] = o;
            });
        }

        return promise;
    }

    // Call can be aggregated for items in single push
    function LoadAnalytics() {
        var promises = [];

        $.each($(".itemLink.ga"), function (n, i) {
            var promise = EnsureAnalytics($(i).data("id"), $(i).data("position"), ListingEnum[$(i).data("lt")]);;
            if (promise) {
                promises.push(promise);
            }
        });

        //console.log('LoadAnalytics', promises);
        $.when.apply($, promises).done(function () {
            ItemListImpression($.map(_litm, function (value, index) { return [value] }));
        });
    }

    function HawkItemListContentChanged() {
        if (hawkitemlistcontent !== $('#hawkitemlist').html()) {
            hawkitemlistcontent = $('#hawkitemlist').html();
            LoadAnalytics();
        }

        setTimeout(function () {
            HawkItemListContentChanged();
        }, 500);
    };

    ListingEnum = {
        IdevListing: 0,
        HawkSearch: 1,
        Detail: 2,
        ShoppingCart: 3,
        Confirmation: 4,
        RelatedItems: 5,
        ShoppingCartRelatedItems: 6,
        FeaturedItems: 7,
        RecentlyViewedItems: 8
    }
    /* ]]> */
</script>

<script type="text/javascript">
    /* <![CDATA[ */
    $(function () {
        
        setTimeout(function () {
            HawkItemListContentChanged();
        }, 500);
        
    });
    /* ]]> */
</script><form method="post" action="/" id="main">
