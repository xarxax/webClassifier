	<script>
		$(document).ready(function() {
		 $('#sliderbanner') 
			.anythingSlider({
				mode : 'f',   // fade mode - new in v1.8!
				hashTags:false,
				autoPlay : true, 
				animationTime: 300,
				delay : 5000,
				buildArrows : false,
				buildNavigation: false,    
				buildStartStop : false,
				expand: true,
                autoPlayLocked: true,
                resumeDelay: 1000,
                onSlideComplete: function (slider) {
                    $(".thumbNav li a").attr("class", "");
                    $(".thumbNav li:nth-child(" + (slider.currentPage - 1) + ") a").attr("class", "Current");
                }
				 })
		  
		  $('#sliderfeatured') 
			.anythingSlider({
				hashTags:false,	
				buildNavigation : false, 
				buildStartStop : false,
				expand: true
		  })
			
			$('ul.thumbNav a').bind('mouseover', function(e) {
				// get slide number
			    var h = $(this).attr('data-slide');
			    h = parseInt(h) + 1;
				// external links will fall through and go to the external url
				if (h) {
					$('#sliderbanner').anythingSlider(h);
				}
			})
		
		});
		$(window).load(function(){
		  $('#sliderfeatured').data('AnythingSlider').makeActive(); // only works in version 1.6.1+
