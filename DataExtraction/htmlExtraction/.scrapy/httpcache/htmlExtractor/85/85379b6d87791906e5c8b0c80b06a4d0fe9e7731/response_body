    function CreateXmlHttpNews()
    {
	    try
	    {
		    XmlHttpNews = new ActiveXObject("Msxml2.XMLHTTP");
	    }
	    catch(e)
	    {
		    try
		    {
			    XmlHttpNews = new ActiveXObject("Microsoft.XMLHTTP");
		    }
		    catch(oc)
		    {
			    XmlHttpNews = null;				
		    }
	    }
	    if(!XmlHttpNews && typeof XMLHttpRequest != "undefined")
	    {
		    XmlHttpNews = new XMLHttpRequest();
	    }
    }
