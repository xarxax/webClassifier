<script>
winChangeSize = 480,currentSlot = "";
function chkWinSize_fun(){
	scrolled = window.pageYOffset || document.documentElement.scrollTop;
	window_w = window.innerWidth;
	window_h = window.innerHeight;	
	if(window_w <= winChangeSize){		
		if(currentSlot != "Mob"){
			currentSlot = 'Mob';
			clear_slots('Desk');
			change_slots('Mob');	
		}
	}else{		
		if(currentSlot != "Desk"){
			currentSlot = 'Desk';
			clear_slots('Mob');
			change_slots('Desk');
		}
	}
		
}
function clear_slots(slotMedia){
	//if(adslots[slotMedia].length > 0){
		adslotsSpec = adslots[slotMedia];
		for(var slot in adslotsSpec){
			document.getElementById(slot).innerHTML="";
		}
	//}
}
function change_slots(slotMedia){
	adslotsSpec = adslots[slotMedia];
	for(var slot in adslotsSpec){
		var parent = slot,
			child = adslotsSpec[slot],
			slotName = child+"_"+slotMedia;
			//alert(slotName);
			//try{
		document.getElementById(parent).innerHTML="";
		document.getElementById(parent).innerHTML="<div id='"+slotName+"'></div>";				
		googletag.cmd.push(function() {googletag.pubads().refresh([window[slotName]]);googletag.display(slotName); });
			//}
			//catch(e){
			//console.log(e);
			//return;	
			//}
	}
}
</script><!DOCTYPE HTML>
