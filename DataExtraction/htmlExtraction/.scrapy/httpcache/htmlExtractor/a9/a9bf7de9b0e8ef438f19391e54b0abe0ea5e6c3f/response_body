	<script language='javascript'>

		function ImageRotator(sImageID) {
			this.ImageID = sImageID;
			this.ImageType = 'src';
			this.PhotoSet = [];
			this.CurrentIndex = 0;
			this.Timer = null;
			this.TimerIsRunning = false;
			this.TimerInterval = 5000;

			window[this.ImageID] = this;

			this.NextImage = function() {
				this.CurrentIndex == this.PhotoSet.length - 1 ? this.CurrentIndex = 0 : this.CurrentIndex++;
				this.ChangeImage();
				if (this.TimerIsRunning) this.ResetRotation();
			}

			this.PreviousImage = function() {
				this.CurrentIndex == 0 ? this.CurrentIndex = this.PhotoSet.length - 1 : this.CurrentIndex--;
				this.ChangeImage();
				if (this.TimerIsRunning) this.ResetRotation();
			}

			this.StartRotation = function() {
				this.Timer = setInterval("window." + this.ImageID + ".NextImage()", this.TimerInterval);
				this.TimerIsRunning = true;
			}

			this.StopRotation = function() {
				clearInterval(this.Timer);
				this.TimerIsRunning = false;
			}

			this.ResetRotation = function() {
				this.StopRotation();
				this.StartRotation();
			}

			this.ToggleRotation = function() {
				this.TimerIsRunning ? this.StopRotation() : this.StartRotation();
			}

			this.ChangeImage = function() {
				if (this.ImageType == 'backgroundImage')
					document.getElementById(this.ImageID).style.backgroundImage = 'url(' + this.PhotoSet[this.CurrentIndex] + ')';
				else
					document.getElementById(this.ImageID).src = this.PhotoSet[this.CurrentIndex];
			}
		}

		function TogglePlayButton(oBtn)
		{
			if (oBtn.src == '/Images/design11/play.gif')
				oBtn.src = '/Images/design11/pause.gif';
			else
				oBtn.src = '/Images/design11/play.gif';
		}

		var r1 = new ImageRotator('Rotate1');
		r1.ImageType = 'backgroundImage';
		r1.PhotoSet = ['/Images/design11/rotate1/1.jpg'
			, '/Images/design11/rotate1/2.jpg'
			, '/Images/design11/rotate1/3.jpg'
			, '/Images/design11/rotate1/4.jpg'
			, '/Images/design11/rotate1/5.jpg'
			, '/Images/design11/rotate1/6.jpg'
			];

		var r2 = new ImageRotator('Rotate2');
		r2.PhotoSet = ['/Images/design11/rotate2/combine1.jpg'
			, '/Images/design11/rotate2/combine2.jpg'
			, '/Images/design11/rotate2/combine3.jpg'
			];

	</script>
