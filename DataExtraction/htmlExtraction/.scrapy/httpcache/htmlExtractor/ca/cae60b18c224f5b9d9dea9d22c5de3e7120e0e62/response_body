      function wdps_resize_slider_0() {
        var full_width = (jQuery(window).width() <= parseInt(0) || 1) ? 1 : 0;
        wdps_glb_margin_0 = parseInt('0');
        if ('style' == 'text') {
          wdps_set_text_dots_cont(0);
