closeClick: false,
css : {
'background' : 'rgba(0, 0, 0, 0.50)'
}
}
}
}).trigger('click');
});
function setupFormField(name, value) {
$(name).val(value);
$(name).blur(function(){ if( $(name).val() == "" ) { $(name).val(value); } });
$(name).focus(function(){ if( $(name).val() == value ) { $(name).val(""); } });
}
</script> 
