function menu_aan(id)
{document.getElementById("topmenu_" + id).style.color = "#000000";
 document.getElementById("topmenu_" + id + "a").src = "https://www.sitewise.nl/images/menu_driehoek.svg";}

function menu_uit(id)
{if (id != 1)
{document.getElementById("topmenu_" + id).style.color = "#929292";
 document.getElementById("topmenu_" + id + "a").src = "https://www.sitewise.nl/images/pixel.gif";}}

menuMobielAan = ""

function menu_mobiel_aan()
{if (menuMobielAan == "")
{document.getElementById("menuknop").src = "https://www.sitewise.nl/images/menuknop1.svg";
 document.getElementById("menu_mobiel").style.display = "block";
 document.getElementById("witte_laag").style.height = getBrowser("bodyH") - 274 + "px";
 document.getElementById("witte_laag").style.display = "block";
 menuMobielAan = "on";}
 else
{document.getElementById("menuknop").src = "https://www.sitewise.nl/images/menuknop.svg";
 document.getElementById("menu_mobiel").style.display = "none";
 document.getElementById("witte_laag").style.display = "none";
 menuMobielAan = "";}}

function portfolio_aan(id)
{document.getElementById("div_portfolio_" + id).style.display = "block";}

function portfolio_uit(id)
{document.getElementById("div_portfolio_" + id).style.display = "none";}
