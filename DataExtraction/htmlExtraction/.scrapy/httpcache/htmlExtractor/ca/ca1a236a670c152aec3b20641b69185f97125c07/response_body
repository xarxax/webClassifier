				geoAddress = document.getElementById('txtState3').value;
			}
			alert(geoAddress);
			
			var Lat = 0;
			var Lon = 0;
			if (geoAddress != "") {		
				geocoder.geocode( 
					{ 
						'address': 
						geoAddress 
