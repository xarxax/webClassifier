                $('.carousel-indicators').append('<li data-target="#home-page-carousel" data-slide-to="' + i + '" class="active"></li>');
            else
                $('.carousel-indicators').append('<li data-target="#home-page-carousel" data-slide-to="' + i + '"></li>');
        }
    });
</script>

