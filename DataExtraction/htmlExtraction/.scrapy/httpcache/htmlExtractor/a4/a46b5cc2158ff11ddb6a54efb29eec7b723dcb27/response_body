<script type="text/javascript">
var gcaptchacontact = '';
var gcaptchacall = '';
var gcaptchaquote = '';
var gcaptchapopup = '';
var CaptchaCallback = function() {
	var existrecaptchaquote = document.getElementById("recaptchaquote");
        if(existrecaptchaquote){
	        gcaptchaquote = grecaptcha.render('recaptchaquote', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("quotegrc-response").value=response;
		}});
	}
	var existrecaptchacall = document.getElementById("recaptchacall");
        if(existrecaptchacall){
		 gcaptchacall = grecaptcha.render('recaptchacall', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("callgrc-response").value=response;
		}});
	}
    var exists = document.getElementById("recaptchacontact");
    
        if(exists){
	        gcaptchacontact = grecaptcha.render('recaptchacontact', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("contactgrc-response").value=response;
		}});
    }
    
    var popexists = document.getElementById("recaptchaquotepopup");
        if(popexists){
	         gcaptchapopup = grecaptcha.render('recaptchaquotepopup', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("quotepopupgrc-response").value=response;
		}});
        }
        
        var leaseexists = document.getElementById("recaptchalease");
        if(leaseexists){
	         grecaptcha.render('recaptchalease', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("leasegrc-response").value=response;
		}});
        }
        
           var catexists = document.getElementById("recaptchacatalog");
        if(catexists){
	         grecaptcha.render('recaptchacatalog', {'sitekey' : '6Lfw3EAUAAAAAEzbpswDVS6oAM8VimY6uMI11YAj','callback' : function(response) {
				document.getElementById("cataloggrc-response").value=response;
		}});
        }
       
    };
    </script>

