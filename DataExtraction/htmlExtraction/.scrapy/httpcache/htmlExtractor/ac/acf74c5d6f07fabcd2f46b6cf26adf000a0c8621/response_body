	//arguments: URL, Title, Width(px), Height(px),scrolling ('Yes' or 'No')
	function explode(targetURL, targetURLTitle, wd, ht, scrolling) {
		var winName;
	
		if (winName)
			win_name++ ;
		else
			win_name = 0;
	
		var lc = (screen.width - wd) * .5;
		var tc = (screen.height - ht) * .5;
	
		windowProperties = 'height='+ht+',width='+wd+',top='+tc+',left='+lc+',scrollbars='+scrolling+',noresize'
		winName = window.open(targetURL, targetURLTitle, windowProperties)
	
		if (parseInt(navigator.appVersion) >= 4) { winName.window.focus(); }
	}
	
	function showHide(id) {
		if (document.getElementById(id).style.display != 'block') {
			document.getElementById(id).style.display = 'block';
		} else {
			document.getElementById(id).style.display='none';
		}
	}
	</script>
	
