   <script type="text/javascript">
      function m_m_C_e27056723_e27056712_setRange(cmb)
      {
         var val = cmb.value;
         var min = val.substring(0, val.indexOf("|"));
         var max = val.substring(val.indexOf("|") + 1);
         
         document.getElementById("m_m_C_e27056723_e27056712_txtMinimumPrice").value = min;
         document.getElementById("m_m_C_e27056723_e27056712_txtMaximumPrice").value = max;
      }
   </script>
   
   
