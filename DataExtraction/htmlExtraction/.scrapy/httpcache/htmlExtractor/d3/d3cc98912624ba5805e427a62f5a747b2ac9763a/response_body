	<script type="text/javascript">
		'use strict';

		function frmSubmit( frmAction )
		{
			var objForm = document.getElementById( "form1" );
			if ( objForm )
			{
				if ( navigator.cookieEnabled )
					objForm.submit();
				else
					ShowCookieWarning( true );
			}
		}

		function ShowLogonError( bShow )
		{
			var objElement = document.getElementById( "ctl00_LogonError" );
			if ( objElement )
			{
				if ( bShow )
					objElement.style.visibility = "visible";
				else
					objElement.style.visibility = "hidden";
			}
		}

		function ShowCookieWarning( bShow )
		{
			if ( typeof ( bshow ) == "boolean" )
			{
				if ( bShow )
				{
					$( "#BlackOut" ).css( "display", "block" );
					$( "#NoCookie" ).css( "display", "block" );
				}
				else
				{
					$( "#BlackOut" ).css( "display", "none" );
					$( "#NoCookie" ).css( "display", "none" );
				}
			}
		}

		$( function ()
		{
			$( 'body' ).on( 'keydown', function ( e )
			{
				ShowLogonError( false );
				if ( e.keyCode == 13 )
				{
					var objUser = document.getElementById( "userName" );
					var objPW = document.getElementById( "Password" );
					var objBtn = document.getElementById( "btnLogon" );
					if ( objUser && objPW && objBtn )
					{
						if ( objUser.value.length > 0 && objPW.value.length > 0 )
							objBtn.click();
					}
				}
			} );
		} );
	</script>
