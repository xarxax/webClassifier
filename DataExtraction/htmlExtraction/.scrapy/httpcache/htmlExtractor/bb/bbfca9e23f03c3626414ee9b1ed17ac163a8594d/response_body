    <script>
function onoff(evt,a){
var display_val = document.getElementById(a).style.display;
if (display_val == 'none')
	{
document.getElementById(a).style.top=evt.clientY+window.pageYOffset-115;
document.getElementById(a).style.left=evt.clientX+window.pageXOffset-150;
	document.getElementById(a).style.display="block";
	}
else
document.getElementById(a).style.display="none";
}
function on_(evt,a){
var display_val = document.getElementById(a).style.display;
if (display_val == 'none')
{
document.getElementById(a).style.top=evt.clientY;
document.getElementById(a).style.left=evt.clientX;
document.getElementById(a).style.display="block";
}
}
function off_(evt,a){
var display_val = document.getElementById(a).style.display;
if (display_val == 'block')
{
document.getElementById(a).style.display="none";
}
}
</script>
