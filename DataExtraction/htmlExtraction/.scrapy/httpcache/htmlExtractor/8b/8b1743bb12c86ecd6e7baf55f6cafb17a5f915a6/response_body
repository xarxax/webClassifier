function _setProvince(d){
        if(d){
        $.ajax({
            type: "POST",
            url: "https://accucarecanada.com/home/set_province/",
            data:{p:d},
            dataType: 'json',
            success: function (response) {
                if(response.status==1){
                    location.reload(true);
                }
                $('#pDiv').hide();
            }
        });
        }
        //alert(d);
    }


