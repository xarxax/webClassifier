// for Preview and Full view mode:
      	  	var filtered_msg = '<span class="filtermsg">This image has been filtered for explicit content. Click here to view it.</span>';
      	


	
	
	// put into a function so that it can be called even when content changes live on page (such as for ajax)
	function set_my_filters(){
		
							$('.gfilter-452').addClass('gallery-filtered-img');
				$('.gfilter-452 .filtermsg').html('filtered');
							$('.gfilter-495').addClass('gallery-filtered-img');
				$('.gfilter-495 .filtermsg').html('filtered');
							$('.gfilter-494').addClass('gallery-filtered-img');
				$('.gfilter-494 .filtermsg').html('filtered');
										$('.gfilter-adult').addClass('gallery-filtered-img');
				$('.gfilter-adult .filtermsg').html('adult imagery');
					
		// remove "loading" class so we can display non-filtered content
		$('.gallery-loading-img').removeClass('gallery-loading-img');
		// then show parent
		//$('.ArtList').show();
		//$('.WritingList').show();
		//$('.ComicsList').show();
		$('#artpreview').show();
		
		// add filter message
		$('#artpreview .gallery-filtered-img .preview').append(filtered_msg);
	
	}

	$(function() { // this line makes sure this code runs on page load
		// hide filtered content
		set_my_filters();
	});



</script>

