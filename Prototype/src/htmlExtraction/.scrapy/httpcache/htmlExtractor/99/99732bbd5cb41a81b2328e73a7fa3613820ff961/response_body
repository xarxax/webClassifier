<script type="text/javascript" src="/store/ext/jquery/jquery.easing.1.3.js"></script>
<script type="text/javascript">

function onPCS1Before() { 
	$('#PCS1Output').animate({ 
        opacity: 0.0
    }, 1000 );
} 
function onPCS1After() { 
    $('#PCS1Output').html($(this).attr("alt"));
	$('#PCS1Output').animate({ 
        opacity: 1.0
    }, 500 );
}

$(document).ready(function(){
    // Inizialize ProductsCycleSlideshow
	$('#PCS1').cycle({ 
		fx:     'scrollHorz',		sync:   1,
		speed:  2000,
		timeout: 2000,
		pause:    1,
		random:  0,
		pager:  '#PCS1Pager',
	    before:  onPCS1Before, 
	    after:   onPCS1After
	});

});
</script>

