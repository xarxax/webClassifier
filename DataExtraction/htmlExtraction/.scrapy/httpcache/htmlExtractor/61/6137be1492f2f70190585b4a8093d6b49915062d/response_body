	<script type="text/javascript">
	var visto = false;
	$(window).scroll(function() {
	var scrollTop     = $(window).scrollTop();
	elementOffset = $('#innovideo').offset().top;
	distance      = (elementOffset - scrollTop);
	console.log(distance);
	if (distance < 450 && visto === false) {
	$('#innovideo').get(0).play();
	visto = true;
	} 
	});

	$('#innovideo').on('ended',function(){
	setTimeout("  $('#innovideo').get(0).pause();");
	});
</script>
