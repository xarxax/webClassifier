	$("body").click(function(e){
		if(e.target.className !== "product-section-in")
		{
			$("#product_section").animate({
			height: 0
			}, 50);
			
		}
	  }
	);
	
	
	
	$(document).ready(function(){
		$(window).scroll(function(){
			checkStickyPos();
		});
	});

	function checkStickyPos() {
		if ($('#sticky').css('position') == 'fixed')
		{
			$('#product_section').css({position: 'fixed', top: '50px'});
		}
		else
		{
			$('#product_section').css({position: 'absolute', top: 'auto'});
		}
		
	}
	</script>
