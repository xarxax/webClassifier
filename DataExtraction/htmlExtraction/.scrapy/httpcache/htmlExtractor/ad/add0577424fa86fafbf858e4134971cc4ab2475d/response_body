<script>
$('.plaque').prop('title', 'MY CUP RUNNETH OVER');
new Tippy('.plaque', {
    position: 'top',
    trigger: 'mouseenter',
    interactive: false,
    delay: 0,
    animation: 'perspective',
    arrow: false,
    arrowSize: 'regular',
    animateFill: false,
    duration: 400,
    hideDuration: 0,
    size: 'small',
    theme: 'theme1',
    distance: -40,
    offset: 0,
    hideOnClick: true,
    multiple: true,
    followCursor: false,
    inertia: false
})
</script>

			<script type="text/javascript">

				console.log("Audio Hover - Version 1.0 - MuseThemes.com");
				
				//load audio file
				var audioElementplaque = document.createElement('audio');
				audioElementplaque.setAttribute('src', 'assets/07-johnny-bush---my-cup-runneth-over.mp3');		
				$.get();
				audioElementplaque.addEventListener("load", function() { }, true); 						
				
				//get all needed parameters and variables
				var $audioplaque = $(audioElementplaque);
				var elemplaque = $(".plaque");
				var elePlayingplaque = false;
				var fadeInDelayplaque = 0 * 1000;
				var fadeOutDelayplaque =  0 * 1000;				
				
				// make sure the element exists
				if(elemplaque.length > 0){ 				
					if('hover' == 'hover'){				
						elemplaque.hover(function (){								
								elePlayingplaque = true;	
								playElement(audioElementplaque, $audioplaque, fadeInDelayplaque);			
							}, function () {
								elePlayingplaque = false;
								stopElement(audioElementplaque, $audioplaque, fadeOutDelayplaque);
						});			
					}
					else{	//onclick
						elemplaque.click(function(){						
							if(!elePlayingplaque){
								elePlayingplaque = true;	
								playElement(audioElementplaque, $audioplaque, fadeInDelayplaque);			
							}
							else{
								elePlayingplaque = false;
								stopElement(audioElementplaque, $audioplaque, fadeOutDelayplaque);
							}
						});
					}
				}
				
				//stop and restart the audio clip
				function stopElement(audioElement, $audio, fadeOut){					
					$audio.stop().animate({volume: 0}, fadeOut, 
					function() { 
						audioElement.pause(); 
						audioElement.currentTime = 0; 
					});								
				}
				
				//start the audio clip
				function playElement(audioElement, $audio, fadeIn){					
					audioElement.volume = 0;
					audioElement.play();
					$audio.stop().animate({volume: 1}, fadeIn);								
				}
						
			</script>
		
	<script>
		console.log("Interactive Scale - Version 1.0 - MuseThemes.com");
	</script>
	 

	
	<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/shine.js/0.2.7/shine.min.js"></script>
	<script type="text/javascript">

		console.log("Moving Text Shadows - Version 1.0 - MuseThemes.com");

      var config_text1 = new shinejs.Config({
            numSteps: 20,
            opacity: 0.3,
            offset: 0.4,
            blur: 40,
            shadowRGB: new shinejs.Color(41,171,226)
      });         

      var shine_text1 = new Shine(document.getElementById('text1'), config_text1);

      function handleMouseMove_text1(event) {
        shine_text1.light.position.x = event.clientX;
        shine_text1.light.position.y = event.clientY;
        shine_text1.draw();
      } 
      
      window.addEventListener('mousemove', handleMouseMove_text1, false);
    </script>

   </body>
</html>
