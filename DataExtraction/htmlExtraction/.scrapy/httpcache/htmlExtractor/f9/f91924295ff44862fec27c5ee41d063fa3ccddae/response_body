        <td height="275px" align="center" valign="middle" background="images/home_top_box.png"><br />
          <table width="600" border="0" align="center" cellpadding="0" cellspacing="0">
            <tr>
              <td colspan="2" width="500" id="slideshow">
              	<div id="image" style="float: left; width:32%; min-height:10px; padding-left: 10%;">



             	</div>
              	<div id="text" style="float: left; width:58%; padding-top: 15px; min-height:10px;">


             	 </div>
             	 <div style="clear: both;"></div>
              </td>


              <script>

              	$(document).ready(function(){

              		var slides = {
              			"slide0":{
              				"image_src":"images/slideshow/better_seal_banner.jpg",
              				"headers":{
              					"header1":"Better Product",
              					"header2":"Better Seal",
              					"header3":"It's that Simple."
              				}
              			},
              			"slide1":{
              				"image_src":"images/slideshow/better_hydraulic-cylinder_banner.jpeg",
              				"headers":{
              					"header1":"Better Product",
              					"header2":"Better Service",
              					"header3":"It's that Simple."
              				}
              			}
              		}
              		var count = 0;
              		function slideshow(count){
              			var image = new Image();
              			image.src = slides['slide'+count].image_src;
              			image.style.opacity = 0;

              			image.onload=function(){
              				$('#image').html(image);
              				$(this).animate({opacity:1}, function(){
              					var headerKeys = Object.keys(slides['slide'+count].headers);
              					for(var i = 0; i < headerKeys.length; i++){

              						$('#text').append('<h3 style="font-family: Times New Roman; padding: 0; margin: 10px 0 0px 20px; font-size: 2.6em; opacity: 0; color: #3b0d10;">'+slides['slide'+count].headers[headerKeys[i]]+'</h3>');

              					}
              					for(var i = 0; i < $('#text h3').length; i++){
              						$($('#text h3')[i]).delay(450*i).animate({'opacity':'1'});
              					}
              					setTimeout(function(){
              						$('#image img').fadeOut(function(){
              							$(this).remove();
              						});
              						$('#text h3').fadeOut(function(){
              							$(this).remove();
              							if($('#image img:animated').length == 0 && $('#text h3').length == 0){
              								count++;
              								if(count == Object.keys(slides).length){
              									count=0;
              								}
              								slideshow(count)
              							}
              						});



              					}, 3000);
              				})
              			}
              		}
              		slideshow(count);
              	});

              </script>
            </tr>
          </table></td>
      </tr>
