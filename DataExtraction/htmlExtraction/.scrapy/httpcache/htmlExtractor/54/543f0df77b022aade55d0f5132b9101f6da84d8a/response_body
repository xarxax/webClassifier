<script>
var pictures=jQuery.parseJSON('[{\"image\":\"http://www.floodheliarc.com/wp-content/uploads/2015/09/Flood-Animation.gif\",\"title\":\"\",\"description\":\"\",\"link\":\"\"},{\"image\":\"http://www.floodheliarc.com/wp-content/uploads/2014/08/homeslide-07.jpg\",\"title\":\"\",\"description\":\"\",\"link\":\"\"},{\"image\":\"http://www.floodheliarc.com/wp-content/uploads/2014/08/homeslide-02.jpg\",\"title\":\"\",\"description\":\"\",\"link\":\"\"},{\"image\":\"http://www.floodheliarc.com/wp-content/uploads/2014/08/homeslide-09.jpg\",\"title\":\"\",\"description\":\"\",\"link\":\"\"},{\"image\":\"http://www.floodheliarc.com/wp-content/uploads/2015/11/Flood-Animation-1.mp4\",\"title\":\"\",\"description\":\"\",\"link\":\"\"}]');
var total_slides=pictures.length-1;
var current_slide=-1;
var slider_width=jQuery(window).outerWidth();
var timer;
var advance_delay=10000;

//console.log(pictures);

jQuery("#slide_wrap").css("width",slider_width*total_slides+slider_width+100);

//LOAD THE IMAGES
jQuery.each(pictures,function(index,picture){
	jQuery("#slide_wrap").append("<div class='slide' style='background:url("+pictures[index].image+") center center no-repeat; background-size:cover; width:"+slider_width+"px;'></div>");
});

jQuery(function(){
	jQuery(window).resize(function() {
        slider_width=jQuery(window).outerWidth();
		jQuery(".slide").css("width",""+slider_width+"px");
		jQuery("#slide_wrap").css("left",""+(current_slide*slider_width)*-1+"px").show();
    });
	
	jQuery("#slide_wrap").css("left",""+slider_width+"px").show();
	
	jQuery.each(pictures,function(index,picture){
		jQuery("#slideshow_controls").append("<span class='slideshow_button' id='"+index+"'></span>");
	});
	
	jQuery(".slideshow_button").click(function(){
		do_slide(parseInt(jQuery(this).attr("id")));
		return false;
	});

	do_slide(0);
	
	timer=setInterval(function(){
		auto_advance();
	}, advance_delay);
	
});

function auto_advance(){
	if(current_slide==total_slides){
		do_slide(0);
	}else{
		do_slide(current_slide+1);
	}
}

function do_slide(slide_num){
	clearInterval(timer);
	//console.log(slide_num);
	jQuery("#slide_wrap").stop().animate({left:(slide_num*slider_width)*-1},1000,'easeInOutCirc');
	jQuery(".slideshow_button").removeClass("active");
	jQuery("#"+slide_num+".slideshow_button").addClass("active");
	jQuery("#slideshow_wrap").click(function(){window.location=pictures[slide_num].link});
	jQuery("#slideshow_text").stop().animate({top:500},300,'easeInOutCirc',function(){
		jQuery("#slideshow_text h2").html(pictures[slide_num].title);
		jQuery("#slideshow_text p").html(pictures[slide_num].description);
		setTimeout(function() {
			jQuery("#slideshow_text").stop().animate({top:114},300,'easeInOutCirc');
		}, 1000);
		
	});
	current_slide=slide_num;
	timer=setInterval(function(){
		auto_advance();
	}, advance_delay);
	
}
</script>



