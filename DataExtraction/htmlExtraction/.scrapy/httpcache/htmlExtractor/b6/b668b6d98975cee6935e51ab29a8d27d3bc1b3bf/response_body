<script type="text/javascript">
    function filterSearchInput(e,t) {
        var keyCode;
        var dashCode;
        var searchText;
        searchText = t.value;
        dashCode = 45;
        if (window.event) {
            keyCode = window.event.keyCode;
            e = window.event;
        } else if (e) {
            keyCode = e.which;
        } else {
            return true;
        }
        if (searchText.substring(0, 1) == "9") {
            if (keyCode == dashCode) {
                alert("Please omit any dashes and only use numeric digits 0-9 when searching for ISBN numbers");
                return false;
            }
        }
        return true;
    }

    function stripDashes() {
        var searchField = document.getElementById("CT_Header_0_txtKeyword")
        var searchText;
        searchText = searchField.value;
        if (searchText.substring(0, 1) == "9") {
            searchText = searchText.replace(/-/g,"");
            searchField.value = searchText;
        }
    }
</script>




