<div  style = 'padding-left: 15px; padding-right: 15px; padding-top: 15px; padding-bottom: 15px;' class = 'col-sm-6'>
<div  style = 'padding-left: 30px; padding-right: 30px; padding-top: 30px; padding-bottom: 30px; border-width: 1px; border-style: solid; border-color: #fff; background-color: #000;' class = 'container text-white'>
