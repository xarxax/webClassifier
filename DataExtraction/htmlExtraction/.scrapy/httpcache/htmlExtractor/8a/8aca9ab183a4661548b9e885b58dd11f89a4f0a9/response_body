        <script type="text/javascript">
            $(document).ready(function () {
                $('#bgCompany_Option_box_button_22791').click(function () {
                    var companyId = $('#bgCompany_Option_box_22791').attr('value');
                    if (companyId != 0) {
                        $.post('/contents/slugUrl/',
                                {
                                    "id1": companyId,
                                    "id2": '',
                                    "class": 'Content'
                                },
                        function (data) {
                            if (data) {
                                window.location = "/buyersguide/profile" + data + '/22791';
                            }
                        });
                    }
                });
                var select = $('#bg_category_22791 select#bgCategory_Select_box_22791 option').length;
                if (select === 2) {
                    $('#bg_category_22791 select#bgCategory_Select_box_22791').hide();
                    $('#bg_category_22791 h4').addClass('hide-h4');
                    var id = $('div#bg_category_22791 select#bgCategory_Select_box_22791 option:nth-child(2)').val();
                    var div_id = "bgCategory_Select_box_Child_Child_22791";
                    $('#' + div_id).show();
                    dev(id, div_id);
                }
                $('#bgCategory_Select_box_22791').on('change', function () {
                    var parent_val = $('#bgCategory_Select_box_22791').val();
                    if (parent_val !== '') {
                        var div_id = 'bgCategory_Select_box_Child_22791';
                        $('#bgCategory_Select_box_Child_Child_22791').hide();
                        $('#bgCategory_Select_box_Child_22791').show();
                        $('#' + div_id).empty();
                        dev(parent_val, div_id);
                    } else {
                        $('#bgCategory_Select_box_Child_Child_22791').hide();
                        $('#bgCategory_Select_box_Child_22791').hide();
                    }
                });
                $('#bgCategory_Select_box_Child_22791').on('change', function () {
                    var child_val = $('#bgCategory_Select_box_Child_22791_OB').val();
                    if (child_val !== '0') {
                        var div_id = 'bgCategory_Select_box_Child_Child_22791';
                        $('#' + div_id).show();
                        $('#' + div_id).empty();
                        dev(child_val, div_id);
                    } else {
                        $('#bgCategory_Select_box_Child_Child_22791').hide();
                    }
                });
                $('#bgCategory_Select_box_Child_Child_22791').on('change', function () {
                    var child_val_1 = $('#bgCategory_Select_box_Child_Child_22791_OB').val();
                    if (child_val_1 !== '0') {
                        var div_id = '';
                        dev(child_val_1, div_id);
                    }
                });
                function dev(id, div_id) {
                    $.post('/contents/getBGCategory/',
                            {
                                "id1": id,
                                "name": div_id,
                                "divClass": 'form-control'
                            },
                    function (data) {
                        var magazine = '10';
                        if (data.match("/buyersguide/")) {
                            if (magazine === '14') {
                                var result = data.substring(1, data.length - 1);
                                var last = result.substring(result.indexOf("/") + 1, result.length);
                                window.location = ("/" + last);
                                return false;
                            }
                            window.location = (data);
                        }
                        else {
                            $('#' + div_id).append('<h4>Select A Sub Category:</h4>');
                            $('#' + div_id).append(data);
                            $('#' + div_id).append('<div class="spacer5"></div>');
                        }
                    }
                    );
                }
            });
            function view_buyersguide(input, slug) {
                var category_parent_val = $('select#bgCategory_Select_box_' + input).val();
                var category_child_val = $('#bgCategory_Select_box_Child_' + input + '_OB').val();
                if (category_parent_val === '') {
                    window.location = ('/' + slug + '/');
                } else if (category_child_val == '0') {
                    var category_val = category_parent_val;
                } else {
                    category_val = category_child_val;
                }
                $.post('/contents/getBGCategory/',
                        {
                            "id1": category_val,
                            "name": '',
                            "divClass": 'form-control'
                        },
                function (data) {
                    if (data.match("/" + slug + "/")) {
                        window.location = (data);
                    }
                });
            }
        </script>
    </div>
