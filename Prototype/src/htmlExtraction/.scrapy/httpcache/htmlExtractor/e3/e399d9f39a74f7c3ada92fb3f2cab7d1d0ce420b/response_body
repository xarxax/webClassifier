				(function($) {
					function switchHeaderStyle(){
						var scrollTop = $(window).scrollTop();
						if(scrollTop > 0){
							$('#header').addClass('solid-header');
						}
						else{
							$('#header').removeClass('solid-header');
						}
					}
					$(document).ready(function(){
						switchHeaderStyle();
					});
					$(window).scroll(function(){
						switchHeaderStyle();
					});
				})( jQuery );
