<script type="text/javascript">
    (function () {
        var initialLoadAds = function (isMobile) {
            if (!isMobile) {
                CMNUNT.load('3x3', {
                    containerId: 'complex_leaderboard_header'
                });
                CMNUNT.load('728x91', {
                    containerId: 'complex_leaderboard_footer'
                    , lazyLoad: true
                });
            } else {
                CMNUNT.load('top', {
                    containerId: 'complex_leaderboard_header'
                });
            }
            return true;
        };
        var firstAdsLoaded = false;
        if (document.readyState === 'complete' && !firstAdsLoaded) {
            firstAdsLoaded = initialLoadAds(isMobile);
        } else {
            document.addEventListener('readystatechange', function () {
                if (document.readyState === 'complete' && !firstAdsLoaded) {
                    firstAdsLoaded = initialLoadAds(isMobile);
                }
            });
        }
    })();
</script>
