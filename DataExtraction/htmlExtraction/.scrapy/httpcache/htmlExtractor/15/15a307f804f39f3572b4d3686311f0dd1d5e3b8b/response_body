                <div class="col-left sidebar"><script>
	function vertnav_accordion(id, level, state, count) {
		if (state == 0) {
			if (count > 0) {
				document.getElementById('cat'+id).style.display = "block";
				document.getElementById('licat'+id).style.backgroundImage = "url(https://ndtsupply.com/skin/frontend/default/default/images/list-active.gif)";
				if (level < 2)document.getElementById('spancat'+id).style.color = "#3F85BE"; document.getElementById('catbutton'+id).innerHTML = '<a href="#" onclick="vertnav_accordion('+id+', '+level+', 1, '+count+'); return false;"><span class="vertnav-button"></span></a>';
			}
		} 
		else {
			if (count > 0) {
				document.getElementById('cat'+id).style.display = "none";
				document.getElementById('licat'+id).style.backgroundImage = "url(https://ndtsupply.com/skin/frontend/default/default/images/list-inactive.gif)";
				if (level < 2) document.getElementById('spancat'+id).style.color = "#181852"; document.getElementById('catbutton'+id).innerHTML = '<a href="#" onclick="vertnav_accordion('+id+', '+level+', 0, '+count+'); return false;"><span class="vertnav-button"></span></a>';
			}
		}
	}
</script>
