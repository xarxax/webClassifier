            },
        },        
        '#emailid' : {
            'required': {
            'message': "Please enter email"
            },
            'custom[email]': {
            'message': "Invalid email address"
            },
        },
       '#phone' : {
            'required': {
            'message': "Please enter phone"
            },
            'custom[phone]': {
