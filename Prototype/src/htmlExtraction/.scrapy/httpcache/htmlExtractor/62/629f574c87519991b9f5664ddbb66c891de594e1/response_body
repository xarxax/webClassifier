<script type="text/javascript">
    (function($) {
        $(function() {
            var validator = $("#loginForm").validate({
                rules:{
                    'data[email]': {
                        required: true,
                        email: true
                    },
                    'data[password]': {
                        required: true,
                        password: true
                    }
                },
                messages:{
                    'data[password]': {
                        required:'required'
                    },
                    'data[email]':{
                        required:'required',
                        email:'invalid'
                    }
                }
            });
            $("#loginForm input").one('blur',function() {
                validator.element(this);
            });
        });
    })(jQuery);
</script>
<script type="text/javascript">
    (function($) {
        $(function() {
            var validator3 = $("#subscribe").validate({
                rules:{
                    'data[email]': {
                        required: true,
                        email: true
                    },
                    'data[first-name]': 'required',
                    'data[last-name]': 'required',
                },
                messages:{
                    'data[first-name]': {
                        required:'required'
                    },
                    'data[last-name]': {
                        required:'required'
                    },
                    'data[email]':{
                        required:'required',
                        email:'invalid'
                    }
                }
            });
            $("#subscribe input").one('blur',function() {
                validator3.element(this);
            });

            $('#subscribe').submit(function(){
                var $this = $(this);
                if(!$this.find('label.error:visible').length){
                    $this.find('input').not('.pinksubmit').attr('readonly', 'readonly').css({backgroundColor:'#ccc'});
                    $this.find('input.pinksubmit').val('Please wait...');
                    $this.find('p.error').hide();

                    $.ajax({
                        url: $this.attr('action'),
                        type: 'POST',
                        data: $this.serialize(),
                        dataType: 'json',
                        success: function(data){
                            resetSubscribe($this);

                            if(data.success){
                                $this.find('p').not('.updates').hide();
                                $this.find('p.success').show();
                            }else{
                                $this.find('p.error').show().text(data.error);
                            }
                        },
                        error: function(){
                            resetSubscribe($this);
                        }
                    });
                }
                return false;
            });

            function resetSubscribe($this){
                $this.find('input').not('.pinksubmit').removeAttr('readonly').css({backgroundColor:'#ddd'});
                $this.find('input.pinksubmit').val('Submit');
            }
        });
    })(jQuery);
</script>
