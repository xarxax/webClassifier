<script type="text/javascript">

function checkMiniSearch()
{
	if(jQuery('#addressORZip').val() == "")
	{
		alert("Please enter address OR zip to search");
		
		return false;
	}
	
	if(jQuery('#searchRadius').val() == "")
	{
		alert("Please select Radius for search");
		return false;	
	}
	return true;
}

</script>    

