    <script language="javascript" type="text/javascript">
        $(document).ready(function () {

            var slideButtons = $('.slide-buttons');

            var $sliderText = $('.sliderText').cycle({
                fx: 'scrollLeft',
                timeout: 8000,
                speedIn: 1500,
                speedOut: 650,
                cleartype: true, cleartypeNoBg: true
            });

            var $sliderImages = $('.slider-images').cycle({
                fx: 'scrollLeft',
                timeout: 8000,
                speedIn: 1500,
                speedOut: 1800
            });

            $sliderText.children().each(function (i) {
                var iplusone = i + 1;
                $('<div class="slide-number slider' + iplusone + '"><a href="#">' + iplusone + '</a>').appendTo(slideButtons).click(function () {

                    $sliderText.cycle(i);
                    $sliderImages.cycle(i);
                    return false;
                });
            });

            $('<div class="slide-number slider-pause"><a href="#"><img class="playbutton" src="http://www.mceproducts.com/images/pause.png" /></a></div>').appendTo(slideButtons).click(function () {

                var paused1 = $sliderText.is(':paused');
                var paused2 = $sliderImages.is(':paused');

                if (paused1) {
                    $('.playbutton').attr('src', "http://www.mceproducts.com/images/pause.png");
                } else {
                    $('.playbutton').attr('src', "http://www.mceproducts.com/images/play.png");
                }

                $sliderText.cycle((paused1 && paused2) ? 'resume' : 'pause', true);
                $sliderImages.cycle((paused1 && paused2) ? 'resume' : 'pause', true);

            });

            //$('.slider-pause').css('visibility', 'hidden');

            //$('#content-slider').live('mouseover mouseout', function (event) {
			//
            //    var paused = $sliderImages.is(':paused');
			//	
            //    if (event.type == 'mouseover') {
            //        $('.slider-pause').css('visibility', 'visible');
            //    } else {
            //        if (paused === false) {
            //            $('.slider-pause').css('visibility', 'hidden');
            //        }
            //    }
            //});

        }); 
    </script>
