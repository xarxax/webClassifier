	function StringtoXML(text){
                if (window.ActiveXObject){
                   var doc = new ActiveXObject("Microsoft.XMLDOM"); 
		   doc.loadXML(text);

                } else { 
                  var parser=new DOMParser();
                  var doc=parser.parseFromString(text,'text/xml');
                }
                return doc;
            }

	
	
