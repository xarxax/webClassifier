<div id="divcomp401426" class="comp-content comp-home-1011"><script type="text/javascript">
//<![CDATA[

var active_slide = 1;
var number_of_slides = 0;
var timer;
var time = 5000;
function slideSwitch2(slide){
  clearInterval(timer);
  
  $("#rotatingContent" + active_slide).fadeOut('fast', function() {
    $(".rotatingDiv").hide();
  
    $("#rotatingContent" + active_slide).removeClass('active-slide');
   
        $("#rotatingContent" + slide).fadeIn('fast', function() {
          $("#rotatingContent" + slide).addClass('active-slide');
         });
   });
  active_slide = slide;
  
  timer = setInterval( "slideSwitch()", time );
}

function slideSwitch() {
    var $active = $('#rotatingContent' + active_slide);
  var next_slide = active_slide + 1;
  if(next_slide > number_of_slides)
    next_slide = 1;
    var $next =  $('#rotatingContent' + next_slide);

   
    $active.fadeOut('fast', function() {
          $active.removeClass('active-slide');
         
          $next.fadeIn('fast', function() {
            $next.addClass('active-slide');
           });
     });
    active_slide = next_slide;
}


$(function() {
  
    if($(".rotatingDiv").size() > 1){
      timer = setInterval( "slideSwitch()", time );
    }
});
//]]>
</script>
  
  
