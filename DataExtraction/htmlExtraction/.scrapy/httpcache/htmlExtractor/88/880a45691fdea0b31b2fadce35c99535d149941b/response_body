<script type="text/javascript">
    $(function () {
        rotateHeader(-1, 0);
    });

    $(function () {
        $("#HeaderPlayPause").click(function () {
        if ($(this).attr("class") == "pauseActive") {
            $("#HeaderPlayPause").removeClass("pauseActive").addClass("playActive");
            vPlay = false;
        }
        else {
            $("#HeaderPlayPause").removeClass("playActive").addClass("pauseActive");
            var thumbIDArray = $(".thumbActive").attr("ID").split("-");
            var curIndex = jQuery.inArray(parseInt(thumbIDArray[1]), vHeaderIDArray);
           
            if (curIndex + 1 == vHeaderIDArray.length) {
               vPlay = true;
               rotateHeader(curIndex, 0);
            }
            else
            {
                vPlay = true;
                rotateHeader(curIndex, curIndex + 1);
            }
        }

        });

        $("#HeaderPrev").click(function () {
            clearTimeout(tOut);
            if (vPlay == false) {
                vPlay = true;
                $("#HeaderPlayPause").removeClass("playActive").addClass("pauseActive");
            }
            var thumbIDArray = $(".thumbActive").attr("ID").split("-");
            var curIndex = jQuery.inArray(parseInt(thumbIDArray[1]), vHeaderIDArray);
            if (curIndex == 0)
            {
                rotateHeader(curIndex, parseInt(vHeaderIDArray.length) - 1);
            }
            else
            {
                rotateHeader(curIndex, curIndex - 1);
            }
        });

        $("#HeaderNext").click(function () {
            clearTimeout(tOut);
            if (vPlay == false) {
                vPlay = true;
                $("#HeaderPlayPause").removeClass("playActive").addClass("pauseActive");
            }
            var thumbIDArray = $(".thumbActive").attr("ID").split("-");
            var curIndex = jQuery.inArray(parseInt(thumbIDArray[1]), vHeaderIDArray);
            if (curIndex + 1 == vHeaderIDArray.length) {
                rotateHeader(curIndex, 0);
            }
            else {
                rotateHeader(curIndex, curIndex + 1);
            }
        });
    });

    $(document).on('mouseenter', '.thumbInactive', function (e) {
        var newIDArray = $(this).attr("ID").split("-");
        $("#ThumbImage" + newIDArray[1]).fadeTo('fast', 1.0);
    });

    $(document).on('mouseout', '.thumbInactive', function (e) {
        var newIDArray = $(this).attr("ID").split("-");
        $("#ThumbImage" + newIDArray[1]).fadeTo('fast', 0.0);
    });

    $(document).on('click', '.thumbInactive', function (e) {
        clearTimeout(tOut);
        if (vPlay == false) {
            vPlay = true;
            $("#HeaderPlayPause").removeClass("playActive").addClass("pauseActive");
        }
            var newIDArray = $(this).attr("ID").split("-");
            var curIDArray = $(".thumbActive").attr("ID").split("-");
            var newIndex = jQuery.inArray(parseInt(newIDArray[1]), vHeaderIDArray);
            var curIndex = jQuery.inArray(parseInt(curIDArray[1]), vHeaderIDArray);
            rotateHeader(curIndex, newIndex);
        });

    function rotateHeader(curArrayIndex, nextArrayIndex) {
        if (curArrayIndex == -1) {
            if (vPlay == true) {
                $("#HeaderImage" + vHeaderIDArray[nextArrayIndex]).fadeTo('slow', 1.0);
                $("#LinkLayer" + vHeaderIDArray[nextArrayIndex]).fadeTo('slow', 1.0);
                $("#AdLayer" + vHeaderIDArray[nextArrayIndex]).fadeTo('slow', 1.0);
                $("#HeaderThumb-" + vHeaderIDArray[nextArrayIndex]).removeClass("thumbInactive").addClass("thumbActive");
                $("#ThumbImage" + vHeaderIDArray[nextArrayIndex]).fadeTo('fast', 0.0);
                tOut = setTimeout(function () { rotateHeader(nextArrayIndex, parseInt(nextArrayIndex) + 1); }, 7500);
            }
        }
        else {
            if (vPlay == true) {
                $("#HeaderImage" + vHeaderIDArray[curArrayIndex]).fadeTo(1000, 0.0);
                $("#LinkLayer" + vHeaderIDArray[curArrayIndex]).hide();
                $("#AdLayer" + vHeaderIDArray[curArrayIndex]).fadeTo(1000, 0.0);
                $("#HeaderThumb-" + vHeaderIDArray[curArrayIndex]).removeClass("thumbActive").addClass("thumbInactive");
                $("#HeaderImage" + vHeaderIDArray[nextArrayIndex]).delay(200).fadeTo(1000, 1.0);
                $("#LinkLayer" + vHeaderIDArray[nextArrayIndex]).delay(200).show();
                $("#AdLayer" + vHeaderIDArray[nextArrayIndex]).delay(200).fadeTo(1000, 1.0);
                $("#ThumbImage" + vHeaderIDArray[nextArrayIndex]).fadeTo('fast', 0.0);
                $("#HeaderThumb-" + vHeaderIDArray[nextArrayIndex]).removeClass("thumbInactive").addClass("thumbActive");
                if (nextArrayIndex + 1 == vHeaderIDArray.length) {
                    tOut = setTimeout(function () { rotateHeader(nextArrayIndex, 0); }, 7500);
                }
                else {
                    tOut = setTimeout(function () { rotateHeader(nextArrayIndex, parseInt(nextArrayIndex) + 1); }, 7500);
                }

            }
        }
    }

    function preload(arrayOfImages) {
        $(arrayOfImages).each(function () {
            $('<img/>')[0].src = this;
        });
    }
</script>


