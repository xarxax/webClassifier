	<script type="text/javascript">
	<!--
		var defaultSlideShowSpeed = 5000;
		var defaultSlideShowFade = 1000;
		var defaultClickFade = 750;
		var curFade = 0;
		var fadeSlideTimer;
		var fadePause = false;
		var numberOfFades = 0;
		var boolInFadeRoutine = false;
		var boolNavShowing = false;

		var imageArray = new Array();
		var linkArray = new Array();
		
		imageArray[0] = [
			new Image(),
			new Image()
		];
		imageArray[0][0].src = 'http://www.helac.com/img/home/jquery-square-off.png';
		imageArray[0][1].src = 'http://www.helac.com/img/home/jquery-square-on.png';
		
		linkArray = [
'','','','','','','',''		];
		
		$(document).ready(function(){
			$('#fade').show();
			
			$('#Navigation-Buttons').position({
				my: 'right bottom',
				at: 'right bottom',
				of: $('.MainSlideShowBox:eq(0)'),
				offset: '-5 -35'
			});
			
			$('.fadePhoto:eq(' + curFade + ')').show();
			$('.fadeInfo:eq(' + curFade + ')').show();
			$('.fadeButton:eq(' + curFade + ')').find('img').attr("src", imageArray[0][1].src);
			
//			$('.MainSlideShowBox').click(function(){
//				window.location = linkArray[curFade];
//			});
			
			numberOfFades = $('.fadePhoto').size();
			
			fadeSlideTimer = setTimeout('fadeNext()', defaultSlideShowSpeed);

			$('.MainSlideShowBox, #Navigation-Buttons').mouseenter(function(){
//					$(this).css({
//						'cursor': 'pointer'
//					});
					if (! boolNavShowing) {
						$('#Navigation-Buttons').show();
						$('#Navigation-Buttons').position({
							my: 'right bottom',
							at: 'right bottom',
							of: $('.MainSlideShowBox:eq(0)'),
							offset: '-5 -35'
						});
						$('#SlideDescriptionBox').show();
						$('.fadeInfo').hide();
						$('.fadeInfo:eq(' + curFade + ')').show();
						
						boolNavShowing = true;
					}
			}).mouseleave(function(){
					$(this).css({
						'cursor': 'default'
					});
					if (boolNavShowing) {
						$('#Navigation-Buttons').hide();
						$('.fadeInfo').hide();
						$('#SlideDescriptionBox').hide();
						boolNavShowing = false;
					}
			});
/*
//			$('.MainSlideShowBox').hover(function() {
//					$(this).css({
//						'cursor': 'pointer'
//					});
//				}, function() {
//					$(this).css({
//						'cursor': 'default'
//					});
//			});
*/
/*
			$('.MainSlideShowBox').click(function() {
				fadeNext();
			});
*/
		});
		
		function fadeTo (fadeNumber, fadeSpeed, fadeSlideShow) {
			if (! boolInFadeRoutine) {
				boolInFadeRoutine = true;
				
//				$('.MainSlideShowBox').css({
//					'cursor': 'progress'
//				});
			fadePause = false;
			var lcurFade = curFade;
			if (! fadeSlideShow) {
				clearTimeout (fadeSlideTimer);
			}
			var fade;
			if (fadeNumber != lcurFade) {
				// FADE IN
					// turn off links
					$('.fadeButton:eq(' + lcurFade + ')').find('img').attr("src", imageArray[0][0].src);
					// turn off links
					fade = $('.fadePhoto:eq(' + fadeNumber + ')');
			    // if the element is currently being animated (to a fadeOut)
			    if (fade.is(':animated')) {
			      // take it's current opacity back up to 1
			      fade.stop(true, false).fadeIn(fadeSpeed);
			    } else {
			      // fade in quickly
			      fade.fadeIn(fadeSpeed);
			    }
					fade = $('.fadeInfo:eq(' + fadeNumber + ')');
			    // if the element is currently being animated (to a fadeOut)
			    if (fade.is(':animated')) {
			      // take it's current opacity back up to 1
			      fade.stop(true, false).fadeIn(fadeSpeed);
			    } else {
			      // fade in quickly
			      fade.fadeIn(fadeSpeed);
			    }
				
				// FADE OUT		
					fade = $('.fadePhoto:eq(' + lcurFade + ')');
			    if (fade.is(':animated')) {
			      fade.stop(true, false).fadeOut(fadeSpeed);
			    } else {
			      // fade away quickly
			      fade.fadeOut(fadeSpeed);
			    }
					fade = $('.fadeInfo:eq(' + lcurFade + ')');
				if (fade.is(':animated')) {
					fade.stop(true, false).fadeOut(fadeSpeed, function() {
						// Animation complete.
						curFade = fadeNumber;
						boolInFadeRoutine = false;
//							$('.MainSlideShowBox').css({
//							'cursor': 'pointer'
//						});
					});
			    } else {
			      // fade away quickly
					fade.fadeOut(fadeSpeed, function() {
						// Animation complete.
						curFade = fadeNumber;
						boolInFadeRoutine = false;
//							$('.MainSlideShowBox').css({
//							'cursor': 'pointer'
//						});
					});
			    }
					// turn on links
					$('.fadeButton:eq(' + fadeNumber + ')').find('img').attr("src", imageArray[0][1].src);
					// turn on links
			}
			else {
				// nothing
				curFade = fadeNumber;
				boolInFadeRoutine = false;
//					$('.MainSlideShowBox').css({
//					'cursor': 'pointer'
//				});
			}
			}
		} // end fadeTo ()
		
		function fadeNext () {
			clearTimeout (fadeSlideTimer);
			if (curFade >= numberOfFades - 1) {
				fadeTo (0, defaultSlideShowFade, true);
			}
			else {
				fadeTo (curFade + 1, defaultSlideShowFade, true);
			}
			fadeSlideTimer = setTimeout('fadeNext()', defaultSlideShowSpeed);
		} // end fadeNext ()
	//-->
	</script>
