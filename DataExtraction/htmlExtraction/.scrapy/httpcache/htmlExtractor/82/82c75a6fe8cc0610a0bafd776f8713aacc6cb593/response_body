	<script>
		$(function() {
			$('.type_radio').click(function(event) {
				if($(this).data('id') == 1) {
					$('#school-group').toggleClass('hide-me');
					if($(this).is(':checked')) {
						$('#school-group input').attr('validate-required', 'true');	
					} else {
						$('#school-group input').removeAttr('validate-required');
					}					
				}
				if($(this).data('id') == 2) {
					$('#company-group').toggleClass('hide-me');
					if($(this).is(':checked')) {
						$('#company-group input').attr('validate-required', 'true');	
					} else {
						$('#company-group input').removeAttr('validate-required');
					}					
				}
			})
			$('.sample_radio').click(function(event) {
				if($(this).data('id') == 1) {
					$('#deets-group').addClass('hide-me');
					$('#deets-group input').removeAttr('validate-required');					
				}
				if($(this).data('id') == 2) {
					$('#deets-group').removeClass('hide-me');
					$('#deets-group input').attr('validate-required', 'true');	
					$('#company-group input').removeAttr('validate-required');
				}
			})
		})
	</script>
		</div>
	</div>
</section>

