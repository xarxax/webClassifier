function validaRegistro(frmForm){
	

	if(frmForm.email.value == ""){
		alert("NO PUEDE QUEDAR VACIO EL CAMPO DE EMAIL, FAVOR DE VERIFICAR...");
		frmForm.email.focus();
		return (false);
	}
	
	
	
else if(frmForm.email.value != ""){
	
	
	var s = frmForm.email.value;
var filter=/^[A-Za-z0-9_.][A-Za-z0-9_.]*@[A-Za-z0-9_]+\.[A-Za-z0-9_.]+[A-za-z]$/;
if (s.length == 0 ){ document.frmForm.submit();}
if (filter.test(s)){
	document.frmForm.submit();}
else{
alert("INGRESE UNA DIRECCION DE EMAIL CORRECTA");
frmForm.email.focus();
return false;}
}	





}


</script>

