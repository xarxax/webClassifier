            <script>

                $(function() {

                    "use strict";

                    var scroll_mode_u1703 = "panoramic";

					if(scroll_mode_u1703 == "standard") {
        				var standardSlider = $('#u1703 .bxslider').bxSlider({
						                    	mode: 'horizontal',
												minSlides: 1,
												maxSlides: 1,
												slideMargin: 0,
												ticker: true,
												speed: 40000,
												autoDirection: 'next'
											});

        				$( window ).resize(function() {
							standardSlider.reloadSlider();
						});
					}
					else {
						(function slide(){
						    $('#u1703 .panoramic-image').animate({backgroundPosition : '-=1px'}, 20, 'linear', slide);
						})();
					}

                });

            </script>
            
        
   </body>
