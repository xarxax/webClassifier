		   <script type="text/javascript">
// 		window.onload=function(){
			$(document).ready(function(){
			$.ajaxSetup({  
			    async : false  
			});
			$.get("/teacher/listAllSchedule.do?rows=10", function(result){
				var data=JSON.parse(result);
				var rows=data.rows;
				if(rows.length==1){
					var temp="<img class='swiper-slide' src='"+rows[0].schedulename+"' alt='' />";
					$("#swiper-slide-parent").removeClass();
					$("#swiper-slide").append(temp);
					$("#swiper-button-prev").removeClass();
					$("#swiper-button-next").removeClass();
				}else{
					var temp="";
					for(var i=0;i<rows.length;i++){
						 temp=temp+"<img class='swiper-slide' src='"+rows[i].schedulename+"' alt='' />";
					}
					$("#swiper-slide").append(temp);
				}
				
			  });
			$.get("/news/findAll.do?rows=1&search_language=1&typeid=1", function(result){
				var data=JSON.parse(result);
				var rows=data.rows;
				if(rows.length==1){
					var content=rows[0].content;
					if(content.indexOf('<img src="')>0){
					var index_a=content.indexOf('<img src="')+10;
					var index_b=content.indexOf('/>',index_a);
					var index_c=content.indexOf('"',index_a);
					var imgsrc=content.substring(index_a,index_c);
					content=content.substring(0,index_a-10)+content.substring(index_b+2,content.length);
					$("#indexNewsImg").attr("src",imgsrc);
					}
					$("#indexNewsContent").html(content);
					$("#indexNewsTitle").html(rows[0].title);
					
					$("#indexNewsDetail").attr("href","html/news_details.html?id="+rows[0].id);
				}
				
			  });
		    var swiper = new Swiper('.swiper-container', {
		        pagination: '.swiper-pagination',
		        slidesPerView: 1,
		        paginationClickable: true,
		        spaceBetween: 30,
		        keyboardControl: true,
		        nextButton: '.swiper-button-next',
		        prevButton: '.swiper-button-prev',
		        autoplay: 3000
		    });
			$.get("/resource/findAll.do?rows=10&search_language=1&favtimes=1", function(result){
				var data=JSON.parse(result);
				var rows=data.rows;
				var length;
				if(rows.length>3){
					length=3;
				}else{
					length=rows.length;
				}
				for(var i=0;i<length;i++){
					var temp="<a href='html/products_details.html?id="+rows[i].id+"'><li><img src='"+rows[i].filename+"' alt=''/><p>"+rows[i].resourcename+"</p></li></a>";
					$("#productShow").append(temp);
				}
				
			  });
		});
		</script>
