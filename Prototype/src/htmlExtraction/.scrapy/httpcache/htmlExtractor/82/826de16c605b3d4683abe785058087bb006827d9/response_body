            $(document).ready(function() {
                // change the category title
                $(".wpgmza_sl_category_innerdiv1").html("Product:");
                // fix the checkboxes
                var checkboxCounter = 0;
                $(".wpgmza_sl_category_innerdiv2 > div > div").each(function() {
                    checkboxCounter++;
                    $(this).removeAttr("style");
                    var text = $(this).text();
                    var inputID = $(this).children("input").attr("id");
                    var input = $(this).children("input")[0].outerHTML;
                    $(this).text("");
                    $(this).append(input);
                    $(this).append("<label for='" + inputID + "'>" + text + "</label>");
                });
                $(".wpgmza_sl_category_innerdiv2 > div > div").unwrap();
                $(".wpgmza_sl_category_innerdiv2 > div").each(function() {
                    if ($(this).index() + 1 > Math.ceil((checkboxCounter / 3 * 2 ))) {
                        $(this).addClass("col3");
                    } else if ($(this).index() + 1 > Math.ceil(checkboxCounter / 3)) {
                        $(this).addClass("col2");
                    } else {
                        $(this).addClass("col1");
                    }
                });
                $(".wpgmza_sl_category_innerdiv2 > div.col1").wrapAll("<div class='col' />");
                $(".wpgmza_sl_category_innerdiv2 > div > div.col1").removeAttr("class");
                $(".wpgmza_sl_category_innerdiv2 > div.col2").wrapAll("<div class='col' />");
                $(".wpgmza_sl_category_innerdiv2 > div.col > div.col2").removeAttr("class");
                $(".wpgmza_sl_category_innerdiv2 > div.col3").wrapAll("<div class='col' />");
                $(".wpgmza_sl_category_innerdiv2 > div.col > div.col3").removeAttr("class");
                // hide the weird select box
                $("#locationSelect").parent("div").css("height", "0");
                $("#locationSelect").parent("div").css("overflow", "hidden");
                $("#locationSelect").parent("div").css("width", "0");
                // fix table title
                setTimeout(function() {
                    $(".wpgmza_table_description.sorting").children("strong").text("Contact");
                }, 500);
                // expand thingy
                $(".wpgmza_sl_category_div .wpgmza_sl_category_innerdiv1").click(function() {
                    $(this).parent(".wpgmza_sl_category_div").toggleClass("open");
                })
            });
