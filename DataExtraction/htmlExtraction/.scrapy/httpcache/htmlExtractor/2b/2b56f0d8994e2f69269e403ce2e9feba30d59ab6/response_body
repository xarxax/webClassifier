    <script>
        $('#showList li').each(function(index){
            $(this).html(index + 1);

            $(this).click(function(){
                window.clearTimeout(window.timer);

                $('#showList li').removeClass('special');
                $(this).addClass('special');

                var li = $(this);

                // 准备背景图片
                var backgroundImage = "url('" + $(li).attr('data-background-image') + "')";
                var old_backgroundImage = $('#mainMidFlash_banner2').css('background-image');

                $('#mainMidFlash_banner2').css('background-image', backgroundImage);
                $('#mainMidFlash_banner')
                    .css('background-image', old_backgroundImage)
                    .css('opacity', '1')
                    .animate({opacity:0}, 2500);

                $('#mainMidFlash').animate({opacity:0}, 1000, function(){
                    $('#flash_div a').attr('href', ($(li).attr('data-url')));
                    $('img', $('#flash_div a').eq(0)).attr('src', $(li).attr('data-img'));
                    $('#flash_div a').eq(1).html($(li).attr('data-title'));
                    $('div', '#flash_div').eq(0).html($(li).attr('data-price'));

                    // 显示出来的效果
                    $(this).animate({opacity:1}, 1000, function(){
                        $('#mainMidFlash_banner').css('background-image', backgroundImage).css('opacity', '1');
                    });

                });

                thisIndex = $(this).index()+1>=$('#showList li').length ? 0 :  $(this).index()+1;

                timer = window.setTimeout( "$('#showList li').eq(thisIndex).trigger('click') ", 5000 );
            });
        });
        var timer = window.setTimeout( "$('#showList li').eq(1).trigger('click')"  , 5000 );
    </script>

