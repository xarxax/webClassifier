		<script type="text/javascript">
			function initParallaxSlider() {
				$('#da-slider').cslider({
					current: 11,
					bgincrement: 50,
					autoplay: false,
					interval: 4000
				});
			}

			$(".da-arrows-next").click(function() {
				displayNextSliderImage();
			});

			$(".da-arrows-prev").click(function() {
				displayPreviousSliderImage();
			});

			jQuery(window).on("load", function() {
				for (var i = 0; i < 13; i++) {
					prefetchImage(i);
				}
			});
			
			function prefetchImage(i) {
				setTimeout(function() { loadSlideImage(i); }, 100 + 500 * i);
			}
			
			function displayNextSliderImage() {
				var elemId = obtainSlideId();
				var nextElemId = (parseInt(elemId) + 1) % 13; 
				loadSlideImage(nextElemId);
			}

			function displayPreviousSliderImage() {
				var elemId = obtainSlideId();
				var prevElemId = parseInt(elemId) - 1; 
				if (prevElemId === -1) {
					prevElemId = 12; 
				}
				loadSlideImage(prevElemId);
			}
			
			function obtainSlideId(){
				var elem = $(".da-slide-current img.da-bg"); 
				var elemId = elem.data('id'); 
				return elemId;
			}

			function loadSlideImage(imageId) {
				var nextElem = $(".da-slide").find('img[data-id=' + imageId + ']');
				if (nextElem.attr('data-src')) { 
					var src = nextElem.data('src'); 
					nextElem.attr('src', src); 
					nextElem.removeAttr('data-src'); 
				}
			}

		</script>
