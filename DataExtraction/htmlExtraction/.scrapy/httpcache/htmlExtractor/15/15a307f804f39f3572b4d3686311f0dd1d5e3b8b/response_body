	 var aux=0;
	 var vectImghead=new Array();
		function presentationScript(num){
			aux+=num; if(aux<0) aux=0; else if(aux>vectImghead.length) aux=vectImghead.length;
			
			if(vectImghead.length>0){
				for(i=0;i<vectImghead.length;i++){
					if(i==aux){
						$('liimgpresentation').innerHTML=vectImghead[i];
						break;
					}
				}
			}
		}
