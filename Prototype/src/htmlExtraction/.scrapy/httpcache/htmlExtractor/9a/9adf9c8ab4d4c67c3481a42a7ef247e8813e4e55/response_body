function popup(URL, WIDTH, HEIGHT){
 var features;
 features = 'toolbar=no,location=no,status=no,menubar=no,scrollbars=no,resizable=yes,width=' + WIDTH + ',height=' + HEIGHT + ',left=0,top=0'
 win = window.open(URL, "preview", features);
 win.focus();
} 
