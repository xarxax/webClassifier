function showContent(arg)
{
	document.getElementById('tab1').style.display="none";
	document.getElementById('tab2').style.display="none";
	document.getElementById('tab3').style.display="none";
	document.getElementById('tab4').style.display="none";
	
	document.getElementById('linktab1').className="tab";
	document.getElementById('linktab2').className="tab";
	document.getElementById('linktab3').className="tab";
	document.getElementById('linktab4').className="tab";
	
	document.getElementById(arg).style.display="block";
	document.getElementById('link'+arg).className="active";
}
</script>
