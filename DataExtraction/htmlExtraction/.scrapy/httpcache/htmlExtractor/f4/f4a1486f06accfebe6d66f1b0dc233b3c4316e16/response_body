    <script type="text/javascript">
        function disableHomeSearchGroup() {
            document.getElementById('ctl00_btnHomeSearch').disabled = true;
        }
        function enableHomeSearchGroup() {
            document.getElementById('ctl00_btnHomeSearch').disabled = false;
        }
        function createCookie(name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = "; expires=" + date.toGMTString();
            }
            else var expires = "";
            document.cookie = name + "=" + value + expires + "; path=/";
        }

        function readCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        var stillOver = false;
        function showSearchPopup() {
            try {
                stillOver = true;
                document.getElementById('SearchByTypeMenu').style.display = 'block';
                document.getElementById('SearchByTypeMenu').style.zIndex = '100000';
                document.getElementById('PopupSearchMenu').style.height = '220px';
            } catch (x) { }
        }

        function toggleSearch(obj) {
            //$(obj).toggleClass('blue');
            //$('#ctl00_HomeSearchBox').slideToggle("fast");
            $(".btn-search").click();
        }

        function toggleCart(obj) {
            $(obj).toggleClass('blue');
            $('.cart-options').slideToggle("fast");
        }

        function hideSearchPopup() {
            try {
                document.getElementById('SearchByTypeMenu').style.display = 'none';
                document.getElementById('SearchByTypeMenu').style.zIndex = '0';
                document.getElementById('PopupSearchMenu').style.height = '0px';
            } catch (x) { }
        }   

        function openMenu(obj) {
            //$(obj).next().slideToggle();
            document.getElementById("myNav").style.height = "100%";
        }

        function closeMenu(obj) {
            //$(obj).parent().parent().slideToggle();
            document.getElementById("myNav").style.height = "0%";
        }

        function menuCloseSoon() {
            stillOver = false;
            setTimeout("closeIfNotOver()", 1000);
        }

        function closeIfNotOver() {
            if (stillOver == false)
                hideSearchPopup();
        }

        function getObUpperLeft(obj) {
            var curLeft = curTop = 0;
            if (obj.offsetParent) {
                curLeft = obj.offsetLeft
                curTop = obj.offsetTop
                //alert("objName= " + obj.id + "  offsetLeft= " + obj.offsetLeft + "  offsetTop= " + obj.offsetTop + "  hasLayout= " + obj.currentStyle.hasLayout);
                while (obj = obj.offsetParent) {
                    //alert("objName= " + obj.id + "  offsetLeft= " + obj.offsetLeft + "  offsetTop= " + obj.offsetTop + "  hasLayout= " + obj.currentStyle.hasLayout);
                    curLeft += obj.offsetLeft
                    curTop += obj.offsetTop
                }
            }
            //alert(curLeft);
            return [curLeft, curTop];
        }

        function placeMenu() {
            try {
                if (document.getElementById('popmen') != null) {
                    document.getElementById('PopupSearchMenu').style.display = 'none';
                    return;
                }

                document.getElementById('PopupSearchMenu').style.display = 'block';

                lnk = document.getElementById('ctl00_CategoryPanel_RPC');
                mnu = document.getElementById('PopupSearchMenu');
                mnu.style.left = (getObUpperLeft(lnk)[0] + 25).toString() + 'px';

                hideSearchPopup();
            } catch (x) { }
        }

        
