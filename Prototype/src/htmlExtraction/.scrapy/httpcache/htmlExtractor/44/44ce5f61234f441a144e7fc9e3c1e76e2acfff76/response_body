                                        <script>
                                          $("#footerContactUsFormSubmit").click(function () {
                                            // If all form fields are valid...
                                            if ($("#Footer_Contact_Us_Form")[0].checkValidity()) {
                                              //...check the captcha
                                              var response = grecaptcha.getResponse();
                                              if(response.length == 0) {
                                                //... If the captcha is NOT valid, display a warning
                                                $("#recaptchaWarning").css('display', 'block');
                                                return false;
                                              } else {
                                                //... otherwise, submit the form
                                                document.forms["Footer_Contact_Us_Form"].submit();
                                              }
                                            }
                                          });
                                      </script>
