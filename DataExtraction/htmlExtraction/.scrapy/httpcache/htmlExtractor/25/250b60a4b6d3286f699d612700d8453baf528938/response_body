<script type="text/javascript">
    $(document).ready(function () {
        $("a[href='" + window.location.pathname + "']").parent().parent().parent().children("a").css("text-decoration", "underline");
        $("a[href='" + window.location.pathname + "']").parent().parent().parent().children("a").hover(
            function () {
                $(this).css("text-decoration", "");
            },
            function () {
                $(this).css("text-decoration", "underline");
            }
        );
        if (window.location.pathname == "/" || window.location.pathname == "/default.aspx") {
            $("a[href='/default.aspx']").css("text-decoration", "underline");
            $("#content").css("margin-top", "0px");
        }
        $("a[href='/default.aspx']").mouseenter(function () {
            $(this).css("background-image", "");
        });
    });
</script>

