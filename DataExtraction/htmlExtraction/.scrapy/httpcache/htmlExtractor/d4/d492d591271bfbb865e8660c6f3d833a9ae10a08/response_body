function showDistributionCenter(code)
	{
	//alert("hi");
	if(code==1)
		{
		//ocument.getElementById("foodservicearea").style.backgroundColor="#A3C0F1";
		document.getElementById("fsbig").src ="../images/spotlights/front/fservice-on.jpg";
		//document.getElementById("grbig").src ="../images/home/welcome/fservice-off.jpg";
		//document.getElementById("showWhat").src ="../images/home/welcome/showFoodService.jpg"; 
		document.getElementById("lowerCell").style.background="";
		//document.getElementById("grocerydiv").innerHTML = "<table width='100%' height='150' border='0' cellspacing='0' cellpadding='0'><tr><td valign='top'><div align='center'><img src='/public/images/gr/theevent.jpg' alt='' width='500' height='111'></p><p align='center'>Plan now on witnessing our spectacular series of events this upcoming Spring.</p></div></td></tr></table>";
		//document.getElementById("fserviceDesc").style.visibility = 'visible';
		document.getElementById("lowerDesc").innerHTML = "<table width='100%' height='150' border='0' cellspacing='0' cellpadding='0'><tr><td valign='top'><div align='center'><img src='/public/images/gr/theevent.jpg' alt='' width='500' height='111'></p><p align='center'>Plan now on witnessing our spectacular series of events this upcoming Spring.</p></div></td></tr></table>";
		document.getElementById("inthespotlight").innerHTML = "<div id='inthespotlight' align='center'><font color='#FFFFFF' size='3' face='Arial, Helvetica, sans-serif'><strong>Coming Soon</strong></font></div></td>";
		
		}
	if(code==2)
		{
		//document.getElementById("groceryarea").style.backgroundColor="#FFE8E8";
		document.getElementById("grbig").src ="../images/spotlights/front/grocery-on.jpg"; 
		//document.getElementById("fsbig").src ="../images/home/welcome/c-store-off.jpg";
